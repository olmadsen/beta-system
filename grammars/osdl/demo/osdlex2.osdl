--- osdlex2: sdlSpecification : osdl ---
system OSDL;
  signal s;
  process type test <signal s1>;
    virtual start;
      stop;
  endprocess;
  block a;
    process type testa <signal s2> inherits test <s2>;
    signalset s2;
      start;
      nextstate wait;
      state wait;
        input s2;
        nextstate -;
    endprocess;
    process t : testa<s>;
  endblock a;
endsystem;
