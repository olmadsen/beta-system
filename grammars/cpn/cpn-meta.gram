(* SBNF types used below *)

<coordinate>            ::= <minusOpt> <const>;
<dimension>             ::= <coordinate>;
<number>		::= <const>;
<minusOpt>		::? <minus>;
<minus>			::= '-';
<points>		::* <point>;
<point>			::= '(' <coordinate> ',' <coordinate> ')' ;
<boolean>		::| <true> | <false>;
<true>			::= 'TRUE';
<false>			::= 'FALSE';

(* Attributes *)

<att_id>		::= 'id' '=' <id:const>;
<att_orientation>       ::= 'orientation' '=' <arcors>;
