(* Possible font names. *)
<fonts>     ::| <Chicago> 
              | <Courier> 
              | <Geneva> 
              | <Helvetica> 
              | <Monaco>
              | <Times> 
              | <Symbol>;

<Chicago>   ::= 'Chicago'; 
