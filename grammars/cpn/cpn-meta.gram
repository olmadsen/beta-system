<att_type>		::= 'type' '=' <lintyps>;
<att_thick>		::= 'thick' '=' <number>;
<att_font>		::= 'font' '=' <fonts>;   
<att_just>		::= 'just' '=' <justs>;  
<att_bold>		::= 'bold' '=' <boolean>;
