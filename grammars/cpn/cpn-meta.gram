<att_points>		::= 'points' '=' '"' <points> '"' ;
<att_x>			::= 'x' '=' <coordinate>;
<att_y>			::= 'y' '=' <coordinate>;
<att_h>			::= 'h' '=' <dimension>;
<att_w>			::= 'w' '=' <dimension>;
<att_stangle>		::= 'stangle' '=' <number>;
