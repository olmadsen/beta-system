specialLexems
-- offmem: aGrammar : metagrammar --
Grammar offmem:

Option 
  version = 1
  comBegin = '/*'
  comEnd = '*/'
  stringChar = ''''
  suffix = '.offm'

     (* the predefined nonterminal nameDecl is unused *)
  nameDecl = unused 
  nameAppl=unused
  const=unused

  splitOnFiles = 3 (* the generated cfl will be split on 3 files *) 
Rule

   <Memo> ::=  '<Memo>' <ToAndFromList> <Body> <CloseOpt> '</Memo>'
	   ;
   <ToAndFromList> ::* <ToAndFrom>
                    ;
   <ToAndFrom> ::| <To> | <From>
		;
   <To> ::= '<To>' <string> '</To>'
	 ;
   <From> ::= '<From>' <string> '</From>'
	   ;
   <Body> ::= '<Body>' <Plist> '</Body>'
	   ;
   <PList> ::* <P>
            ;
   <P> ::= '<P>' <PelmList> '</P>'
        ;
   <PelmList> ::* <Pelm>
	       ;
   <Pelm> ::| <PelmString> | <Q> | <Pref>
	     ;
   <PelmString> ::=  <string> 
	        ;
   <Q> ::= '<Q>' <string> '</Q>'
	;
   <Pref> ::= 'EMPTY'
	   ;
   <CloseOpt> ::? <Close>
	       ;
   <Close> ::= '<Close>' <string> '</Close>'