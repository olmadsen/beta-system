ORIGIN '../objectserverdiradds';
INCLUDE '~beta/ntlib/v1.0/ntinterface';

--- objectserverdirectorychar:dopart ---
do '\' -> value;
   
--- objectservercurrentdir:dopart ---
do getCurrentDir -> dir[];
   (if dir.t[dir.lgth]//directoryChar then else
       dir.lgth -> dir.pos;
       directoryChar -> dir.put;
   if);
   
