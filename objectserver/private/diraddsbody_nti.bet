ORIGIN '../diradds';
INCLUDE '~beta/win32lib/ntinterface';
INCLUDE '~beta/sysutils/pathhandler';

--- objectserverdirectorychar:dopart ---
do '\\' -> value;
   
--- objectservercurrentdir:dopart ---
do getCurrentDir -> dir[];
   (if dir.t[dir.lgth] -> isDirectoryChar then else
       dir.lgth -> dir.pos;
       directoryChar -> dir.put;
   if);
   
