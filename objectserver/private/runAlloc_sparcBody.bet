INCLUDE '~beta/sysutils/objinterface';

--- lib:attributes ---

CAlloRR: External
  (# theObj: @Integer;
     offset: @Integer;
     range: @Integer;
     theRep: @Integer;
     i1,i3,i4: @Integer;
  enter (theObj, i1, offset, i3, i4, range)
  exit theRep
  #);

CAlloVR1: External 
  (# theObj: @Integer;
     offset: @Integer;
     range: @Integer;
     theRep: @Integer;
     i1,i3,i4: @Integer;
  enter (theObj, i1, offset, i3, i4, range)
  exit theRep
  #);

CAlloVR2: External 
  (# theObj: @Integer;
     offset: @Integer;
     range: @Integer;
     theRep: @Integer;
     i1,i3,i4: @Integer;
  enter (theObj, i1, offset, i3, i4, range)
  exit theRep
  #);

CAlloVR4: External 
  (# theObj: @Integer;
     offset: @Integer;
     range: @Integer;
     theRep: @Integer;
     i1,i3,i4: @Integer;
  enter (theObj, i1, offset, i3, i4, range)
  exit theRep
  #);

CAlloVR8: External 
  (# theObj: @Integer;
     offset: @Integer;
     range: @Integer;
     theRep: @Integer;
     i1,i3,i4: @Integer;
  enter (theObj, i1, offset, i3, i4, range)
  exit theRep
  #);

CAlloS: External
  (# origin: @Integer;
     i1: @Integer;
     protoAdr: @Integer;
     i3, i4: @Integer;
     objAdr: @Integer;
  enter (origin, i1, protoAdr, i3, i4)
  exit objAdr
  #);

CAlloH: External
  (# origin: @Integer;
     i1: @Integer;
     protoAdr: @Integer;
     i3, i4: @Integer;
     objAdr: @Integer;
  enter (origin, i1, protoAdr, i3, i4)
  exit objAdr
  #);

--- allocByteRepBody:dopart ---
do ((%getLongAt(@@ori)), 0, offset, 0, 0, high) -> CAlloVR1;
(* Above line tos_converted from: do (@@ori -> tos'%adrGetLong', 0, offset, 0, 0, high) -> CAlloVR1; *)
--- allocWordRepBody:dopart ---
do ((%getLongAt(@@ori)), 0, offset, 0, 0, high) -> CAlloVR2;
(* Above line tos_converted from: do (@@ori -> tos'%adrGetLong', 0, offset, 0, 0, high) -> CAlloVR2; *)
--- allocValRepBody:dopart ---
do ((%getLongAt(@@ori)), 0, offset, 0, 0, high) -> CAlloVR4;
(* Above line tos_converted from: do (@@ori -> tos'%adrGetLong', 0, offset, 0, 0, high) -> CAlloVR4; *)
--- allocDoubleRepBody:dopart ---
do ((%getLongAt(@@ori)), 0, offset, 0, 0, high) -> CAlloVR8;
(* Above line tos_converted from: do (@@ori -> tos'%adrGetLong', 0, offset, 0, 0, high) -> CAlloVR8; *)
--- allocRefRepBody:dopart ---
do ((%getLongAt(@@ori)), 0, offset, 0, 0, high) -> CAlloRR;
(* Above line tos_converted from: do (@@ori -> tos'%adrGetLong', 0, offset, 0, 0, high) -> CAlloRR; *)

--- allocStructureBody:dopart ---
do ((%getLongAt(@@iOrigin)), 0, iProto, 0, 0) -> CAlloS -> structAdr;
(* Above line tos_converted from: do (@@iOrigin -> tos'%adrGetLong', 0, iProto, 0, 0) -> CAlloS -> structAdr; *)
   (((%getLongAt(@@obj))) + offset, structAdr) -> AssignRef;
   (* Above line tos_converted from: ((@@obj -> tos'%adrGetLong') + offset, structAdr) -> AssignRef; *)

--- allocHeapBody:dopart ---
do 
   (0, 0, proto, 0, 0) -> CAlloH -> objAdr;
   (@@o, objAdr) -> assignRef;
