ORIGIN '../diradds';
INCLUDE '~beta/maclib/v3.1/macdirectory';

--- objectserverdirectorychar:dopart ---
do ':'->value;
   
--- objectservercurrentdir:dopart ---
do getCurrentDir -> dir[];
   (if dir.t[dir.lgth]<>directoryChar then
       dir.lgth -> dir.pos;
       directoryChar -> dir.put;
   if);
