ORIGIN 'execGroupTableBody';

--- segtePTtoInx:dopart ---
do (if len > 0 then
       0 -> low;
       len-1 -> high;
       loop:
		(for i: len repeat
			(if (ptListAdr + 4*(i - 1) -> tos'%adrGetLong') = ptAdr then
				(i - 1) -> inx;
				leave loop;
			if);
		for);
    else
       -1 -> inx;
   if)
