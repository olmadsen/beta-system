MDBODY mac     'private/filebin_macBody'
       ppc     'private/filebin_macBody'
       ppcmac  'private/filebin_macBody'
       nti     'private/filebin_ntiBody'
       default 'private/filebin_unixBody';
       
--- FileLib:Attributes ---

binGetBytes:
  (# repAdr: @Integer; (* Address of buffer *)
     length: @Integer; (* In bytes *)
  enter (repAdr,length)
  <<SLOT filebinGetBytes:dopart>>
  exit length
  #);
binPutBytes:
  (# repAdr: @Integer; (* Address of buffer *)
     length: @Integer; (* In bytes *)
  enter (repAdr,length)
  <<SLOT filebinPutBytes:dopart>>
  exit length
  #);
