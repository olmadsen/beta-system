ORIGIN 'STL'
---lib:attributes---
sort: 
  (# A: ^container; swapped: @boolean
  enter A[]
  do false -> swapped;
     ((A.begin).next,A.end) -> A.scan
     (#
     do (if ((currentIT.pred).val,current[]) -> A.greater then
            (currentIt.pred,currentIt[]) -> A.swap;
            True -> swapped
        if)
     #);
     (if swapped then restart sort if)
  #)
 
