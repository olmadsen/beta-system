ORIGIN '../STL';
INCLUDE '../Sort'
---program:descriptor---
(# Register:
     (# elements:
          (# name: ^text;
          enter name[]
          exit this(elements)[]
          #);
        Cont: @ List
          (# elm:: elements;
             greater::
               (#
               do left.name[] -> right.name.greater -> value
               #)
          #);
        insert: (# enter elements -> Cont.insert #);
        doSort: (# do Cont[] -> Sort #);
        print: (# do Cont.scan(#do current.name[] -> putline #)#)    
     #);
   R: @Register;
do 'Marianne' -> R.insert;
   'Kirsten' -> R.insert;
   'Johanna' -> R.insert;
   'Lis' -> R.insert;
   R.doSort;
   R.print   
#)

