XTYP_CONNECT: (# exit 16x0060 + XCLASS_BOOL + XTYPF_NOBLOCK #);
XTYP_CONNECT_CONFIRM: (# exit 16x0070 + XCLASS_NOTIFICATION + XTYPF_NOBLOCK #);
XTYP_XACT_COMPLETE: (# exit 16x0080 + XCLASS_NOTIFICATION   #);
XTYP_POKE: (# exit 16x0090 + XCLASS_FLAGS          #);
XTYP_REGISTER: (# exit 16x00A0 + XCLASS_NOTIFICATION + XTYPF_NOBLOCK #);
XTYP_REQUEST: (# exit 16x00B0 + XCLASS_DATA           #);
XTYP_DISCONNECT: (# exit 16x00C0 + XCLASS_NOTIFICATION + XTYPF_NOBLOCK #);
XTYP_UNREGISTER: (# exit 16x00D0 + XCLASS_NOTIFICATION + XTYPF_NOBLOCK #);
