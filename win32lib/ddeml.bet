XCLASS_DATA: (# exit 16x2000 #);
XCLASS_FLAGS: (# exit 16x4000 #);
XCLASS_NOTIFICATION: (# exit 16x8000 #);

XTYP_ERROR: (# exit 16x0000 + XCLASS_NOTIFICATION + XTYPF_NOBLOCK  #);
