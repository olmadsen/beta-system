(* WINUSERAPI DWORD WINAPI WaitForInputIdle(HANDLE hProcess, DWORD dwMilliseconds); *)
WaitForInputIdle: external
  (# hProcess,milliSeconds: @integer;
     waitResult: @integer;
  enter (hProcess,milliSeconds) 
  do callStd;
  exit waitResult
  #);
