-- Lib: Attributes --

(******* Catch/Throw and stack management ************************************)

(* int     WINAPI Catch(int FAR* ); *)
Catch: external
  (#
     in0: @integer; (* int FAR* *)
     out: @integer; (* int *)
  enter in0
  do callStd;
  exit out
  #);

(* void    WINAPI Throw(const int FAR*, int); *)
Throw: external
  (#
     in0: @integer; (* const int FAR* *)
     in1: @integer; (* int *)
  enter (in0,in1)
  do callStd;
  #);

(* void    WINAPI SwitchStackBack(void); *)
SwitchStackBack: external
  (#
  do callStd;
  #);

(* void    WINAPI SwitchStackTo(UINT, UINT, UINT); *)
SwitchStackTo: external
  (#
     in0: @integer; (* UINT *)
     in1: @integer; (* UINT *)
     in2: @integer; (* UINT *)
  enter (in0,in1,in2)
  do callStd;
  #);
