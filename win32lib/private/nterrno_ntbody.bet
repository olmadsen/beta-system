--- lib:attributes ---

notOnThisArch:
  (#
     exc: @exception;
     symbol: ^Text;
     
     enter symbol[]
  do
     'C-level symbol "%s" not defined on this architecture'
       -> exc.msg.putformat
     (#
     do symbol[]->s;
     #);
     exc;
  #);

