(****** Network support *****************************************************)

(* UINT WINAPI WNetAddConnection(LPSTR, LPSTR, LPSTR); *)
WNetAddConnection: external
  (#
     in0: [1] @char; (* LPSTR *)
     in1: [1] @char; (* LPSTR *)
     in2: [1] @char; (* LPSTR *)
     out: @integer; (* UINT *)
  enter (in0,in1,in2)
  do callStd;
  exit out
  #);

(* UINT WINAPI WNetGetConnection(LPSTR, LPSTR, UINT FAR* ); *)
WNetGetConnection: external
  (#
     in0: [1] @char; (* LPSTR *)
     in1: [1] @char; (* LPSTR *)
     in2: @integer; (* UINT FAR* *)
     out: @integer; (* UINT *)
  enter (in0,in1,in2)
  do callStd;
  exit out
  #);

(* UINT WINAPI WNetCancelConnection(LPSTR, BOOL); *)
WNetCancelConnection: external
  (#
     in0: [1] @char; (* LPSTR *)
     in1: @boolean; (* BOOL *)
     out: @integer; (* UINT *)
  enter (in0,in1)
  do callStd;
  exit out
  #);
