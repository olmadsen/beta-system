(****** Profiling support ***************************************************)

(* int     WINAPI ProfInsChk(void); *)
ProfInsChk: external
  (# out: @integer; (* int *)
  do callStd;
  exit out
  #);

(* void    WINAPI ProfSetup(int,int); *)
ProfSetup: external
  (# in0: @integer; (* int *)
     in1: @integer; (* int *)
  enter (in0,in1)
  do callStd;
  #);

(* void    WINAPI ProfSampRate(int,int); *)
ProfSampRate: external
  (# in0: @integer; (* int *)
     in1: @integer; (* int *)
  enter (in0,in1)
  do callStd;
  #);

(* void    WINAPI ProfStart(void); *)
ProfStart: external
  (#
  do callStd;
  #);

(* void    WINAPI ProfStop(void); *)
ProfStop: external
  (#
  do callStd;
  #);

(* void    WINAPI ProfClear(void); *)
ProfClear: external
  (#
  do callStd;
  #);

(* void    WINAPI ProfFlush(void); *)
ProfFlush: external
  (#
  do callStd;
  #);

(* void    WINAPI ProfFinish(void); *)
ProfFinish: external
  (#
  do callStd;
  #);
