ORIGIN '~beta/newmps/astlevel';
INCLUDE 'unparser';

BODY 'private/astviewerbody';



-- AstInterfaceLib: Attributes --

AstViewer:
  (# <<SLOT AstViewerLib: Attributes>>;
     width: @integer;
     
     init:
       (# grammarname: ^Text;
          filename: ^Text;
       enter (grammarname[], filename[])
       <<SLOT AstViewerInit: doPart>>
       #);
     
     printOn:
       (# node: ^AST;
          output: ^Printer;
       enter (node[], output[])
       <<SLOT AstViewerPrintOn: doPart>>
       #);
     printFragmentOn:
       (# frag: ^FragmentForm;
          output: ^Printer;
       enter (frag[], output[])
       <<SLOT AstViewerPrintFragmentOn: doPart>>
       #);
     printFragmentGroupOn:
       (# group: ^FragmentGroup;
          output: ^Printer;
       enter (group[], output[])
       <<SLOT AstViewerPrintFragmentGroupOn: doPart>>
       #);
     
     print:
       (# node: ^AST;
          output: ^Stream;
       enter (node[], output[])
       <<SLOT AstViewerPrint: doPart>>
       #);
     printFragment:
       (# frag: ^FragmentForm;
          output: ^Stream;
       enter (frag[], output[])
       <<SLOT AstViewerPrintFragment: doPart>>
       #);
     printFragmentGroup:
       (# group: ^FragmentGroup;
          output: ^Stream;
       enter (group[], output[])
       <<SLOT AstViewerPrintFragmentGroup: doPart>>
       #);
     
     printAsHTML:
       (# node: ^AST;
          output: ^Stream;
       enter (node[], output[])
       <<SLOT AstViewerPrintAsHTML: doPart>>
       #);
     printFragmentAsHTML:
       (# frag: ^FragmentForm;
          output: ^Stream;
       enter (frag[], output[])
       <<SLOT AstViewerPrintFragmentAsHTML: doPart>>
       #);
     printFragmentGroupAsHTML:
       (# group: ^FragmentGroup;
          output: ^Stream;
       enter (group[], output[])
       <<SLOT AstViewerPrintFragmentGroupAsHTML: doPart>>
       #);
     
     
     gram: ^TreeLevel;
     unparser: ^TreeLevel.UnParser;
  #);

