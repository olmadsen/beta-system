ORIGIN '~beta/guienv/v1.6/guienv';

--- guienvlib:attributes ---

getIconDB: objectPool.get
  (# type::< iconDB; init::< (# do obj.init; INNER #)#);

iconDB:
  (# init:< Object;
     getIcon:<
       (# name: ^Text;
          theIcon: ^pixmap;
       enter name[]
       do INNER;
          (if theIcon[]=NONE then
              'FATAL ERROR!! iconDB.getIcon: Icon "' -> putText;
              name[]->putText;
              '" not found !!!' -> putLine;
              (failuretrace,'')->stop;
          if);
       exit theIcon[]
       #)
  #)
