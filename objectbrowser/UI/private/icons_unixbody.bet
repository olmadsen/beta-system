ORIGIN '../icons';
INCLUDE '~beta/sysutils/v1.6/envstring';
--- psbrowserMakeIconFileName:dopart ---
do 
   '$(BETALIB)/bitmap/objectbrowser/'->fileName[];
   iconName[]->fileName.append;
   '.xbm'->fileName.append;
   fileName[] ->expandEnvVar
   (# defaultvalue::
        (# do '/usr/local/lib/beta'->envvarvalue[] #) 
   #) -> fileName[]
   
