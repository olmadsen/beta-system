-- graphicalEditorGetStripes: doPart --
do  (# getStripes1: external
         (# disp: @integer;
            pix: @integer;
         enter disp
         exit pix
         #);
       getStripes2: external
         (# disp: @integer;
            pix: @integer;
         enter disp
         exit pix
         #);
       getStripes3: external
         (# disp: @integer;
            pix: @integer;
         enter disp
         exit pix
         #);
       getStripes4: external
         (# disp: @integer;
            pix: @integer;
         enter disp
         exit pix
         #);
       getStripes5: external
         (# disp: @integer;
            pix: @integer;
         enter disp
         exit pix
         #);
       getStripes6: external
         (# disp: @integer;
            pix: @integer;
         enter disp
         exit pix
         #);
       getStripes7: external
         (# disp: @integer;
            pix: @integer;
         enter disp
         exit pix
         #);
       getStripes8: external
         (# disp: @integer;
            pix: @integer;
         enter disp
         exit pix
         #);
    do
       &bitmap[] -> stripes[1][];
       display -> getStripes1 -> stripes[1].rasterPrivatePart.xpixmap;
       &bitmap[] -> stripes[2][];
       display -> getStripes2 -> stripes[2].rasterPrivatePart.xpixmap;
       &bitmap[] -> stripes[3][];
       display -> getStripes3 -> stripes[3].rasterPrivatePart.xpixmap;
       &bitmap[] -> stripes[4][];
       display -> getStripes4 -> stripes[4].rasterPrivatePart.xpixmap;
       &bitmap[] -> stripes[5][];
       display -> getStripes5 -> stripes[5].rasterPrivatePart.xpixmap;
       &bitmap[] -> stripes[6][];
       display -> getStripes6 -> stripes[6].rasterPrivatePart.xpixmap;
       &bitmap[] -> stripes[7][];
       display -> getStripes7 -> stripes[7].rasterPrivatePart.xpixmap;
       &bitmap[] -> stripes[8][];
       display -> getStripes8 -> stripes[8].rasterPrivatePart.xpixmap;
    #)
   
--- obGrabPointer:descriptor ---
(# status: @Integer;
   mask: @Integer;
do
   XButtonReleaseMask + XButtonPressMask + XPointerMotionMask -> mask;
   (if widgetID>0 then
       (widgetID,
       0,
       mask,
       grabModeASync,
       grabModeAsync,
       0,
       0,
       currentTime)->XtGrabPointer->status;
       (if status
        // 0 then 
        // 1 then
           'Failed to grab X Pointer: allready grabbed' -> putline;
        // 2 then
           'Failed to grab X Pointer: invalid time' -> putline;
        // 3 then
           'Failed to grab X Pointer: not viewable' -> putline;
        // 4 then
           'Failed to grab X Pointer: frozen' -> putline;
       if);
   if);
#)

--- obUnGrabPointer: descriptor ---
(# do (widgetID,currentTime) -> XtUnGrabPointer #)
