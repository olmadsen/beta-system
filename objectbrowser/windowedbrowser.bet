ORIGIN '~beta/guienv/v1.6/guienv';
INCLUDE 'mpsinterface'
        'localdumper'
        'UI/objectview';
-- windowLib: Attributes --
windowedbrowser:
  (#
     viewFather: ^Canvas;
     mpsinterfaceType:< mpsinterface
       (#
          od: @localdumper;
          init::<  (#  do (AST[],BETACFL[])->od.init;  #)
       #);
     objectViewType:<
     (* Furtherbind to specify precise type of moveableObjectView
      * to be used for new objectrepresentations in the objectbrowser
      *) moveableObjectView;
     be: @mpsinterfaceType;
     init:< (#  enter viewFather[] do be.init; INNER ;  #);
     showObject:
       (# obj: ^object; bo: ^be.localBetaObject; 
       enter obj[]
       do
          (be.od[],obj[])->be.newLocalObject->bo[];
          (bo[],FALSE)
            ->OpenMoveableObjectView
              (#
                 moveableObjectViewType:: ObjectViewType;
                 getFather::  (#  do viewFather[]->father[] #)
              #)
       #);
     
  #);
  

