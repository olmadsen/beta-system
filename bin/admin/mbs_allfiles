#!/usr/local/bin/perl5 -s
#-*-Perl-*-

require "dirlist";

foreach $dir (@Dirs) {
    if (-d &path("$betalib/$dir")) {
	@todo = ("$betalib/$dir");
	while ($dir = shift @todo) {
	    opendir (DIR, $dir) or die "Unable to readdir: $!";
	    foreach (readdir(DIR)) {
		next if ($_ eq "." || $_ eq "..");
		if (-d "$dir/$_") {
		    push (@todo, "$dir/$_");
		} else {
		    print "$dir/$_\n";
		}
	    }
	}
    }
}
