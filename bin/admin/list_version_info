#!/bin/sh

cd /users/beta
dirs=`ls -F | egrep '/|@' | sed -e s%@%/%gp | \
grep -v GNU/ | \
grep -v OldStuff/ | \
grep -v X11/ | \
grep -v archive/ | \
grep -v bin/ | \
grep -v bitmap/ | \
grep -v configuration/ | \
grep -v contrib/ | \
grep -v demo/ | \
grep -v doc/ | \
grep -v dprog2/ | \
grep -v export/ | \
grep -v miadoc/ | \
grep -v patch/ | \
grep -v projects/ | \
grep -v public_html/ | \
grep -v quark/ | \
grep -v statistik/ | \
grep -v test/ `

for d in $dirs
do
  #echo $d
  ls -l ${d}VERSION_INFO
done
