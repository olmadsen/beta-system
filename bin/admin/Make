#!/bin/sh

# used by runtime system generators - do not delete.

case "`uname -m`" in
	sun4*)
		case "`uname -r`" in
		5.[234])
			PATH=$PATH:/users/beta/GNU/bin/sun4s:/usr/ccs/bin:/usr/sbin
			#echo sun4s
			;;
		4.1*) 
			PATH=$PATH:/users/beta/GNU/bin/sun4
			#echo sun4
			;;
		*)	
			;;
		esac
		;;
	IP22)
		PATH=$PATH:/users/beta/GNU/bin/sgi:/usr/sbin:/usr/bsd
		;;
	*)
		;;
esac
export PATH
BETALIB=/users/beta
export BETALIB

echo $0: Entering \"`pwd`\"
make $* 2>&1