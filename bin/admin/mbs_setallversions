#!/usr/local/bin/perl5 -s
#-*-Perl-*-

push(@INC, $ENV{'BETALIB'} . "/bin/admin");

require "dirlist";

foreach $dir (@Dirs) {
    if ($dir =~ /^([^\/]+)\/(v\d+\.\d+)/) {
	$Versions{$1} = $2;
    } else {
	print "Problem: $dir\n";
    }
}

foreach $mod (sort keys %Versions) {
    $vstring .= "$mod $Versions{$mod} ";
}

print "\n$vstring\n";
