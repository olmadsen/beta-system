ORIGIN 'sematt';
LIB_ITEM 'betacompiler';
BODY 'nxallocbody'
---semattlib:attributes---
nxalloc:
  (# init: (# first,last: @integer 
           enter(first,last) <<SLOT nxAlloc_init:doPart>> 
           #);
     alloc: integervalue(# <<SLOT nxAlloc_alloc:doPart>> #);
     tmpAdr: 
       (# inx,off: @integer
       enter inx
       <<SLOT nxAlloc_tmpAdr:doPart>>
       exit off
       #);
     deAlloc: 
       (# tmpInx: @integer 
       enter tmpInx <<SLOT nxAlloc_deAlloc:doPart>> 
       #);
     rep: @ <<SLOT nxAlloc_rep:descriptor>>
  #);
nxDepth:
  (# testAndCheckDesc:<
       (# desc: ^ASTindex
       enter desc[]
       do INNER
       #);
     desc: ^ASTindex;
     isEnter: @boolean;
     chain: ^BV.superChain;
     NXlevel: @integer;   
     hasCode: @boolean
  enter(desc[],isEnter,chain[])
  <<SLOT nxDepth:doPart>>
  exit(nxLevel,hasCode)
  #);

 
