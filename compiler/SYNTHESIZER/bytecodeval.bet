     asgToRefAdr::
       (# sourceClassName,destClassName: ^text;
          dDesc,destEvalname,SORT: @ASTindex; RC: ^DH.superChain;
          rA: ^mch.address; destEval,dcl: @ASTindex; 
          rAx: @mch.RegAdr
       do (if stest then
              301->trace(#
                        do 'asgToRefAdr:' -> display; destEv[] -> xA
          #)if);
          destEv.son -> destEval;
          (if destEv.label = gram.structureReference then
              sematt.structureDesc -> dDesc;
           else
              (rChain[],destEval) 
                -> DH.theDesc 
                ->(dDesc,RC[]);
          if);
          destEval -> sematt.getName -> destEvalName;
          destEvalName.dclref -> dcl;
          dcl.sort -> sort;
          (if (sort.label = gram.repetitionDecl) 
              and 
              ((destEvalName.label=gram.nameApl)
                or 
              (destEvalName.label=gram.remote))
              then
              dDesc[] 
                -> theGen.mkArraySignature 
                -> sourceClassName[] 
                -> destClassName[]
           else
              dDesc[] 
                -> theGen.mkSignature 
                -> sourceClassName[] 
                -> destClassName[]
          if);
          (rAbase.copy,destEval[],false,rChain[])->genAdr->rA[];
          sourceClassName[] -> pushNext;
          dcl.gettext -> rA.fieldName[];
          rAx.mkCall; (* signal that register is on stack *)
          (rAx[],false,sourceClassName[],destClassName[])
            -> rA.asgRefAdr;
          (destEV[],rChain[]) -> asComputedRefVal -> V[]
       #);
     asgToRepAdr::
       (# sourceClassName,destClassName: ^text;
