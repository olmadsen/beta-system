       do '!EnterArgs:asgToRepAdr' -> mch.comment;
          (if true then
              rA.fieldType[] 
                -> sourceClassName[] 
                -> destClassName[]
                -> pushNext; 
           else
              '[C' -> pushNext; 
              '[C' -> sourceClassName[] -> destClassName[];
          if);
          Ax.mkCall; (* signal reg on stack *)
          (Ax[],false,sourceClassName[],destClassName[])
            -> rA.asgRefAdr
