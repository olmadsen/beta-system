              true -> rA.noTmp;
              (0,0) -> mch.duplicate; (* for execution of do-method *)
          if);
          (rDesc[],rChain[],false(*?*)) -> sig.setMethodDesc;
          rDesc[] -> sig.addEnter;
          sig.exitVoid;
          common.enterMethod -> sig.addMethod;  

          sig.scanEnter
