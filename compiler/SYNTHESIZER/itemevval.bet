loadReceiver:
  (# A: ^mch.address; desc: ^ASTindex; descKind: @integer
  enter(A[],desc[],descKind)
  do (if not A.noTmp then
         '!exeobj:push receiver:'->mch.comment;
         true -> A.isRef;
         (* Handle possible virtual desc *) 
         (if A.fieldType[] <> none then
             (# S,Q: ^text
             do A.fieldType[] -> S[];                
                (* we may need a cast - make this
                 * much more efficient - we should
                 * probably have Q as part of A
                 *)
                (if descKind = gram.unExpanded then
                    (* we should make a real solution here;
                     * A should contain cast info
                     *)
                    none -> Q[]
                 else
                    desc[]->theGen.mkSignature -> Q[];
                    (if S[]->Q.equal then none -> Q[] if);
                if);
                (A[],S[],Q[]) -> mch.loadRef
             #)
          else (* should never be none *)
             (A[],desc[]->theGen.mkSignature,none) -> mch.loadRef; 
         if);
         true -> A.noTmp
     if)     
  #);
