ORIGIN 'tstenv';      
BUILD javabc 'javabc/BetaArray.class' 'BetaArray.java' 'javac -O -d javabc -classpath javabc $1'
      dotnet 'dotnet/BetaArray.dll' 'BetaArray.cs' 'csc  -nologo -t:library -debug -optimize -out:$0 $1';
-- program: Descriptor --
(# r: [1]@char;
   r2, r3, r4: [0]@char;
do 'a' -> r[1];
   2->r.extend;
   'b' -> r[2];
   r[1] -> put; (* 'a' *)
   r[2] -> put; (* 'b' *)
   newline;
   r -> r2; (* FIXME: CopyVR1 is NOT called!! Becomes array reference assignment *)
   r2[1] -> put; (* 'a' *)
   r2[2] -> put; (* 'b' *)
   newline;
   r[1:1] -> r3; (* FIXME: CopySVR1 is NOT called!! Becomes array reference assignment *)
   r3[1] -> put; (* 'a' *)
   newline;
   r[2:2] -> r4; (* FIXME: CopySVR1 is NOT called!! Becomes array reference assignment *)
   r4[1] -> put; (* 'b' *)
   newline;
#)  

