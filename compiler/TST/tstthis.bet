     do
        (# x: @char
        do (# y: @char do '0' -> this(foo1).ch #)
        #);
        ch -> put;
        ch+1 -> ch;
        copy
     #);
   foo2: 
     (# ch: @char
       <<SLOT foo2:doPart>>
     #);
   
do 'abcdef'->X.T; X->R[]; 
