 	 a3: (# R: ^ P do L: A(#do this(p)[]->R[] #) exit R[] #);
         T: @ text; S: ^object 
      enter T
      do  T->putText;
          this(P)[]-> S[]    
      exit S[]
      #); 
   A: (# do INNER #);   
   T1: (#do'q'->put #); 
