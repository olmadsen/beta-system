ORIGIN 'tstenv';

--CstructLib: attributes--

Byte: (* Used for declaring CStruct fields *) 
  (# p: @pos; pos:< IntegerObject; val: @integer;
     set: @
       (# 
       enter val 
       do (val,p)->R.%PutByte
       #);
  enter set 
  exit p -> R.%GetByte
  #);  
Long: (* Used for declaring CStruct fields *)
  (# p: @pos; pos:< IntegerObject; val: @integer;
     set: @
       (# 
       enter val 
       do (val,p) -> R.%PutLong;
       #);
  enter set   
  exit p -> R.%GetLong
  #);
