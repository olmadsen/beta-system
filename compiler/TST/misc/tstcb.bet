ORIGIN '../tstenv'; 
(* Test intensive immediate callback.
 * Should be able to run in IOA=1K.
 *)

OBJFILE default	'../$/ctstcproc.o';
ON 13;
MAKE    default '../tstMake.make';
[[
-----program:descriptor-----
(# pp: @put;
   doCallBar: External(# F: ##barbar enter F## #);
   barbar: External
     (# 
     do cExternalEntry;
        '7'->pp;
     #);
do
   loop: (# do barbar##->doCallBar; '2'->pp; restart loop#)
#)
---]]
