ORIGIN '../tstenv';
MAKE default '../tstMake.make';
INCLUDE '../tstcomp'
        '../tstref'
        '../tstrep'
        '../tstrefrep'
        '../tstrelop'
        '../tstbool'
        '../tstarith'
        '../tstcontrol'
        '../tstblock'
        '../tstoldprim'
        '../tstnewprim'
        '../tstnx'
        '../tstnx2'
        '../tstnx3'
        '../tstvirt'
        '../tstvirt2'
        '../tstvarptn'
        '../tstthis'
        '../tstcproc'
        '../tstreal'
        '../tstdopart'
        (* '../tstmainp' FEJL i objectsize *)
        '../tstcompref'
        '../tstcomprem2'
        '../tstlvra'
        '../tstbigobject'
        (*'../tstgeneralrep'*)
        '../tstrefrep'
        '../tstnx3';

-----program:descriptor---
(# 
   putint:
     (# i: @integer
     enter i
     do (if i > 9 // true then i div 10 -> &putint if);
        (i mod 10) + '0' -> put;
     #);

   i: @integer;
   r: [1]@char; (* Quickly ends up in AOA *)
   lvra: [1]@integer;
   lvra2: [1]@integer;
   lvra3: [1]@integer;
do
   loop:
     (# do
        'xyxyxyxyxyxyxyxyxy' -> r; (* Force reference from AOA to IOA *)
        '\n--------------------------' -> putText;
        i+1 -> i -> putInt;
        newline;
        250 -> lvra.new;  (* generates lvra object, referenced from AOA *)
        300 -> lvra2.new; (* generates lvra object, referenced from AOA *)
        350 -> lvra3.new; (* generates lvra object, referenced from AOA *)
        0 -> Indent;
        'Repetition'->newl;     TstRep;
        'Reference'->newl;	TstRef;
        'Ref. rep'->newl;	TstRefRep;
        'RelOp'->newl;	   	TstRelOp;
        'BoolOp'->newl;	        TstBool;
        'ArithOp'->newl;        TstArith;
        'Control'->newl;	tstFor;
        newl;	                TstIf;
        'Case'->newl;	        TstCase;
        'Block'->newl;	        TstBlock;
        'Comp'->newl;	        TstComp;
        'InlinePrim'->newl;	TstOldPrim;
        'NewPrim'->newl;	TstNewPrim;
        'NX'->newl;	        TstNX;
        'NX2'->newl;	        TstNX2;
        'NX3'->newl;	        TstNX3;
        'Virtual'->newl;	TstVirt;
        newl;	                tstvirt2;
        'VarPtn'->newl;	        TstVarPtn;
        'This'->newl;	        TstThis;
        'Real'->newl;	        TstReal;
        'DoPart'->newl;	        TstDoPart;
        (*'MainPart'->newl;	TstMainPart;*)
        'CompRef'->newl;	TstCompRef;
        'CompRem2:'->newl;	TstCompRem2;
        'LVRA'->newl;	        TstLVRA;
        'BigObject'->newl;	TstBigObject; 
        'CProc'->newl;          TstCProc;
        (* 'GeneralRep'->newl;     tstgeneralrep; *)
        
        (if i < 100 then restart loop if);
     #);
#)
