---lib:attributes----
xcons: externalClass
  (# init: cons
       (# a,b: @integer;
          V: ^xcons
       enter(a,b)
       exit V[]
       #);
     new: cons  
       (# a,b,c: @integer;
          V: ^xcons 
       enter(a,b,c) 
       exit V[]
       #); 
     foo: proc(#       #); 
     bar: (#  #) 
  do 
  #);
ycons: xcons 
  (# gen: cons
       (# V: ^ycons
       do
       exit V[]
       #);
     fisk: 
       (# 
       do 'fisk'->putline
       #)
  #)
      
