ORIGIN 'tstenv'
[[--include 'tstlib'
----LIB:attributes---
TstRelOp:
(# x,y: @integer; b: @boolean; ch,c1,c2: @char;
   qqq: (# do exit X #)
do 'a'->ch; 
   333->x; 1011->y; (x=y)->b; (if b then (ch+1)->put else ch->put if);
   333->y; (x=y)->b; (if b then (ch+1)->put else ch->put if);
   ch+2->ch;
   (x=111)->b; (if b then (ch+1)->put else ch->put if);
   (x=333)->b; (if b then (ch+1)->put else ch->put if);
   ch+2->ch;
   111->y; (x<y)->b; (if b then (ch+1)->put else ch->put if);
   555->y; (x<y)->b; (if b then (ch+1)->put else ch->put if);

   (x=y) and (y=x)->b; (b,false,'g')->chk;
   (if ((x=x) and (y=y))// true then 'h'->put else '!'->put if);
   (if true 
    // (x=y) and (y=x) then '?'->put 
    // (x<>y) and (y<>x) then 'i'->put
    // (x+y) = (y-x) then '?'->put
    // (x-y) = (y+x) then '?'->put
    else '!'->put   
   if);

    999->X; (X=QQQ,true,'j')->chk; (QQQ=X,true,'k')->chk;
   ((x-14)<QQQ,true,'l')->chk; (QQQ>(x-15),true,'m')->chk;
   
   65->c1; 255->c2;
   (if (c1 < c2) // true then 'n'->put else '!'->put if);
   (for i: 255 repeat
        i-1->c1;
        (if c1<c2 // true then else '!'->put if);
        (if c2>c1 // true then else '!'->put if);
        i->c1;
        (if c1<=c2 // true then else '!'->put if);
        (if c2>=c1 // true then 'o'->ch else '!'->put if);
   for);
   ch->put;
   'pqrstuvwxyz0123456789'->puttext
#)
---]]

