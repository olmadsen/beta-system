origin '../tstenv';
-- program: descriptor --
(# 
   a: @|(# p: (# do inner #);
           pp: @p(# x, y: @integer;
                 do 0x7fffffff-10 -> x (* maxint-10 *);
                    -20 -> y;
                    x - y -> x;
                    (* ArithErr: Arithmetic Exceoption *)
                    x -> putint; newline;
                 #);
        do pp
        #);
   b: @|(# q: (# do inner #);
           qq: @q(# do a #);
