--PROGRAM:descriptor--
(# t: @text;
   a: @|(# p: (# do inner #);
           pp: @p
             (# 
             do t -> ident -> t; (* CTextPoolErr *)
             #);
        do pp
        #);
   b: @|(# q: (# do inner #);
           qq: @q(# do a; #);
        do qq
        #);
   
   printf1: external
     (# f: [1]@char; i:@Integer;
     enter f
     do 'printf'->callC;
     exit i
     #)
        
do (for 110 (* 10 *) repeat
        '1234567890' -> t.append;
   for);
   b;
   
