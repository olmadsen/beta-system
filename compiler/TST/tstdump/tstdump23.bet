ORIGIN '~beta/basiclib/betaenv'; 


BUILD  ppcmac	'::$$:ctstcproc.obj' '::ctstcproc.c'  'mrc -o $0 $1'
       nti      '..\$$\ctstcproc.obj' '..\ctstcproc.c'  'betacc $0 $1'
       default	'../$$/ctstcproc.o'   '../ctstcproc.c'  '\$CC -c -o $0 $1'
--PROGRAM:descriptor--
(# ident: external
     (# r: [1]@char
     enter r
     exit r
     #);
   t: @text;
   a: @|(# p: (# do inner #);
           pp: @p
             (# 
             do t -> ident -> t; (* CTextPoolErr *)
             #);
        do pp
        #);
   b: @|(# q: (# do inner #);
           qq: @q(# do a; #);
        do qq
        #);
do (for 110 repeat
        '1234567890' -> t.append;
   for);
   b;
#)


