ORIGIN 'tstenv'; 
BODY 'tstlibbody';       
(*LIBRARY 'tstlib'*)
--LIB: attributes--
chk:
  (# v,r: @integer; ch: @char
  enter(v,r,ch)
  do <<SLOT chk:descriptor>>
  #);
chkBool:
  (# v,r: @boolean; ch: @char
  enter(v,r,ch)
  <<SLOT chkBool:doPart>>
  #);
fill:
  (# ch: @char 
  enter ch 
  do <<SLOT fill:descriptor>>
  #)

