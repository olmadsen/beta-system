ORIGIN 'classfile';
INCLUDE '~beta/sysutils/endian';
INCLUDE '~beta/basiclib/private/binfile_unixbody';

--ClassFilePutHeader: dopart--
do magic -> htonl -> putLong;
   minor -> htons -> putShort;
   major -> htons -> putShort;
   
--ClassFilePutAccessFlags: dopart--
do access_flags -> htons -> putShort;
   
--ClassFilePutThisClass: dopart--
do this_class -> htons -> putShort;
   
--ClassFilePutSuperClass: dopart--
do super_class -> htons -> putShort;
   
