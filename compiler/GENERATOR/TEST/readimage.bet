ORIGIN '../imageread';
--PROGRAM: descriptor--
(# dat, txt: ^image;
   fname: ^text;
do (if NoOfArguments<>2 then
       'Usage: readimage objfile' -> putline;
       stop;
   if);
   2->Arguments->fname[];
   fname[] -> readDataAndTextImages -> (dat[],txt[]);
   'Displaying data image:' -> putline;
   dat.display;
   'Displaying text image:' -> putline;
   txt.display;
#)
