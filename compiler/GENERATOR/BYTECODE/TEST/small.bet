origin 'tstenv'
---lib:attributes--
small: class
  (# X: @integer;
     mini: class
       (# Y: @integer;
          go: proc
            (# N: @integer
            enter N
            do N -> Y;
               N + 1-> X;
            #);
          gogo:
            (# N: @integer
            enter N
            do N -> Y;
               N + 1-> X;
            #);
          display: proc
            (# 
            do X -> putint; ' ' -> put; Y -> putint; newline
            #)
       #);
  #)
---program:descriptor---
(# S: ^small; R: ^S.mini
do &small[] -> S[];  
   &S.mini[] -> R[];
   17 -> R.go;
   R.display;
   21 -> R.gogo;
   R.display
#)




