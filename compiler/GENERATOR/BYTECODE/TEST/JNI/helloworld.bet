ORIGIN '~beta/basiclib/betaenv';
INCLUDE '../javastring';
BUILD jvm 'jvm/hellolib.class' 'hellolib.java' 'hellolib.c' 'javac -d jvm hellolib.java; javah -jni -d jvm jvm.hellolib; gcc -Wl,-G -I /users/datpete/src/j2sdk1.4.0_01/include -I /users/datpete/src/j2sdk1.4.0_01/include/solaris -I jvm -o jvm/libhellolib.so hellolib.c'

--LIB:attributes--
hellolib: ExternalClass
  (# hello: proc
       (# who: ^String;
       enter who[]
       #)
  do 'hellolib'->classname; 
  #)

--PROGRAM: descriptor--
(# h: ^hellolib;
do &hellolib[] -> h[];
   'BETA World' -> h.hello;
#)
