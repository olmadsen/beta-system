ORIGIN '~beta/basiclib/betaenv'
---LIB:attributes---

Bclass2: class
  (# V: @integer;
     
     __init: (* parameterless constructor: skal udkommenteres (eller flyttes efter _init) for at få &Bclass2[]->... til at virke *)
       (# 
       do %setup 'betaenv';
          513 -> V;
       #);
     
     _init: (* regular constructor: OK (alene) *)
       (# 
       do 512 -> V;
       #);
     
     set: proc
       (# n: @integer
       enter n
       do 'BETA: set called' -> putline;
          n   -> V; 
          n+V -> V
       #);
     get: proc
       (# 
       do 'BETA: get called' -> putline;
       exit V 
       #);
     compute: proc
       (# n: @integer
       enter n
       do 'BETA: compute called' -> putline;
          (if (n < V) and (n >= 0) then
              'BETA: compute: addding' -> putline;
              V+n -> V
           else
              (if (n < V) or ( n < 0) then
                  'BETA: compute: subtracting' -> putline;
                  V-n -> V
               else
                  'BETA: compute: negating' -> putline;
                  -V -> V
              if)
          if)
       #);
  #);
