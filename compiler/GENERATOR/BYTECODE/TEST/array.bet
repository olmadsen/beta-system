origin 'betaenv'       
---program:descriptor---
(# R: [3] @integer; V: @integer;
   Q: [6] @char; c: @char
do (for i: 3 repeat
        i*3 -> R[i];
        i -> putint;  ' '->put;
   for);
   (for i: 3 repeat 
        R[i] -> V;
        V -> putint;  ' '-> put
   for);
   (for i: 6 repeat
        'a' + i -1 -> Q[i]  
   for);
   (for i: 6 repeat
        Q[i] -> c;
        c -> put
   for);
   ' ' -> put;
   Q.range -> putint; 
   newline
#)

