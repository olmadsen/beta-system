ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/javalib/java/lang/String';
BUILD jvm 'jvm/ItestImpl.class' 'ItestImpl.java' 'javac -classpath jvm -d jvm $1';
BUILD jvm 'jvm/Itest.class'     'Itest.java'     'javac -classpath jvm -d jvm $1';

--LIB: attributes--

Itest: ExternalClass (* INTERFACE!! *)
  (# describe: proc
       (# result: ^String;
       exit result[]
       #);
  do '|Itest' -> className;
     INNER;
  #);

ItestImpl: Itest
  (# 
  do 'ItestImpl'->classname
  #)

--PROGRAM: descriptor--
(# myItest: ^Itest;
do &ItestImpl[] -> myItest[];
   'Itest implemented by ' -> puttext;
   myItest.describe -> putline;
#)
