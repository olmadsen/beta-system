origin 'tstenv'
---put:doPart--
do(# Write: External   
        (# ch: @char
        enter ch
           (* NOTICE: BETA char 8 bit, IL char 16 bit *)
        do 'void [mscorlib]System.Console::Write(char)' -> callC 
        #);
   do ch -> Write 
   #)   
---putint:doPart--
do(# Write: External   
        (# i: @int32
        enter i
        do 'void [mscorlib]System.Console::Write(int32)' -> callC 
        #);
   do n -> Write 
   #)   
---newline:doPart---
do '\n' -> put
   
