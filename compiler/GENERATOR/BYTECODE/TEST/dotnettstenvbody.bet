ORIGIN 'tstenv';

--LIB: attributes--
String: ExternalClass
  (# 
  do '[mscorlib]System.String' -> className;
  #);
ReadLine: External   
  (# result: ^String;
  do 'class [mscorlib]System.String [mscorlib]System.Console::ReadLine()' -> callC;
  exit result[]
  #);
Read: External   
  (# result: @int32;
  do 'int32 [mscorlib]System.Console::Read()' -> callC;
  exit result
  #); 
WriteChar: External   
  (# ch: @char
  enter ch
     (* NOTICE: BETA char 8 bit, IL char 16 bit *)
  do 'void [mscorlib]System.Console::Write(char)' -> callC 
  #);
WriteInt32: External   
  (# i: @int32
  enter i
  do 'void [mscorlib]System.Console::Write(int32)' -> callC 
  #);

---put:doPart--
do ch -> WriteChar 
---putint:doPart--
do n -> WriteInt32 
---newline:doPart---
do '\n' -> put
---getline:doPart--
do ReadLine -> T[];
---get:doPart--
do Read -> ch;
    
   
