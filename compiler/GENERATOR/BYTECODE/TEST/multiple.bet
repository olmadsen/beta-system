origin 'tstenv'   
---lib:attributes---
multiple:
  (# x,y,z: @integer;
     set: proc
       (# n: @integer
       enter n 
       do n+1 -> x -> y -> z;
       #);
     sett:
       (# v: @integer
       enter v
       do v+3 -> x -> y -> z; 
       #);
     display:
       (#
       do x -> putint; ' '->put; y -> putint; newline
       #);
     test: proc
       (# a,b,c: @integer
       enter(a,b)
       do a+b -> c
       exit c
       #)
  #)
---program:descriptor---
(# S: ^multiple; V: @integer
do &multiple[] -> S[];
   14 -> S.set;
   S.display;
   16 -> S.sett;
   S.display;   
   (4,5) -> S.test -> V;
   V -> putint;
   newline 
#)








