ORIGIN 'betaenv'
---LIB:attributes---

Bclass: class
  (# V: @integer;
     set: proc
       (# n: @integer
       enter n
       do 'BETA: set called' -> putline;
          n   -> V; 
          n+V -> V
       #);
     get: proc
       (# 
       do 'BETA: get called' -> putline;
       exit V 
       #);
     compute: proc
       (# n: @integer
       enter n
       do 'BETA: compute called' -> putline;
          (if (n < V) and (n >= 0) then
              'BETA: compute: addding' -> putline;
              V+n -> V
           else
              (if (n < V) or ( n < 0) then
                  'BETA: compute: subtracting' -> putline;
                  V-n -> V
               else
                  'BETA: compute: negating' -> putline;
                  -V -> V
              if)
          if)
       #) 
  #);
