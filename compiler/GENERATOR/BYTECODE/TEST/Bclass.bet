ORIGIN 'tstenv'
---LIB:attributes---

Bclass: class
  (# V: @integer;
     set: proc
       (# n: @integer
       enter n
       do 17 -> V;
          n -> V; 
          n+V -> V
       #);
     set2: proc
       (# n: @integer; X: @integer
       enter n
       do 17 -> X;
          n -> X;
          n+1 -> X
       #);
     get: proc(#  do exit V #);
     fisk: proc
       (# n: @integer
       enter n
       do (if (n < V) and (n >= 0) then
              'add' -> putline;
              V+n -> V
           else
              (if (n < V) or ( n < 0) then
                  'sub' -> putline;
                  V-n -> V
               else
                  'neg' -> putline;
                  -V -> V
              if)
          if)
       #) 
  #);
