origin 'tstenv'   
---lib:attributes---
String: externalClass
  (#
  do '[mscorlib]System.String' -> classname
  #);
nstringuser: externalclass
  (# display: proc
       (# S: ^String
       enter S[]
       #);
     get:
       (# S: ^String
       exit S[]
       #);
  do '[nstringuser]nstringuser' -> classname;
  #);
---program:descriptor---
(# X: ^nstringuser;
   T: ^ text; 
do &nstringuser[] -> X[];
   'Greatings from BETA' -> X.display;
   'More greatings from BETA' -> T[];
   T[] -> X.display;
   X.get -> T[];
   T[] -> putline;    
#)



