origin 'betaenv'   
---lib:attributes---
List: class
  (# elem:< object;
     insert: proc
       (# S: ^elem
       enter S[]
       do S[] -> el[];
       #); 
     get: proc
       (# S: ^elem
       do el[] -> S[]
       exit S[]
       #);   
     el: ^elem 
  #);
number: class  
  (# V: @integer; 
     set: proc(# n: @integer enter n do n -> V #);
     get: proc(# do exit V #)
  #);  
   
---program:descriptor---  
(# L: ^List; N1,N2: ^number; V: @integer
do &List[] -> L[];     
   &number[] -> N1[];  
   98 -> N1.set;
   N1[] -> L.insert; 
   L.get -> N2[];   
   N2.get -> V; 
   V-> putint;   
   newline    
#) 



 


 


