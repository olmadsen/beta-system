ORIGIN 'googlesearch';  
--PROGRAM:descriptor--
(# query: ^text;
   i: @integer;
do &text[] -> query[]; 
   'Search term for Google: ' -> puttext;
   getLine -> query[];
   (if query.length=0 then
       'Ole Lehrmann Madsen BETA.NET' -> query[];
   if);
   'Results for Google search: ' -> putText; query[] -> putLine;
   (BetaLicenseKey, query[]) 
     -> SearchGoogle(#  
                    do newline;
                       (* i -> putInt; '. ' -> putText; *)
                       'URL: ' -> puttext;
                       url[] -> putline;
                       'Snippet:\n ' -> puttext;
                       snippet[] -> putline;
                       i+1 -> i;
                    #); 
#)

   
