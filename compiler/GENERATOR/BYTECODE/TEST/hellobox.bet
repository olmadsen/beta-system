(# MessageBox_Show: External
     (# message, title: [0]@char;
        buttons, icon: @integer;
        result: @integer;
     enter (message, title, buttons, icon)
     do 'valuetype [System.Windows.Forms]System.Windows.Forms.DialogResult '
        '[System.Windows.Forms]System.Windows.Forms.MessageBox::Show'
        '(string,'
        ' string,'
        ' valuetype [System.Windows.Forms]System.Windows.Forms.MessageBoxButtons,'
        ' valuetype [System.Windows.Forms]System.Windows.Forms.MessageBoxIcon'
        ')' -> callC;
     exit result
     #);
   MessageBoxButton_OK:        (# exit 0 #);
   MessageBoxIcon_Exclamation: (# exit 48 #);
   result: @integer;
do ('Hej Jørgen Thyme', 'Hello Århus', MessageBoxButton_OK, MessageBoxIcon_Exclamation) 
     -> MessageBox_Show
     -> result;
#)
