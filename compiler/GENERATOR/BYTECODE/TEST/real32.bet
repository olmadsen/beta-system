ORIGIN 'tstenv'; 
BUILD dotnet 'xreal32class.dll' 'xreal32class.cs' 'csc -t:library -out:xreal32class.dll -r:System.dll xreal32class.cs'
      javabc 'xreal32class.class' 'xreal32class.java' 'javac xreal32class.java';

---lib:attributes---
xreal32class: externalClass
  (# put: proc(# x: @real32 enter x #)
  #)
---program:descriptor---
(# x,y: @real;
   S: ^xreal32class; 
do 1.0 -> x; 
   2.3 -> y;
   x+y -> x;
   &xreal32class[] -> S[];   
   3.14 -> S.put;
   x -> S.put    
#)

 
