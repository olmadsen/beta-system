ORIGIN 'tstenv';
---lib:attributes---
Applet: ExternalClass
  (# paint:< proc
       (# g: ^Graphics 
       enter g[]
       do inner
       #); 
  do 'java/applet/Applet' -> className;
  #);
Graphics: ExternalClass
  (# drawString: proc
       (# string: [0]@char;
          x, y: @integer;
       enter (string, x, y)
       #);
  do 'java/awt/Graphics' -> className;
  #);
helloapplet: Applet
  (# paint: proc
       (# g: ^Graphics 
       enter g[]
       do ('Hello Applet world from BETA!', 50, 25) -> g.drawString
       #);
  #);






