LIB_ITEM 'tstenv'; 
origin 'tstenv'
---put:doPart--
do
   (# write: External   
        (# ch: @char
        enter ch
        do 'java/io/PrintStream/print(C)V' -> callC 
        #);

   do ch -> write 

   #)   
---putint:doPart--
do
   (# write: External   
        (# ch: @char
        enter ch
        do 'java/io/PrintStream/print(I)V' -> callC 
        #);

   do n -> write 

   #)   
---newline:doPart---
do '\n' -> put
   
