(* Example of using package property.
 * Use with bpackageuser.jave
 * Execute:
 * (1) jbeta -qw -s 188 bpackage
 * (2) javac -classpath jvm/bpackage.jar -d jvm bpackageuser.java
 * (3) java  -classpath "jvm/bpackage.jar:jvm" bpackageuser  # for unix
 * (3) java  -classpath "jvm/bpackage.jar;jvm" bpackageuser  # for windows
 *)
 
ORIGIN '~beta/basiclib/betaenv';
PACKAGE 'foo.bar';
---lib:attributes---
bpackage:
  (# 
     go: 
       (# do 'bpackage::go'->putline #)
  #)

     
 

