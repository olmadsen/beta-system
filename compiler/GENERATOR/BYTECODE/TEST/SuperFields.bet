origin '~beta/basiclib/betaenv'       
---lib:attributes---
SuperFields:
  (# x: @integer;
     Q: ^SuperFieldsSub
  #);
SuperFieldsSub: SuperFields
  (# y: @integer;
     R: ^SuperFieldsSub;
     get: proc
       (# z: @integer
       do x -> z;
          z + y -> z;
       exit z
       #); 
     call: proc
       (# N: @integer; P1,P2: ^SuperFieldsSub
       do Q[] -> P1[];
          R[] -> P2[];
          P1.get -> N; N ->putint; newline;
          P2.get -> N; N ->putint; newline; 
          Q.get -> N; N ->putint; newline;
          R.get -> N; N ->putint; newline
       #)
  #)
---program:descriptor---
(# S: ^SuperFieldsSub;
   N: @integer
do &SuperFieldsSub[] -> S[];
   33 -> S.x;
   44 -> S.y;
   S.x -> putint;
   newline;
   S.y -> putint; 
   newline;
   S.get -> N;    
   N -> putint; 
   newline;
   S[] -> S.Q[]; 
   S[] -> S.R[]; 
   S.call
#)
 

 
 

 
