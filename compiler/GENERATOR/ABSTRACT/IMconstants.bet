(* Emacs code to fix them all:
  (defun fix-constant ()
  (interactive)
  (let ((cstCounter 1))
    (while t
      (search-forward-regexp "exit[ ]+-?[0-9]+[ ]*#")
      (replace-match (format "exit %d #" cstCounter))
      (setq cstCounter (1+ cstCounter)))))
 * 
