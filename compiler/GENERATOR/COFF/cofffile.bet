ORIGIN '~beta/basiclib/binfile';
INCLUDE 'coff';
MDBODY mac     'cofffile_macbody'
       ppc     'cofffile_macbody'
       ppcmac  'cofffile_macbody'
       nti 'cofffile_ntbody'
       default 'cofffile_unixbody';
(* cofffile.bet:
 *   Pattern for manipulating object files in COFF format.
 *)
--LIB: attributes--
cofffile: binfile
  (# <<SLOT CoffFileLib: attributes>>;
     
     putfilehdr:
       (# f: ^filehdr
       enter f[]
       <<SLOT CoffFilePutFileHdr: dopart>>
       #);
     putaouthdr:
       (# a: ^aouthdr
       enter a[]
       <<SLOT CoffFileAoutHdr: dopart>>
       #);
     putscnhdr:
       (# s: ^scnhdr
       enter s[]
       <<SLOT CoffFilePutScnHdr: dopart>>
       #);
     putreloc:
       (# r: ^reloc
       enter r[]
       <<SLOT CoffFilePutReloc: dopart>>
       #);
     putlineno:
       (# l: ^lineno
       enter l[]
       <<SLOT CoffFilePutLineno: dopart>>
       #);
     putsyment:
       (# s: ^syment
       enter s[]
       <<SLOT CoffFilePutSyment: dopart>>
       #);
     putauxent:
       (# a: ^auxent
       enter a[]
       <<SLOT CoffFilePutAuxent: dopart>>
       #);
  #);
