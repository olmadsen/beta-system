ORIGIN 'BetaRun'
---AllocCOM:descriptor---
(* Obsolete: Now in C betarun.
 * 
 * Here we call AlloI and adjusts the address of the prototype
 * of %I1 by adding 24;
 * Input: %o0: origin, %i1: prototype
 * 
 *)
(#
do (if false then
       'AlloCOM' -> ExtLabel;
       (I1,O1) -> MOV -> mstate.b.emit;
       (SP,- 64,SP) -> SAVE_I -> mstate.b.emit;
       (I0,O0) -> MOV -> mstate.b.emit;
       
       'AlloI'-> jsrT;
       
       (* add 24 to prototye start *)
       (I1,0,L7) -> LD_I -> mstate.b.emit;   
       (L7,24,L7) -> ADD_I -> mstate.b.emit;
       (I1,0,L7) -> ST_I -> mstate.b.emit;
       
       (I1,O1) -> MOV -> mstate.b.emit;
       (G0,G0,G0) -> RESTORE -> mstate.b.emit; 
       (O1,I1) -> MOV -> mstate.b.emit;
       RETL -> mstate.b.emit; 
       xNOP -> mstate.b.emit;
   if);
#)
