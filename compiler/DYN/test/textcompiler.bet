ORIGIN '~beta/basiclib/systemenv';
INCLUDE '../compilerfacade';
-- program: Descriptor --
systemEnv
  (#
     aTextCompiler: @protoTextCompiler;
     anObject: @ (# a: @integer; b: @text #);
     t: ^text
  do
     &text[]->t[];
     7->anObject.a;
     'This is a very simple test of the textCompiler'->anObject.b;
     aTextCompiler.init;
     anObject[]->aTextcompiler.setContext;
     '(# do a->putInt; newLine; b[]->putLine #)'->aTextCompiler.compile
  #)  

