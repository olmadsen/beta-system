origin 'tstenv';
---lib:attributes---
myClass:  
  (# x,y,z: @integer;
     foo:	
       (# a: @integer; b: @integer;          
       enter(a,b)
       do x+a -> x;  
          y+b -> y;
          x * y -> a;
          a ->putint;
       exit a
       #);
     elem:
       (# q: @integer; w: @integer
       #)
  #);
---program:descriptor---
(# S: ^myClass;
   V: @integer;
do &myClass[] -> S[];
   (11,22) -> S.foo -> V;
   'V = ' -> puttext; V -> putint; newline
#)



  
