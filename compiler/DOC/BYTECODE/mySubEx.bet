origin 'tstenv'
---lib:attributes---
myClass:
  (# x,y,z: @integer;
     foo:<	
       (# a: @integer; b: @integer;
       enter(a,b)
       do x+a -> x; 
          INNER foo;
          y+b -> y;
          x*y -> a;
       exit a
       #)
  #);
yourClass: myClass
  (# u,v: @integer;
     foo::<
       (#
       do x + y -> y;
          'yourClass:foo'->putline
       #)
  #)         
---program:descriptor---
(# S: ^yourClass;
   V: @integer
do &yourClass[] -> S[];
   (11,22)-> S.foo -> V;
   'V = ' -> puttext; V -> putint; newline
#)
