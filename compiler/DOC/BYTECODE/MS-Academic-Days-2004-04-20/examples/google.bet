ORIGIN 'googlesearch';   
--PROGRAM:descriptor--
(# query: ^text;
   i: @integer;
do '' -> query[]; 
   (for i:noOfArguments-1 repeat
        (if i>1 then ' ' -> query.put; if);
        i+1 -> arguments -> query.append;
   for);
   (if query.length=0 then
       'Search term for Google: ' -> puttext;
       getLine -> query[];
   if);
   (if query.length=0 then
       'Ole Lehrmann Madsen BETA.NET' -> query[];
   if);
   'Results for Google search: "' -> putText; query[] -> putText; '"' -> putLine;
   (BetaLicenseKey, query[]) 
     -> SearchGoogle(#  
                    do (* i -> putInt; '. ' -> putText; *)
                       url[] -> putText;
                       newline; 
                       i+1 -> i;
                    #); 
   'done.' -> putline;
#)

   
