origin 'tstenv'
---lib:attributes---
aClass:
  (# c: @integer;
     
     bClass:
       (# n: @integer;
          up:
            (# m: @integer
            enter m
            do n+m -> m;
               c + 1 -> c
            #);
       #);
     stat: (# do c ->putint; newline #)
  #)
---program:descriptor---
(# ac: ^aClass;
   bc: ^aClass.bClass
do &aClass[] -> ac[];
   &ac.bClass[] -> bc[];
   33 -> bc.up;
   44 -> bc.up;
   ac.stat
#)



