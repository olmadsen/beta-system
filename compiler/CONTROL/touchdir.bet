ORIGIN '~beta/basiclib/directory';
LIB_ITEM 'betacompiler';
---DirectoryLib:attributes---
TouchDir:
  (* creates the directory if it does not exists *)
  (# error:< entry.DiskEntryTouchException
  enter entry.path
  do (if entry.exists then
         (if entry.isFile then
             error
         if)
      else touch(# error::(# do this(touchDir).error #)#)
     if)
  exit this(directory)[]
  #)

         
     
