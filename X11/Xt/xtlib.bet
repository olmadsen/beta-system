XtAddCallback:
  (* Compatible with previous versions, but exits callback ID *)
  (# w: @integer;
     name: @integer;
     callBack: ##external;     
     data: @integer;
     cb: @integer;
     doit: external
       (# w,n,c,d: @integer;
       enter (w,n,c,d)
       do 'XtAddCallback' -> callC;
       #)
  enter (w,name,callBack##,data)
  do callback## -> makeCBF -> cb;
     (w,name,cb,data) -> doit;
  exit cb
  #);

