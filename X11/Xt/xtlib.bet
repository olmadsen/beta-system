argList: cStruct
  (#
     byteSize::< (# do 100 -> value; INNER #);
     
     max: (# exit R.range div 2 #);
     
     extend: 
       (# size: @integer;
       enter size
       do (if size=0 then R.range -> size
           else size-R.range->size;
          if);
          size -> R.extend;
       #);
     set: @
       (# number: @integer;
          cStr: @integer;
          value: @integer;
       enter (number,cstr,value)
       do (* Cannot check ranges since no GC's may occur.
           * The user needs to do the bookkeeping himself
           * using 'max' and 'extend'.
           * The reason for this is that 'value' may be 
           * the computed address of an integer.
           *)
          cstr -> R[number*2-1];
          value -> R[number*2]
       #);
     get: @
       (# number: @integer;
       enter number
       exit R[number*2]
       #);
     getName: @
       (# number: @integer;
          t: ^text;
       enter number
       do r[2*number-1] -> CStringToText -> t[];
       exit t[]
       #);
  #);

actionTable: externalRecord
  (#
     init: 
       (# enter max
       do (if max=0 then 50->max if);
          (8*max, 1) -> xtcalloc -> ptr;
          0 -> number;
       #);
     
     number, max: @integer; (* current and max number of XtActionRecs *)
       
     (* printf: external
      *        (# str: [1]@char;
      *           arg: @integer;
      *        enter (str, arg)
      *        #);
      *)
          
     addAction: 
       (# actionName: @text;
          actionProc: ##external;
          cStr: ^cString;
          offset: @integer;
          value: @integer;
        
       enter (actionName,actionProc##)
       do (if ptr=0 then init if);
          
          (* 'actionTable.addaction: actionName=''->puttext;
           * actionName[] -> puttext; '\''->putline;
           *)
           
          (if number >= max then
              'actionTable.addaction: too many actions!' -> putline;
           else
              8*number -> offset;
              &cString[] -> cStr[];
              actionName[] -> cStr.set -> value;
              (offset, value) -> putLong;
              actionProc## -> makeCBF -> value;
              (* ('actionTable.addaction: ptr=0x%x\n', ptr) -> printf;
               * ('actionTable.addaction: offset=%d\n', offset) -> printf;
               * ('actionTable.addaction: string=%s\n', cStr.address) -> printf;
               * ('actionTable.addaction: proc=0x%x\n', value) -> printf;
               *)
              offset+4 -> offset;
              (offset, value) -> putLong;
              number + 1 -> number;
          if);
       #);
  #);


resourceList: cStruct
  (#
     bytesize::< (# do 300 -> value; INNER #);
     
     add: 
       (#
          resourceName,resourceString: @integer;
          representationType: @integer;
          size: @integer;
          offset: @integer;
          defaultValueRepresentationType: @integer;
          defaultValue: @integer;
          
       enter (resourceName,resourceString,representationType,size,offset,
          defaultValueRepresentationType,defaultValue)
       do 
          resourceName -> r[number*7+1];
          resourceString -> r[number*7+2];
          representationType -> r[number*7+3];
          size -> r[number*7+4];
          offset -> r[number*7+5];
          defaultValueRepresentationType -> r[number*7+6];
          defaultValue -> r[number*7+7];
          number + 1 -> number;
       #);
     
     number: @integer;
     
     init:< object;
  #);

XrmOptionDescRecList: cStruct
  (#
     bytesize::< (# do 200 -> value; INNER #);
     number: @integer;
     
     add: 
       (# option: @text;
          resourceSpecification,value: @text;
          argumentStyle: @integer;
          c: ^cstring;
       enter (option,resourceSpecification,argumentStyle,value)
       do
          &cstring[] -> c[];
          option[] -> c.set -> r[number*4+1];
          &cstring[] -> c[];
          resourceSpecification[] -> c.set -> r[number*4+2];
          (if (value.t.range > 0) then
              &cstring[] -> c[];
              value[] -> c.set -> r[number*4+4];
          if); 
          argumentStyle -> r[number*4+3];
          number + 1 -> number
       #);
     
     init:< object;
  #);

XrmoptionNoArg: (# exit 0 #);     (* Value is specified in OptionDescRec.value        *)
XrmoptionIsArg: (# exit 1 #);     (* Value is the option string itself                *)
XrmoptionStickyArg: (# exit 2 #); (* Value is characters immediately following option *)
XrmoptionSepArg: (# exit 3 #);    (* Value is next argument in argv                   *)
XrmoptionResArg: (# exit 4 #);    (* Resource and value in next argument in argv      *)
XrmoptionSkipArg: (# exit 5 #);   (* Ignore this option and the next argument in argv *)
XrmoptionSkipLine: (# exit 6 #);  (* Ignore this option and the rest of argv          *)
XrmoptionSkipNArgs: (# exit 7 #); (* Ignore this option and the next
                                   * OptionDescRes.value arguments in argv
                                   *)

