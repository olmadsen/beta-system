--- lib:attributes ---
componentStack:
  (# returnAdrs: [1]@Integer;
     returnObjs: [1]@Integer;
     last: @Integer;
  #);

--- processInterfaceLib:attributes ---
getStack:
  (# theStackType:< componentStack;
     s: ^theStackType;
     compAdr: @Integer;
  enter compAdr
  do compAdr->processComm.scanStack
     (# 
     do &theStackType[]->s[];
        returnObjs[1:last]->s.returnObjs;
        returnAdrs[1:last]->s.returnAdrs;
        last->s.last;
        INNER getStack;
     #);
  exit s[]
  #);
