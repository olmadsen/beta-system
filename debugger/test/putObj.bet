--- program:descriptor ---
(# breakpoint: external
     (# objAdr: @Integer
     enter objAdr
     #);
   io: ^IntegerObject;
   io2: ^IntegerObject;
   
do &IntegerObject[]->io[];
   @@io->TOS'%adrGetLong'->breakpoint;
   (for i:100000 repeat &IntegerObject[]->io2[] for);
   1->io;
   0->breakpoint;
   NONE->io[]->io2[];
   (for i:100000 repeat &IntegerObject[]->io[] for);
   0->breakpoint;
#)
