--- withdopart1:dopart ---
do
   
--- withdopart2:dopart ---
do putText; putInt;

--- lib:attributes ---

dyt: external
  (# a,b: @Integer;
  enter (a,b)
  do CallC;
  #);

baat: external
  (# a,b: @Integer;
  enter (a,b)
  #);

alle:
  (# a,b,c,d,e,f,g: @IntegerObject;
  do 
     (if (a=0) then
         b;c
     if);
     
     (if (# do b->c exit c #)->a
      //1 then b
      //2 then c; d;
      else
         e;f;g;
     if);
     
     (for i:a repeat
          b;c;d
     for);
     
     label1:
       (for i:a repeat
            b;c;
       for);
     
     label2:
       (if a=b then
           c;d;e;;restart label2;
       if);
     
     label2:
       (if (a=0) then b;c if);
  #);
