ORIGIN '~beta/basiclib/v1.4/basicsystemenv';
BODY '~beta/guienv/v1.2/private/X11/guienvxsystemenvbody';
INCLUDE '~beta/guienv/v1.2/guienv';

--- program:descriptor ---
systemenv
(# setWindowEnv:: (# do gui[]->theWindowEnv[] #);
   
   sleeper: @|system
     (# 
     do loop:
          (# 
          do 1->sleep; 'hej'->putLine; restart loop;
          #);
     #);
   
   gui: @guienv
     (# main: @window (# #);
        init:
          (#
          do sleeper[]->fork;
             main.open;
          #);
     #);
   
do gui.init;
#)
