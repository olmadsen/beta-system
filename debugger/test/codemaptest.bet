---lib:attributes---
throughC: external
  (# back: ##External
  enter back##
  #);
fromC: external
  (# 
  do CExternalEntry;
     'fromC'->putline;
  #);
--- throughDopartObject:dopart ---
do (for i:3 repeat fromDopart for);
--- throughINNER3:descriptor ---
(# do 'throughINNER3'->putline; INNER throughINNER3 #)
--- program:descriptor ---
(# f: [1]@Integer; 
   throughDopartObject:
     (# 
     <<SLOT throughDopartObject:dopart>>
     #);
   fromDopart:
     (# 
     do '2'->putLine; f[1]->putInt; 
     #);
   throughINNER:
     (# 
     do INNER;
     #);
   throughINNER2: throughINNER
     (# 
     do throughDopartObject;
     #);
   throughINNER3: throughINNER
     (#
     do <<SLOT throughINNER3:descriptor>>;
     #);
   comp: @|
     (# 
     do 'comp1'->putline;
        suspend;
        'comp2'->putline;
     #);
   dyt: [1]@Integer;
   klyt: ^IntegerObject;
do throughINNER3 (# do '1'->putLine; f[1]->putInt; #);
   dyt[4]->putint;
   throughINNER2;
   fromC##->throughC;
   comp; comp;
   &IntegerObject[]->klyt[];
   klyt->putint; newline;
#)
