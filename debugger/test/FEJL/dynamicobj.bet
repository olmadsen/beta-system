ORIGIN '~beta/basiclib/betaenv';
-- program: Descriptor --
(#
   cnt,a,b,c: @integer;
   p: q (#  do 1+cnt->cnt; 32->a; INNER ; cnt->putint #);
   pp: ##p;
   dd: ##q;
   s: q (#  do 1+cnt->cnt; 32->a; THIS(s)##->dd##; dd; cnt->putint #);
   q: (#  do INNER #);
   r: ^q;
   
do
   0->cnt;
   23->a;
   &p;
   32->b;
   &p
     (#  do 230->c #);
   32->b;
   (p,a->b,p,b->c,p);
   32->a;
   p##->pp##;
   pp;
   32->a;
   &p[]->r[];
   r;
   p##->dd##;
   dd;
   32->a;
   s;
   32->a;
   object
#)  

