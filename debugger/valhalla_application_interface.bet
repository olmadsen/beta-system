ORIGIN '~beta/basiclib/v1.4/betaenv';

--- lib:attributes ---

connected_to_valhalla: external
  (# value: @Integer; (* TRUE <=> value<>0 *)
  exit value
  #);

startValhalla:
  (# valhallaInit: external (# #);
  do (if not connected_to_valhalla then valhallaInit if);
  #);

breakpoint: external
  (# objAdr: @Integer
  enter objAdr
  #);
