
CC        = gcc -g -O 
BASEDIR   = $(BETALIB)/debugger/v2.0/private
EXTERNALDIR = $(BASEDIR)/external
OBJECTDIR = $(BASEDIR)/$(MACHINETYPE)

make: $(OBJECTDIR)/processComm.o

$(OBJECTDIR): 
	mkdir $(OBJECTDIR)

$(OBJECTDIR)/processComm.o: $(EXTERNALDIR)/processComm.c $(OBJECTDIR) 
	$(CC) -D$(MACHINETYPE) -c -o $(OBJECTDIR)/processComm.o $(EXTERNALDIR)/processComm.c