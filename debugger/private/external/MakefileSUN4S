CC        = gcc -g -O 
BASEDIR   = $(BETALIB)/debugger/v2.0/private
EXTERNALDIR = $(BASEDIR)/external
OBJECTDIR = $(BASEDIR)/$(MACHINETYPE)

make: $(OBJECTDIR)/coreaccess_sun4s.o

$(OBJECTDIR)/coreaccess_sun4s.o: $(EXTERNALDIR)/coreaccess_sun4s.c $(EXTERNALDIR)/coreaccess_sun4s.h
	gcc -D$(MACHINETYPE) -c -o $(OBJECTDIR)/coreaccess_sun4s.o $(EXTERNALDIR)/coreaccess_sun4s.c