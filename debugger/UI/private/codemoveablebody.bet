ORIGIN '../codemoveable';

--- codemoveableEditorOpen:descriptor ---
(# 
do father.size->size;
#)

--- codemoveableContentsOpen:descriptor ---
(# t: ^Text; sz: @Point;
do 
   (debuggee.AST[],debuggee.BETACFL[],lastcast.frag[],lastcast[])->editor.init;
   editor.open;
   'Fragment: '->t[];
   lastcast.frag.name->t.append;
   t[]->THIS(codemoveable).title;
   editor.size->size;
#)

--- codemoveableopen:descriptor ---
(# 
do lastcast.frag[]->frag[];
#)

--- codemoveableclose:descriptor ---
(# 
#)

--- codemoveableSelect:dopart ---
do (lastcast[],0,0,0)->contents.editor.SifViewer.setFocus;
