ORIGIN '~beta/basiclib/v1.4/betaenv';

(* This is a list of options currently supported by valhalla. It is
 * an extension to the list of options in 
 * ~beta/objectbrowser/v2.0/optionslist.bet.
 * 
 * Options are set by putting them into the global optionDB. (See options.bet)
 *
 * Do not change the value of these constants, since their absolute value
 * is used instead of the pattern name to keep implementations from being
 * dependent on this fragment. *)

--- lib: attributes ---

(* Whether to use truncated imperative names for code expressions on
 * the stack. *)
useShortCodeNames: (# exit 'Boolean_Option_Use_Short_Code_Names' #);

(* Whether to read labels from process at startup instead of on demand. *)
readLabelsOnStartUp: (# exit 'Boolean_Option_Read_Labels_On_StartUp' #);
