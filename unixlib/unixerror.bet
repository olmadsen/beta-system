---- LIB: attributes ----

      (************************ UNIX ERROR CODES **************************)
 
      EPERM:  (# exit 1 (* Not owner *) #);
      ENOENT: (# exit 2  (* No such file or directory *) #);
      ESRCH:  (# exit  3  (* No such process *) #);
      EINTR:  (# exit  4  (* Interrupted system call *) #);
      EIO:    (# exit  5  (* I/O error *) #);
      ENXIO:  (# exit  6  (* No such device or address *) #);
      E2BIG:  (# exit  7  (* Arg list too long *) #);
      ENOEXEC: (# exit  8  (* Exec format error *) #);
      EBADF:  (# exit  9  (* Bad file number *) #);
      ECHILD: (# exit  10  (* No children *) #);
      EAGAIN: (# exit  11  (* No more processes *) #);
      ENOMEM: (# exit  12  (* Not enough core *) #);
      EACCES: (# exit  13  (* Permission denied *) #);
      EACCESS: (#exit 13 #); (* Permission denied (alias) *)
      EFAULT: (# exit  14  (* Bad address *) #);
      ENOTBLK: (# exit  15  (* Block device required *) #);
      EBUSY:  (# exit  16  (* Mount device busy *) #);
      EEXIST: (# exit  17  (* File exists *) #);
      EXDEV:  (# exit  18  (* Cross-device link *) #);
      ENODEV: (# exit  19  (* No such device *) #);
      ENOTDIR : (# exit 20  (* Not a directory*) #);
      EISDIR: (# exit  21  (* Is a directory *) #);
      EINVAL: (# exit  22  (* Invalid argument *) #);
      ENFILE: (# exit  23  (* File table overflow *) #);
      EMFILE: (# exit  24  (* Too many open files *) #);
      ENOTTY: (# exit  25  (* Not a typewriter *) #);
      ETXTBSY: (# exit  26  (* Text file busy *) #);
      EFBIG:  (# exit  27  (* File too large *) #);
      ENOSPC: (# exit  28  (* No space left on device *) #);
      ESPIPE: (# exit  29  (* Illegal seek *) #);
      EROFS:  (# exit  30  (* Read-only file system *) #);
      EMLINK: (# exit  31  (* Too many links *) #);
      EPIPE:  (# exit  32  (* Broken pipe *) #);
 
      (* argument errors *)
 
      ENOTSOCK: (# exit   38  (* Socket operation on non-socket *) #);
      EMSGSIZE: (# exit   40  (* Message too long *) #);
      EADDRINUSE: (# exit   48  (* Address already in use *) #);
      EADDRNOTAVAIL: (# exit   49  (* Can't assign requested address *) #);

      (* operational errors *)
 
      ENETDOWN: (# exit 50  (* Network is down *) #);
      ENETUNREACH: (# exit  51  (* Network is unreachable *) #);
      ENETRESET: (# exit  52  (* Network dropped connection on reset *) #);
      ECONNABORTED: (# exit  53  (* Software caused connection abort *) #);
      ECONNRESET: (# exit  54  (* Connection reset by peer *) #);
      ENOBUFS: (# exit 55  (* No buffer space available *) #);
      EISCONN: (# exit  56  (* Socket is already connected *) #);
      ENOTCONN: (# exit 57  (* Socket is not connected *) #);
      ESHUTDOWN: (# exit 58  (* Can't send after socket shutdown *) #);
      ETOOMANYREFS: (# exit 59  (* Too many references: can't splice *) #);
      ETIMEDOUT: (# exit 60  (* Connection timed out *) #);
      ECONNREFUSED: (# exit 61  (* Connection refused *) #);
      ELOOP: (# exit 62  (* Too many levels of symbolic links *) #);
      ENAMETOOLONG: (# exit 63  (* File name too long *) #);
      ENOTEMPTY: (# exit 66  (* Directory not empty *) #);

      (***************** UNIX ERROR CODES *******************************)
	dummy: (# do #)

