ORIGIN '../errorscreen'

--LIB: attributes--
PutToStdErr: External(# ch: @char enter ch do CallC #);
PutTextToStdErr: External(# str: [1]@char enter str #);

--ErrorScreenError: dopart--
do 'ErrorScreen error'->msg.puttext
   
--ErrorScreenGet: dopart--
do 'Warning: attempt to Get from ErrorScreen' -> screen.putline;
   
--ErrorScreenPut: dopart--
do ch->PutToStdErr;

--ErrorScreenPutText: dopart--
do txt->PutTextToStdErr;
