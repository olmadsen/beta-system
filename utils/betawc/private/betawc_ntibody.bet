ORIGIN '../betawc.bet';
-- prefsfull : doPart --
do
   ('~beta','.')->ast.thePathHandler.convertFilePath->regexp[];
   &text[]->temp[];
   regexp.scanAll
   (# 
   do (if ch='\\' then '\\\\'->temp.puttext;
       else ch->temp.put;
      if)
   #);
   temp[]->regexp[];
  (resfiles[],regexp[]->expandEnvVar)
     ->regexpFilter->resfiles[];
   'done'->putline;

