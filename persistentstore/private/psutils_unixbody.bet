ORIGIN 'psutilsbody';
INCLUDE '~beta/unixlib/v1.6/unixfile';
INCLUDE '~beta/unixlib/v1.6/unixdirectory';

--- PersistentStoreChmod:descriptor ---
(# myfile: unixfile
     (# binary:: TrueObject;
        entryDesc::
          (# permissionDesc::
               (# wrongUserError::
                    (#
                    do THIS(myfile).name ->THIS(chmod).wrongUserError;
                       true -> continue;
                    #);
                  wrongModeError::
                    (#
                    do THIS(myfile).name ->THIS(chmod).wrongModeError;
                       true -> continue;
                    #);
                  changePermError::
                    (#
                    do THIS(myfile).name ->THIS(chmod).changePermError;
                       true -> continue;
                    #);
                  otherError::
                    (#
                    do THIS(myfile).name ->THIS(chmod).otherError;
                       true -> continue;
                    #)
               #)
          #)
     #);
   locg, oinx, data: @myfile;
   dir: @unixdirectory;
   
do pspriv.location.fullName[] -> makeFileNames
     -> (dir.name, locg.name, oinx.name, data.name);
   
   (who,mode)->locg.entry.permission.add;
   (who,mode)->oinx.entry.permission.add;
   (who,mode)->data.entry.permission.add;
   (who,mode)->dir.entry.permission.add;
#)
                       
