ORIGIN '~beta/basiclib/systemenv';
INCLUDE '~beta/persistentstore/persistentstore';
INCLUDE 'systemenvdemolib';

--PROGRAM: descriptor--
systemenv
(# PS: @persistentstore;
   myr: ^myRecord;
do 
   'sysenvStore' -> PS.openread;
   'opened' -> putLine;
   
   (this(systemEnv)[], 'systemEnv') -> PS.registerSpecialInstance;
   
   ('myr', myRecord##)->PS.get->myr[];
   myr->screen.putLine;
   
   PS.close;   
