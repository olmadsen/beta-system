BODY 'private/relationimpl'

-- Include '~beta/sysutils/v1.4/objinterface'
-- Lib: Attributes --
Hierarchy:
  (#
     <<SLOT HierarchyLib: Attributes>>;
     Entity:
       (#
          Relation:
            (#
               Relate:	(# R: ^Relation enter R[] do <<SLOT doRelate: Descriptor>> exit R[] #);
               isRelate: (# E: ^Entity; isRelated: @boolean enter E[] do <<SLOT doCheckMember: Descriptor>> exit isRelated #);
               scan:	(# theEntity,Current: ^Entity	enter theEntity[] do <<SLOT doScan: Descriptor>> #);
               Attach: (# E: ^Entity enter E[] do <<SLOT doAttach: Descriptor>> #);
               relates: [1] ^Entity;
               thisEntity: ^Entity;
               NumRelated: @Integer;
            #);
          RelativeDistinguishedName: @Text;
          DistinguishedName: @Text;
          ClassName: ^Text;
          AssociatedWith: ^Relation;
          ContainedIn: ^Relation;
          MonitoredBy: ^Relation;
          Init:<
            (#
               theDistName,theRelDistName: @Text
            enter (theDistName,theRelDistName)
            do
               theDistName -> DistinguishedName;
               theRelDistName -> RelativeDistinguishedName;
               This(Entity)[] -> GetPatternName -> ClassName[];
               &Relation[] -> AssociatedWith[]; This(Entity)[] -> AssociatedWith.Attach;
               &Relation[] -> ContainedIn[];    This(Entity)[] -> ContainedIn.Attach;
               &Relation[] -> MonitoredBy[];    This(Entity)[] -> MonitoredBy.Attach;
               inner;
            #);
       #);		
