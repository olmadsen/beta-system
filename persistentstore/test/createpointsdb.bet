ORIGIN 'point';
INCLUDE '../persistentstore';



-- program: descriptor --
(# top: ^Point;
   txt: ^text;
   PS: @PersistentStore;
   db: ^DataBase;
   
do &DataBase[] -> db[];
   40 * 1024 -> MakePointList -> db.top[];
   'name: ' -> puttext;
   getline -> txt[];
   PS.init;
   txt[] -> PS.create;
   (db[], 'database') -> PS.put;
   PS.checkpoint;
   'done' -> puttext;
   getline -> txt[];
   PS.close;
#)
