ORIGIN 'point';
INCLUDE '../persistentstore';



-- program: descriptor --
(# top: ^Point;
   txt: ^text;
   PS: @PersistentStore;
   db: ^DataBase;
   current: ^Point;
   z: @integer;
do 
   'name: ' -> puttext;
   getline -> txt[];
   PS.init;
   txt[] -> PS.openWrite;
   ('database', Database##) -> PS.get -> db[];
   db.top[] -> current[];
   loop:
     (if current[] <> NONE then
         current.next[] -> current[];
         restart loop;
     if);
   'done inspecting - press return to continue' -> puttext;
   getline -> txt[];
   db.top[] -> sort;
   'press return to continue' -> puttext;
   getline -> txt[];
#)
