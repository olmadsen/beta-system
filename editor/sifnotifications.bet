ORIGIN '~beta/mps/notifications';
-- astinterfacelib: Attributes --
  

-- fragmentGroupLib: Attributes --
sifFragmentGroupSubject: fragmentGroupSubject
  (#
     fragmentGroupObserverType::< sifFragmentGroupObserver;
     notifyRefresh: notify (#  do current.onRefresh #)
  #);
sifFragmentGroupObserver: fragmentGroupObserver
  (#
     fragmentGroupSubjectType::< sifFragmentGroupSubject;
     onRefresh:< (#  do INNER #)
  #)  

-- fragmentFormLib: Attributes --
sifFragmentFormSubject: fragmentFormSubject
  (#
     fragmentFormObserverType::< sifFragmentFormObserver;
     notifyRefresh: notify
       (# node: ^astInterface.ast
       enter node[]
       do node[]->current.onRefresh
       #);
     notifyFocusChanged: notify
       (# newCS: @astFocus enter newCS do newCS->current.onFocusChanged #)
  #);
sifFragmentFormObserver: fragmentFormObserver
  (#
     fragmentFormSubjectType::< sifFragmentFormSubject;
     onRefresh:< (# node: ^astInterface.ast enter node[] do INNER #);
     onFocusChanged:< (# newCS: @astFocus enter newCS do INNER #)
  #)  

