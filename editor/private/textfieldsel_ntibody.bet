ORIGIN '../textfieldsel';
INCLUDE '~beta/guienv/private/winnt/fields_ntibody'
        '~beta/guienv/private/winnt/guienvattributes';

-- GUIENVtextFieldHideSelection: doPart --
do (# fHide, fChangeStyle: @integer;
   do assertOpen;
      (if bHide then 1 -> fHide; if);
      (if bChangeStyle then 1 -> fChangeStyle; if);
      (interfaceObjectID,EM_HIDESELECTION, fHide, fChangeStyle) ->
sendMessage;
   #);
   

