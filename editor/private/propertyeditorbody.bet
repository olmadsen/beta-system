ORIGIN '../codeeditor';
INCLUDE 'editorenvbody';
-- propertyeditorUpdateProperties: DoPart --
do
   (frag[],theGroupEditor.fg[])->edenv.editorenvPrivate.astToProps;
   theGroupEditor.groupTouched;
   (if edenv.ymerCallBack[] <> none then
       theGroupEditor.fg[]->edenv.ymerCallback.refreshGroup
    else
       'edenv.ymercallback is none!!'->putLine
   if)  

-- propertyeditorAstReplaced: DoPart --
do updateProperties  

-- propertyeditorAstReplacedSequence: DoPart --
do updateProperties  

-- propertyeditorListElementInserted: DoPart --
do updateProperties  

-- propertyeditorListElementsDeleted: DoPart --
do updateProperties  

-- propertyeditorListElementsReplaced: DoPart --
do updateProperties  

