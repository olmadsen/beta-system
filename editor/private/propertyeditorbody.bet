-- propertyEditorPrivate: Descriptor --
(#
   updateProperties: doCommand
     (# uncomplete: @boolean; unexp: ^astinterface.unexpanded
     do
        frag[]->edenv.editorEnvPrivate.nonterminalExists->(uncomplete,unexp[]);
        (if not uncomplete then
            (if (frag[],theGroupEditor.fg[])->edenv.editorenvPrivate.astToProps
             then
                theGroupEditor.checkAutoSave;
                theGroupEditor.fg.notifyPropertiesChanged
            if)
        if)
     #)
#)  

-- propertyeditorOnAstReplaced: DoPart --
do private.updateProperties  

-- propertyeditorOnAstReplacedSequence: DoPart --
do private.updateProperties  

-- propertyeditorOnListElementInserted: DoPart --
do private.updateProperties  

-- propertyeditorOnListElementsDeleted: DoPart --
do private.updateProperties  

-- propertyeditorOnListElementsReplaced: DoPart --
do private.updateProperties  

