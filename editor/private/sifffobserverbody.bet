-- sifffobserverOnFocusChanged: DoPart --
do
   (if isAFrejaEditor and (* must only be in sync. if it is a freja editor  *)
   ((newCS.node[],editorRoot[])->isNodeInTree) then
       (newCS,false)->updatePresentation
   if);
   INNER onFocusChanged  

