ORIGIN '../ymersiflib'
-- ymersiflibCheckOngoingTextEditing: DoPart --
do
   (if browser.cfe[] <> none then
       browser.cfe.frag.locked (*texteditMode*) ->value
   if)  

-- ymersiflibFormEditorExists: DoPart --
do
   (if browser.cfe[] <> none then
       (if not browser.cfe.frag.locked (*texteditMode*) then
           true->value; 
       if)
   if)  

