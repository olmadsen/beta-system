ORIGIN '../codeeditor';
-- fgObserverPrivate: Descriptor --
(#
   refresh:
     (# 
     do
        (if ymerCallBack[] <> none then
            (theBrowser[],fg[])->ymerCallback.refreshGroup; 
         else
            'fgObserver.refresh: ymercallback is none!!'->putLine
        if)
     #)
#)  

-- fgObserverOnNameChanged: DoPart --
do
   (if trace[7] then
       '----- onNameChanged '->putText;
       fg.name->putText;
       ' '->put;
       fgObserver.no->putInt;
       newLine;
       
   if);
   (if isAfrejaEditor then ff[]->frejaInterface.fragmentChanged if);
   private.refresh  

