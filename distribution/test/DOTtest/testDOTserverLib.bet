ORIGIN '~beta/distribution/v1.01/shell';
--- shellenvlib:attributes ---
testDOTServer: shell
  (# callback: entry
       (# cb: ^humle
       enter cb[]
       do cb.cb;
       #);
     
     onKill::< 
       (# 
       do 'testDOTserver killed' -> screen.putLine; 
       #);
     
  do (THIS(testDOTServer)[],'testDOTServer') -> myEnsemble.ns.put
     (# overWrite::< (# do true -> value; 'overWriting' -> screen.putLine #)#)
  #);

humle: remoteable
  (# cb: entry (# do cbImpl #);
     cbImpl:< Object;
  #)
