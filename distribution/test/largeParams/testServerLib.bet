ORIGIN '~beta/distribution/v1.0/shell';

--- lib:attributes ---
IntList: list
  (# element:: IntegerObject
  #);

--- shellenvlib:attributes ---
testServer: shell
  (# ml: ^IntList;
     
     printList: entry
       (# l: ^IntList;
       enter l[]
       do l.scan (# do current -> putInt; newline; #);
          l[] -> ml[];
          NONE -> l[];
       #);
     
     getList: entry
       (# l: ^IntList;
       do ml[] -> l[]
       exit l[]
       #);
     
     onKill::< 
       (# 
       do 'kill called' -> screen.putLine; 
       #);
     
  do (THIS(testServer)[],'largeParams') -> myEnsemble.ns.put
     (# overWrite::< (# do true -> value; 'overWriting' -> screen.putLine #)#)
  #);
