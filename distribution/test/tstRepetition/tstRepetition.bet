ORIGIN '~beta/distribution/v1.1/shell';
INCLUDE 'tstRepetitionLib';

--- program:descriptor ---
shellEnv
(# shellType::<
     (# server: ^tstRep;
        r0,r1: ^RepetitionObject;
     do 
        (tstRep##,'tstrep') -> myEnsemble.ns.get -> server[];
        &RepetitionObject[] -> r0[];
        r0.init;
        'Dette er r0' -> r0.putText;
        &RepetitionObject[] -> r1[];
        r1.init;
        'Dette er r1' -> r1.putText;
        (r0[],r1[]) -> server.put;
        r0.reset; r1.reset;
        r0.getText -> screen.putLine;
        r1.getText -> screen.putLine;
        kill;
     #);
#)
