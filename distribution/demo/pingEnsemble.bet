ORIGIN '~beta/distribution/v1.6/shell';
--- program:descriptor ---
shellEnv
(# shellType::
     (# ensembleName: ^Text;
        ens: ^ensemble
     do (if NoOfArguments <> 2 then
            'Usage: pingEnsemble <ensembleName>'->putLine;
            kill;
        if);
        2 -> arguments -> ensembleName[];
        (ensemble##, ensembleName[]) -> myEnsemble.ns.get -> ens[];
        (if ens[] = NONE then
            'Network host ' -> putText;
            ensembleName[] -> putText;
            ' not found.' -> putLine;
            kill;
        if);
        (if ens.ping then
            'Ok.ensembleDeamon found on ' -> putText;
            ensembleName[] -> putLine;
         else
            'No ensembleDeamon on ' -> putText;
            ensembleName[] -> putLine;
        if);
        kill;
     #);
#)
