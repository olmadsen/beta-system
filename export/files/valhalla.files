. ${BETALIB}/export/misc/echonl.sh

echo +valhalla >/dev/tty

if [ "$TARGET" = "nti" ] ;then
echonl "\
./bitmap/objectbrowser/*.bmp \
./bin/${CODEDIR}/valhalla${EXE} \
"
else
echonl "\
./bin/valhalla${BAT}  \
./debugger/${CODEDIR}/valhalla${EXE} \
./debugger/XRESOURCES/Valhalla \
./bitmap/objectbrowser/*.xpm \
"
fi

if [ "$TARGET" = "sgi" ]
then
echonl "\
./debugger/${CODEDIR}/*.*so \
"
fi
