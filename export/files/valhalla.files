. ${BETALIB}/export/misc/echonl.sh

echo +valhalla >/dev/tty

echonl "\
./bin/valhalla${BAT}  \
./debugger/v2.3/${CODEDIR}/valhalla${EXE} \
"

if [ "$TARGET" = "nti" ] ;then
echonl "\
./bitmap/objectbrowser/*.bmp \
"
else
echonl "\
./debugger/v2.3/XRESOURCES/Valhalla \
./bitmap/objectbrowser/*.xpm \
"
fi

if [ "$TARGET" = "sgi" ]
then
echonl "\
./debugger/v2.3/${CODEDIR}/*.*so \
"
fi
