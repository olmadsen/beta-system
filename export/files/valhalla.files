. ${BETALIB}/export/misc/echonl.sh

echo +valhalla >/dev/tty

BINVALHALLA=0  # Now part of Mjolner.

if [ "$BINVALHALLA" = "1" ]
then

if [ "$TARGET" = "nti" ] ;then
echonl "\
./bin/${CODEDIR}/valhalla${EXE} \
"
else
echonl "\
./bin/valhalla${BAT}  \
./debugger/${CODEDIR}/valhalla${EXE} \
./debugger/XRESOURCES/Valhalla \
"
fi

if [ "$TARGET" = "sgi" ]
then
echonl "\
./debugger/${CODEDIR}/*.*so \
"
fi
fi
