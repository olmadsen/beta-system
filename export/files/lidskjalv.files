./guienv/v1.3.1/utils/private/${IMPL}/*.BMP \
"
else
  IMPL=X11
fi

echonl "\
./guienv/v1.3.1/*.bet \
./guienv/v1.3.1/*.${AST} \
./guienv/v1.3.1/${CODEDIR}/*.${OBJ} \
./guienv/v1.3.1/private/*.bet \
./guienv/v1.3.1/private/*.${AST} \
./guienv/v1.3.1/private/${CODEDIR}/*.${OBJ} \
./guienv/v1.3.1/private/${IMPL}/*.bet \
./guienv/v1.3.1/private/${IMPL}/*.${AST} \
./guienv/v1.3.1/private/${IMPL}/${CODEDIR}/*.${OBJ} \
./guienv/v1.3.1/private/${IMPL}/external/*.* \
./guienv/v1.3.1/README \
./demo/r3.1.1/guienv/ReferenceDemos/*.bet \
./demo/r3.1.1/guienv/TutorialDemos/*.bet \
./guienv/v1.3.1/utils/README \
./guienv/v1.3.1/utils/*.bet \
./guienv/v1.3.1/utils/*.${AST} \
./guienv/v1.3.1/utils/${CODEDIR}/*.${OBJ} \
./guienv/v1.3.1/utils/private/*.bet \
