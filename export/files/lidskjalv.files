. /users/beta/export/distribution/r3.1.1/misc/echonl.sh

echo +lidskjalv >/dev/tty

# Have to add the Xt/v1.8/motif stuff here, because PE
# does not include this.

if [ "$PACKLIDSKJALV_XT" = "yes" ]; then
echo +xt/motif >/dev/tty
echonl "\
./Xt/v1.8.1/motif/motiflib.bet \
./Xt/v1.8.1/motif/motiflib.${AST} \
./Xt/v1.8.1/motif/${CODEDIR}/motiflib.*.${OBJ} \
./Xt/v1.8.1/motif/motifprop.bet \
./Xt/v1.8.1/motif/motifprop.${AST} \
./Xt/v1.8.1/motif/${CODEDIR}/motifprop.*.${OBJ} \
./Xt/v1.8.1/motif/callbackstruct.bet \
./Xt/v1.8.1/motif/callbackstruct.${AST} \
./Xt/v1.8.1/motif/${CODEDIR}/callbackstruct.*.${OBJ} \
./Xt/v1.8.1/motif/private/wideboolean.bet \
./Xt/v1.8.1/motif/private/wideboolean.${AST} \
./Xt/v1.8.1/motif/private/${CODEDIR}/wideboolean.*.${OBJ} \
"

if [ "$PACKDOTDB" = "yes" ]
then
echo +..db >/dev/tty
echonl "\
./Xt/v1.8.1/motif/${CODEDIR}/motiflib..db \
./Xt/v1.8.1/motif/${CODEDIR}/motifprop..db \
./Xt/v1.8.1/motif/${CODEDIR}/callbackstruct..db \
./Xt/v1.8.1/motif/private/${CODEDIR}/wideboolean..db \
"
fi

fi

if [ "$TARGET" = "nti" ]
then
  IMPL=winnt
echonl "\
