./guienv/v1.3.1/utils/private/${CODEDIR}/*.${OBJ} \
./guienv/v1.3.1/utils/private/${IMPL}/*.bet \
./guienv/v1.3.1/utils/private/${IMPL}/*.${AST} \
./guienv/v1.3.1/utils/private/${IMPL}/${CODEDIR}/*.${OBJ} \
./guienv/v1.3.1/utils/private/${IMPL}/external/*.* \
"

if [ "$PACKDOTDB" = "yes" ]
then
echo +..db >/dev/tty
echonl "\
./guienv/v1.3.1/${CODEDIR}/*..db \
./guienv/v1.3.1/private/${CODEDIR}/*..db \
./guienv/v1.3.1/private/${IMPL}/${CODEDIR}/*..db \
./guienv/v1.3.1/utils/${CODEDIR}/*..db \
./guienv/v1.3.1/utils/private/${CODEDIR}/*..db \
./guienv/v1.3.1/utils/private/${IMPL}/${CODEDIR}/*..db \
"
fi
