echo +objectbrowser >/dev/tty

. /users/beta/export/distribution/r4.0.1/misc/echonl.sh

if [ "$TARGET" = "nti" ]
# Windows
then
echonl "\
./bitmap/objectbrowser/*.bmp \
./bitmap/objectbrowser/*.BMP \
"
else
# UNIX
echonl "\
