. ${BETALIB}/export/misc/echonl.sh

echo +frigg >/dev/tty

if [ "$TARGET" = "nti" ]
# Windows
then
IMPL=winnt
echonl "\
./bitmap/interfacebuilders/*.bmp \
./bitmap/interfacebuilders/*.BMP \
"
else
# UNIX
IMPL=X11
echonl "\
./bitmap/interfacebuilders/*.xbm \
"
fi

echonl "\
./bin/frigg${BAT} \
./interfacebuilder/v1.5/${MACHINE}frigg${EXE} \
"

if [ "$TARGET" = "sgi" ]
then
echonl "\
./interfacebuilder/v1.5/${CODEDIR}/*.*so \
"
fi
