. ${BETALIB}/export/misc/echonl.sh

echo +system-pe >/dev/tty

echonl "\
./configuration/*.* \
./configuration/homeDirectories \
./configuration/mjolnerrc \
./configuration/mbs_id \
./grammars/beta/*.* \
./grammars/metagram/*.* \
./grammars/pretty/*.* \
./grammars/property/*.* \
./grammars/mylang/*.* \
"

if [ "$TARGET" = "nti" ]
then
echonl "\
./README.txt \
./bitmap/icons/*.ICO \
./bin/beta.bat \
./bin/betacc.exe \
./bin/betacc.bet \
./bin/betafs.bat \
./bin/betatar.bat \
./bin/betawc.bat \
./compiler/${CODEDIR}/beta.exe \
./utils/betafs/${CODEDIR}/betafs.exe \
./utils/betatar/${CODEDIR}/betatar.exe \
./utils/betawc/${CODEDIR}/betawc.exe \
./bin/%SDK%/rules*.make \
./emacs/beta-*.el \
./emacs/make-regexp.el \
"
else

if [ "$TARGET" = "ppcmac" ]
then
# power mac
echonl "\
./scripts/beta \
./scripts/betawc \
./scripts/betafs \
./scripts/BetaResource \
./scripts/BetaRez \
./scripts/BetaStartup*Menu \
./scripts/CheckMPWsize \
./scripts/OpenFile \
./compiler/${CODEDIR}/beta \
./utils/betawc/${CODEDIR}/betawc \
./utils/betafs/${CODEDIR}/betafs \
"
else

# UNIX
echonl "\
./bin/exectool \
./compiler/${CODEDIR}/beta \
"

echonl "\
./bin/betatar \
./bin/betawc \
./bin/betafs \
./utils/betawc/${CODEDIR}/betawc \
./utils/betatar/${CODEDIR}/betatar \
./utils/betafs/${CODEDIR}/betafs \
./bin/beta \
./emacs/beta-*.el \
./emacs/make-regexp.el \
./man/man1/*.1 \
./bin/make_currentlinks \
"

if [ "$TARGET" = "sgi" ]
then
echonl "\
./compiler/${CODEDIR}/*.*so \
./utils/betawc/${CODEDIR}/*.*so \
./utils/betatar/${CODEDIR}/*.*so \
./utils/betafs/${CODEDIR}/*.*so \
"
fi

fi
fi
