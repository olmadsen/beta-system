. ${BETALIB}/export/misc/echonl.sh

echo +mjolnertool >/dev/tty

if [ "$TARGET" = "nti"   ]
then
echonl "\
./bin/${CODEDIR}/mjolner${EXE} \
"
else

if [ "$TARGET" = "ppcmac" ]
then
echonl "\
./applications/mjolner${EXE} \
"
else
# Unix:
echonl "\
./bin/mjolner \
./bin/${CODEDIR}/mjolner \
"

#if [ "$TARGET" = "sgi" ]
# packed by system-pe.files
#then
#echonl "\
#./mjolnertool/${CODEDIR}/*.*so \
#"
#fi

fi
fi

