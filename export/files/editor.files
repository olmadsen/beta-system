. ${BETALIB}/export/misc/echonl.sh

echo +sif >/dev/tty

if [ "$TARGET" = "ppcmac" ]
then
echonl "\
./applications/siflight \
./editor/v5.2/doc/BUGS \
"
exit 0     # Do NOT run rest of script!
fi


SIF="sif"

if [ "$TARGET" = "sgi" ]
then
echonl "\
./editor/v5.2/${CODEDIR}/*.*so \
"
fi

if [ "$TARGET" = "nti" ]
then
echonl "\
./editor/v5.2/${CODEDIR}/sif.exe \
"
else
# UNIX
echonl "\
./editor/v5.2/${CODEDIR}/sif \
"
fi

echonl "\
./bin/sif${BAT}  \
./editor/v5.2/doc/BUGS \
"

