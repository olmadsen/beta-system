. ${BETALIB}/export/misc/echonl.sh

echo +sif >/dev/tty

if [ "$TARGET" = "ppcmac" ]
then
echonl "\
./applications/siflight \
./editor/doc/BUGS \
"
exit 0     # Do NOT run rest of script!
fi

if [ "$TARGET" = "nti" ]
then
echonl "\
./bin/${CODEDIR}/sif${EXE} \
./editor/doc/BUGS \
"
else
# UNIX
echonl "\
./editor/${CODEDIR}/sif \
./bin/sif${BAT}  \
./editor/doc/BUGS \
"
fi

if [ "$TARGET" = "sgi" ]
then
echonl "\
./editor/${CODEDIR}/*.*so \
"
fi
