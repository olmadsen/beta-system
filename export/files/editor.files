. /users/beta/export/distribution/r3.1.1/misc/echonl.sh

echo +sif >/dev/tty

SIF="sif"

if [ "$TARGET" = "sgi" ]
then
echonl "\
./editor/v5.0.2/${CODEDIR}/*.*so \
"
