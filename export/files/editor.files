. ${BETALIB}/export/misc/echonl.sh

echo +sif >/dev/tty

SIF="sif"

if [ "$TARGET" = "sgi" ]
then
echonl "\
./editor/v5.1/${CODEDIR}/*.*so \
"
#SIF="siflight"
fi

if [ "$TARGET" = "nti" ]
then
SIF="compsif"
fi

echonl "\
./bin/sif${BAT}  \
./editor/v5.1/doc/BUGS \
./editor/v5.1/${CODEDIR}/${MACHINE}${SIF}${EXE} \
"

