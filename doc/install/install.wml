enerated web.#NODE FirstPage{  TITLE maintitle()  HEADER  {    @NavPanel=concatenate(      button(        image("../images/next.gif",     align: BOTTOM, alt: "Next",     border: 0,	                      class: "NavPanel"),        filename(next),        image("../images/nextg.gif",    align: BOTTOM, alt: "Next",     border: 0,                              class: "NavPanel")),      "\n",       button(        image("../images/index.gif",  align: BOTTOM, alt: "Index", border: 0,			      class: "NavPanel"),        "inx.html",        image("../images/indexg.gif", align: BOTTOM, alt: "Index", border: 0,			      class: "NavPanel")),      "\n",       button(        image("../images/top.gif",    align: BOTTOM, alt: "Top",    border: 0,			      class: "NavPanel"),        "../index.html",        image("../images/topg.gif",   align: BOTTOM, alt: "Top",    border: 0,	 		      class: "NavPanel"))              );    write(*,paragraph(@NavPanel));  }  FOOTER  {	write(*,"<A NAME=_toc></A>");	write(*,toc(4,global));	write(*,"<TABLE align=center border=1 cellpadding=3>");	write(*,"<TR><TD align=center>");	write(*,"<FONT size=\"-1\">");	write(*,"Copyright &copy; 1991-99 <A HREF=\"http://www.mjolner.com\">Mj&oslash;lner Informatics</A>.<BR>");	write(*,"All rights reserved.<BR>");	write(*,"No part of this document may be copied or distributed<BR>");	write(*,"without the prior written permission of Mj&oslash;lner Informatics");	write(*,"</FONT>");	write(*,"</TD></TR>");	write(*,"</TABLE>");	write(*,"<P></P>");	write(*,"<HR>");	write(*,"<P>");	write(*,"<TABLE cols=3 border=0 width=100%>");	write(*,"<TR>");	write(*,"<TD width=\"33%\" align=\"left\">");    write(*,address(maintitle()));    write(*,"</TD>");	write(*,"<TD width=\"34%\" align=\"center\">&nbsp</TD>");	write(*,"<TD width=\"33%\" align=\"right\"><FONT size=-1><SCRIPT LANGUAGE=JavaScript SRC=\"../javascript/lastmod.js\"></SCRIPT></FONT></TD>");	write(*,"</TABLE>");	write(*,"<P>");    write(*,paragraph(@NavPanel));  }}#-------------------------------------------------------------------------##  Define other lower level nodes. #NODE LevelOne{  TITLE concatenate(headingnumber()," ",headingtext())  HEADER  {    @NavPanel=concatenate(      button(        image("../images/next.gif",     align: BOTTOM, alt: "Next",     border: 0,	                      class: "NavPanel"),        filename(next),        image("../images/nextg.gif",    align: BOTTOM, alt: "Next",     border: 0,                              class: "NavPanel")),      "\n",       button(        image("../images/prev.gif",     align: BOTTOM, alt: "Previous", border: 0,                              class: "NavPanel"),        filename(previous),        image("../images/prevg.gif",    align: BOTTOM, alt: "Previous", border: 0,                              class: "NavPanel")),      "\n",       button(        image("../images/top.gif",      align: BOTTOM, alt: "Top",      border: 0,	 		      class: "NavPanel"),        "../index.html",        image("../images/topg.gif",     align: BOTTOM, alt: "Top",      border: 0,			      class: "NavPanel")),      "\n",       button(        image("../images/content.gif",  align: BOTTOM, alt: "Contents", border: 0,			      class: "NavPanel"),        "index.html#_toc",        image("../images/contentg.gif", align: BOTTOM, alt: "Contents", border: 0,			      class: "NavPanel")),      "\n",       button(        image("../images/index.gif",    align: BOTTOM, alt: "Index",    border: 0,			      class: "NavPanel"),        "inx.html",        image("../images/indexg.gif",   align: BOTTOM, alt: "Index",    border: 0,	 		      class: "NavPanel"))              );    write(*,paragraph(@NavPanel));    write(*,paragraph(maintitle()));  }  FOOTER  {	write(*,"<HR>");	write(*,"<P>");	write(*,"<TABLE cols=3 border=0 width=100%>");	write(*,"<TR>");	write(*,"<TD width=\"33%\" align=\"left\">");    write(*,address(maintitle()));    write(*,"</TD>");	write(*,"<TD width=\"34%\" align=\"center\"><FONT size=-1>&COPY; <A HREF=\"http://www.mjolner.com\">Mj&oslash;lner Informatics</A></FONT></TD>");	write(*,"<TD width=\"33%\" align=\"right\"><FONT size=-1><SCRIPT LANGUAGE=JavaScript SRC=\"../javascript/lastmod.js\"></SCRIPT></FONT></TD>");	write(*,"</TABLE>");    write(*,paragraph(@NavPanel));  }}NODE LevelTwo{  TITLE concatenate(headingnumber()," ",headingtext())  HEADER  {    @NavPanel=concatenate(      button(        image("../images/next.gif",     align: BOTTOM, alt: "Next",     border: 0,	                      class: "NavPanel"),        filename(next),        image("../images/nextg.gif",    align: BOTTOM, alt: "Next",     border: 0,                              class: "NavPanel")),      "\n",       button(        image("../images/prev.gif",     align: BOTTOM, alt: "Previous", border: 0,                              class: "NavPanel"),        filename(previous),        image("../images/prevg.gif",    align: BOTTOM, alt: "Previous", border: 0,                              class: "NavPanel")),      "\n",       button(        image("../images/up.gif",       align: BOTTOM, alt: "Up",       border: 0,			      class: "NavPanel"),        filename(up),        image("../images/upg.gif",      align: BOTTOM, alt: "Up",       border: 0,                              class: "NavPanel")),      "\n",       button(        image("../images/top.gif",      align: BOTTOM, alt: "Top",      border: 0,	 		      class: "NavPanel"),        "../index.html",        image("../images/topg.gif",     align: BOTTOM, alt: "Top",      border: 0,			      class: "NavPanel")),      "\n",       button(        image("../images/content.gif",  align: BOTTOM, alt: "Contents", border: 0,			      class: "NavPanel"),        "index.html#_toc",        image("../images/contentg.gif", align: BOTTOM, alt: "Contents", border: 0,			      class: "NavPanel")),      "\n",       button(        image("../images/index.gif",    align: BOTTOM, alt: "Index",    border: 0,			      class: "NavPanel"),        "inx.html",        image("../images/indexg.gif",   align: BOTTOM, alt: "Index",    border: 0,	 		      class: "NavPanel"))              );    write(*,paragraph(@NavPanel));    write(*,paragraph(concatenate(headingnumber(up)," ",headingtext(up))));  }  FOOTER  {	write(*,"<HR>");	write(*,"<P>");	write(*,"<TABLE cols=3 border=0 width=100%>");	write(*,"<TR>");	write(*,"<TD width=\"33%\" align=\"left\">");    write(*,address(maintitle()));    write(*,"</TD>");	write(*,"<TD width=\"34%\" align=\"center\"><FONT size=-1>&COPY; <A HREF=\"http://www.mjolner.com\">Mj&oslash;lner Informatics</A></FONT></TD>");	write(*,"<TD width=\"33%\" align=\"right\"><FONT size=-1><SCRIPT LANGUAGE=JavaScript SRC=\"../javascript/lastmod.js\"></SCRIPT></FONT></TD>");	write(*,"</TABLE>");	write(*,"<P>");    write(*,paragraph(@NavPanel));  }}#-------------------------------------------------------------------------##  Define node for the external global Table of Contents.#NODE TOCNode{  TITLE headingtext()  HEADER  {      @NavPanel=concatenate(      button(        image("../images/next.gif",     align: BOTTOM, alt: "Next",     border: 0,	                      class: "NavPanel"),        filename(next),        image("../images/nextg.gif",    align: BOTTOM, alt: "Next",     border: 0,                              class: "NavPanel")),      "\n",       button(        image("../images/prev.gif",     align: BOTTOM, alt: "Previous", border: 0,                              class: "NavPanel"),        filename(previous),        image("../images/prevg.gif",    align: BOTTOM, alt: "Previous", border: 0,                              class: "NavPanel")),      "\n",       button(        image("../images/top.gif",      align: BOTTOM, alt: "Top",      border: 0,	 		      class: "NavPanel"),        "../index.html",        image("../images/topg.gif",     align: BOTTOM, alt: "Top",      border: 0,			      class: "NavPanel")),      "\n",       button(        image("../images/index.gif",    align: BOTTOM, alt: "Index",    border: 0,			      class: "NavPanel"),        "inx.html",        image("../images/indexg.gif",   align: BOTTOM, alt: "Index",    border: 0,	 		      class: "NavPanel"))              );    write(*,paragraph(@NavPanel));    write(*,paragraph(maintitle()));  }  FOOTER  {    write(*,toc(4,global));	write(*,"<HR>");	write(*,"<P>");	write(*,"<TABLE cols=3 border=0 width=100%>");	write(*,"<TR>");	write(*,"<TD width=\"33%\" align=\"left\">");    write(*,address(maintitle()));    write(*,"</TD>");	write(*,"<TD width=\"34%\" align=\"center\"><FONT size=-1>&COPY; <A HREF=\"http://www.mjolner.com\">Mj&oslash;lner Informatics</A></FONT></TD>");	write(*,"<TD width=\"33%\" align=\"right\"><FONT size=-1><SCRIPT LANGUAGE=JavaScript SRC=\"../javascript/lastmod.js\"></SCRIPT></FONT></TD>");	write(*,"</TABLE>");	write(*,"<P>");    write(*,paragraph(@NavPanel));  }}#-------------------------------------------------------------------------##  Define nodes for the index.##-------------------------------------------------------------------------##  Index node, simple. Index entries are sorted alphabetically and displayed#  without headers dividing entries into sections.#NODE IndexNodeS{  TITLE headingtext()  HEADER  {      @NavPanel=concatenate(      button(        image("../images/next.gif",     align: BOTTOM, alt: "Next",     border: 0,	                      class: "NavPanel"),        filename(next),        image("../images/nextg.gif",    align: BOTTOM, alt: "Next",     border: 0,                              class: "NavPanel")),      "\n",       button(        image("../images/prev.gif",     align: BOTTOM, alt: "Previous", border: 0,                              class: "NavPanel"),        filename(previous),        image("../images/prevg.gif",    align: BOTTOM, alt: "Previous", border: 0,                              class: "NavPanel")),      "\n",       button(        image("../images/top.gif",      align: BOTTOM, alt: "Top",      border: 0,	 		      class: "NavPanel"),        "../index.html",        image("../images/topg.gif",     align: BOTTOM, alt: "Top",      border: 0,			      class: "NavPanel")),      "\n",       button(        image("../images/content.gif",  align: BOTTOM, alt: "Contents", border: 0,			      class: "NavPanel"),        "index.html#_toc",        image("../images/contentg.gif", align: BOTTOM, alt: "Contents", border: 0,			      class: "NavPanel"))              );    write(*,paragraph(@NavPanel));    write(*,paragraph(maintitle()));  }  FOOTER  {    write(*,"<STRONG><A HREF=\"#_A\">A</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_B\">B</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_C\">C</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_D\">D</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_E\">E</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_F\">F</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_G\">G</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_H\">H</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_I\">I</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_J\">J</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_K\">K</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_L\">L</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_M\">M</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_N\">N</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_O\">O</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_P\">P</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_Q\">Q</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_R\">R</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_S\">S</A></STRONG> &nbsp; ");    write(*,"<STRONG><A HREF=\"#_T\">T</A></STRONG> &