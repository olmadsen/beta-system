ORIGIN '~beta/dependency/v1.1/dependency';
BODY 'private/checkslotsBody';
---dependencyGraphLib:attributes---
(* Pattern for scanning and checking consitency of slots
 * in the extent of a program.
 * This is a first and preliminary version!
 * Note: a ScanExtent/Domain/NonCompiled MUST have been performed before
 * calling CheckSlots!
 *)
CheckSlots:
  (# UnboundSlot:<
       (* The slot "slot" defined in fragmentGroup "FG"
        * is not bound by any slot in the fragment.
        * If hasProgramSlot is true then the program slot
        * is deined by some fragment
        *)
       (# FG: ^fragmentGroup; slot: ^text; hasProgramSlot: @boolean
       enter(FG[],slot[],hasProgramSlot)
       do INNER
       #);
     DoubleDeclaration:<
       (* The slot "slot" of category "category" is defined in
        * the fragments FG1 and Fg2
        *)
       (# slot,category, FG1,FG2: ^text
       enter(slot[],category[],FG1[],FG2[])
       do INNER
       #);
     PerhapsMissingBinds:<
       (* The slot "slot" has been deined more times than it has
        * been bound; DoubleDeclaration is a special case of this.
        * This probably needs to be redesigned!!
        *)
       (# slot: ^text; noOfDefs,noOfBinds: @integer
       enter(slot[],noOfDefs,noOfBinds) 
       do INNER
       #);
     withTrace: @boolean
  enter withTrace
  <<SLOT CheckSlotsBody:doPart>>
  #)
