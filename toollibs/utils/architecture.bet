ORIGIN '~beta/basiclib/betaenv';
LIB_DEF 'architecture' '../../lib';
BODY 'private/architecturebody';
--LIB: attributes--

is_native_executable: booleanValue
  (* Tests whether the file with name 'filename' is an executable
   * file for the current platform.
   *)
  (# filename: ^text
  enter (filename[])
  do <<SLOT is_native_executable:descriptor>>
  #);
type_of_executable: 
  (* A text describing the architecture of filename is returned.
   *)
  (# filename, exetype: ^text
  enter (filename[])
  do <<SLOT type_of_executable:descriptor>>
  exit exetype[]
  #)
