ORIGIN 'pnginterface';

BUILD nti '$$/winpng.obj' 'external/winpng.c' 'betacc -I external/libpng -I external/zlib $0 $1';
BUILD nti '$$/betaimage.obj' 'external/betaimage.c' 'betacc -I external/libpng -I external/zlib $0 $1';
BUILD nti '$$/inv_cmap.obj' 'external/inv_cmap.c' 'betacc -I external/libpng -I external/zlib $0 $1';
BUILD nti '$$/colorhash.obj' 'external/colorhash.c' 'betacc -I external/libpng -I external/zlib $0 $1';


-- lib: attributes --


write_bmp_to_png: external
  (# hdc: @int32;
     hbitmap: @int32;
     file_name: [1] @char;
  enter (hdc, hbitmap, file_name)
  #);

write_dib_to_png: external
  (# hdc: @int32;
     hbitmap: @int32;
     file_name: [1] @char;
  enter (hdc, hbitmap, file_name)
  #);

