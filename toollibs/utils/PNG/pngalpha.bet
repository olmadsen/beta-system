ORIGIN '~beta/guienv/graphics';
BODY 'private/pngalphabody';
-- pixmapLib: attributes --


readAlpha:
  (# error:< exception
       (# what: ^Text;
       enter what[]
       do what[] -> msg.puttext;
          INNER;
       #);
     name: ^Text;
  enter name[]
  <<SLOT pixmapReadAlpha: doPart>>
  #);
readRGB:
  (# error:< exception
       (# what: ^Text;
       enter what[]
       do what[] -> msg.puttext;
          INNER;
       #);
     name: ^Text;
  enter name[]
  <<SLOT pixmapReadRGB: doPart>>
  #);
readAlphaImage:
  (# error:< exception
       (# what: ^Text;
       enter what[]
       do what[] -> msg.puttext;
          INNER;
       #);
     name: ^Text;
  enter name[]
  <<SLOT pixmapReadAlphaImage: doPart>>
  #);
-- graphicsLib: attributes --

drawRasterAlpha:
  (# pm: ^Pixmap;
     x, y: @integer;
  enter (pm[], x, y)
  <<SLOT graphicsDrawRasterAlpha: doPart>>
  #);
