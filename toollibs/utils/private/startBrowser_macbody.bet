ORIGIN 'startBrowserBody';
INCLUDE '~beta/maclib/signaturetoapp';

--startBrowserdopart:dopart--
do (# PSN: @ProcessSerialNumber;
		err: @integer;
	do (netscapeSignature, NONE, PSN[], NONE, NONE, Sig2App_FindProcess, launchContinue) -> SignatureToApp -> err;
		(if err <> 0 then
			(explorerSignature, NONE, PSN[], NONE, NONE, Sig2App_FindProcess, launchContinue) -> SignatureToApp -> err;
		if);
		(if err <> 0 then
			(netscapeSignature, NONE, PSN[], NONE, NONE, Sig2App_LaunchApplication, launchContinue) -> SignatureToApp -> err;
		if);
		(if err <> 0 then
			(explorerSignature, NONE, PSN[], NONE, NONE, Sig2App_LaunchApplication, launchContinue) -> SignatureToApp -> err;
		if);
		(if err = 0 then
			(if url[] <> NONE then
				(if url.length <> 0 then
					(@@url.T[1], url.lgth, PSN[]) -> SendGetURL -> err;
				if);
			if);
		if);
	#)
