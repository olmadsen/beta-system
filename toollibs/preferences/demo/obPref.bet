ORIGIN '~beta/toollibs/preferences/preferencesDB';
INCLUDE '~beta/basiclib/formatio';
INCLUDE 'obPrefDefns';

--- program: descriptor ---
(# prefs: @preferences; t: @text
do 
   prefs.open(# do prefs.registerObjectbrowserPreferences #);
   
   l: ('testPreferences', 'ObjectBrowser')
     ->prefs.load(# optionsError:: (# do leave l #) #);
   
   true->prefs.fastBrowseMode;
   
   '***final preferences:'->putline;
   (screen[],true)->prefs.dump;
   
   ('testPreferences', 'ObjectBrowser')->prefs.save;
#)
