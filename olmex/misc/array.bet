ORIGIN '~beta/basiclib/v1.4/betaenv';
BODY 'arrayImpl'
---lib:attributes--
array:
  (# element:< object;
     low:< integerValue(#<<SLOT low:doPart>>#);
     high:< integerValue(#<<SLOT high:doPart>>#);
     put:
       (# index: @integer; e: ^element
       enter(index,e[]) <<SLOT put:doPart>> 
       #);
     get:
       (# index: @integer; e: ^element
       enter index <<SLOT get:doPart>>
       exit e[]
       #);
     extendLow: (# n: @integer enter n <<SLOT extendLow:doPart>> #);
     extendHigh: (# n: @integer enter n <<SLOT extendHigh:doPart>> #);
     indexOutOfRange:< exception
       (# index: @integer enter index <<SLOT indexOutOfRange:doPart>> #);
     
     rep: @<<SLOT rep:descriptor>>;
     initialized: @boolean; (* must be here due to init problem*)
  #);
