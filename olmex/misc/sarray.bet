ORIGIN '~beta/basiclib/betaenv'
---lib:attributes---
indexed: (# #);
array: indexed
  (# range:< integerValue;
     element:< object;
     R: [range] ^element;
     init:<
       (#
       do INNER;
          (for i: range repeat
               &element[] -> R[i][]
          for)
       #);
     
     get:
       (# inx: @integer
       enter inx
       exit R[inx][]
       #);
     put:
       (# V: ^element; inx: @integer
       enter(V[],inx)
       do V[] -> R[inx][]
       exit V
       #)
  #)
---program:descriptor---
(# X: @array
     (# range::< (# do 10 -> value #);
        element::< text
     #);
   T: ^text
do X.init;
   (for i: X.range repeat
        ('Hello',i) -> X.put
   for);
   (for i: X.range repeat 
        i -> (i -> X.get).putint
   for);
   (for i: X.range repeat
        i -> X.get -> T[];
        T[] -> puttext;
   for);
   newline
#)

