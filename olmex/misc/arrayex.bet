ORIGIN '~beta/basiclib/v1.4/betaenv';
INCLUDE 'array'
--program:descriptor--
(#
   A: @array
     (# element::text; low::(#do 12->value #); high::(#do 15->value#);
        indexOutOfRange::(#do msg[]->putline; true->continue#)          
     #);
   
   T: @text;
   T1: ^text 
do (A.low,A.high)->forTo
   (#
   do T.clear;; inx->T.putint; ':Hello\n'->T.append;  (inx,T.copy)->A.put
   #);
   
   (A.low,A.high)->forTo
   (#
   do inx->A.get->puttext;
   #);
   
   newline;
   
   5->A.extendHigh;
   
   (16,A.high)->forTo
   (#
   do T.clear; inx->T.putint; ':Hello\n'->T.append;  (inx,T.copy)->A.put
   #);
   
   (A.low,A.high)->forTo
   (#
   do inx->A.get->puttext
   #);
   
   1->A.get->puttext;
   12->A.extendLow
#)
