ORIGIN '~beta/basiclib/betaenv'
---lib:attributes---
Unit: 
  (# isAtom:< BooleanValue;
     PP:< (# log: @text do INNER exit log[] #);
  do INNER
  #);

Atom: Unit
  (# isAtom:: TrueValue;
     a: ^text; 
     PP::< (#do a[] -> log.putText #)
  enter a[] 
  exit this(Atom)[]
  #);

Conss: Unit
  (# car: ^Unit; cdr: ^Conss;
     PP::< 
       (# L: ^Conss
       do '(' -> log.put;
          (if car[] <> NONE then car.PP->log.puttext if);
          cdr[] -> L[];
          Loop:
            (if L[] <> NONE then 
                (if L.car[] <> NONE then 
                    ' ' -> log.put; L.car.PP->log.puttext 
                if);
                L.cdr[] -> L[];
                restart Loop
            if);
          ')' -> log.put
       #)
  enter(car[],cdr[])
  exit this(Conss)[]
  #);


Append:
  (* [L1[],L2[],...,Ln[]] -> Append ->  R[] *)
  (# L1,L2,R: ^Conss
  enter(L1[],L2[])
  do (if L1[] = NONE then 
         L2[] -> R[]
      else 
         (L1.car[], (L1.cdr[],L2[]) -> &Append) -> &Conss -> R[] 
     if)
  exit R[]
  #);




