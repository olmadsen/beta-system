ORIGIN 'betacfl';

-- treeLevelLib: attributes --


betaGrammarInit:
	(# path: ^Text;
		name: ^Text;
		out: ^Stream;
		
		MPSError:<
			(# msg: ^Text;
			do INNER;
			#);
		noParserAvailable:<
			(# msg: ^Text;
			do INNER;
			#);
	enter (path[], name[], out[])
	do (# grammarpath: ^Text;
		do '-parser' -> (path.copy).append -> grammarpath[];
			(grammarpath[], '.') -> thePathHandler.convertFilePath -> init;
			THIS(TreeLevel)[] -> grammartable.beta[];
		#)
	#);