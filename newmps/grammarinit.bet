ORIGIN 'betacfl';
INCLUDE 'private/treelevelbody';

-- treeLevelLib: attributes --


betaGrammarInit:
  (# path: ^Text;
     name: ^Text;
     out: ^Stream;
     
     MPSError:<
       (# msg: ^Text;
       do INNER;
       #);
     noParserAvailable:<
       (# msg: ^Text;
       do INNER;
       #);
  enter (path[], name[], out[])
  do (# grammarpath: ^Text;
     do '-parser' -> (path.copy).append -> grammarpath[];
        (grammarpath[], '.') -> thePathHandler.convertFilePath -> init;
        ('objectdescriptor','descriptor') -> parser.private.b.defineNonTAlias;
        ('attributedecl','attributes') -> parser.private.b.defineNonTAlias;
        THIS(TreeLevel)[] -> grammartable.default[];
        THIS(TreeLevel)[] -> grammartable.beta[];
     #)
  #);
