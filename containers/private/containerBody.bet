--- containertheScannerDopart : doPart ---
do
   true->s.first;
   s.start;
   INNER theScanner;
   s.end;
   false->s.first; (* enable static rescanning *)
   
--- containerScanDopart:DoPart ---
do
   (if not first then 
       (this(scan)[],&where[])->theScanner
    else
       INNER Scan;
   if);
   
--- containerFindDopart:DoPart ---
do
   start;
   INNER find;
   end;
   
   

