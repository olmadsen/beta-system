          arg2: @int32;
       enter (arg1, arg2)
       exit result[]
       #);
     getAtIndex: proc
       (# result: ^_String;
          arg1: @int32;
          arg2: @int32;
       enter (arg1, arg2)
       exit result[]
       #);
