       #);
     getCaretPosition: proc
       (# result: @int32;
       exit result
       #);
     getCharCount: proc
       (# result: @int32;
       exit result
       #);
     getCharacterAttribute: proc
       (# result: ^_AttributeSet;
          arg1: @int32;
       enter (arg1)
       exit result[]
       #);
     getCharacterBounds: proc
       (# result: ^_Rectangle;
          arg1: @int32;
       enter (arg1)
       exit result[]
       #);
     getIndexAtPoint: proc
       (# result: @int32;
          arg1: ^_Point;
