ORIGIN '_AccessibleText';
INCLUDE '~beta/javalib/java/awt/_Rectangle';
INCLUDE '~beta/javalib/java/awt/_Point';
INCLUDE '~beta/javalib/java/lang/_String';
INCLUDE '~beta/javalib/javax/swing/text/_AttributeSet';
--LIB: attributes--

(* Java AccessibleText interface members.
 * See _AccessibleText.bet for class declaration.
 *)
AccessibleText: _AccessibleText
  (#
     (* Public/protected fields *)

     (* STATIC: *)
     CHARACTER: @int32;
     (* STATIC: *)
     WORD: @int32;
     (* STATIC: *)
     SENTENCE: @int32;

