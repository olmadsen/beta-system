ORIGIN '~beta/basiclib/betaenv';
BUILD jvm 'jvm/beta/BetaHelper.class' 'BetaHelper.java' 'javac -classpath jvm -d jvm $1';

--LIB: attributes--

BetaHelper: ExternalClass
  (# 
     (* static methods *)
     int2long: proc
       (# I: @int32;
          J: @int64;
       enter I
       exit J
       #);

     long2int: proc
       (# J: @int64;
          I: @int32;
       enter J
       exit I
       #);
  do 'beta/BetaHelper' -> classname;
  #);
