ORIGIN '~beta/basiclib/betaenv';
BUILD jvm 'jvm/beta/BetaHelper.class' 'BetaHelper.java' 'javac -classpath jvm -d jvm $1';
INCLUDE 'java/io/InputStream';
INCLUDE 'java/io/PrintStream';

--LIB: attributes--

BetaHelper: ExternalClass
  (# 
     (* static methods *)
     int2long: static_proc
       (# I: @int32;
          J: @int64;
       enter I
       exit J
       #);

     long2int: static_proc
       (# J: @int64;
          I: @int32;
       enter J
       exit I
       #);
     
     get_System_in: static_proc
       (# in: ^InputStream
       exit in[]
       #);
     
     get_System_out: static_proc
       (# out: ^PrintStream
       exit out[]
       #);
     
  do 'beta/BetaHelper' -> classname;
  #);
