ORIGIN '_AppletContext';
INCLUDE '~beta/javalib/java/applet/_Applet';
INCLUDE '~beta/javalib/java/util/_Iterator';
INCLUDE '~beta/javalib/java/net/_URL';
INCLUDE '~beta/javalib/java/lang/_String';
INCLUDE '~beta/javalib/java/applet/_AudioClip';
INCLUDE '~beta/javalib/java/util/_Enumeration';
INCLUDE '~beta/javalib/java/awt/_Image';
INCLUDE '~beta/javalib/java/io/_InputStream';
--LIB: attributes--

(* Java AppletContext interface members.
 * See _AppletContext.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/applet/AppletContext.html
 *)
AppletContext: _AppletContext
  (#
     (* Public/protected methods *)

     getAudioClip: proc
       (# result: ^_AudioClip;
          arg1: ^_URL;
       enter (arg1[])
       exit result[]
       #);
     getImage: proc
       (# result: ^_Image;
          arg1: ^_URL;
       enter (arg1[])
       exit result[]
       #);
     showStatus: proc
       (# 
          arg1: ^_String;
       enter (arg1[])
       #);
     getApplet: proc
       (# result: ^_Applet;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);
     getApplets: proc
       (# result: ^_Enumeration;
       exit result[]
       #);
     getStream: proc
       (# result: ^_InputStream;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);
     getStreamKeys: proc
       (# result: ^_Iterator;
       exit result[]
       #);
     setStream: proc
       (# 
          arg1: ^_String;
          arg2: ^_InputStream;
       enter (arg1[], arg2[])
       #);
     showDocument_URL: proc (* showDocument *)
       (# 
          arg1: ^_URL;
       enter (arg1[])
       do 'showDocument' -> procname;
       #);
     showDocument_URL_String: proc (* showDocument *)
       (# 
          arg1: ^_URL;
          arg2: ^_String;
       enter (arg1[], arg2[])
       do 'showDocument' -> procname;
       #);
  do '|java/applet/AppletContext' -> className;
     INNER;
  #);

