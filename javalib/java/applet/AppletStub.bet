 * See http://java.sun.com/j2se/1.4.2/docs/api/java/applet/AppletStub.html
 *)
AppletStub: _AppletStub
  (#
     (* Public/protected methods *)

     getCodeBase: proc
       (# result: ^_URL;
       exit result[]
       #);
     appletResize: proc
       (# 
          arg1: @int32;
          arg2: @int32;
       enter (arg1, arg2)
       #);
