       (# result: ^_AppletContext;
       exit result[]
       #);
     getDocumentBase: proc
       (# result: ^_URL;
       exit result[]
       #);
     getParameter: proc
       (# result: ^_String;
          arg1: ^_String;
       enter (arg1[])
