ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/javalib/java/io/File';
INCLUDE '~beta/javalib/java/lang/String';
INCLUDE '~beta/javalib/java/nio/channels/FileChannel';
INCLUDE '~beta/javalib/java/io/FileDescriptor';
INCLUDE '~beta/javalib/java/io/InputStream';
--LIB: attributes--

(* Java FileInputStream class.
 * See http://java.sun.com/j2se/1.4.1/docs/api/java/io/FileInputStream.html
 *)
FileInputStream: InputStream
  (#
     (* Public/protected constructors *)

     (* FIXME: hand written *)
     _init:
       (# 
          arg1: ^JavaFile;
       enter (arg1[])
       #);

     _init_String: cons (* constructor *)
       (# 
          arg1: ^String;
       enter (arg1[])
       #);
     _init_File: cons (* constructor *)
       (# 
          arg1: ^JavaFile;
       enter (arg1[])
       #);
     _init_FileDescriptor: cons (* constructor *)
       (# 
          arg1: ^FileDescriptor;
       enter (arg1[])
       #);

     (* Public/protected methods *)

     finalize: proc
       (# 
       #);
     read: proc (* read *)
       (# result: @int32;
       do 'read' -> procname;
       exit result
       #);
     read_ArrayOfB: proc (* read *)
       (# result: @int32;
          arg1: [0]@char;
       enter (arg1)
       do 'read' -> procname;
       exit result
       #);
     read_ArrayOfB_I_I: proc (* read *)
       (# result: @int32;
          arg1: [0]@char;
          arg2: @int32;
          arg3: @int32;
       enter (arg1, arg2, arg3)
       do 'read' -> procname;
       exit result
       #);
     skip: proc
       (# result: @int64;
          arg1: @int64;
       enter (arg1)
       exit result
       #);
     available: proc
       (# result: @int32;
       exit result
       #);
     close: proc
       (# 
       #);
     getFD: proc
       (# result: ^FileDescriptor;
       exit result[]
       #);
     getChannel: proc
       (# result: ^FileChannel;
       exit result[]
       #);
  do 'java/io/FileInputStream' -> className;
  INNER;
  #);

