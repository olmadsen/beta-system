ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/javalib/java/io/Reader';
INCLUDE '~beta/javalib/java/lang/String';
--LIB: attributes--

(* Java BufferedReader class.
 * See http://java.sun.com/j2se/1.4.1/docs/api/java/io/BufferedReader.html
 *)
BufferedReader: Reader
  (#
     (* Public/protected constructors *)

     (* FIXME: hand written *)
     _init: (* constructor *)
       (# 
          arg1: ^Reader;
       enter (arg1[])
       #);
     
     _init_Reader_I: cons (* constructor *)
       (# 
          arg1: ^Reader;
          arg2: @int32;
       enter (arg1[], arg2)
       #);
     _init_Reader: cons (* constructor *)
       (# 
          arg1: ^Reader;
       enter (arg1[])
       #);

     (* Public/protected methods *)

     readLine: proc
       (# result: ^String;
       exit result[]
       #);
     mark: proc
       (# 
          arg1: @int32;
       enter (arg1)
       #);
     reset: proc
       (# 
       #);
     read: proc (* read *)
       (# result: @int32;
       do 'read' -> procname;
       exit result
       #);
     read_ArrayOfC_I_I: proc (* read *)
       (# result: @int32;
          arg1: [0]@char;
          arg2: @int32;
          arg3: @int32;
       enter (arg1, arg2, arg3)
       do 'read' -> procname;
       exit result
       #);
     skip: proc
       (# result: @int64;
          arg1: @int64;
       enter (arg1)
       exit result
       #);
     close: proc
       (# 
       #);
     markSupported: proc
       (# result: @boolean;
       exit result
       #);
     ready: proc
       (# result: @boolean;
       exit result
       #);
  do 'java/io/BufferedReader' -> className;
  INNER;
  #);

