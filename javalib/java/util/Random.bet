ORIGIN '_Random';
--LIB: attributes--

(* Java Random class members.
 * See _Random.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/util/Random.html
 *)
Random: _Random
  (#
     (* Public/protected constructors *)

     _init: cons (* constructor *)
       (# result: ^Random;
       exit result[]
       #);
     _init_J: cons (* constructor *)
       (# result: ^Random;
          arg1: @int64;
       enter (arg1)
       exit result[]
       #);

     (* Public/protected methods *)

     next: proc
       (# result: @int32;
          arg1: @int32;
       enter (arg1)
       exit result
       #);
     nextDouble: proc
       (# result: @real;
       exit result
       #);
     nextInt: proc (* nextInt *)
       (# result: @int32;
       do 'nextInt' -> procname;
       exit result
       #);
     nextInt_I: proc (* nextInt *)
       (# result: @int32;
          arg1: @int32;
       enter (arg1)
       do 'nextInt' -> procname;
       exit result
       #);
     nextBytes: proc
       (# 
          arg1: [0]@int8;
       enter (arg1[])
       #);
     nextBoolean: proc
       (# result: @boolean;
       exit result
       #);
     nextFloat: proc
       (# result: @real32;
       exit result
       #);
     nextGaussian: proc
       (# result: @real;
       exit result
       #);
     nextLong: proc
       (# result: @int64;
       exit result
       #);
     setSeed: proc
       (# 
          arg1: @int64;
       enter (arg1)
       #);
  do 'java/util/Random' -> className;
     INNER;
  #);

