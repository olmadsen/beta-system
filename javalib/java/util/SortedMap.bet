ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/javalib/java/util/Comparator';
--LIB: attributes--

(* Java SortedMap class.
 * See http://java.sun.com/j2se/1.4.1/docs/api/java/util/SortedMap.html
 *)
SortedMap: ExternalClass
  (#
     (* Public/protected methods *)

     comparator: proc
       (# result: ^Comparator;
       exit result[]
       #);
     firstKey: proc
       (# result: ^Object;
       exit result[]
       #);
     lastKey: proc
       (# result: ^Object;
       exit result[]
       #);
     subMap: proc
       (# result: ^SortedMap;
          arg1: ^Object;
          arg2: ^Object;
       enter (arg1[], arg2[])
       exit result[]
       #);
     headMap: proc
       (# result: ^SortedMap;
          arg1: ^Object;
       enter (arg1[])
       exit result[]
       #);
     tailMap: proc
       (# result: ^SortedMap;
          arg1: ^Object;
       enter (arg1[])
       exit result[]
       #);
  do 'java/util/SortedMap' -> className;
  INNER;
  #);

