ORIGIN '_Throwable';
INCLUDE '~beta/javalib/java/lang/_StackTraceElement';
INCLUDE '~beta/javalib/java/io/_PrintStream';
INCLUDE '~beta/javalib/java/io/_PrintWriter';
INCLUDE '~beta/javalib/java/lang/_String';
--LIB: attributes--

(* Java Throwable class members.
 * See _Throwable.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/lang/Throwable.html
 *)
Throwable: _Throwable
  (#
     (* Public/protected constructors *)

     _init: cons (* constructor *)
       (# result: ^Throwable;
       exit result[]
       #);
     _init_String_Throwable: cons (* constructor *)
       (# result: ^Throwable;
          arg1: ^_String;
          arg2: ^Throwable;
       enter (arg1[], arg2[])
       exit result[]
       #);
     _init_Throwable: cons (* constructor *)
       (# result: ^Throwable;
          arg1: ^Throwable;
       enter (arg1[])
       exit result[]
       #);
     _init_String: cons (* constructor *)
       (# result: ^Throwable;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);

     (* Public/protected methods *)

     printStackTrace: proc (* printStackTrace *)
       (# 
       do 'printStackTrace' -> procname;
       #);
     printStackTrace_PrintStream: proc (* printStackTrace *)
       (# 
          arg1: ^_PrintStream;
       enter (arg1[])
       do 'printStackTrace' -> procname;
       #);
     printStackTrace_PrintWriter: proc (* printStackTrace *)
       (# 
          arg1: ^_PrintWriter;
       enter (arg1[])
       do 'printStackTrace' -> procname;
       #);
     fillInStackTrace: proc
       (# result: ^Throwable;
       exit result[]
       #);
     toString: proc
       (# result: ^_String;
       exit result[]
       #);
     initCause: proc
       (# result: ^Throwable;
          arg1: ^Throwable;
       enter (arg1[])
       exit result[]
       #);
     getCause: proc
       (# result: ^Throwable;
       exit result[]
       #);
     getLocalizedMessage: proc
       (# result: ^_String;
       exit result[]
       #);
     getMessage: proc
       (# result: ^_String;
       exit result[]
       #);
     getStackTrace: proc
       (# result: [0]^_StackTraceElement;
       exit result[]
       #);
     setStackTrace: proc
       (# 
          arg1: [0]^_StackTraceElement;
       enter (arg1[])
       #);
  do 'java/lang/Throwable' -> className;
     INNER;
  #);

