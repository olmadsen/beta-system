ORIGIN '_Number';
--LIB: attributes--

(* Java Number class members.
 * See _Number for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/lang/Number.html
 *)
Number: _Number
  (#
     (* Public/protected constructors *)

     _init: cons
       (# result: ^Number;
       exit result[]
       #);

     (* Public/protected methods *)

     byteValue: proc
       (# result: @int8;
       exit result
       #);
     doubleValue: proc
       (# result: @real;
       exit result
       #);
     floatValue: proc
       (# result: @real32;
       exit result
       #);
     intValue: proc
       (# result: @int32;
       exit result
       #);
     longValue: proc
       (# result: @int64;
       exit result
       #);
     shortValue: proc
       (# result: @int16;
       exit result
       #);
  do 'java/lang/Number' -> className;
     INNER;
  #);

