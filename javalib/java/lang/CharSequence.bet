ORIGIN '_CharSequence';
INCLUDE '~beta/javalib/java/lang/_String';
--LIB: attributes--

(* Java CharSequence class members.
 * See _CharSequence for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/lang/CharSequence.html
 *)
CharSequence: _CharSequence
  (#
     (* Public/protected methods *)

     toString: proc
       (# result: ^_String;
       exit result[]
       #);
     charAt: proc
       (# result: @char;
          arg1: @int32;
       enter (arg1)
       exit result
       #);
     length: proc
       (# result: @int32;
       exit result
       #);
     subSequence: proc
       (# result: ^CharSequence;
          arg1: @int32;
          arg2: @int32;
       enter (arg1, arg2)
       exit result[]
       #);
  do 'java/lang/CharSequence' -> className;
     INNER;
  #);

