ORIGIN '_Method';
INCLUDE '~beta/javalib/java/lang/_String';
INCLUDE '~beta/javalib/java/lang/reflect/_AccessibleObject';
INCLUDE '~beta/javalib/java/lang/_Class';
--LIB: attributes--

(* Java Method class members.
 * See _Method for class declaration.
 * See http://java.sun.com/j2se/1.4.1/docs/api/java/lang/reflect/Method.html
 *)
Method: _Method
  (#
     (* Public/protected methods *)

     hashCode: proc
       (# result: @int32;
       exit result
       #);
     getModifiers: proc
       (# result: @int32;
       exit result
       #);
     invoke: proc
       (# result: ^Object;
          arg1: ^Object;
          arg2: [0]^Object;
       enter (arg1[], arg2[])
       exit result[]
       #);
     equals: proc
       (# result: @boolean;
          arg1: ^Object;
       enter (arg1[])
       exit result
       #);
     getName: proc
       (# result: ^_String;
       exit result[]
       #);
     toString: proc
       (# result: ^_String;
       exit result[]
       #);
     getDeclaringClass: proc
       (# result: ^_Class;
       exit result[]
       #);
     getParameterTypes: proc
       (# result: [0]^_Class;
       exit result[]
       #);
     getReturnType: proc
       (# result: ^_Class;
       exit result[]
       #);
     getExceptionTypes: proc
       (# result: [0]^_Class;
       exit result[]
       #);
  do 'java/lang/reflect/Method' -> className;
     INNER;
  #);

