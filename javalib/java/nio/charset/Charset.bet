ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/javalib/java/util/SortedMap';
INCLUDE '~beta/javalib/java/util/Set';
INCLUDE '~beta/javalib/java/lang/String';
INCLUDE '~beta/javalib/java/nio/charset/CharsetDecoder';
INCLUDE '~beta/javalib/java/nio/ByteBuffer';
INCLUDE '~beta/javalib/java/nio/charset/CharsetEncoder';
INCLUDE '~beta/javalib/java/nio/CharBuffer';
INCLUDE '~beta/javalib/java/util/Locale';
--LIB: attributes--

(* Java Charset class.
 * See http://java.sun.com/j2se/1.4.1/docs/api/java/nio/charset/Charset.html
 *)
Charset: ExternalClass
  (#
     (* Public/protected constructors *)

     _init: cons
       (# 
          arg1: ^String;
          arg2: [0]^String;
       enter (arg1[], arg2)
       #);

     (* Public/protected methods *)

     hashCode: proc
       (# result: @int32;
       exit result
       #);
     compareTo: proc
       (# result: @int32;
          arg1: ^Object;
       enter (arg1[])
       exit result
       #);
     name: proc
       (# result: ^String;
       exit result[]
       #);
     equals: proc
       (# result: @boolean;
          arg1: ^Object;
       enter (arg1[])
       exit result
       #);
     toString: proc
       (# result: ^String;
       exit result[]
       #);
     decode: proc
       (# result: ^CharBuffer;
          arg1: ^ByteBuffer;
       enter (arg1[])
       exit result[]
       #);
     encode_String: proc (* encode *)
       (# result: ^ByteBuffer;
          arg1: ^String;
       enter (arg1[])
       do 'encode' -> procname;
       exit result[]
       #);
     encode_CharBuffer: proc (* encode *)
       (# result: ^ByteBuffer;
          arg1: ^CharBuffer;
       enter (arg1[])
       do 'encode' -> procname;
       exit result[]
       #);
     forName: static_proc
       (# result: ^Charset;
          arg1: ^String;
       enter (arg1[])
       exit result[]
       #);
     contains: proc
       (# result: @boolean;
          arg1: ^Charset;
       enter (arg1[])
       exit result
       #);
     isSupported: static_proc
       (# result: @boolean;
          arg1: ^String;
       enter (arg1[])
       exit result
       #);
     aliases: proc
       (# result: ^Set;
       exit result[]
       #);
     availableCharsets: static_proc
       (# result: ^SortedMap;
       exit result[]
       #);
     displayName: proc (* displayName *)
       (# result: ^String;
       do 'displayName' -> procname;
       exit result[]
       #);
     displayName_Locale: proc (* displayName *)
       (# result: ^String;
          arg1: ^Locale;
       enter (arg1[])
       do 'displayName' -> procname;
       exit result[]
       #);
     isRegistered: proc
       (# result: @boolean;
       exit result
       #);
     newDecoder: proc
       (# result: ^CharsetDecoder;
       exit result[]
       #);
     newEncoder: proc
       (# result: ^CharsetEncoder;
       exit result[]
       #);
     canEncode: proc
       (# result: @boolean;
       exit result
       #);
  do 'java/nio/charset/Charset' -> className;
  INNER;
  #);

