ORIGIN '~beta/basiclib/betaenv';
(* INCLUDE '~beta/javalib/java/nio/charset/Charset';*)
INCLUDE '~beta/javalib/java/nio/charset/CodingErrorAction';
INCLUDE '~beta/javalib/java/nio/ByteBuffer';
INCLUDE '~beta/javalib/java/nio/charset/CoderResult';
INCLUDE '~beta/javalib/java/lang/CharSequence';
INCLUDE '~beta/javalib/java/nio/CharBuffer';
--LIB: attributes--

(* FIXME: To prevent circular include: *)
Charset: ExternalClass
  (# 
  do 'java/nio/charset/Charset'->className;
  #);

(* Java CharsetEncoder class.
 * See http://java.sun.com/j2se/1.4.1/docs/api/java/nio/charset/CharsetEncoder.html
 *)
CharsetEncoder: ExternalClass
  (#
     (* Public/protected constructors *)

     _init_Charset_F_F_ArrayOfB: cons (* constructor *)
       (# 
          arg1: ^Charset;
          arg2: @real32;
          arg3: @real32;
          arg4: [0]@char;
       enter (arg1[], arg2, arg3, arg4)
       #);
     _init_Charset_F_F: cons (* constructor *)
       (# 
          arg1: ^Charset;
          arg2: @real32;
          arg3: @real32;
       enter (arg1[], arg2, arg3)
       #);

     (* Public/protected methods *)

     encode_CharBuffer_ByteBuffer_Z: proc (* encode *)
       (# result: ^CoderResult;
          arg1: ^CharBuffer;
          arg2: ^ByteBuffer;
          arg3: @boolean;
       enter (arg1[], arg2[], arg3)
       do 'encode' -> procname;
       exit result[]
       #);
     encode_CharBuffer: proc (* encode *)
       (# result: ^ByteBuffer;
          arg1: ^CharBuffer;
       enter (arg1[])
       do 'encode' -> procname;
       exit result[]
       #);
     flush: proc
       (# result: ^CoderResult;
          arg1: ^ByteBuffer;
       enter (arg1[])
       exit result[]
       #);
     reset: proc
       (# result: ^CharsetEncoder;
       exit result[]
       #);
     charset: proc
       (# result: ^Charset;
       exit result[]
       #);
     implFlush: proc
       (# result: ^CoderResult;
          arg1: ^ByteBuffer;
       enter (arg1[])
       exit result[]
       #);
     canEncode_CharSequence: proc (* canEncode *)
       (# result: @boolean;
          arg1: ^CharSequence;
       enter (arg1[])
       do 'canEncode' -> procname;
       exit result
       #);
     canEncode_C: proc (* canEncode *)
       (# result: @boolean;
          arg1: @char;
       enter (arg1)
       do 'canEncode' -> procname;
       exit result
       #);
     onMalformedInput: proc
       (# result: ^CharsetEncoder;
          arg1: ^CodingErrorAction;
       enter (arg1[])
       exit result[]
       #);
     onUnmappableCharacter: proc
       (# result: ^CharsetEncoder;
          arg1: ^CodingErrorAction;
       enter (arg1[])
       exit result[]
       #);
     encodeLoop: proc
       (# result: ^CoderResult;
          arg1: ^CharBuffer;
          arg2: ^ByteBuffer;
       enter (arg1[], arg2[])
       exit result[]
       #);
     averageBytesPerChar: proc
       (# result: @real32;
       exit result
       #);
     maxBytesPerChar: proc
       (# result: @real32;
       exit result
       #);
     replacement: proc
       (# result: [0]@char;
       exit result
       #);
     malformedInputAction: proc
       (# result: ^CodingErrorAction;
       exit result[]
       #);
     unmappableCharacterAction: proc
       (# result: ^CodingErrorAction;
       exit result[]
       #);
     replaceWith: proc
       (# result: ^CharsetEncoder;
          arg1: [0]@char;
       enter (arg1)
       exit result[]
       #);
     implReplaceWith: proc
       (# 
          arg1: [0]@char;
       enter (arg1)
       #);
     isLegalReplacement: proc
       (# result: @boolean;
          arg1: [0]@char;
       enter (arg1)
       exit result
       #);
     implOnMalformedInput: proc
       (# 
          arg1: ^CodingErrorAction;
       enter (arg1[])
       #);
     implOnUnmappableCharacter: proc
       (# 
          arg1: ^CodingErrorAction;
       enter (arg1[])
       #);
     implReset: proc
       (# 
       #);
  do 'java/nio/charset/CharsetEncoder' -> className;
  INNER;
  #);

