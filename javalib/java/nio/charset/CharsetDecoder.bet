ORIGIN '~beta/basiclib/betaenv';
(*INCLUDE '~beta/javalib/java/nio/charset/Charset';*)
INCLUDE '~beta/javalib/java/lang/String';
INCLUDE '~beta/javalib/java/nio/charset/CodingErrorAction';
INCLUDE '~beta/javalib/java/nio/charset/CoderResult';
INCLUDE '~beta/javalib/java/nio/ByteBuffer';
INCLUDE '~beta/javalib/java/nio/CharBuffer';
--LIB: attributes--

(* FIXME: To prevent circular include: *)
Charset: ExternalClass
  (# 
  do 'java/nio/charset/Charset'->className;
  #);

(* Java CharsetDecoder class.
 * See http://java.sun.com/j2se/1.4.1/docs/api/java/nio/charset/CharsetDecoder.html
 *)
CharsetDecoder: ExternalClass
  (#
     (* Public/protected constructors *)

     _init_Charset_F_F: cons (* constructor *)
       (# 
          arg1: ^Charset;
          arg2: @real32;
          arg3: @real32;
       enter (arg1[], arg2, arg3)
       #);

     (* Public/protected methods *)

     decode_ByteBuffer: proc (* decode *)
       (# result: ^CharBuffer;
          arg1: ^ByteBuffer;
       enter (arg1[])
       do 'decode' -> procname;
       exit result[]
       #);
     decode_ByteBuffer_CharBuffer_Z: proc (* decode *)
       (# result: ^CoderResult;
          arg1: ^ByteBuffer;
          arg2: ^CharBuffer;
          arg3: @boolean;
       enter (arg1[], arg2[], arg3)
       do 'decode' -> procname;
       exit result[]
       #);
     flush: proc
       (# result: ^CoderResult;
          arg1: ^CharBuffer;
       enter (arg1[])
       exit result[]
       #);
     reset: proc
       (# result: ^CharsetDecoder;
       exit result[]
       #);
     charset: proc
       (# result: ^Charset;
       exit result[]
       #);
     implFlush: proc
       (# result: ^CoderResult;
          arg1: ^CharBuffer;
       enter (arg1[])
       exit result[]
       #);
     onMalformedInput: proc
       (# result: ^CharsetDecoder;
          arg1: ^CodingErrorAction;
       enter (arg1[])
       exit result[]
       #);
     onUnmappableCharacter: proc
       (# result: ^CharsetDecoder;
          arg1: ^CodingErrorAction;
       enter (arg1[])
       exit result[]
       #);
     replacement: proc
       (# result: ^String;
       exit result[]
       #);
     malformedInputAction: proc
       (# result: ^CodingErrorAction;
       exit result[]
       #);
     unmappableCharacterAction: proc
       (# result: ^CodingErrorAction;
       exit result[]
       #);
     replaceWith: proc
       (# result: ^CharsetDecoder;
          arg1: ^String;
       enter (arg1[])
       exit result[]
       #);
     implReplaceWith: proc
       (# 
          arg1: ^String;
       enter (arg1[])
       #);
     implOnMalformedInput: proc
       (# 
          arg1: ^CodingErrorAction;
       enter (arg1[])
       #);
     implOnUnmappableCharacter: proc
       (# 
          arg1: ^CodingErrorAction;
       enter (arg1[])
       #);
     implReset: proc
       (# 
       #);
     maxCharsPerByte: proc
       (# result: @real32;
       exit result
       #);
     decodeLoop: proc
       (# result: ^CoderResult;
          arg1: ^ByteBuffer;
          arg2: ^CharBuffer;
       enter (arg1[], arg2[])
       exit result[]
       #);
     averageCharsPerByte: proc
       (# result: @real32;
       exit result
       #);
     isAutoDetecting: proc
       (# result: @boolean;
       exit result
       #);
     isCharsetDetected: proc
       (# result: @boolean;
       exit result
       #);
     detectedCharset: proc
       (# result: ^Charset;
       exit result[]
       #);
  do 'java/nio/charset/CharsetDecoder' -> className;
  INNER;
  #);

