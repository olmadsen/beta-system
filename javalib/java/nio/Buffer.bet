ORIGIN '_Buffer';
--LIB: attributes--

(* Java Buffer class members.
 * See _Buffer for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/nio/Buffer.html
 *)
Buffer: _Buffer
  (#
     (* Public/protected methods *)

     limit: proc (* limit *)
       (# result: @int32;
       do 'limit' -> procname;
       exit result
       #);
     limit_I: proc (* limit *)
       (# result: ^Buffer;
          arg1: @int32;
       enter (arg1)
       do 'limit' -> procname;
       exit result[]
       #);
     clear: proc
       (# result: ^Buffer;
       exit result[]
       #);
     capacity: proc
       (# result: @int32;
       exit result
       #);
     flip: proc
       (# result: ^Buffer;
       exit result[]
       #);
     hasRemaining: proc
       (# result: @boolean;
       exit result
       #);
     isReadOnly: proc
       (# result: @boolean;
       exit result
       #);
     mark: proc
       (# result: ^Buffer;
       exit result[]
       #);
     position: proc (* position *)
       (# result: @int32;
       do 'position' -> procname;
       exit result
       #);
     position_I: proc (* position *)
       (# result: ^Buffer;
          arg1: @int32;
       enter (arg1)
       do 'position' -> procname;
       exit result[]
       #);
     remaining: proc
       (# result: @int32;
       exit result
       #);
     reset: proc
       (# result: ^Buffer;
       exit result[]
       #);
     rewind: proc
       (# result: ^Buffer;
       exit result[]
       #);
  do 'java/nio/Buffer' -> className;
     INNER;
  #);

