ORIGIN '_TileObserver';
INCLUDE '~beta/javalib/java/awt/image/_WritableRenderedImage';
--LIB: attributes--

(* Java TileObserver interface members.
 * See _TileObserver.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/awt/image/TileObserver.html
 *)
TileObserver: _TileObserver
  (#
     (* Public/protected methods *)

     tileUpdate: proc
       (# 
          arg1: ^_WritableRenderedImage;
          arg2: @int32;
          arg3: @int32;
          arg4: @boolean;
       enter (arg1[], arg2, arg3, arg4)
       #);
  do '|java/awt/image/TileObserver' -> className;
     INNER;
  #);

