ORIGIN '_BufferedImageOp';
INCLUDE '~beta/javalib/java/awt/image/_ColorModel';
INCLUDE '~beta/javalib/java/awt/geom/_Rectangle2D';
INCLUDE '~beta/javalib/java/awt/_RenderingHints';
INCLUDE '~beta/javalib/java/awt/geom/_Point2D';
INCLUDE '~beta/javalib/java/awt/image/_BufferedImage';
--LIB: attributes--

(* Java BufferedImageOp interface members.
 * See _BufferedImageOp.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/awt/image/BufferedImageOp.html
 *)
BufferedImageOp: _BufferedImageOp
  (#
     (* Public/protected methods *)

     getBounds2D: proc
       (# result: ^_Rectangle2D;
          arg1: ^_BufferedImage;
       enter (arg1[])
       exit result[]
       #);
     getRenderingHints: proc
       (# result: ^_RenderingHints;
       exit result[]
       #);
     createCompatibleDestImage: proc
       (# result: ^_BufferedImage;
          arg1: ^_BufferedImage;
          arg2: ^_ColorModel;
       enter (arg1[], arg2[])
       exit result[]
       #);
     filter: proc
       (# result: ^_BufferedImage;
          arg1: ^_BufferedImage;
          arg2: ^_BufferedImage;
       enter (arg1[], arg2[])
       exit result[]
       #);
     getPoint2D: proc
       (# result: ^_Point2D;
          arg1: ^_Point2D;
          arg2: ^_Point2D;
       enter (arg1[], arg2[])
       exit result[]
       #);
  do '|java/awt/image/BufferedImageOp' -> className;
     INNER;
  #);

