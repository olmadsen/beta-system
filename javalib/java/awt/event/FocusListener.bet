ORIGIN '_FocusListener';
INCLUDE '~beta/javalib/java/awt/event/_FocusEvent';
--LIB: attributes--

(* Java FocusListener interface members.
 * See _FocusListener.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/awt/event/FocusListener.html
 *)
FocusListener: _FocusListener
  (#
     (* Public/protected methods *)

     focusGained: proc
       (# 
          arg1: ^_FocusEvent;
       enter (arg1[])
       #);
     focusLost: proc
       (# 
          arg1: ^_FocusEvent;
       enter (arg1[])
       #);
  do '|java/awt/event/FocusListener' -> className;
     INNER;
  #);

