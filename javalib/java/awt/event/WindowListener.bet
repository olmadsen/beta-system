ORIGIN '_WindowListener';
INCLUDE '~beta/javalib/java/awt/event/_WindowEvent';
--LIB: attributes--

(* Java WindowListener interface members.
 * See _WindowListener.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/awt/event/WindowListener.html
 *)
WindowListener: _WindowListener
  (#
     (* Public/protected methods *)

     windowActivated: proc
       (# 
          arg1: ^_WindowEvent;
       enter (arg1[])
       #);
     windowClosed: proc
       (# 
          arg1: ^_WindowEvent;
       enter (arg1[])
       #);
     windowClosing: proc
       (# 
          arg1: ^_WindowEvent;
       enter (arg1[])
       #);
     windowDeactivated: proc
       (# 
          arg1: ^_WindowEvent;
       enter (arg1[])
       #);
     windowDeiconified: proc
       (# 
          arg1: ^_WindowEvent;
       enter (arg1[])
       #);
     windowIconified: proc
       (# 
          arg1: ^_WindowEvent;
       enter (arg1[])
       #);
     windowOpened: proc
       (# 
          arg1: ^_WindowEvent;
       enter (arg1[])
       #);
  do '|java/awt/event/WindowListener' -> className;
     INNER;
  #);

