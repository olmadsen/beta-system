ORIGIN '_WindowFocusListener';
INCLUDE '~beta/javalib/java/awt/event/_WindowEvent';
--LIB: attributes--

(* Java WindowFocusListener interface members.
 * See _WindowFocusListener.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/awt/event/WindowFocusListener.html
 *)
WindowFocusListener: _WindowFocusListener
  (#
     (* Public/protected methods *)

     windowGainedFocus: proc
       (# 
          arg1: ^_WindowEvent;
       enter (arg1[])
       #);
     windowLostFocus: proc
       (# 
          arg1: ^_WindowEvent;
       enter (arg1[])
       #);
  do '|java/awt/event/WindowFocusListener' -> className;
     INNER;
  #);

