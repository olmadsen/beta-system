ORIGIN '_DropTargetListener';
INCLUDE '~beta/javalib/java/awt/dnd/_DropTargetDragEvent';
INCLUDE '~beta/javalib/java/awt/dnd/_DropTargetEvent';
INCLUDE '~beta/javalib/java/awt/dnd/_DropTargetDropEvent';
--LIB: attributes--

(* Java DropTargetListener interface members.
 * See _DropTargetListener.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/awt/dnd/DropTargetListener.html
 *)
DropTargetListener: _DropTargetListener
  (#
     (* Public/protected methods *)

     drop: proc
       (# 
          arg1: ^_DropTargetDropEvent;
       enter (arg1[])
       #);
     dragEnter: proc
       (# 
          arg1: ^_DropTargetDragEvent;
       enter (arg1[])
       #);
     dragExit: proc
       (# 
          arg1: ^_DropTargetEvent;
       enter (arg1[])
       #);
     dragOver: proc
       (# 
          arg1: ^_DropTargetDragEvent;
       enter (arg1[])
       #);
     dropActionChanged: proc
       (# 
          arg1: ^_DropTargetDragEvent;
       enter (arg1[])
       #);
  do '|java/awt/dnd/DropTargetListener' -> className;
     INNER;
  #);

