ORIGIN '_Image';
INCLUDE '~beta/javalib/java/awt/image/_ImageProducer';
INCLUDE '~beta/javalib/java/awt/_Graphics';
INCLUDE '~beta/javalib/java/lang/_String';
INCLUDE '~beta/javalib/java/awt/image/_ImageObserver';
--LIB: attributes--

(* Java Image class members.
 * See _Image.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/awt/Image.html
 *)
Image: _Image
  (#
     (* Public/protected fields *)

     (* STATIC: *)
     UndefinedProperty: ^Object;
     (* STATIC: *)
     SCALE_DEFAULT: @int32;
     (* STATIC: *)
     SCALE_FAST: @int32;
     (* STATIC: *)
     SCALE_SMOOTH: @int32;
     (* STATIC: *)
     SCALE_REPLICATE: @int32;
     (* STATIC: *)
     SCALE_AREA_AVERAGING: @int32;

     (* Public/protected constructors *)

     _init: cons
       (# result: ^Image;
       exit result[]
       #);

     (* Public/protected methods *)

     getProperty: proc
       (# result: ^Object;
          arg1: ^_String;
          arg2: ^_ImageObserver;
       enter (arg1[], arg2[])
       exit result[]
       #);
     flush: proc
       (# 
       #);
     getGraphics: proc
       (# result: ^_Graphics;
       exit result[]
       #);
     getHeight: proc
       (# result: @int32;
          arg1: ^_ImageObserver;
       enter (arg1[])
       exit result
       #);
     getWidth: proc
       (# result: @int32;
          arg1: ^_ImageObserver;
       enter (arg1[])
       exit result
       #);
     getSource: proc
       (# result: ^_ImageProducer;
       exit result[]
       #);
     getScaledInstance: proc
       (# result: ^Image;
          arg1: @int32;
          arg2: @int32;
          arg3: @int32;
       enter (arg1, arg2, arg3)
       exit result[]
       #);
  do 'java/awt/Image' -> className;
     INNER;
  #);

