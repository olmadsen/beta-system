ORIGIN '_MenuBar';
INCLUDE '~beta/javalib/java/awt/_MenuShortcut';
INCLUDE '~beta/javalib/java/awt/_MenuItem';
INCLUDE '~beta/javalib/java/awt/_Menu';
INCLUDE '~beta/javalib/javax/accessibility/_AccessibleRole';
INCLUDE '~beta/javalib/java/util/_Enumeration';
INCLUDE '~beta/javalib/javax/accessibility/_AccessibleContext';
INCLUDE '~beta/javalib/java/awt/_MenuComponent';
--LIB: attributes--

(* Java MenuBar class members.
 * See _MenuBar.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/awt/MenuBar.html
 *)
MenuBar: _MenuBar
  (#
     (* Public/protected constructors *)

     _init: cons
       (# result: ^MenuBar;
       exit result[]
       #);

     (* Public/protected methods *)

     add: proc
       (# result: ^_Menu;
          arg1: ^_Menu;
       enter (arg1[])
       exit result[]
       #);
     remove_MenuComponent: proc (* remove *)
       (# 
          arg1: ^_MenuComponent;
       enter (arg1[])
       do 'remove' -> procname;
       #);
     remove_I: proc (* remove *)
       (# 
          arg1: @int32;
       enter (arg1)
       do 'remove' -> procname;
       #);
     getAccessibleContext: proc
       (# result: ^_AccessibleContext;
       exit result[]
       #);
     addNotify: proc
       (# 
       #);
     removeNotify: proc
       (# 
       #);
     deleteShortcut: proc
       (# 
          arg1: ^_MenuShortcut;
       enter (arg1[])
       #);
     getShortcutMenuItem: proc
       (# result: ^_MenuItem;
          arg1: ^_MenuShortcut;
       enter (arg1[])
       exit result[]
       #);
     shortcuts: proc
       (# result: ^_Enumeration;
       exit result[]
       #);
     countMenus: proc
       (# result: @int32;
       exit result
       #);
     getHelpMenu: proc
       (# result: ^_Menu;
       exit result[]
       #);
     getMenu: proc
       (# result: ^_Menu;
          arg1: @int32;
       enter (arg1)
       exit result[]
       #);
     getMenuCount: proc
       (# result: @int32;
       exit result
       #);
     setHelpMenu: proc
       (# 
          arg1: ^_Menu;
       enter (arg1[])
       #);

     (* Inner classes *)

     AccessibleAWTMenuBar: MenuComponent$AccessibleAWTMenuComponent
       (#
          (* Public/protected constructors *)

          _init: cons
            (# result: ^AccessibleAWTMenuBar;
               arg1: ^MenuBar;
            enter (arg1[])
            exit result[]
            #);

          (* Public/protected methods *)

          getAccessibleRole: proc
            (# result: ^_AccessibleRole;
            exit result[]
            #);
       do 'java/awt/MenuBar$AccessibleAWTMenuBar' -> className;
          INNER;
       #);

  do 'java/awt/MenuBar' -> className;
     INNER;
  #);

