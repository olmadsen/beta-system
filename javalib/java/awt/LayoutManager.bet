ORIGIN '_LayoutManager';
INCLUDE '~beta/javalib/java/awt/_Container';
INCLUDE '~beta/javalib/java/awt/_Component';
INCLUDE '~beta/javalib/java/awt/_Dimension';
INCLUDE '~beta/javalib/java/lang/_String';
--LIB: attributes--

(* Java LayoutManager interface members.
 * See _LayoutManager.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/awt/LayoutManager.html
 *)
LayoutManager: _LayoutManager
  (#
     (* Public/protected methods *)

     addLayoutComponent: proc
       (# 
          arg1: ^_String;
          arg2: ^_Component;
       enter (arg1[], arg2[])
       #);
     layoutContainer: proc
       (# 
          arg1: ^_Container;
       enter (arg1[])
       #);
     minimumLayoutSize: proc
       (# result: ^_Dimension;
          arg1: ^_Container;
       enter (arg1[])
       exit result[]
       #);
     preferredLayoutSize: proc
       (# result: ^_Dimension;
          arg1: ^_Container;
       enter (arg1[])
       exit result[]
       #);
     removeLayoutComponent: proc
       (# 
          arg1: ^_Component;
       enter (arg1[])
       #);
  do '|java/awt/LayoutManager' -> className;
     INNER;
  #);

