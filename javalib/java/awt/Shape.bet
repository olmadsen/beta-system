ORIGIN '_Shape';
INCLUDE '~beta/javalib/java/awt/_Rectangle';
INCLUDE '~beta/javalib/java/awt/geom/_AffineTransform';
INCLUDE '~beta/javalib/java/awt/geom/_PathIterator';
INCLUDE '~beta/javalib/java/awt/geom/_Rectangle2D';
INCLUDE '~beta/javalib/java/awt/geom/_Point2D';
--LIB: attributes--

(* Java Shape interface members.
 * See _Shape.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/awt/Shape.html
 *)
Shape: _Shape
  (#
     (* Public/protected methods *)

     contains_D_D: proc (* contains *)
       (# result: @boolean;
          arg1: @real;
          arg2: @real;
       enter (arg1, arg2)
       do 'contains' -> procname;
       exit result
       #);
     contains_D_D_D_D: proc (* contains *)
       (# result: @boolean;
          arg1: @real;
          arg2: @real;
          arg3: @real;
          arg4: @real;
       enter (arg1, arg2, arg3, arg4)
       do 'contains' -> procname;
       exit result
       #);
     contains_Point2D: proc (* contains *)
       (# result: @boolean;
          arg1: ^_Point2D;
       enter (arg1[])
       do 'contains' -> procname;
       exit result
       #);
     contains_Rectangle2D: proc (* contains *)
       (# result: @boolean;
          arg1: ^_Rectangle2D;
       enter (arg1[])
       do 'contains' -> procname;
       exit result
       #);
     intersects_D_D_D_D: proc (* intersects *)
       (# result: @boolean;
          arg1: @real;
          arg2: @real;
          arg3: @real;
          arg4: @real;
       enter (arg1, arg2, arg3, arg4)
       do 'intersects' -> procname;
       exit result
       #);
     intersects_Rectangle2D: proc (* intersects *)
       (# result: @boolean;
          arg1: ^_Rectangle2D;
       enter (arg1[])
       do 'intersects' -> procname;
       exit result
       #);
     getBounds: proc
       (# result: ^_Rectangle;
       exit result[]
       #);
     getBounds2D: proc
       (# result: ^_Rectangle2D;
       exit result[]
       #);
     getPathIterator_AffineTransform: proc (* getPathIterator *)
       (# result: ^_PathIterator;
          arg1: ^_AffineTransform;
       enter (arg1[])
       do 'getPathIterator' -> procname;
       exit result[]
       #);
     getPathIterator_AffineTransform_D: proc (* getPathIterator *)
       (# result: ^_PathIterator;
          arg1: ^_AffineTransform;
          arg2: @real;
       enter (arg1[], arg2)
       do 'getPathIterator' -> procname;
       exit result[]
       #);
  do '|java/awt/Shape' -> className;
     INNER;
  #);

