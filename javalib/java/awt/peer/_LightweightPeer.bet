ORIGIN '~beta/basiclib/betaenv';
--LIB: attributes--

(* Java LightweightPeer class declaration.
 * This wrapper is needed to prevent circular fragment INCLUDE.
 * See LightweightPeer.bet for members.
 *)
_LightweightPeer: ExternalClass
  (#
  do '|java/awt/peer/LightweightPeer' -> className;
     INNER;
  #);

