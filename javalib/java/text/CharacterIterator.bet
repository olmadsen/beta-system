ORIGIN '_CharacterIterator';
--LIB: attributes--

(* Java CharacterIterator interface members.
 * See _CharacterIterator.bet for class declaration.
 * See http://java.sun.com/j2se/1.4.2/docs/api/java/text/CharacterIterator.html
 *)
CharacterIterator: _CharacterIterator
  (#
     (* Public/protected fields *)

     (* STATIC: *)
     DONE: @char;

     (* Public/protected methods *)

     clone: proc
       (# result: ^Object;
       exit result[]
       #);
     next: proc
       (# result: @char;
       exit result
       #);
     previous: proc
       (# result: @char;
       exit result
       #);
     getIndex: proc
       (# result: @int32;
       exit result
       #);
     first: proc
       (# result: @char;
       exit result
       #);
     last: proc
       (# result: @char;
       exit result
       #);
     current: proc
       (# result: @char;
       exit result
       #);
     getBeginIndex: proc
       (# result: @int32;
       exit result
       #);
     getEndIndex: proc
       (# result: @int32;
       exit result
       #);
     setIndex: proc
       (# result: @char;
          arg1: @int32;
       enter (arg1)
       exit result
       #);
  do '|java/text/CharacterIterator' -> className;
     INNER;
  #);

