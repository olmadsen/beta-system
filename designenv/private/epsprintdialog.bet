ORIGIN '~beta/guienv/guienvall';
INCLUDE '~beta/basiclib/formatio'
        '~beta/guienv/utils/iconname';
BODY 'epsprintdialogbody';
-- guienvLib: Attributes --
ppageDialog: window
  (#
     onOK:<
       (# 
	  fileName: ^text;
	  scaled,scaleToFit: @boolean;
	  scale: @integer
       enter (filename[],scaled,scaleToFit,scale)
       do
	  INNER
       #);
     onCancel:<
       (#
       do
	  INNER;
	  close
       #);
     private: @<<SLOT ppageDialogprivate:Descriptor>>;
     open::<
       (# 
	  fileName: ^text;
	  scaled,scaleToFit: @boolean;
	  scale: @integer
       enter (filename[],scaled,scaleToFit,scale)
       <<SLOT ppageDialogOpen:DoPart>>
       #)
  #)  
