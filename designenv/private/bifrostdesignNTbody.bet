(#
(* enter (shiftKey,controlKey,altKey,shiftLockKey,keyNr,asciiCode) *)
do
   (if UI.veryVerbose then
       'onKeyDown: %d\n' -> putFormat(# do ch -> d #)
   if);
   (shiftKey, controlKey, metaKey, false (* shift lock *), ch, ch)
     -> theDocument.onKeyDown
#)

