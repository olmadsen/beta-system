ORIGIN 'gppinterface';
MDBODY nti 'private/prettyprintersetupNTbody'
       ppcmac 'private/prettyprintersetupmac'
       default 'private/prettyprintersetupX11body';
-- gppProperties: Descriptor --
(#
   IntegerValue: (# value: @integer do INNER exit value #);
   IntegerObject: IntegerValue (#  enter value do INNER #);
   IntegerDefault: IntegerValue
     (#
        changed: @Boolean;
        set: (#  enter value do true->changed #);
        reset: (#  do false->changed #);
        
     enter set
     do (if changed // false then INNER if)
     #);
   BooleanValue: (# value: @boolean do INNER exit value #);
   BooleanObject: BooleanValue (#  enter value do INNER #);
   BooleanDefault: BooleanValue
     (#
        changed: @Boolean;
        set: (#  enter value do true->changed #);
        reset: (#  do false->changed #);
        
     enter set
     do (if changed // false then INNER if)
     #);
   PatternShape: @IntegerDefault (#  do shape.rectangle->value #);
   ObjectShape: @IntegerDefault (#  do shape.rndrect->value #);
   (* width  of subbox of pattern diagram *)
   width: @IntegerDefault (#  do 140->value #);
   (* height of subbox of pattern diagram *)
   height: @IntegerDefault (#  do 16->value #);
   (* width of title of pattern diagram *)
   titlewidth: @IntegerDefault (#  do 55->value #);
   (*width of subbox of pattern diagram*)
   fragmentwidth: @IntegerDefault (#  do 220->value #);
   (* width of property diagram *)
   propertywidth: @IntegerDefault (#  do 270->value #);
   halfDiamondNodeWidth: @integerDefault (#  do 8->value #);
   (* where to place titleNode of the PatternDiagram *)
   halfDiamondNodeHeight: @integerDefault (#  do 3->value #);
   (* when no prefix *)
   DownOnPage: @IntegerDefault (#  do 3*height->value #);
   (* when no prefix *)
   RightOnPage: @IntegerDefault (#  do width div 2->value #);
   NextPositionStartX: @IntegerDefault (#  do 300->value #);
   NextPositionStartY: @IntegerDefault (#  do 300->value #);
   NextPositionOffset: @IntegerDefault (#  do - 20->value #);
   (* when we have a prefix *)
   DownFromPrefix: @IntegerDefault (#  do 3*height->value #);
   (* when we have a prefix *)
   LeftFromPrefix: @IntegerDefault (#  do 2*width->value #);
   (* when we have more
    * subpattern Diagrams *)
   RightFromPrevious: @IntegerDefault (#  do 20->value #);
   RefConnectorLeftFromNode: @IntegerDefault (#  do 20->value #);
   (* offset the next refCon. *)
   RefConnectorOffset: @IntegerDefault (#  do 3->value #);
   FontSize: @IntegerDefault
     (#  <<SLOT prettyprinterSetupFontSize:DoPart>> #);
   AbstractedStyle: @IntegerDefault
     (# 
     do textStyle.bold->value
     #);
   DetailedStyle: @IntegerDefault
     (# 
     do textStyle.bold->value
     #);
   (* if false then split the diagram
    * into more columns in case it
    * does'nt fit on the page *)
   OneColumn: @BooleanDefault (#  do false->value #);
   ReferenceEdges: @BooleanDefault (#  do false->value #);
   OriginOffset: @IntegerDefault (#  do 4*height->value #);
   (* should composition diagrams be trees or just simple pattern diagrams *)
   (* CompositionPartWhole: @IntegerDefault (# do 3->value #);
    * CompositionReference: @IntegerDefault (# do 2->value #);
    * CompositionBlock: @IntegerDefault (# do 1->value #);
    * CompositionAll: @IntegerDefault (# do 0->value #);
    * CompositionWith: @Integer;
    * CompositionTree: @IntegerDefault (# do 1->value #);
    * CompositionNested: @IntegerDefault (# do 0->value #);
    * CompositionDType: @Integer;
    * CompositionWithMarks: @BooleanDefault (# do true->value #);
    *)
   DetailedOnSamePage: @BooleanDefault (#  do true->value #);
   ShowLocals: @BooleanDefault (#  do true->value #);
   SimpleHierarchyPresentation: @BooleanDefault (#  do true->value #);
   UsingExpandMenu: @BooleanDefault (#  do true->value #);
   (* how to display simple declarations *)
   MarkSize: @IntegerDefault (#  do 9->value #);
   AsName: (#  exit 0 #);
   AsNameAndType: (#  exit 1 #);
   AsType: (#  exit 2 #);
   AsNameAndKind: (#  exit 3 #);
   (* Global display modes *)
   SimpleDeclDisplay: @IntegerDefault (#  do AsName->value #);
   showAttributes: @BooleanDefault (#  do true->value #);
   references: @BooleanDefault (#  do true->value #);
   specializations: @BooleanDefault (#  do true->value #);
   associations: @BooleanDefault (#  do true->value #);
   edgedSpecializationConnectors: @BooleanDefault (#  do false->value #);
   (* how to position diagrams on pages *)
   subRegionsOfSuper: @BooleanDefault (#  do true->value #);
   (* diagram positioning modes *)
   DiagramPositioning: @IntegerDefault (#  do AllAuto->value #);
   AllManual: (#  exit 0 #);
   AllAuto: (#  exit 1 #);
   AutoSubPatterns: (#  exit 2 #);
   AutoDetailing: (#  exit 3 #);
   (* set to value>0 if Freja is currently waiting for user interaction *)
   AutoNewPatterns: (#  exit 4 #);
   (* global interactive modes *)
   globalInteractiveMode: @IntegerDefault
     (#  do noGlobalInteractiveMode->value #);
   noGlobalInteractiveMode: (#  exit 0 #);
   creatingAggregation: (#  exit 1 #);
   creatingAssociation: (#  exit 2 #);
   (* Aggregation kinds *)
   busy: (#  exit 99 #);
   byValue: (#  exit 1 #);
   (* implementations of aggregations *)
   byReference: (#  exit 2 #);
   noImplementation: (#  exit 0 #);
   repetition: (#  exit 1 #);
   arrayContainer: (#  exit 2 #);
   hashtable: (#  exit 3 #);
   ExtensibleHashTable: (#  exit 4 #);
   List: (#  exit 5 #);
   Set: (#  exit 6 #);
   MultiSet: (#  exit 7 #);
   Stack: (#  exit 8 #);
   Queue: (#  exit 9 #);
   PriorityQueue: (#  exit 10 #);
   ignoreFocusChanged: @boolean;
   (* attribute types *)
   simpleAttribute: (#  exit 1 #);
   operation: (#  exit 2 #);
   localClass: (#  exit 3 #);
   (* ordering of attributes *)
   inCompartments: (#  exit 1 #);
   asCode: (#  exit 2 #);
   (* globals *)
   loadingGraphicsPages: @boolean;
   
#)  

