-- treeCompositionHit: Descriptor --
(#
   thePage: ^OADPage;
   theObject: ^thePage.DesignObject;
   theNode: ^thePage.PatternDiagramNode;
   
do
   CurrentPage->thePage[];
   (if thePage[] <> none
    // true then
       thePage.CurrentObject->theObject[];
       (if theObject[] <> none
        // true then
           (if theObject.struc <= thePage.PatternDiagramNode##
            // true then
