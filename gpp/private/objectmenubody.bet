-- ObjectMenuPrintNodeID: DescriptorForm --
(# thePage: ^Page; theObject: ^thePage.DesignObject
do
   CurrentPage->thePage[];
   (if thePage[] <> none
    // true then
       thePage.CurrentObject->theObject[];
       (if theObject[] <> none
        // true then 'ID: '->puttext; theObject.ID->putint; newline;
       if)
   if)
#)  

