   (if thePage[] <> none
    // true then
       thePage.CurrentObject->theObject[];
       (if theObject[] <> none
        // true then
           'Ast Dump: '->puttext; theObject.theObjectDescriptor.dump; newline;
       if)
   if)
#)  

