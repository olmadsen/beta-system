ORIGIN '~beta/basiclib/systemenv';
INCLUDE '~beta/process/processmanager';

(* Simple demonstration of how to invoke UNIX command 'ls -l'
 * See also demo activateDemo and activate_unix.
 *)

--PROGRAM: descriptor--
systemenv
(# ls: @Process;
do '/bin/ls' -> ls.init;
   '-l' -> ls.argument.putArg;
   ls.start;
   ls.awaitStopped;
   'done' -> putline;
#)
