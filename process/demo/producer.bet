ORIGIN '~beta/basiclib/v1.5/systemenv';
INCLUDE '~beta/basiclib/v1.5/file';

--- program:descriptor ---
systemenv(#
   inFile: @file;
do
   2 -> Arguments (* first argument *) -> inFile.name;
   inFile.OpenRead;

   readFile: 
     (# 
     do (if inFile.eos
         //false then 
            inFile.getAtom -> putText;
            newLine;
            restart readFile
         else leave readFile
        if)
     #);
   inFile.close;
#)
