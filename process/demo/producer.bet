ORIGIN '~beta/basiclib/v1.4/betaenv';
[[
--- include '~beta/basiclib/v1.4/file'
--- program:descriptor ---
(#
   inFile: @file;
do
   2 -> Arguments (* first argument *) -> inFile.name;
   inFile.OpenRead;

   readFile: 
     (# 
     do (if inFile.eos
         //false then 
            inFile.getAtom -> putText;
            newLine;
            restart readFile
         else leave readFile
        if)
     #);
   inFile.close;
#)
---]]
