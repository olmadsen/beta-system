ORIGIN '~beta/basiclib/v1.6/systemenv';
INCLUDE '~beta/process/v1.6/binarysocket';

--- program:descriptor ---
systemEnv
(# 
   port: (# exit 7899 #);
   host: (# exit 'fraxinus' #);
   
   s: @BinarySocket;

do s.init;
   (10, host, port) -> s.connect;
   
   waitForever -> s.getInt -> putInt;
   s.close;
#)
