ORIGIN '~beta/basiclib/v1.4/betaenv';
[[
--- lib:attributes ---

(* from libc.a *)
time: external
  (# address,result: @integer;
  enter address
  do callC
  exit result
  #);

getTime: (# exit 0->time #);

timeout:
  (# deadline: @integer;
  enter (# i: @integer; enter i do getTime+i -> deadline #)
  exit (getTime>=deadline)
  #);

---]]
