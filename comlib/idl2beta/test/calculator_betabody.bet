ORIGIN 'calculator'

--CalculatorAdd: descriptor--
(# do (op1 + op2) -> op1 #)
--CalculatorSub: descriptor--
(# do (op1 - op2) -> op1; #)
--CalculatorDiv: descriptor--
(# 
do (if op2 = 0 then 
       0 -> op1 
    else
       (op1 div op2) -> op1;
   if);
#)
--CalculatorMul: descriptor--
(# do (op1 * op2) -> op1; #)


