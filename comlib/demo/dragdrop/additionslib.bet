ORIGIN '~beta/comlib/comlib';
INCLUDE '~beta/comlib/oleidl';
-- lib: Attributes --
SOURCE_RegisterDragDrop: external
  (# hwnd: @int32; pDropTarget: ^IDropSource; hr: @int32
  enter (hwnd,pDropTarget[])
  do 'RegisterDragDrop'->callStd
  exit hr
  #);
TARGET_RegisterDragDrop: external
  (# hwnd: @int32; pDropTarget: ^IDropTarget; hr: @int32
  enter (hwnd,pDropTarget[])
  do 'RegisterDragDrop'->callStd
  exit hr
  #)  

