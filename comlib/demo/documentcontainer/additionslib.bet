ORIGIN '~beta/comlib/comlib';
INCLUDE '~beta/comlib/OleIdl';
-- lib: Attributes --
OLEIVERB_SHOW: (* findes i OLE2.H *) (#  exit - 1 #);
OLEIVERB_DISCARDUNDOSTATE: (* findes i OLE2.H *) (#  exit - 6 #);
OleCreateFromFile: external
  (#
     hr: @int32;
     rclsid: ^GUIDdata (* REFCLSID *) ;
     lpszFileName: @int32 (* LPCOLESTR *) ;
     riid: ^GUIDdata (* REFIID *) ;
     renderopt: @int32 (* DWORD *) ;
     lpFormatEtc: @int32 (* LPFORMATETC *) ;
     pClientSite: ^IOleClientSite (* LPOLECLIENTSITE *) ;
     pStg: ^IStorage (* LPSTORAGE *) ;
     ppvObj: ^IUnknownHolder (* LPVOID FAR * *)
  enter
  (rclsid[],lpszFileName,riid[],renderopt,lpFormatEtc,pClientSite[],pStg[],
   ppvObj[])
  do callStd
  exit hr
  #);
OleSave: external
  (#
     hr: @int32;
     pPS: ^IPersistStorage (* LPPERSISTSTORAGE *) ;
     pStg: ^IStorage (* LPSTORAGE *) ;
     fSameAsLoad: @boolean
  enter (pPS[],pStg[],fSameAsLoad)
  do callStd
  exit hr
  #);
OleRun: external
  (# hr: @int32; pUnknown: ^IUnknown (* LPUNKNOWN *)
  enter pUnknown[]
  do callStd
  exit hr
  #);
getGUID_NULL: (* a GUID containing only zeros *)
  (# aGUID: ^GUID
  do
     &GUID[]->aGUID[];
     aGUID.init;
     '00000000-0000-0000-0000-000000000000'->aGUID.setFromText
  exit aGUID[]
  #);
GetClientRect2: external
  (#
     hr: @int32;
     hWnd: @integer (* HWND *) ;
     lpRect: ^struct_tagRECT (* LPRECT *)
  enter (hWnd,lpRect[])
  do 'GetClientRect'->callStd
  exit hr
  #);
GetClientRect3: external
  (#
     hr: @int32;
     hWnd: ^int32Holder (* HWND *) ;
     lpRect: ^struct_tagRECT (* LPRECT *)
  enter (hWnd[],lpRect[])
  do 'GetClientRect'->callStd
  exit hr
  #);
CACCELERATORS: (* defineret i Resource.h i C++-eksemplet til 2 *)
  (#  exit 2 #)  

