ORIGIN '~beta/basiclib/v1.4/betaenv';
BODY 'private/astcommunicationbody';

INCLUDE '~beta/mps/current/astlevel';

-- lib: attributes --

astController:
  (# <<SLOT astControllerLib: attributes>>;
     node: ^astInterface.AST;
     
     init:<
       (# 
       enter node[]
       <<SLOT MPSastControllerInit: doPart>>
       #);
     release:<
       (# <<SLOT MPSastControllerRelease: doPart>> #);
     
     prepareUpdate:
       (# <<SLOT MPSastControllerPrepareUpdate: doPart>> #);
     
     change:
       (# <<SLOT MPSastControllerChange: doPart>> #);
     delete:
       (# <<SLOT MPSastControllerDelete: doPart>> #);
     replace:
       (# 
       enter node[]
       <<SLOT MPSastControllerReplace: doPart>>
       #);
     
     dirty:
       (# value: @boolean
       enter (# enter value <<SLOT MPSastControllerEnterDirty: doPart>> #)
       exit (# <<SLOT MPSastControllerExitDirty: doPart>> exit value #)
       #);
     beeingEdited: booleanValue
       (# <<SLOT MPSastControllerBeeingEdited: doPart>> #);
     
     ancestorReplaced:<
       (# do inner #);
     deleted:<
       (# do inner #);
     replaced:<
       (# 
       enter node[]
       do inner
       #);
     changed:<
       (# do inner  #);
     
     present:<
       (# do inner #);
     private: @<<SLOT MPSAstControllerPrivate: descriptor>>;
  #);
