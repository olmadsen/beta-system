ORIGIN '~beta/guienv/v1.6/guienv';
INCLUDE 'auxcontrols';
BODY 'private/editlabelprivate';
-- windowLib: Attributes --
editLabel: staticText
  (#
     startEdit: (#  <<SLOT editLabelStartEdit:DoPart>> #);
     endEdit:
       (# 
       <<SLOT editLabelEndEdit:DoPart>>
       #);
     confirm:<
       (# value: ^text; ok: @boolean
       enter value[]
       do true->ok; INNER
       exit ok
       #);
     eventHandler::< 
       (# onMouseDown::<  (#  do startEdit; INNER ;  #);  #);
     private: @<<SLOT editLabelPrivate:Descriptor>>;
     
  #)  

