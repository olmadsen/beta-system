ORIGIN '~beta/guienv/guienv';

BODY 'private/framerbody';


-- windowLib: attributes --


bevelKind: (# exit 1 #);
bevelOutlineKind: (# exit 2 #);
bevelRoundedOutlineKind: (# exit 3 #);
etchedKind: (# exit 4 #);

framer: canvas
	(# <<SLOT framerLib: attributes>>;
		kind:
			(#
			enter (# enter theKind do update #)
			exit theKind
			#);
		borderwidth:
			(#
			enter (# enter theBorderwidth do update #)
			exit theBorderwidth
			#);
		inset:
			(#
			enter (# enter insetFlag do update #)
			exit insetFlag
			#);
		open::<
			(# <<SLOT framerOpen: doPart>> #);
		eventHandler::<
			(# onRefresh::<
					(# <<SLOT framerOnRefresh: doPart>> #);
			#);
		theKind: @integer;
		theBorderwidth: @integer;
		insetFlag: @boolean;
	#);
