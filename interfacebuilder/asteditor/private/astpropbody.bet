ORIGIN '../astprop'
[[
-- findAndParseAstProps: doPart --
do (#
   do theAst.SuffixWalk
      (# theComment: ^comment;
         prop: ^propertyList;
         t: ^text;
      do (if current.hasComment//true then 
             current.GetComment -> theComment[];
             theComment.getText -> t[];
             '[[' -> screen.putText;
             (t[], screen[]) -> printComment;
             ']]' -> screen.putText;
         if);
      #);
   #)

--]]
