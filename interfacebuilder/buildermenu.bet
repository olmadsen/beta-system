ORIGIN '~beta/guienv/v1.3.1/guienv';

-- guienvLib: attributes --

builderMenu: menu
  (# builderItem: menuItem
       (# open::<
            (# 
            do inner;
               this(builderItem)[] -> append;
            #);
       #);
     
     addSeparator:
       (# aSeparator: ^separator;
       do &separator[] -> aSeparator[];
          aSeparator.open;
          aSeparator[] -> append;
       #);
  #);
