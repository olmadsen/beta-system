ORIGIN '~beta/guienv/v1.3.1/guienv';
BODY 'private/codeviewbody';

INCLUDE '~beta/guienv/v1.3.1/stddialogs';
INCLUDE '~beta/guienv/v1.3.1/controls';
INCLUDE '~beta/guienv/v1.3.1/fields';
INCLUDE '~beta/interfacebuilder/v1.1/dialogs/dialogfield';

-- WindowLib: Attributes --
CodeView: LabeledCanvas
  (# <<SLOT CodeViewLib: Attributes>>;
     EditScript:<
       (# scriptName: ^Text;
       enter scriptName[]
       do Inner EditScript;
       #);
     NameField,SuperPatternField: ^EditText;
     Init:<
       (# Names: ^List;
       enter Names[]
       do <<SLOT CodeViewInit: Descriptor>>;
       #);
     Open::< (# do <<SLOT CodeViewOpen: Descriptor>> #)  ;
     PrivatePart: @<<SLOT CodeViewPrivatePart: Descriptor>>;
  #);
















