ORIGIN  'ClassEditorDialogs' 
[[
-- Include '~beta/macenv/v3.5/Control'
-- Include '~michael/macenvstuff/DialogField'

-- WINDResourceInfoUse: Descriptor --
(#
do Create;
	WindowID -> Private.ResourceIDFld.IntegerContents;
	ContentsID -> Private.ContentsResIDFld.IntegerContents;
	ShowModal;
	Close;
#)

-- ResourceInfoDialogPrivate: Descriptor --
(#
   CancelButton: (*[ Resource  PushButton 1  ]*) @PushButton
      (#
         EventHandler::< 
            (# MouseUp::< (# do This(ResourceInfoDialog).Hide; #);
            #);
      #);
   OKButton: (*[ Resource  PushButton 2  ]*) @PushButton
      (#
         EventHandler::< 
            (#
               MouseUp::< 
                  (# 
						do (if ResourceIDFld.IntegerContents -> OK//True then
								This(ResourceInfoDialog).Hide;
							if);
						#);
            #);
         Open::< (# do OKButton[] -> DefaultButton #)
      #);
   ResourceIDLabel: (*[ Resource  StaticText 3  ]*) @StaticText(# #);
   ResourceIDFld: (*[ Resource  EditText 4  ]*) @NumberField
      (# Open::< (# do ResourceIDFld[] -> Target; #); #)
#)  

-- ResourceInfoDialogOpen: Descriptor --
(#
do
   1 -> (*[  ]*) Private.CancelButton.Open;
   2 -> (*[  ]*) Private.OKButton.Open;
   3 -> (*[  ]*) Private.ResourceIDLabel.Open;
   4 -> (*[  ]*) Private.ResourceIDFld.Open;
#)  

-- ResourceInfoDialogUse: Descriptor --
(#
do
   Create;
   ResourceID -> Private.ResourceIDFld.IntegerContents;
   (0,32000) -> Private.ResourceIDFld.Selection;
   ShowModal;
   Close;
#)  

-- WINDResourceInfoPrivate: Descriptor --
(# 
   ResourceIDFld: (*[ Resource  EditText 1  ]*) @NumberField(# #);
   ContentsResIDFld: (*[ Resource  EditText 2  ]*) @NumberField(# #);
   ResourceIDFldLabel: (*[ Resource  StaticText 3  ]*) @StaticText(# #);
   ContentsResIDFldLabel: (*[ Resource  StaticText 4  ]*) @StaticText(# #);
   CancelBtn: (*[ Resource  PushButton 5  ]*) @PushButton
		(#
         EventHandler::< 
            (# MouseUp::< (# do This(WindResourceInfo).Hide; #);
            #);
      #);
   OKBtn: (*[ Resource  PushButton 6  ]*) @PushButton
      (# EventHandler::< 
            (#
               MouseUp::< 
                  (#
						do (if (ResourceIDFld.IntegerContents,ContentsResIDFld.IntegerContents) -> OK//True then
								This(WindResourceInfo).Hide;
							if);
						#);
            #);
			Open::< (# do OKBtn[] -> DefaultButton #) #)

#)  

-- WINDResourceInfoOpen: Descriptor --
(#
do
   1 -> (*[  ]*) Private.ResourceIDFld.Open;
   2 -> (*[  ]*) Private.ContentsResIDFld.Open;
   3 -> (*[  ]*) Private.ResourceIDFldLabel.Open;
   4 -> (*[  ]*) Private.ContentsResIDFldLabel.Open;
   5 -> (*[  ]*) Private.CancelBtn.Open;
   6 -> (*[  ]*) Private.OKBtn.Open;
#)  

--]]
