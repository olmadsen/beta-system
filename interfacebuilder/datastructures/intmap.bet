ORIGIN 'abstractcontainer';
BODY 'private/intmapbody';


-- lib: attributes --

intMap: container
  (# <<SLOT intMapLib: attributes>>;
     put:<
       (# inx: @integer;
          elm: ^element;
          doPut: @<<SLOT intMapPut: descriptor>>;
       enter (inx, elm[])
       do doPut; inner;
       #);
     get:<
       (# inx: @integer;
          elm: ^element;
          doGet: @<<SLOT intMapGet: descriptor>>;
       enter inx
       do doGet; inner;
       exit elm[]
       #);
     
     (*
      * Configuration
      *)
     
     rangeInitial:< integerValue
       (# 
       do 17 -> value;
          inner;
       #);
     
     (*
      * Further bindings
      *)
     
     init::<
       (# <<SLOT intMapInit: doPart>> #);
     clear::<
       (# <<SLOT intMapClear: doPart>> #);
     
     (*
      * Final bindings
      *)
     
     empty::
       (# <<SLOT intMapEmpty: doPart>> #);
     size::
       (# <<SLOT intMapSize: doPart>> #);
     
     storage: @<<SLOT intMapStorage: descriptor>>
  #);
