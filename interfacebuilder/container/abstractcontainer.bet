ORIGIN '~beta/basiclib/v1.4/betaenv';

-- lib: attributes --

container:
  (# <<SLOT containerLib: attributes>>;
     
     element:< object;
     
     init:<
       object;
     empty:<
       booleanValue;
     size:<
       integerValue;
     clear:<
       object;
     
     has:< booleanValue
       (# elm: ^element;
       enter elm[]
       do inner;
       #);
     
     elementCallback:
       (# current: ^element;
       enter current[]
       do inner;
       #);
     forEach:<
       (# callback: ^elementCallback;
       enter callback[]
       do inner;
       #);
     
     emptyContainer:< exception
       (# 
       do 'container is empty' -> msg.append;
          inner;
       #);
  #);
