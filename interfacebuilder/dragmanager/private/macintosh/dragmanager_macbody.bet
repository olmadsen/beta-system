ORIGIN '../dragmanagerbody';
INCLUDE '~beta/guienv/v1.4/private/macintosh/guienv_macprivate';

-- windowItemFromGlobal: doPart --
do (# originX, originY: @integer;
	do	privateOrigin -> (originX, originY);
		global.h - originX -> local.h;
		global.v - originY -> local.v;
	#);
   
-- windowItemToGlobal: doPart --
do (# originX, originY: @integer;
	do	privateOrigin -> (originX, originY);
		local.h + originX -> global.h;
		local.v + originY -> global.v;
	#);