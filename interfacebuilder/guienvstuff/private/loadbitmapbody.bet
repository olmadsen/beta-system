ORIGIN '../loadbitmap';

-- GUIENVloadbitmap: doPart --
do &pixmap[] -> theRaster[];
   name[] -> theRaster.read
   (# 
      error:: 
        (# errorMsg: ^text;
        do msg[] -> errorMsg[];
           exception
           (# 
           do 'Failed to read pixmap "' -> msg.putText;
              name[] -> msg.putText;
              '"' -> msg.putLine;
              '==>' -> msg.putLine;
              errorMsg[] -> msg.putLine;
           #);
        #);
   #);
