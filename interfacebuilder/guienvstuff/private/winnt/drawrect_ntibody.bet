-- GUIENVgraphicsFrameRect: descriptor --
(# FrameRect2: external
     (# hdc: @integer; (* HDC *)
        theRect: ^ntRECT; (* const RECT FAR* *)
        hbr: @integer; (* HBRUSH *)
        result: @integer; (* int *)
     enter (hdc,theRect[],hbr)
     do 'FrameRect' -> callStd;
     exit result
     #);
   
   theRect: @ntRECT;
do r.left -> theRect.left;
   r.right -> theRect.right;
   r.top -> theRect.top;
   r.bottom -> theRect.bottom;
   
   (private.hdc,theRect[],private.penBrush) -> FrameRect2;
#)
