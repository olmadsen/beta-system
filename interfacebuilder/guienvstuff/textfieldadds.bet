ORIGIN '~beta/guienv/v1.2/fields';
MDBODY default 'private/X11/textfieldadds_X11body';
-- textFieldLib: Attributes --
posToRowCol:
  (# pos: @integer; row,col: @integer;
  enter pos
  do <<SLOT UIENVTextFieldPositionToRowCol:ObjectDescriptor>>;
  exit (row,col)
  #);
rowColToPos:
  (#
     row,col: @integer;
     pos: @integer;
  enter (row,col)
  do <<SLOT UIENVTextFieldRowColToPosition:ObjectDescriptor>>;
  exit pos
  #);  

