ORIGIN '~beta/betaast/betacfl';
BODY 'private/semanticlinkbody';
LIB_DEF 'frigg_semantic' '../../lib';

-- lib: attributes --

removeDonecheck:
  (# frag: ^astInterface.fragmentForm;
     group: ^astInterface.fragmentGroup;
  enter frag[]
  do frag.father -> group[];
     (if group[] <> NONE then
         'donecheck' -> group.prop.addprop(# do 0 -> addConst #); 
     if);
  #);
-- NameAplAttributes: attributes --


declaration:
  (# theDeclaration: ^nameDcl;
  enter (# enter theDeclaration[] <<SLOT nameAplEnterDeclaration: doPart>> #)
  exit (# <<SLOT nameAplExitDeclaration: doPart>> exit theDeclaration[] #)
  #);
