ORIGIN '~beta/betaast/v5.1/betacfl';
BODY 'private/followsemanticlinkbody';

-- lib: attributes --

followSemanticLink:
  (# mps: ^astInterface;
     node: ^astInterface.AST;
     destNode: ^astInterface.AST;
  enter (mps[], node[])
  <<SLOT followSemanticLinkBody: doPart>>
  exit destNode[]
  #);
setSemanticLink:
  (# 
     node, super: ^astInterface.beta.patternDecl;
  enter (node[], super[])
  <<SLOT setSemanticLinkBody: doPart>>
  #);
