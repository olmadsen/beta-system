ORIGIN '~beta/betaast/betacfl';
BODY 'private/followsemanticlinkbody';

LIB_DEF 'frigg_followsem'  '../../lib';

-- lib: attributes --

followSemanticLink:
  (# mps: ^astInterface;
     node: ^astInterface.AST;
     destNode: ^astInterface.AST;
  enter (mps[], node[])
  <<SLOT followSemanticLinkBody: doPart>>
  exit destNode[]
  #);
setSemanticLink:
  (# super: ^astInterface.beta.patternDecl;
     node: ^astInterface.beta.attributeDecl;
  enter (node[], super[])
  <<SLOT setSemanticLinkBody: doPart>>
  #);
