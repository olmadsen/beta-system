-- pixmapDialogOpen: DoPart --
do
   (4,4)->position;
   (325,248)->size;
   ;
   private.staticText3.open;
   private.pixmapList.open;
   private.cancelBtn.open;
   private.okBtn.open;
   INNER;

   
-- pixmapDialogEnterPixmaps: doPart --
do thePixmaps.size -> private.pixmapList.append;
   thePixmaps.scan
   (# inx: @integer;
   do inx + 1 -> inx;
      (inx, current[]) -> private.pixmapList.setText;
   #);
   
-- pixmapDialogExitCurrentPixmap: doPart --
do (if private.pixmapList.selection.first <> 0 then
       private.pixmapList.selection.first -> private.pixmapList.getText -> name[];
   if);
   
