ORIGIN '~beta/guienv/v1.3/guienv';
BODY 'private/spacingdialogbody';

-- lib: attributes --
spacingValues:
  (# doWidth, doHeight, doVerticalDist, doHorizontalDist: @boolean;
     width, height, verticalDist, horizontalDist: @integer;
  #);

-- windowLib: attributes --


spacingDialog: canvas
  (# <<SLOT SpacingDialogLib: Attributes>>;
     values: ^spacingValues;
     accept:<
       (#  <<SLOT SpacingDialogAccept: doPart>> #);
     cancel:< 
       (# <<SLOT SpacingDialogCancel: doPart>> #);
     open::< 
       (# 
       enter values[]
       <<SLOT SpacingDialogOpen: doPart>> 
       #);
     
     private: @<<SLOT SpacingDialogPrivate: descriptor>>;
  #);





