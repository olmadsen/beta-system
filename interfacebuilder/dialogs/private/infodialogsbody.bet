-- WindowItemDialogPrivate: Descriptor --
(# 
   (* NameDirty,PrefixDirty: @Boolean;*)

   itsFrame: @group
     (# thePosition:
          (# 
          enter (itsH.integerContents, itsV.integerContents)
          exit (itsH.integerContents, itsV.integerContents)
          #);
        theSize:
          (# 
          enter (ItsWidth.integerContents, ItsHeight.integerContents)
          exit (ItsWidth.integerContents, ItsHeight.integerContents)
          #);
        
        ItsH,ItsV: @NumberField(# #);
        ItsHLabel,ItsVLabel: @StaticText(# #);
        
        ItsWidth,ItsHeight: @NumberField(# #);
        ItsWidthLabel,ItsHeightLabel: @StaticText(# #);
        
        Open::< 
          (#
          do ((10, 10), (390, 65)) -> frame;
             'Frame' -> label;
             
             
             ItsHLabel.Open;
             'h' -> itsHLabel.label;
             (5, 22) -> itsHLabel.Position; (* *)
             (9,12) -> itsHLabel.Size;
             ItsH.Open;
             (20, 20) -> itsH.Position; (* *)
             (40,27) -> itsH.Size;
             
             itsVLabel.Open;
             'v' -> itsVLabel.label;
             (70, 22) -> itsVLabel.Position; (* *)
             (9,12) -> itsVLabel.Size;
             ItsV.Open;
             (85, 20) -> ItsV.Position; (* *)
             (40, 27) -> ItsV.Size;
             
             ItsWidthLabel.Open;
             'Width' -> ItsWidthLabel.Label;
             (135, 22) -> ItsWidthLabel.Position; (* *)
             (30,12) -> ItsWidthLabel.Size;
             ItsWidth.Open;
             (170, 20) -> itsWidth.Position; (* *)
             (40,27) -> itsWidth.Size;
             
             ItsHeightLabel.Open;
             'Height' -> ItsHeightLabel.Label;
             (220, 22) -> ItsHeightLabel.Position; (* *)
             (39,12) -> ItsHeightLabel.Size;
             itsHeight.Open;
             (265, 20) -> itsHeight.Position; (* *)
             (40,27) -> itsHeight.Size;
             
             
             inner Open;
          #);
     #);
   
   ItsBorder: @group
     (#        
        VisibleFlag: @CheckButton(# #);
        Width,Height:@Integer;
        StyleMenu: @menu
          (# 
             style1,style2,style3,style4,style5: @menuItem;
             open::<
               (# 
               do style1.open;
                  'Simple' -> style1.name;
                  style1[] -> append;
                  
                  style5.open;
                  'EtechedOut' -> style5.name;
                  style5[] -> append;
                  
                  style4.open;
                  'EtechedIn' -> style4.name;
                  style4[] -> append;
                  
                  style2.open;
                  'ShadowIn' -> style2.name;
                  style2[] -> append;
                  
                  style3.open;
                  'ShadowOut' -> style3.name;
                  style3[] -> append;
                  
                  
                                    
                  
               #);
          #);
        StyleOptionButton: @OptionButton
          (# eventHandler::<
               (# onMouseDown::<
                    (# 
                    #);
                  onMouseUp::<
                    (# 
                    #);
               #);
             open::<
               (# 
               do 
                  StyleMenu.open;
                  StyleMenu[] -> popupMenu;
                  preferredSize -> size;
                  'Border Style' -> label;
               #);
          #);
        Open::< 
          (#
          do ((10, 100), (390, 160)) -> frame;
                                       
             'Border' -> label;
             
             ItsBorder[]->StyleoptionButton.open;
             (100, 22) -> StyleoptionButton.Position;
             
             ItsBorder[]->VisibleFlag.open;
             'Visible' -> VisibleFlag.Label;
             (5,22) -> VisibleFlag.Position;
             
             inner Open;
          #);
     #);

  SizeConstraints: @group
     (# Width,Height:@Integer;     
        BindLeft,BindTop,BindRight,BindBottom: @CheckButton(# #);
        Open::<
          (#
          do ((10, 170), (390, 220)) -> frame;
                          
             'Constraints' -> label;
             
             BindLeft.Open;
             (5,22) -> BindLeft.Position;
             'BindLeft' -> BindLeft.Label;
             
             BindTop.Open;
             (95,22) -> BindTop.Position;
             'BindTop' -> BindTop.Label;
             
             BindRight.Open;
             (185,22) -> BindRight.Position;
             'BindRight' -> BindRight.Label;
             
             BindBottom.Open;
             (275,22) -> BindBottom.Position;
             'BindBottom' -> BindBottom.Label;
             
             inner Open;
          #);
     #);
  
   VisibleFlag: @CheckButton
     (# Open::<
          (#
          do 'Visible' -> Label;
             (10,75) -> Position;
          #);
     #);
   
   EnabledFlag: @CheckButton
     (# Open::<
          (#
          do 'Enabled' -> Label;
             (110,75) -> Position;
          #);
     #);
   
   HiliteFlag: @CheckButton
     (# Open::<
          (#
          do 'Hilite' -> Label;
             (210,75) -> Position;
          #);
     #);     
   
   OkBtn: @PushButton
     (# EventHandler::<
          (# onMouseUp::< 
               (# 
               do apply;
                  this(WindowItemDialog).hide;
                  onOk;
               #);
          #);
        Open::<
          (#
          do 'OK' -> Label;
             (260,230) -> Position;
             (60,20) -> Size;
             OkBtn[] -> DefaultButton;
             True -> BindBottom;
             False -> BindTop;
          #);
        
     #);
   
   CancelBtn: @PushButton
     (# EventHandler::<
          (# onMouseUp::< 
               (# 
               do this(WindowItemDialog).hide;
               #);
          #);
        Open::<
          (#
          do 'Cancel' -> Label;
             (187,230) -> Position;
             (60,20) -> Size;
             True -> BindBottom;
             False -> BindTop;
          #);
     #);
