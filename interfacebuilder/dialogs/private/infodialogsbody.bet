-- WindowLib: Attributes --
CheckButton: CheckBox
  (# EventHandler::<
       (# onMouseUp::< 
            (# 
            do 
               inner onMouseUp;   
            #);
       #);
     Open::< 
       (#
       do (80,15) -> Size;
          Inner Open;
       #);
  #);

TheTextstyle: group
  (#   
     ItsSize: @NumberField(# #);
     ItsSizeLabel: @StaticText(# #);
     ItalicFlag, BoldFlag: @CheckButton(# #);

     FontMenu: @menu
       (# 
          style1,style2,style3,style4,style5: @menuItem;
          open::<
            (# 
            do style1.open;
               'Helvetica' -> style1.name;
               style1[] -> append;
                  
               style2.open;
               'style2' -> style2.name;
               style2[] -> append;
               
               style3.open;
               'style3' -> style3.name;
               style3[] -> append;
                  
               style4.open;
               'style4' -> style4.name;
               style4[] -> append;
               
               style5.open;
               'style5' -> style5.name;
               style5[] -> append;
            #);
       #);
     FontOptionButton: @OptionButton
       (# eventHandler::<
            (# onMouseDown::<
                 (# 
                 #);
               onMouseUp::<
                    (# 
                    #);
            #);
          open::<
            (# 
            do  
               FontMenu.open;
               FontMenu[] -> popupMenu;
               'Font' -> label;
            #);
       #);
     Open::< 
          (#
          do (250,87) -> Size;
             
            ' TextStyle : ' -> label;
             
             this(TheTextstyle)[]->FontOptionButton.open;
             (5,39) -> FontOptionButton.Position;
             
             this(TheTextstyle)[]->ItsSize.Open;
             (34,25) -> itsSize.Size;
             (200,24) -> itsSize.Position;
             
             this(TheTextstyle)[]->ItsSizeLabel.Open;
             'Size' -> itsSizelabel.label;
             (165,28) -> itsSizeLabel.Position;
             (30,12) -> itsSizeLabel.Size; 
             
             this(TheTextstyle)[]->ItalicFlag.open;
             'Italic' -> ItalicFlag.Label;
             (5,28) -> ItalicFlag.Position;
             
             this(TheTextstyle)[]->BoldFlag.open;
             'Bold' -> BoldFlag.Label;
             (78,28) -> BoldFlag.Position;
             
             inner Open;
          #);
     #);


