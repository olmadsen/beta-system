   private.itsBorder.StyleOptionButton.currentItem -> data.borderStyle;
   private.SizeConstraints.bindLeft.state -> data.bindLeft;
   private.SizeConstraints.bindRight.state -> data.bindRight;
   private.SizeConstraints.bindTop.state -> data.bindTop;
   private.SizeConstraints.bindBottom.state -> data.bindBottom;
   private.visibleFlag.state -> data.visible;
