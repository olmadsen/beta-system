-- WindowItemDialogOpen: Descriptor --
(# theWindow: ^Window;
   
do 
   (400,260) -> Size;
   this(WindowItemDialog)[] -> theWindow[];
   Private.CancelBtn.Open;
   Private.OkBtn.Open;
   (* private.codeBtn.open; *)
   Private.itsFrame.open;
   Private.SizeConstraints.Open;
   Private.VisibleFlag.Open;
   Private.EnabledFlag.Open;
   Private.HiliteFlag.Open;
   Private.ItsBorder.Open;
   Inner Open;
   (100,100) -> Position;
   updateFields;
#)

