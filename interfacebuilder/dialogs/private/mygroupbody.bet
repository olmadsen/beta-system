ORIGIN '../mygroup';
INCLUDE '~beta/guienv/v1.4/utils/guienvadds'
        '~beta/guienv/v1.4/controls'
        '~beta/sysutils/v1.5/objinterface';
-- mygroupSetLabel: DoPart --
do
   theLabel[]->private.theLabel.label;
   private.theLabel.preferredSize->private.theLabel.size;
     

-- mygroupGetLabel: DoPart --
do private.theLabel.label->theLabel[];   

-- mygroupCreate: DoPart --
do INNER create;   

-- mygroupOpen: DoPart --
do
   THIS(group)[]->private.theLabel.open;
   THIS(group)[]->getPatternName->label;
   INNER open;
     

-- groupPrivateAttributes: Descriptor --
(#
   theLabel: @staticText
     (#
        open::<  (#  do 'Label'->label; (80,15)->size; (5,0)->position;  #);
        
     #);
   
#)  

-- mygroupOnRefresh: DoPart --
do
     (# r: @rectangle; 
     do
        size->r.bottomRight;
        (0,10)->r.topLeft;
        (r,borderStyles.etchedIn)->DrawShadows;
        INNER onRefresh;
        
     #);
     

