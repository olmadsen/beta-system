ORIGIN '~beta/guienv/v1.2/guienv';
BODY 'private/promptfortextdialogbody';

-- lib: attributes --
promptForTextValues:
  (# prompt: ^text;
     theText: ^text;
     acceptLabel: ^text;
     dismissLabel: ^text;
  #);

-- windowLib: attributes --

promptForText: canvas
  (# <<SLOT promptForTextLib: attributes>>;
     values: ^promptForTextValues;
     
     accept:<
       (# <<SLOT PromptForTextAccept: doPart>> #);
     dismiss:<
       (# <<SLOT PromptForTextDismiss: doPart>> #);
     
     open::<
       (# 
       enter values[]
       <<SLOT PromptForTextOpen: doPart>>
       #);
     private: @<<SLOT PromptForTextPrivate: descriptor>>;
  #);
