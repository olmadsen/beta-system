ORIGIN '~beta/guienv/guienv';
INCLUDE '~beta/guienv/utils/auxcontrols'
        '../dialogs/friggdialog'
        '~beta/guienv/controls';
-- guienvLib: Attributes --
newPropertyView: friggDialog
  (#
     type:: windowTypes.dialog;
     open::< 
       (# 
       do
          (355,100)->size;
          'Add'->confirmLabel;
          'Add Property'->dialogTitle;
          'Add Property'->title;
          nameFld.open;
          caption2.open;
          typeFld.open;
          caption1.open;
          INNER
       #);
     nameFld: @dialogField
       (# open::<  (#  do (190,21)->size; (60,40)->position; INNER #)
       #);
     caption2: @caption
       (#
          open::< 
            (# 
            do 'Type:'->label; (33,13)->size; (15,74)->position; INNER
            #)
       #);
     typeFld: @dialogField
       (# open::<  (#  do (190,21)->size; (60,70)->position; INNER #)
       #);
     caption1: @caption
       (#
          open::< 
            (# 
            do 'Name:'->label; (36,13)->size; (15,45)->position; INNER
            #)
       #)
  #)  

