ORIGIN '~beta/guienv/guienv';
INCLUDE '../support/auxcontrols'
        '~beta/guienv/controls';
-- guienvLib: Attributes --
newPropertyView: window
  (#
     type:: windowTypes.dialog;
     onAdd:< (#  do INNER #);
     onCancel:< (#  do INNER #);
     open::< 
       (# 
       do
          (300,145)->size;
          caption1.open;
          nameFld.open;
          addButton.open;
          separator2.open;
          cancelBtn.open;
          caption2.open;
          typeFld.open;
          INNER
       #);
     caption1: @caption
       (#
          open::< 
            (# 
            do
               'Name of new property:'->label;
               (130,17)->size;
               (10,5)->position;
               INNER
            #)
       #);
     nameFld: @dialogField
       (# open::<  (#  do (280,21)->size; (10,30)->position; INNER #)
       #);
     addButton: @pushbutton
       (#
          open::< 
            (# 
            do
               'Add'->label;
               (60,20)->size;
               (225,115)->position;
               THIS(pushButton)[]->defaultButton;
               INNER
            #);
          eventHandler::<  (# onMouseUp::<  (#  do onAdd #) #)
       #);
     separator2: @separator
       (# open::<  (#  do (280,4)->size; (10,105)->position; INNER #)
       #);
     cancelBtn: @pushbutton
       (#
          open::< 
            (# 
            do 'Cancel'->label; (60,20)->size; (155,115)->position; INNER
            #);
          eventHandler::<  (# onMouseUp::<  (#  do onCancel #) #)
       #);
     caption2: @caption
       (#
          open::< 
            (# 
            do
               'Type of new property:'->label;
               (130,17)->size;
               (10,55)->position;
               INNER
            #)
       #);
     typeFld: @dialogField
       (# open::<  (#  do (280,21)->size; (10,80)->position; INNER #)
       #)
  #)  

