     menubarVisible::< (# do false -> value #);
  #);

WindowItemDialog: Window
  (# <<SLOT WindowItemDialogLib: Attributes>>;
     EditorType:< WindowItemEditor;
     editor: ^EditorType;
     Apply:< (# <<SLOT WindowItemDialogApply: doPart>> #);
     UpdateFields:< (# <<SLOT WindowItemDialogUpdateFields: doPart>> #);
     
     Open::< (# do <<SLOT WindowItemDialogOpen: Descriptor>> #);
     Private: @<<SLOT WindowItemDialogPrivate: Descriptor>>;
     Grow: (* Private *)
       (# Height: @Integer;
       enter Height
       do <<SLOT WindowItemDialogGrow: Descriptor>>
       #);
     CloseItsSize:
       (# 
       do <<SLOT WindowItemDialogCloseItsSize: Descriptor>>
       #);
     
     menubarVisible::< (# do false -> value #);
  #);
