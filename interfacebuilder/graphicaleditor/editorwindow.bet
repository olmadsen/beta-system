ORIGIN '~beta/guienv/v1.6/guienv';
INCLUDE 'patterndictionary';
BODY 'private/editorwindowbody';
-- guienvLib: Attributes --
editorWindow: window
  (# <<SLOT editorWindowLib:Attributes>>;
     editorTimer: timer
       (# action::<
            (#
            do (if isOpen then
            	  INNER;
                else
                  stop;
               if);
            #);
       #);
     isOpen: booleanValue
       (# <<SLOT editorWindowIsOpen: doPart>> #);
     clearSelection:< (#  do INNER #);
     interior:
       (# theInterior: ^windowItem
       enter theInterior[]
       <<SLOT editorWindowInterior:DoPart>>
       #);
     status:
       (# msg: ^text enter msg[] <<SLOT editorWindowStatus:DoPart>> #);
     titleBar:
       (# str: ^text
       enter str[]
       <<SLOT editorWindowTitleBar:DoPart>>
       #);
     registerItem:
       (# name: ^text; item: ##windowItem
       enter (name[],item##)
       <<SLOT editorWindowRegisterItem:DoPart>>
       #);
     lookupItem:
       (#
          name: ^text;
          item: ##windowItem
       enter name[]
       <<SLOT editorWindowLookupItem:DoPart>>
       exit item##
       #);
     mousedownInItem:<
       (#
          name: ^text; p: @point; event: ^windowItem.eventHandler.mouseDown; 
       enter (name[],p,event[])
       do INNER ; 
       #);
     private: @<<SLOT editorWindowprivate:Descriptor>>;
     open::< 
       (# 
       <<SLOT editorWindowOpen:DoPart>>
       #);
     close::<  (#  <<SLOT editorWindowClose:DoPart>> #);
     eventHandler::< 
       (#
          onActivate::< 
            (# 
            <<SLOT editorWindowonActivate:DoPart>>
            #);
          onDeactivate::<  (#  <<SLOT editorWindowonDeactivate:DoPart>> #)
       #)
  #)  

