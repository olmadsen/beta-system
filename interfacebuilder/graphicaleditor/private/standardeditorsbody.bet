-- IBbuttonApplyObjectData: DoPart --
do
     (# lab: ^text; 
     do
        data.label->lab[];
        (if lab[] = none then
            '<computed>'->theWindowItem.label; 
         else
            lab[]->theWindowItem.label; 
        if);
        
     #);
   INNER ;
     

-- IBiconButtonapplyObjectData: DoPart --
do
     (# iconName: ^text; 
     do
        data.iconName->iconName[];
        (if iconName.empty then
            
         else
            iconName[]->theWindowItem.iconName; 
        if);
        data.showLabel->theWindowItem.showLabel;
        
     #);
   INNER ;
     

