   8->trace (#  do 'newnode'->xt; xN; node[]->xA #)  

-- iconButtonEditorInitDoPart: DoPart --
do
     (#
        theImp: ^astInterface.beta.imp;
        theImperatives: ^astInterface.beta.imperatives
     do
        (theDoPart.frag[],'thePixmap[] -> icon',betaGram.assignmentEvaluation)
          ->parsetext->theImp[];
        theDoPart.getImperatives->theImperatives[];
        theImp[]->theImperatives.append
     #)  

