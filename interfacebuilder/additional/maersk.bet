ORIGIN '../graphicaleditor/graphicaleditor';
INCLUDE '../graphicaleditor/standardeditors'
        '~beta/guienv/preliminary/tabControl/tabControl'
        '~beta/guienv/preliminary/treeView/treeView';
-- graphicalEditorEnvLib: Attributes --
insertMaerskSuite:
  (# 
  do
     (('canvas',canvasIcon)->loadBitMap,'tabControl',&instantiator
        (# editorType:: theGraphicalEditor.tabControlEditor #)[],
      '~beta/guienv/preliminary/tabControl/tabControl','PRE',200,150, NONE, true)
       ->factory.insert;
     (('canvas',canvasIcon)->loadBitMap,'treeView',&instantiator
        (# editorType:: theGraphicalEditor.treeViewEditor #)[],
      '~beta/guienv/preliminary/treeView/treeView','PRE',200,150, NONE, true)->factory.insert;
     
  #);
  

-- graphicalEditorLib: Attributes --
tabControlEditor: canvasEditor
  (#
     windowItemType::< tabControl;
     createTheWindowItem:: 
       (# aTab: ^theWindowItem.tab; 
       do
          children.scan
            (# count: @integer; txt: ^text; 
            do
               &theWindowItem.tab[]->aTab[];
               current.item[]->aTab.page;
               'Tab '->txt[];
               count+1->count->txt.putInt;
               txt[]->aTab.label;
               aTab.open;
               
            #);
          
       #);
     
  #);
treeViewEditor: windowItemEditor (# windowItemType::< treeView;  #);
  

