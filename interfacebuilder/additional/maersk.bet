ORIGIN '../graphicaleditor/graphicaleditor';

INCLUDE '../graphicaleditor/standardeditors';
INCLUDE '~beta/guienv/preliminary/tabControl/tabControl';
INCLUDE '~beta/guienv/preliminary/treeView/treeView';

-- graphicalEditorEnvLib: attributes --

insertMaerskSuite:
  (#
  do ('tabControl',&instantiator
     (# editorType:: theGraphicalEditor.tabControlEditor #)[],
     '~beta/guienv/preliminary/tabControl/tabControl','PRE', 'canvas', canvasIcon, 200, 150)
       ->insertCustom;
     ('treeView',&instantiator
     (# editorType:: theGraphicalEditor.treeViewEditor #)[],
     '~beta/guienv/preliminary/treeView/treeView','PRE', 'canvas', canvasIcon, 200, 150)
       ->insertCustom;
  #);

-- graphicalEditorLib: Attributes --

tabControlEditor: windowItemEditor
  (# windowItemType::< tabControl;
  #);
treeViewEditor: windowItemEditor
  (# windowItemType::< treeView;
  #);
