ORIGIN '../graphicaleditor/graphicaleditor';
INCLUDE '../support/groupbox'
        '../support/column'
        '../support/row';
-- graphicalEditorLib: Attributes --
groupBoxEditor: canvasEditor (# windowitemType:: groupBox;  #);
columnEditor: canvasEditor
  (#
     windowitemType:: column;
     newData::<
       (# 
       do
          ('alignment.vertical',alignTop)->data.setInt;
          ('distance',5)->data.setInt
       #);
     applyObjectData::< 
       (# 
       do
          'alignment.vertical'->data.getInt->theWindowItem.alignment.vertical;
          'distance'->data.getInt->theWindowItem.distance
       #);
     
  #);
rowEditor: canvasEditor
  (#
     windowitemType:: row;
     newData::< 
       (# 
       do
          ('alignment.horizontal',alignLeft)->data.setInt;
          ('distance',5)->data.setInt
       #);
     applyObjectData::< 
       (# 
       do
          'alignment.horizontal'->data.getInt
            ->theWindowItem.alignment.horizontal;
          'distance'->data.getInt->theWindowItem.distance
       #);
     
  #);
  

