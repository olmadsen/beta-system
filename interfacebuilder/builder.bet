ORIGIN 'editorenv/editorenv';
BODY 'private/builderbody';

INCLUDE 'project/project';
INCLUDE 'projectbrowser/projectbrowser';
INCLUDE 'graphicaleditor/graphicaleditor';

-- program: descriptor --
editorEnv
(# <<SLOT InterfaceBuilderLib: attributes>>;
   projectBrowserWindow: window
     (# <<SLOT projectBrowserWindowLib: attributes>>;
        menubarType::<
          (# open::<
               (# <<SLOT InterfaceBuilderProjectBrowserWindowMenubarOpen: doPart>> #);
          #);
        open::<
          (# <<SLOT InterfaceBuilderProjectBrowserWindowOpen: doPart>> #);
        private: @<<SLOT InterfaceBuilderProjectBrowserPrivate: descriptor>>;
     #);
   theGraphicalEditorEnv: @graphicalEditorEnv
     (# openAstEditor:: openSeparateAstEditorMethod;
        
        graphicalEditorWindow: graphicalEditor
          (# <<SLOT graphicalEditorWindowLib: attributes>>;
             menubarType::<
               (# open::<
                    (# <<SLOT InterfaceBuilderGraphicalEditorWindowMenubarOpen: doPart>> #);
               #);
             open::<
               (# <<SLOT InterfaceBuilderGraphicalEditorWindowOpen: doPart>> #);
             private: @<<SLOT InterfaceBuilderGraphicalEditorPrivate: descriptor>>;
          #);
        showParseErrors::
          (# 
          do msg[] -> this(editorEnv).showParseErrors;
          #);
     #);
   mainProjectBrowserWindow: @projectBrowserWindow;
   currentProject: ^project;
   
   private: @<<SLOT InterfaceBuilderPrivate: descriptor>>; 
   
<<SLOT InterfaceBuilderDoPart: doPart>>
#)
