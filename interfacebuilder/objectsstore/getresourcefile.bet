ORIGIN '~beta/guienv/v1.4/guienv';
BODY 'private/getresourcefilebody';

-- guienvLib: attributes --

getResourceFile:
  (# default: ^text;
     name: ^text;
  enter default[]
  <<SLOT getResourceFileDoPart: doPart>>
  exit name[]
  #);

interactiveGetResourceFile:
  (# resourceFileName: ^text;
  <<SLOT interactiveGetResourceFileDoPart: doPart>>
  exit resourceFileName[]
  #);
