ORIGIN '~beta/guienv/v1.6/guienv';
INCLUDE '~beta/mps/v5.2/astlevel';
INCLUDE '~beta/betaast/v5.2/betacfl';
BODY 'private/patterneditorbody';

-- windowLib: attributes --

attributesEditor: canvas
  (# <<SLOT attributesEditorLib: attributes>>;
    
     theAttributes:
       (# value: ^astInterface.beta.attributes;
       enter (# enter value[] <<SLOT AttributesEditorEnterTheAttributes: doPart>> #)
       exit (# <<SLOT AttributesEditorExitTheAttributes: doPart>> exit value[] #)
       #);
     
     interactiveCreateAttribute:
       (# <<SLOT attributesEditorInteractiveCreateAttribute: doPart>> #);
     attributeUpdated:
       (# theAttribute: ^astInterface.beta.attributeDecl;
       enter theAttribute[]
       <<SLOT attributesEditorAttributeUpdated: doPart>>
       #);
     open::<
       (# initialAttributes: ^astInterface.beta.attributes;
       enter initialAttributes[]
       <<SLOT AttributesEditorOpen: doPart>> 
       #);
     attributeSelected:<
       (# theAttribute: ^astInterface.beta.attributeDecl;
          doubleClick: @boolean;
       enter(theAttribute[], doubleClick)
       do inner;
       #);
     reportParseErrors:<
       (# errors: ^text;
       enter errors[]
       do inner;
       #);
     private: @<<SLOT AttributesEditorPrivate: descriptor>>;
  #);
