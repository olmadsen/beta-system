ORIGIN '~beta/basiclib/v1.6/basicsystemenv';
INCLUDE '~beta/basiclib/v1.6/file'
        '~beta/compiler/CONTROL/system';
MDBODY sun4s 'private/dynenvbody'
       default 'private/dynenvbody_empty';
-- systemLib: Attributes --
dynenv:
  (# available: booleanValue
       (#
       <<slot dynenvAvailable: doPart>>
       #);
     comp: ^compiler;
     evaluateFile:
       (# name: ^text; theOrigin: ^object; P: ##object; 
       enter (name[],theOrigin[])
       <<SLOT dynenvEvaluateFile:DoPart>>
       exit P##
       #);
     activateCompiler:
       (# error: @boolean
       <<SLOT dynenvActivateCompiler:DoPart>>
       exit error
       #);
     linkPattern:
       (# theOrigin: ^object; F: ##object
       enter theOrigin[]
       <<SLOT dynenvLinkPattern:DoPart>>
       exit F##
       #);
     init:
       (#
          T: ^text;
          progress:< (#  do INNER #);
          
       enter comp[]
       <<SLOT dynenvInit:DoPart>>
       #);
     info,msg:
       ^stream;
     noExe,trace: @boolean;
     private: @<<SLOT dynenvPrivate:Descriptor>>;
     
  do INNER ; 
  #)  

