ORIGIN '~beta/dotnetlib/beta_helpers';
--PROGRAM: descriptor--
(# ones: ThreadWrapper
     (# run:
          (# 
          do (for i:100 repeat 1->putint; for);
          #);
     #);
   twos: ThreadWrapper
     (# run:
          (# 
          do (for i:100 repeat 2->putint; for);
          #);
     #);
   t1: ^ones;
   t2: ^twos;
