ORIGIN '_TypeCode';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET TypeCode class. *)
TypeCode: _TypeCode
  (#
     (* Enum Literals *)

     Empty:
       (# exit 0 #);
     Object:
       (# exit 1 #);
     DBNull:
       (# exit 2 #);
     DotnetBoolean:
       (# exit 3 #);
     DotnetChar:
       (# exit 4 #);
     SByte:
       (# exit 5 #);
     Byte:
       (# exit 6 #);
     DotnetInt16:
       (# exit 7 #);
     UInt16:
       (# exit 8 #);
     DotnetInt32:
       (# exit 9 #);
     UInt32:
       (# exit 10 #);
     DotnetInt64:
       (# exit 11 #);
     UInt64:
       (# exit 12 #);
     Single:
       (# exit 13 #);
     Double:
       (# exit 14 #);
     Decimal:
       (# exit 15 #);
     DateTime:
       (# exit 16 #);
     String:
       (# exit 18 #);
  do '@valuetype [mscorlib]System.TypeCode' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asTypeCode:
  (# _T: ^_TypeCode;
     T: ^TypeCode;
  enter _T[]
  do _T[]->T[];
  exit T[]
  #);
