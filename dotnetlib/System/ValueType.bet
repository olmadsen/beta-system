ORIGIN '_ValueType';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_String';
--LIB: attributes--

(* .NET ValueType class. *)
ValueType: _ValueType
  (#
     (* Public/family CLS compliant methods *)

     GetHashCode: proc
       (# result: @int32;
       exit result
       #);
     Equals: proc
       (# result: @boolean;
          arg1: ^Object;
       enter (arg1[])
       exit result
       #);
     ToString: proc
       (# result: ^_String;
       exit result[]
       #);
  do '[mscorlib]System.ValueType' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asValueType:
  (# _V: ^_ValueType;
     V: ^ValueType;
  enter _V[]
  do _V[]->V[];
  exit V[]
  #);
