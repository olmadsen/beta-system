ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/dotnetlib/System/Reflection/_MemberInfo' (* Cannot use non-wrapper *);
--LIB: attributes--

(* Dotnet Type class declaration.
 * This wrapper is needed to prevent circular fragment INCLUDE.
 * See Type.bet for members.
 *)
_Type: _MemberInfo
