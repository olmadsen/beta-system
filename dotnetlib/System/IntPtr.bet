ORIGIN '_IntPtr';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_ValueType';
INCLUDE '~beta/dotnetlib/System/_String';
--LIB: attributes--

(* .NET IntPtr class. *)
IntPtr: _IntPtr
  (#
     (* Public/family CLS compliant fields *)

     (* STATIC: *)
     Zero: ^IntPtr;

     (* Public/family CLS compliant constructors *)

     _init_int32: cons (* constructor *)
       (# result: ^IntPtr;
          arg1: @int32;
       enter (arg1)
       exit result[]
       #);
     _init_int64: cons (* constructor *)
       (# result: ^IntPtr;
          arg1: @int64;
       enter (arg1)
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     GetHashCode: proc
       (# result: @int32;
       exit result
       #);
     Equals: proc
       (# result: @boolean;
          arg1: ^Object;
       enter (arg1[])
       exit result
       #);
     ToString: proc
       (# result: ^_String;
       exit result[]
       #);
     ToInt32: proc
       (# result: @int32;
       exit result
       #);
     ToInt64: proc
       (# result: @int64;
       exit result
       #);
     op_Equality: static_proc
       (# result: @boolean;
          arg1: ^IntPtr;
          arg2: ^IntPtr;
       enter (arg1[], arg2[])
       exit result
       #);
     op_Inequality: static_proc
       (# result: @boolean;
          arg1: ^IntPtr;
          arg2: ^IntPtr;
       enter (arg1[], arg2[])
       exit result
       #);
     get_Size: static_proc
       (# result: @int32;
       exit result
       #);
  do 'valuetype [mscorlib]System.IntPtr' -> className;
  INNER;
  #);

