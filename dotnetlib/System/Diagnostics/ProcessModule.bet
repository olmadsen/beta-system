ORIGIN '_ProcessModule';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/ComponentModel/_Component';
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/_IntPtr';
INCLUDE '~beta/dotnetlib/System/Diagnostics/_FileVersionInfo';
--LIB: attributes--

(* .NET ProcessModule class. *)
ProcessModule: _ProcessModule
  (#
     (* Public/family CLS compliant methods *)

     ToString: proc
       (# result: ^_String;
       exit result[]
       #);
     get_ModuleName: proc
       (# result: ^_String;
       exit result[]
       #);
     get_FileName: proc
       (# result: ^_String;
       exit result[]
       #);
     get_BaseAddress: proc
       (# result: ^_IntPtr;
       exit result[]
       #);
     get_ModuleMemorySize: proc
       (# result: @int32;
       exit result
       #);
     get_EntryPointAddress: proc
       (# result: ^_IntPtr;
       exit result[]
       #);
     get_FileVersionInfo: proc
       (# result: ^_FileVersionInfo;
       exit result[]
       #);
  do '[System]System.Diagnostics.ProcessModule' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asProcessModule:
  (# _P: ^_ProcessModule;
     P: ^ProcessModule;
  enter _P[]
  do _P[]->P[];
  exit P[]
  #);
