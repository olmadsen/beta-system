ORIGIN '_ProcessThread';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/ComponentModel/_Component';
INCLUDE '~beta/dotnetlib/System/Diagnostics/_ThreadPriorityLevel';
INCLUDE '~beta/dotnetlib/System/_TimeSpan';
INCLUDE '~beta/dotnetlib/System/_IntPtr';
INCLUDE '~beta/dotnetlib/System/_DateTime';
INCLUDE '~beta/dotnetlib/System/Diagnostics/_ThreadState';
INCLUDE '~beta/dotnetlib/System/Diagnostics/_ThreadWaitReason';
--LIB: attributes--

(* .NET ProcessThread class. *)
ProcessThread: _ProcessThread
  (#
     (* Public/family CLS compliant methods *)

     get_BasePriority: proc
       (# result: @int32;
       exit result
       #);
     get_CurrentPriority: proc
       (# result: @int32;
       exit result
       #);
     get_Id: proc
       (# result: @int32;
       exit result
       #);
     set_IdealProcessor: proc
       (# 
          arg1: @int32;
       enter (arg1)
       #);
     get_PriorityBoostEnabled: proc
       (# result: @boolean;
       exit result
       #);
     set_PriorityBoostEnabled: proc
       (# 
          arg1: @boolean;
       enter (arg1)
       #);
     get_PriorityLevel: proc
       (# result: ^_ThreadPriorityLevel;
       exit result[]
       #);
     set_PriorityLevel: proc
       (# 
          arg1: @int32;
       enter (arg1)
       #);
     get_PrivilegedProcessorTime: proc
       (# result: ^_TimeSpan;
       exit result[]
       #);
     get_StartAddress: proc
       (# result: ^_IntPtr;
       exit result[]
       #);
     get_StartTime: proc
       (# result: ^_DateTime;
       exit result[]
       #);
     get_ThreadState: proc
       (# result: ^_ThreadState;
       exit result[]
       #);
     get_TotalProcessorTime: proc
       (# result: ^_TimeSpan;
       exit result[]
       #);
     get_UserProcessorTime: proc
       (# result: ^_TimeSpan;
       exit result[]
       #);
     get_WaitReason: proc
       (# result: ^_ThreadWaitReason;
       exit result[]
       #);
     ResetIdealProcessor: proc
       (# 
       #);
     set_ProcessorAffinity: proc
       (# 
          arg1: ^_IntPtr;
       enter (arg1[])
       #);
  do '[System]System.Diagnostics.ProcessThread' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asProcessThread:
  (# _P: ^_ProcessThread;
     P: ^ProcessThread;
  enter _P[]
  do _P[]->P[];
  exit P[]
  #);
