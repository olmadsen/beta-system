ORIGIN '_ThreadState';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET ThreadState class. *)
ThreadState: _ThreadState
  (#
     (* Enum Literals *)

     Initialized:
       (# exit 0 #);
     Ready:
       (# exit 1 #);
     Running:
       (# exit 2 #);
     Standby:
       (# exit 3 #);
     Terminated:
       (# exit 4 #);
     Wait:
       (# exit 5 #);
     Transition:
       (# exit 6 #);
     Unknown:
       (# exit 7 #);
  do '@valuetype [System]System.Diagnostics.ThreadState' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asThreadState:
  (# _T: ^_ThreadState;
     T: ^ThreadState;
  enter _T[]
  do _T[]->T[];
  exit T[]
  #);
