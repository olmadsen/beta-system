ORIGIN '_BaseCollection';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_MarshalByRefObject';
INCLUDE '~beta/dotnetlib/System/Collections/_IEnumerator';
INCLUDE '~beta/dotnetlib/System/_Array';
--LIB: attributes--

(* .NET BaseCollection class. *)
BaseCollection: _BaseCollection
  (#
     (* Public/family CLS compliant constructors *)

     _init: cons
       (# result: ^BaseCollection;
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     GetEnumerator: proc
       (# result: ^_IEnumerator;
       exit result[]
       #);
     get_IsSynchronized: proc
       (# result: @boolean;
       exit result
       #);
     get_SyncRoot: proc
       (# result: ^Object;
       exit result[]
       #);
     get_Count: proc
       (# result: @int32;
       exit result
       #);
     CopyTo: proc
       (# 
          arg1: ^_Array;
          arg2: @int32;
       enter (arg1[], arg2)
       #);
     get_IsReadOnly: proc
       (# result: @boolean;
       exit result
       #);
  do '[System.Windows.Forms]System.Windows.Forms.BaseCollection' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asBaseCollection:
  (# _B: ^_BaseCollection;
     B: ^BaseCollection;
  enter _B[]
  do _B[]->B[];
  exit B[]
  #);
