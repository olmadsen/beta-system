ORIGIN '_DragAction';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET DragAction class. *)
DragAction: _DragAction
  (#
     (* Enum Literals *)

     Continue:
       (# exit 0 #);
     Drop:
       (# exit 1 #);
     Cancel:
       (# exit 2 #);
  do '@valuetype [System.Windows.Forms]System.Windows.Forms.DragAction' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asDragAction:
  (# _D: ^_DragAction;
     D: ^DragAction;
  enter _D[]
  do _D[]->D[];
  exit D[]
  #);
