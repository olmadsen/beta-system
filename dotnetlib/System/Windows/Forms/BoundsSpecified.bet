ORIGIN '_BoundsSpecified';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET BoundsSpecified class. *)
BoundsSpecified: _BoundsSpecified
  (#
     (* Public/family CLS compliant fields *)

     value__: @int32;

     (* Enum Literals *)

     DotnetNone:
       (# exit 0 #);
     X:
       (# exit 1 #);
     Y:
       (# exit 2 #);
     Location:
       (# exit 3 #);
     Width:
       (# exit 4 #);
     Height:
       (# exit 8 #);
     Size:
       (# exit 12 #);
     All:
       (# exit 15 #);
  do 'valuetype [System.Windows.Forms]System.Windows.Forms.BoundsSpecified' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asBoundsSpecified:
  (# _B: ^_BoundsSpecified;
     B: ^BoundsSpecified;
  enter _B[]
  do _B[]->B[];
  exit B[]
  #);
