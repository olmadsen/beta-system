ORIGIN '_ControlEventArgs';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_EventArgs';
INCLUDE '~beta/dotnetlib/System/Windows/Forms/_Control';
--LIB: attributes--

(* .NET ControlEventArgs class. *)
ControlEventArgs: _ControlEventArgs
  (#
     (* Public/family CLS compliant constructors *)

     _init: cons
       (# result: ^ControlEventArgs;
          arg1: ^_Control;
       enter (arg1[])
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     get_Control: proc
       (# result: ^_Control;
       exit result[]
       #);
  do '[System.Windows.Forms]System.Windows.Forms.ControlEventArgs' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asControlEventArgs:
  (# _C: ^_ControlEventArgs;
     C: ^ControlEventArgs;
  enter _C[]
  do _C[]->C[];
  exit C[]
  #);
