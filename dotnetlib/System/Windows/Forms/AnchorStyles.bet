ORIGIN '_AnchorStyles';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET AnchorStyles class. *)
AnchorStyles: _AnchorStyles
  (#
     (* Public/family CLS compliant fields *)

     value__: @int32;

     (* Enum Literals *)

     DotnetNone:
       (# exit 0 #);
     Top:
       (# exit 1 #);
     Bottom:
       (# exit 2 #);
     Left:
       (# exit 4 #);
     Right:
       (# exit 8 #);
  do 'valuetype [System.Windows.Forms]System.Windows.Forms.AnchorStyles' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asAnchorStyles:
  (# _A: ^_AnchorStyles;
     A: ^AnchorStyles;
  enter _A[]
  do _A[]->A[];
  exit A[]
  #);
