ORIGIN '_IWin32Window';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_IntPtr';
--LIB: attributes--

(* .NET IWin32Window class. *)
IWin32Window: _IWin32Window
  (#
     (* Public/family CLS compliant methods *)

     get_Handle: proc
       (# result: ^_IntPtr;
       exit result[]
       #);
  do '[System.Windows.Forms]System.Windows.Forms.IWin32Window' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asIWin32Window:
  (# _I: ^_IWin32Window;
     I: ^IWin32Window;
  enter _I[]
  do _I[]->I[];
  exit I[]
  #);
