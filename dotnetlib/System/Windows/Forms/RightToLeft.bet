ORIGIN '_RightToLeft';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET RightToLeft class. *)
RightToLeft: _RightToLeft
  (#
     (* Public/family CLS compliant fields *)

     value__: @int32;

     (* Enum Literals *)

     No:
       (# exit 0 #);
     Yes:
       (# exit 1 #);
     Inherit:
       (# exit 2 #);
  do 'valuetype [System.Windows.Forms]System.Windows.Forms.RightToLeft' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asRightToLeft:
  (# _R: ^_RightToLeft;
     R: ^RightToLeft;
  enter _R[]
  do _R[]->R[];
  exit R[]
  #);
