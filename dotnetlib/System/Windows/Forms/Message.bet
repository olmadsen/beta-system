ORIGIN '_Message';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_ValueType';
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/_IntPtr';
INCLUDE '~beta/dotnetlib/System/_Type';
--LIB: attributes--

(* .NET Message class. *)
Message: _Message
  (#
     (* Public/family CLS compliant methods *)

     GetHashCode: proc
       (# result: @int32;
       exit result
       #);
     Equals: proc
       (# result: @boolean;
          arg1: ^Object;
       enter (arg1[])
       exit result
       #);
     ToString: proc
       (# result: ^_String;
       exit result[]
       #);
     get_HWnd: proc
       (# result: ^_IntPtr;
       exit result[]
       #);
     set_HWnd: proc
       (# 
          arg1: ^_IntPtr;
       enter (arg1[])
       #);
     get_Msg: proc
       (# result: @int32;
       exit result
       #);
     set_Msg: proc
       (# 
          arg1: @int32;
       enter (arg1)
       #);
     get_WParam: proc
       (# result: ^_IntPtr;
       exit result[]
       #);
     set_WParam: proc
       (# 
          arg1: ^_IntPtr;
       enter (arg1[])
       #);
     get_LParam: proc
       (# result: ^_IntPtr;
       exit result[]
       #);
     set_LParam: proc
       (# 
          arg1: ^_IntPtr;
       enter (arg1[])
       #);
     get_Result: proc
       (# result: ^_IntPtr;
       exit result[]
       #);
     set_Result: proc
       (# 
          arg1: ^_IntPtr;
       enter (arg1[])
       #);
     GetLParam: proc
       (# result: ^Object;
          arg1: ^_Type;
       enter (arg1[])
       exit result[]
       #);
     Create: static_proc
       (# result: ^Message;
          arg1: ^_IntPtr;
          arg2: @int32;
          arg3: ^_IntPtr;
          arg4: ^_IntPtr;
       enter (arg1[], arg2, arg3[], arg4[])
       exit result[]
       #);
  do 'valuetype [System.Windows.Forms]System.Windows.Forms.Message' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asMessage:
  (# _M: ^_Message;
     M: ^Message;
  enter _M[]
  do _M[]->M[];
  exit M[]
  #);
