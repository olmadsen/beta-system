INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET FormStartPosition class. *)
FormStartPosition: _FormStartPosition
  (#
     (* Enum Literals *)

     Manual:
       (# exit 0 #);
     CenterScreen:
       (# exit 1 #);
     WindowsDefaultLocation:
       (# exit 2 #);
     WindowsDefaultBounds:
       (# exit 3 #);
