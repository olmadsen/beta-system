ORIGIN '_IContainerControl';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/Windows/Forms/_Control';
--LIB: attributes--

(* .NET IContainerControl class. *)
IContainerControl: _IContainerControl
  (#
     (* Public/family CLS compliant methods *)

     ActivateControl: proc
       (# result: @boolean;
          arg1: ^_Control;
       enter (arg1[])
       exit result
       #);
     set_ActiveControl: proc
       (# 
          arg1: ^_Control;
       enter (arg1[])
       #);
     get_ActiveControl: proc
       (# result: ^_Control;
       exit result[]
       #);
  do '[System.Windows.Forms]System.Windows.Forms.IContainerControl' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asIContainerControl:
  (# _I: ^_IContainerControl;
     I: ^IContainerControl;
  enter _I[]
  do _I[]->I[];
  exit I[]
  #);
