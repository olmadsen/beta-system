ORIGIN '_BindingContext';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/Windows/Forms/_BindingManagerBase';
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/ComponentModel/_CollectionChangeEventHandler';
--LIB: attributes--

(* .NET BindingContext class. *)
BindingContext: _BindingContext
  (#
     (* Public/family CLS compliant constructors *)

     _init: cons
       (# result: ^BindingContext;
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     get_IsReadOnly: proc
       (# result: @boolean;
       exit result
       #);
     get_Item_Object: proc (* get_Item *)
       (# result: ^_BindingManagerBase;
          arg1: ^Object;
       enter (arg1[])
       do 'get_Item' -> procname;
       exit result[]
       #);
     get_Item_Object_String: proc (* get_Item *)
       (# result: ^_BindingManagerBase;
          arg1: ^Object;
          arg2: ^_String;
       enter (arg1[], arg2[])
       do 'get_Item' -> procname;
       exit result[]
       #);
     add_CollectionChanged: proc
       (# 
          arg1: ^_CollectionChangeEventHandler;
       enter (arg1[])
       #);
     remove_CollectionChanged: proc
       (# 
          arg1: ^_CollectionChangeEventHandler;
       enter (arg1[])
       #);
     Contains_Object: proc (* Contains *)
       (# result: @boolean;
          arg1: ^Object;
       enter (arg1[])
       do 'Contains' -> procname;
       exit result
       #);
     Contains_Object_String: proc (* Contains *)
       (# result: @boolean;
          arg1: ^Object;
          arg2: ^_String;
       enter (arg1[], arg2[])
       do 'Contains' -> procname;
       exit result
       #);
  do '[System.Windows.Forms]System.Windows.Forms.BindingContext' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asBindingContext:
  (# _B: ^_BindingContext;
     B: ^BindingContext;
  enter _B[]
  do _B[]->B[];
  exit B[]
  #);
