ORIGIN '_DialogResult';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET DialogResult class. *)
DialogResult: _DialogResult
  (#
     (* Enum Literals *)

     DotnetNone:
       (# exit 0 #);
     OK:
       (# exit 1 #);
     Cancel:
       (# exit 2 #);
     Abort:
       (# exit 3 #);
     Retry:
       (# exit 4 #);
     Ignore:
       (# exit 5 #);
     Yes:
       (# exit 6 #);
     No:
       (# exit 7 #);
  do 'valuetype [System.Windows.Forms]System.Windows.Forms.DialogResult' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asDialogResult:
  (# _D: ^_DialogResult;
     D: ^DialogResult;
  enter _D[]
  do _D[]->D[];
  exit D[]
  #);
