ORIGIN '_ICustomAttributeProvider';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Type';
--LIB: attributes--

(* .NET ICustomAttributeProvider class. *)
ICustomAttributeProvider: _ICustomAttributeProvider
  (#
     (* Public/family CLS compliant methods *)

     IsDefined: proc
       (# result: @boolean;
          arg1: ^_Type;
          arg2: @boolean;
       enter (arg1[], arg2)
       exit result
       #);
     GetCustomAttributes_boolean: proc (* GetCustomAttributes *)
       (# result: [0]^Object;
          arg1: @boolean;
       enter (arg1)
       do 'GetCustomAttributes' -> procname;
       exit result[]
       #);
     GetCustomAttributes_Type_boolean: proc (* GetCustomAttributes *)
       (# result: [0]^Object;
          arg1: ^_Type;
          arg2: @boolean;
       enter (arg1[], arg2)
       do 'GetCustomAttributes' -> procname;
       exit result[]
       #);
  do '[mscorlib]System.Reflection.ICustomAttributeProvider' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asICustomAttributeProvider:
  (# _I: ^_ICustomAttributeProvider;
     I: ^ICustomAttributeProvider;
  enter _I[]
  do _I[]->I[];
  exit I[]
  #);
