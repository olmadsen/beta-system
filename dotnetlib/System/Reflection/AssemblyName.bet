ORIGIN '_AssemblyName';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/Runtime/Serialization/_SerializationInfo';
INCLUDE '~beta/dotnetlib/System/Runtime/Serialization/_StreamingContext';
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/_Version';
INCLUDE '~beta/dotnetlib/System/Globalization/_CultureInfo';
INCLUDE '~beta/dotnetlib/System/Reflection/_AssemblyNameFlags';
INCLUDE '~beta/dotnetlib/System/Configuration/Assemblies/_AssemblyHashAlgorithm';
INCLUDE '~beta/dotnetlib/System/Configuration/Assemblies/_AssemblyVersionCompatibility';
INCLUDE '~beta/dotnetlib/System/Reflection/_StrongNameKeyPair';
--LIB: attributes--

(* .NET AssemblyName class. *)
AssemblyName: _AssemblyName
  (#
     (* Public/family CLS compliant constructors *)

     _init: cons
       (# result: ^AssemblyName;
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     OnDeserialization: proc
       (# 
          arg1: ^Object;
       enter (arg1[])
       #);
     GetObjectData: proc
       (# 
          arg1: ^_SerializationInfo;
          arg2: ^_StreamingContext;
       enter (arg1[], arg2[])
       #);
     Clone: proc
       (# result: ^Object;
       exit result[]
       #);
     ToString: proc
       (# result: ^_String;
       exit result[]
       #);
     get_Name: proc
       (# result: ^_String;
       exit result[]
       #);
     set_Name: proc
       (# 
          arg1: ^_String;
       enter (arg1[])
       #);
     get_Version: proc
       (# result: ^_Version;
       exit result[]
       #);
     set_Version: proc
       (# 
          arg1: ^_Version;
       enter (arg1[])
       #);
     get_CultureInfo: proc
       (# result: ^_CultureInfo;
       exit result[]
       #);
     set_CultureInfo: proc
       (# 
          arg1: ^_CultureInfo;
       enter (arg1[])
       #);
     get_CodeBase: proc
       (# result: ^_String;
       exit result[]
       #);
     set_CodeBase: proc
       (# 
          arg1: ^_String;
       enter (arg1[])
       #);
     get_EscapedCodeBase: proc
       (# result: ^_String;
       exit result[]
       #);
     GetAssemblyName: static_proc
       (# result: ^AssemblyName;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);
     GetPublicKey: proc
       (# result: [0]@int8u;
       exit result
       #);
     SetPublicKey: proc
       (# 
          arg1: [0]@int8u;
       enter (arg1[])
       #);
     GetPublicKeyToken: proc
       (# result: [0]@int8u;
       exit result
       #);
     SetPublicKeyToken: proc
       (# 
          arg1: [0]@int8u;
       enter (arg1[])
       #);
     get_Flags: proc
       (# result: ^_AssemblyNameFlags;
       exit result[]
       #);
     set_Flags: proc
       (# 
          arg1: @int32;
       enter (arg1)
       #);
     get_HashAlgorithm: proc
       (# result: ^_AssemblyHashAlgorithm;
       exit result[]
       #);
     set_HashAlgorithm: proc
       (# 
          arg1: @int32;
       enter (arg1)
       #);
     get_VersionCompatibility: proc
       (# result: ^_AssemblyVersionCompatibility;
       exit result[]
       #);
     set_VersionCompatibility: proc
       (# 
          arg1: @int32;
       enter (arg1)
       #);
     get_KeyPair: proc
       (# result: ^_StrongNameKeyPair;
       exit result[]
       #);
     set_KeyPair: proc
       (# 
          arg1: ^_StrongNameKeyPair;
       enter (arg1[])
       #);
     get_FullName: proc
       (# result: ^_String;
       exit result[]
       #);
  do '[mscorlib]System.Reflection.AssemblyName' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asAssemblyName:
  (# _A: ^_AssemblyName;
     A: ^AssemblyName;
  enter _A[]
  do _A[]->A[];
  exit A[]
  #);
