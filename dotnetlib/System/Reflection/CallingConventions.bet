ORIGIN '_CallingConventions';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET CallingConventions class. *)
CallingConventions: _CallingConventions
  (#
     (* Enum Literals *)

     Standard:
       (# exit 1 #);
     VarArgs:
       (# exit 2 #);
     Any:
       (# exit 3 #);
     HasThis:
       (# exit 32 #);
     ExplicitThis:
       (# exit 64 #);
  do '@valuetype [mscorlib]System.Reflection.CallingConventions' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asCallingConventions:
  (# _C: ^_CallingConventions;
     C: ^CallingConventions;
  enter _C[]
  do _C[]->C[];
  exit C[]
  #);
