ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/dotnetlib/System.Reflection/MethodBase';
--LIB: attributes--

(* Dotnet MethodInfo class declaration.
 * This wrapper is needed to prevent circular fragment INCLUDE.
 * See MethodInfo.bet for members.
 *)
_MethodInfo: MethodBase
  (#
  do '[mscorlib]System.Reflection.MethodInfo' -> className;
  INNER;
  #);

