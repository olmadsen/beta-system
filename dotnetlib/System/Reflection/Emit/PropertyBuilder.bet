  do '[mscorlib]System.Reflection.Emit.PropertyBuilder' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asPropertyBuilder:
  (# _P: ^_PropertyBuilder;
     P: ^PropertyBuilder;
  enter _P[]
  do _P[]->P[];
  exit P[]
  #);
