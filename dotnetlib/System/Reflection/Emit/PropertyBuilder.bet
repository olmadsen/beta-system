          arg5: [0]^Object;
          arg6: ^_CultureInfo;
       enter (arg1[], arg2[], arg3, arg4[], arg5[], arg6[])
       do 'SetValue' -> procname;
       #);
     SetValue_Object_Object_ArrayOfObject: proc (* SetValue *)
       (# 
          arg1: ^Object;
          arg2: ^Object;
          arg3: [0]^Object;
       enter (arg1[], arg2[], arg3[])
       do 'SetValue' -> procname;
       #);
