ORIGIN '_ParameterBuilder';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/Reflection/Emit/_ParameterToken';
INCLUDE '~beta/dotnetlib/System/Reflection/Emit/_UnmanagedMarshal';
INCLUDE '~beta/dotnetlib/System/Reflection/_ConstructorInfo';
INCLUDE '~beta/dotnetlib/System/Reflection/Emit/_CustomAttributeBuilder';
--LIB: attributes--

(* .NET ParameterBuilder class. *)
ParameterBuilder: _ParameterBuilder
  (#
     (* Public/family CLS compliant methods *)

     get_Attributes: proc
       (# result: @int32;
       exit result
       #);
     get_Position: proc
       (# result: @int32;
       exit result
       #);
     get_Name: proc
       (# result: ^_String;
       exit result[]
       #);
     GetToken: proc
       (# result: ^_ParameterToken;
       exit result[]
       #);
     SetConstant: proc
       (# 
          arg1: ^Object;
       enter (arg1[])
       #);
     SetMarshal: proc
       (# 
          arg1: ^_UnmanagedMarshal;
       enter (arg1[])
       #);
     SetCustomAttribute_ConstructorInfo_ArrayOfInt8u: proc (* SetCustomAttribute *)
       (# 
          arg1: ^_ConstructorInfo;
          arg2: [0]@int8u;
       enter (arg1[], arg2[])
       do 'SetCustomAttribute' -> procname;
       #);
     SetCustomAttribute_CustomAttributeBuilder: proc (* SetCustomAttribute *)
       (# 
          arg1: ^_CustomAttributeBuilder;
       enter (arg1[])
       do 'SetCustomAttribute' -> procname;
       #);
     get_IsIn: proc
       (# result: @boolean;
       exit result
       #);
     get_IsOut: proc
       (# result: @boolean;
       exit result
       #);
     get_IsOptional: proc
       (# result: @boolean;
       exit result
       #);
  do '[mscorlib]System.Reflection.Emit.ParameterBuilder' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asParameterBuilder:
  (# _P: ^_ParameterBuilder;
     P: ^ParameterBuilder;
  enter _P[]
  do _P[]->P[];
  exit P[]
  #);
