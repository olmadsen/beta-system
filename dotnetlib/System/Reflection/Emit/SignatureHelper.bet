ORIGIN '_SignatureHelper';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/Reflection/_Module';
INCLUDE '~beta/dotnetlib/System/Runtime/InteropServices/_CallingConvention';
INCLUDE '~beta/dotnetlib/System/_Type';
INCLUDE '~beta/dotnetlib/System/Reflection/_CallingConventions';
--LIB: attributes--

(* .NET SignatureHelper class. *)
SignatureHelper: _SignatureHelper
  (#
     (* Public/family CLS compliant methods *)

     GetHashCode: proc
       (# result: @int32;
       exit result
       #);
     Equals: proc
       (# result: @boolean;
          arg1: ^Object;
       enter (arg1[])
       exit result
       #);
     ToString: proc
       (# result: ^_String;
       exit result[]
       #);
     GetMethodSigHelper_Module_CallingConvention_Type: static_proc (* GetMethodSigHelper *)
       (# result: ^SignatureHelper;
          arg1: ^_Module;
          arg2: ^_CallingConvention;
          arg3: ^_Type;
       enter (arg1[], arg2[], arg3[])
       do 'GetMethodSigHelper' -> procname;
       exit result[]
       #);
     GetLocalVarSigHelper: static_proc
       (# result: ^SignatureHelper;
          arg1: ^_Module;
       enter (arg1[])
       exit result[]
       #);
     GetFieldSigHelper: static_proc
       (# result: ^SignatureHelper;
          arg1: ^_Module;
       enter (arg1[])
       exit result[]
       #);
     GetMethodSigHelper_Module_CallingConventions_Type: static_proc (* GetMethodSigHelper *)
       (# result: ^SignatureHelper;
          arg1: ^_Module;
          arg2: ^_CallingConventions;
          arg3: ^_Type;
       enter (arg1[], arg2[], arg3[])
       do 'GetMethodSigHelper' -> procname;
       exit result[]
       #);
     GetMethodSigHelper_Module_Type_ArrayOf_Type: static_proc (* GetMethodSigHelper *)
       (# result: ^SignatureHelper;
          arg1: ^_Module;
          arg2: ^_Type;
          arg3: [0]^_Type;
       enter (arg1[], arg2[], arg3[])
       do 'GetMethodSigHelper' -> procname;
       exit result[]
       #);
     GetPropertySigHelper: static_proc
       (# result: ^SignatureHelper;
          arg1: ^_Module;
          arg2: ^_Type;
          arg3: [0]^_Type;
       enter (arg1[], arg2[], arg3[])
       exit result[]
       #);
     AddArgument: proc
       (# 
          arg1: ^_Type;
       enter (arg1[])
       #);
     AddSentinel: proc
       (# 
       #);
     GetSignature: proc
       (# result: [0]@int8u;
       exit result
       #);
  do '[mscorlib]System.Reflection.Emit.SignatureHelper' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asSignatureHelper:
  (# _S: ^_SignatureHelper;
     S: ^SignatureHelper;
  enter _S[]
  do _S[]->S[];
  exit S[]
  #);
