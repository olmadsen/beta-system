ORIGIN '_PropertyAttributes';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET PropertyAttributes class. *)
PropertyAttributes: _PropertyAttributes
  (#
     (* Enum Literals *)

     DotnetNone:
       (# exit 0 #);
     SpecialName:
       (# exit 512 #);
     RTSpecialName:
       (# exit 1024 #);
     HasDefault:
       (# exit 4096 #);
     Reserved2:
       (# exit 8192 #);
     Reserved3:
       (# exit 16384 #);
     Reserved4:
       (# exit 32768 #);
     ReservedMask:
       (# exit 62464 #);
  do '@valuetype [mscorlib]System.Reflection.PropertyAttributes' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asPropertyAttributes:
  (# _P: ^_PropertyAttributes;
     P: ^PropertyAttributes;
  enter _P[]
  do _P[]->P[];
  exit P[]
  #);
