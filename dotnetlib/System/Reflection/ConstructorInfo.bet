ORIGIN '_ConstructorInfo';
INCLUDE '~beta/dotnetlib/System/Reflection/_MethodBase';
INCLUDE '~beta/dotnetlib/System/Reflection/_BindingFlags';
INCLUDE '~beta/dotnetlib/System/Reflection/_Binder';
INCLUDE '~beta/dotnetlib/System/Globalization/_CultureInfo';
INCLUDE '~beta/dotnetlib/System/Reflection/_MemberTypes';
--LIB: attributes--

(* .NET ConstructorInfo class. *)
ConstructorInfo: _ConstructorInfo
  (#
     (* Public/protected CLS compliant methods *)

     Invoke_BindingFlags_Binder_ArrayOf_Object_CultureInfo: proc (* Invoke *)
       (# result: ^_Object;
          arg1: ^_BindingFlags;
          arg2: ^_Binder;
          arg3: [0]^_Object;
          arg4: ^_CultureInfo;
       enter (arg1[], arg2[], arg3, arg4[])
       do 'Invoke' -> procname;
       exit result[]
       #);
     get_MemberType: proc
       (# result: ^_MemberTypes;
       exit result[]
       #);
     Invoke_ArrayOf_Object: proc (* Invoke *)
       (# result: ^_Object;
          arg1: [0]^_Object;
       enter (arg1)
       do 'Invoke' -> procname;
       exit result[]
       #);
  do '[mscorlib]System/Reflection.ConstructorInfo' -> className;
  INNER;
  #);

