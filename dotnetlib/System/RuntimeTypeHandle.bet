ORIGIN '_RuntimeTypeHandle';
INCLUDE '~beta/dotnetlib/System/_ValueType';
INCLUDE '~beta/dotnetlib/System/Runtime/Serialization/_SerializationInfo';
INCLUDE '~beta/dotnetlib/System/Runtime/Serialization/_StreamingContext';
INCLUDE '~beta/dotnetlib/System/_IntPtr';
--LIB: attributes--

(* .NET RuntimeTypeHandle class. *)
RuntimeTypeHandle: _RuntimeTypeHandle
  (#
     (* Public/family CLS compliant methods *)

     GetObjectData: proc
       (# 
          arg1: ^_SerializationInfo;
          arg2: ^_StreamingContext;
       enter (arg1[], arg2[])
       #);
     get_Value: proc
       (# result: ^_IntPtr;
       exit result[]
       #);
  do 'valuetype [mscorlib]System.RuntimeTypeHandle' -> className;
  INNER;
  #);

