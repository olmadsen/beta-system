ORIGIN '_Double';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_ValueType';
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/_IFormatProvider';
INCLUDE '~beta/dotnetlib/System/_TypeCode';
INCLUDE '~beta/dotnetlib/System/Globalization/_NumberStyles';
--LIB: attributes--

(* .NET Double class. *)
Double: _Double
  (#
     (* Public/family CLS compliant fields *)

     (* STATIC: *)
     MinValue: @real;
     (* STATIC: *)
     MaxValue: @real;
     (* STATIC: *)
     Epsilon: @real;
     (* STATIC: *)
     NegativeInfinity: @real;
     (* STATIC: *)
     PositiveInfinity: @real;
     (* STATIC: *)
     NaN: @real;

     (* Public/family CLS compliant methods *)

     ToString_IFormatProvider: proc (* ToString *)
       (# result: ^_String;
          arg1: ^_IFormatProvider;
       enter (arg1[])
       do 'ToString' -> procname;
       exit result[]
       #);
     GetTypeCode: proc
       (# result: ^_TypeCode;
       exit result[]
       #);
     ToString_String_IFormatProvider: proc (* ToString *)
       (# result: ^_String;
          arg1: ^_String;
          arg2: ^_IFormatProvider;
       enter (arg1[], arg2[])
       do 'ToString' -> procname;
       exit result[]
       #);
     CompareTo: proc
       (# result: @int32;
          arg1: ^Object;
       enter (arg1[])
       exit result
       #);
     GetHashCode: proc
       (# result: @int32;
       exit result
       #);
     Equals: proc
       (# result: @boolean;
          arg1: ^Object;
       enter (arg1[])
       exit result
       #);
     ToString: proc (* ToString *)
       (# result: ^_String;
       do 'ToString' -> procname;
       exit result[]
       #);
     IsInfinity: static_proc
       (# result: @boolean;
          arg1: @real;
       enter (arg1)
       exit result
       #);
     IsPositiveInfinity: static_proc
       (# result: @boolean;
          arg1: @real;
       enter (arg1)
       exit result
       #);
     IsNegativeInfinity: static_proc
       (# result: @boolean;
          arg1: @real;
       enter (arg1)
       exit result
       #);
     IsNaN: static_proc
       (# result: @boolean;
          arg1: @real;
       enter (arg1)
       exit result
       #);
     ToString_String: proc (* ToString *)
       (# result: ^_String;
          arg1: ^_String;
       enter (arg1[])
       do 'ToString' -> procname;
       exit result[]
       #);
     Parse_String: static_proc (* Parse *)
       (# result: @real;
          arg1: ^_String;
       enter (arg1[])
       do 'Parse' -> procname;
       exit result
       #);
     Parse_String_NumberStyles: static_proc (* Parse *)
       (# result: @real;
          arg1: ^_String;
          arg2: @int32;
       enter (arg1[], arg2)
       do 'Parse' -> procname;
       exit result
       #);
     Parse_String_IFormatProvider: static_proc (* Parse *)
       (# result: @real;
          arg1: ^_String;
          arg2: ^_IFormatProvider;
       enter (arg1[], arg2[])
       do 'Parse' -> procname;
       exit result
       #);
     Parse_String_NumberStyles_IFormatProvider: static_proc (* Parse *)
       (# result: @real;
          arg1: ^_String;
          arg2: @int32;
          arg3: ^_IFormatProvider;
       enter (arg1[], arg2, arg3[])
       do 'Parse' -> procname;
       exit result
       #);
     TryParse: static_proc
       (# result: @boolean;
          arg1: ^_String;
          arg2: @int32;
          arg3: ^_IFormatProvider;
          arg4: @real;
       enter (arg1[], arg2, arg3[], arg4)
       exit result
       #);
  do '@valuetype [mscorlib]System.Double' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asDouble:
  (# _D: ^_Double;
     D: ^Double;
  enter _D[]
  do _D[]->D[];
  exit D[]
  #);
