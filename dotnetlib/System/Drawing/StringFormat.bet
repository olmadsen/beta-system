ORIGIN '_StringFormat';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_MarshalByRefObject';
INCLUDE '~beta/dotnetlib/System/Drawing/_StringFormatFlags';
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/Drawing/_CharacterRange';
INCLUDE '~beta/dotnetlib/System/Drawing/_StringAlignment';
INCLUDE '~beta/dotnetlib/System/Drawing/Text/_HotkeyPrefix';
INCLUDE '~beta/dotnetlib/System/Drawing/_StringTrimming';
INCLUDE '~beta/dotnetlib/System/Drawing/_StringDigitSubstitute';
--LIB: attributes--

(* .NET StringFormat class. *)
StringFormat: _StringFormat
  (#
     (* Public/family CLS compliant constructors *)

     _init: cons (* constructor *)
       (# result: ^StringFormat;
       exit result[]
       #);
     _init_StringFormatFlags: cons (* constructor *)
       (# result: ^StringFormat;
          arg1: ^_StringFormatFlags;
       enter (arg1[])
       exit result[]
       #);
     _init_StringFormatFlags_int32: cons (* constructor *)
       (# result: ^StringFormat;
          arg1: ^_StringFormatFlags;
          arg2: @int32;
       enter (arg1[], arg2)
       exit result[]
       #);
     _init_StringFormat: cons (* constructor *)
       (# result: ^StringFormat;
          arg1: ^StringFormat;
       enter (arg1[])
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     Dispose: proc
       (# 
       #);
     Clone: proc
       (# result: ^Object;
       exit result[]
       #);
     ToString: proc
       (# result: ^_String;
       exit result[]
       #);
     get_FormatFlags: proc
       (# result: ^_StringFormatFlags;
       exit result[]
       #);
     set_FormatFlags: proc
       (# 
          arg1: ^_StringFormatFlags;
       enter (arg1[])
       #);
     SetMeasurableCharacterRanges: proc
       (# 
          arg1: [0]^_CharacterRange;
       enter (arg1[])
       #);
     get_Alignment: proc
       (# result: ^_StringAlignment;
       exit result[]
       #);
     set_Alignment: proc
       (# 
          arg1: ^_StringAlignment;
       enter (arg1[])
       #);
     get_LineAlignment: proc
       (# result: ^_StringAlignment;
       exit result[]
       #);
     set_LineAlignment: proc
       (# 
          arg1: ^_StringAlignment;
       enter (arg1[])
       #);
     get_HotkeyPrefix: proc
       (# result: ^_HotkeyPrefix;
       exit result[]
       #);
     set_HotkeyPrefix: proc
       (# 
          arg1: ^_HotkeyPrefix;
       enter (arg1[])
       #);
     SetTabStops: proc
       (# 
          arg1: @real32;
          arg2: [0]@real32;
       enter (arg1, arg2[])
       #);
     GetTabStops: proc
       (# result: [0]@real32;
          arg1: @real32;
       enter (arg1)
       exit result
       #);
     get_Trimming: proc
       (# result: ^_StringTrimming;
       exit result[]
       #);
     set_Trimming: proc
       (# 
          arg1: ^_StringTrimming;
       enter (arg1[])
       #);
     get_GenericDefault: static_proc
       (# result: ^StringFormat;
       exit result[]
       #);
     get_GenericTypographic: static_proc
       (# result: ^StringFormat;
       exit result[]
       #);
     SetDigitSubstitution: proc
       (# 
          arg1: @int32;
          arg2: ^_StringDigitSubstitute;
       enter (arg1, arg2[])
       #);
     get_DigitSubstitutionMethod: proc
       (# result: ^_StringDigitSubstitute;
       exit result[]
       #);
     get_DigitSubstitutionLanguage: proc
       (# result: @int32;
       exit result
       #);
  do '[System.Drawing]System.Drawing.StringFormat' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asStringFormat:
  (# _S: ^_StringFormat;
     S: ^StringFormat;
  enter _S[]
  do _S[]->S[];
  exit S[]
  #);
