ORIGIN '_DashStyle';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET DashStyle class. *)
DashStyle: _DashStyle
  (#
     (* Public/family CLS compliant fields *)

     value__: @int32;

     (* Enum Literals *)

     Solid:
       (# exit 0 #);
     Dash:
       (# exit 1 #);
     Dot:
       (# exit 2 #);
     DashDot:
       (# exit 3 #);
     DashDotDot:
       (# exit 4 #);
     Custom:
       (# exit 5 #);
  do 'valuetype [System.Drawing]System.Drawing.Drawing2D.DashStyle' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asDashStyle:
  (# _D: ^_DashStyle;
     D: ^DashStyle;
  enter _D[]
  do _D[]->D[];
  exit D[]
  #);
