ORIGIN '_GraphicsState';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_MarshalByRefObject';
--LIB: attributes--

(* .NET GraphicsState class. *)
GraphicsState: _GraphicsState
  (#  do '[System.Drawing]System.Drawing.Drawing2D.GraphicsState' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asGraphicsState:
  (# _G: ^_GraphicsState;
     G: ^GraphicsState;
  enter _G[]
  do _G[]->G[];
  exit G[]
  #);
