ORIGIN '_WarpMode';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET WarpMode class. *)
WarpMode: _WarpMode
  (#
     (* Enum Literals *)

     Perspective:
       (# exit 0 #);
     Bilinear:
       (# exit 1 #);
  do '@valuetype [System.Drawing]System.Drawing.Drawing2D.WarpMode' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asWarpMode:
  (# _W: ^_WarpMode;
     W: ^WarpMode;
  enter _W[]
  do _W[]->W[];
  exit W[]
  #);
