ORIGIN '_PenType';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET PenType class. *)
PenType: _PenType
  (#
     (* Public/family CLS compliant fields *)

     value__: @int32;

     (* Enum Literals *)

     SolidColor:
       (# exit 0 #);
     HatchFill:
       (# exit 1 #);
     TextureFill:
       (# exit 2 #);
     PathGradient:
       (# exit 3 #);
     LinearGradient:
       (# exit 4 #);
  do 'valuetype [System.Drawing]System.Drawing.Drawing2D.PenType' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asPenType:
  (# _P: ^_PenType;
     P: ^PenType;
  enter _P[]
  do _P[]->P[];
  exit P[]
  #);
