ORIGIN '_ColorPalette';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/Drawing/_Color';
--LIB: attributes--

(* .NET ColorPalette class. *)
ColorPalette: _ColorPalette
  (#
     (* Public/family CLS compliant methods *)

     get_Flags: proc
       (# result: @int32;
       exit result
       #);
     get_Entries: proc
       (# result: [0]^_Color;
       exit result
       #);
  do '[System.Drawing]System.Drawing.Imaging.ColorPalette' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asColorPalette:
  (# _C: ^_ColorPalette;
     C: ^ColorPalette;
  enter _C[]
  do _C[]->C[];
  exit C[]
  #);
