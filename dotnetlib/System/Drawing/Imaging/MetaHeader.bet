ORIGIN '_MetaHeader';
ON 19 (* Disable warnings about runtime qualification tests *);
--LIB: attributes--

(* .NET MetaHeader class. *)
MetaHeader: _MetaHeader
  (#
     (* Public/family CLS compliant constructors *)

     _init: cons
       (# result: ^MetaHeader;
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     get_Type: proc
       (# result: @int16;
       exit result
       #);
     set_Type: proc
       (# 
          arg1: @int16;
       enter (arg1)
       #);
     get_HeaderSize: proc
       (# result: @int16;
       exit result
       #);
     set_HeaderSize: proc
       (# 
          arg1: @int16;
       enter (arg1)
       #);
     get_Version: proc
       (# result: @int16;
       exit result
       #);
     set_Version: proc
       (# 
          arg1: @int16;
       enter (arg1)
       #);
     get_Size: proc
       (# result: @int32;
       exit result
       #);
     set_Size: proc
       (# 
          arg1: @int32;
       enter (arg1)
       #);
     get_NoObjects: proc
       (# result: @int16;
       exit result
       #);
     set_NoObjects: proc
       (# 
          arg1: @int16;
       enter (arg1)
       #);
     get_MaxRecord: proc
       (# result: @int32;
       exit result
