INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET GraphicsUnit class. *)
GraphicsUnit: _GraphicsUnit
  (#
     (* Public/family CLS compliant fields *)

     value__: @int32;

     (* Enum Literals *)

     World:
       (# exit 0 #);
     Display:
       (# exit 1 #);
     Pixel:
       (# exit 2 #);
     Point:
       (# exit 3 #);
     Inch:
       (# exit 4 #);
     Document:
       (# exit 5 #);
