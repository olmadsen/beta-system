ORIGIN '_IPrincipal';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/Security/Principal/_IIdentity';
--LIB: attributes--

(* .NET IPrincipal class. *)
IPrincipal: _IPrincipal
  (#
     (* Public/family CLS compliant methods *)

     IsInRole: proc
       (# result: @boolean;
          arg1: ^_String;
       enter (arg1[])
       exit result
       #);
     get_Identity: proc
       (# result: ^_IIdentity;
       exit result[]
       #);
  do '[mscorlib]System.Security.Principal.IPrincipal' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asIPrincipal:
  (# _I: ^_IPrincipal;
     I: ^IPrincipal;
  enter _I[]
  do _I[]->I[];
  exit I[]
  #);
