ORIGIN '_IMembershipCondition';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/Security/Policy/_Evidence';
--LIB: attributes--

(* .NET IMembershipCondition class. *)
IMembershipCondition: _IMembershipCondition
  (#
     (* Public/family CLS compliant methods *)

     Equals: proc
       (# result: @boolean;
          arg1: ^Object;
       enter (arg1[])
       exit result
       #);
     ToString: proc
       (# result: ^_String;
       exit result[]
       #);
     Copy: proc
       (# result: ^IMembershipCondition;
       exit result[]
       #);
     Check: proc
       (# result: @boolean;
          arg1: ^_Evidence;
       enter (arg1[])
       exit result
       #);
  do '[mscorlib]System.Security.Policy.IMembershipCondition' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asIMembershipCondition:
  (# _I: ^_IMembershipCondition;
     I: ^IMembershipCondition;
  enter _I[]
  do _I[]->I[];
  exit I[]
  #);
