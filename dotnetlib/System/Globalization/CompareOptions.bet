ORIGIN '_CompareOptions';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET CompareOptions class. *)
CompareOptions: _CompareOptions
  (#
     (* Enum Literals *)

     DotnetNone:
       (# exit 0 #);
     IgnoreCase:
       (# exit 1 #);
     IgnoreNonSpace:
       (# exit 2 #);
     IgnoreSymbols:
       (# exit 4 #);
     IgnoreKanaType:
       (# exit 8 #);
     IgnoreWidth:
       (# exit 16 #);
     StringSort:
       (# exit 536870912 #);
     Ordinal:
       (# exit 1073741824 #);
  do 'valuetype [mscorlib]System.Globalization.CompareOptions' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asCompareOptions:
  (# _C: ^_CompareOptions;
     C: ^CompareOptions;
  enter _C[]
  do _C[]->C[];
  exit C[]
  #);
