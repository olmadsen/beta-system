          arg5: @boolean;
       enter (arg1[], arg2[], arg3, arg4, arg5)
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     Unlock: proc
       (# 
          arg1: @int64;
          arg2: @int64;
       enter (arg1, arg2)
       #);
     Lock: proc
       (# 
          arg1: @int64;
          arg2: @int64;
       enter (arg1, arg2)
       #);
     get_Handle: proc
       (# result: ^_IntPtr;
       exit result[]
       #);
     get_IsAsync: proc
       (# result: @boolean;
       exit result
       #);
     WriteByte: proc
       (# 
          arg1: @int8u;
       enter (arg1)
       #);
     Write: proc
       (# 
          arg1: [0]@int8u;
          arg2: @int32;
          arg3: @int32;
       enter (arg1[], arg2, arg3)
       #);
     ReadByte: proc
       (# result: @int32;
       exit result
       #);
     Read: proc
       (# result: @int32;
          arg1: [0]@int8u;
