     get_Position: proc
       (# result: @int64;
       exit result
       #);
     get_Length: proc
       (# result: @int64;
       exit result
       #);
     get_CanWrite: proc
       (# result: @boolean;
       exit result
       #);
     get_CanSeek: proc
       (# result: @boolean;
       exit result
       #);
     get_CanRead: proc
       (# result: @boolean;
       exit result
       #);
     get_Name: proc
       (# result: ^_String;
       exit result[]
       #);
  do '[mscorlib]System.IO.FileStream' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asFileStream:
  (# _F: ^_FileStream;
     F: ^FileStream;
  enter _F[]
  do _F[]->F[];
  exit F[]
  #);
