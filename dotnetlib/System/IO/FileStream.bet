          arg3: @int32;
       enter (arg1[], arg2, arg3)
       exit result
       #);
     SetLength: proc
       (# 
          arg1: @int64;
       enter (arg1)
       #);
     Seek: proc
       (# result: @int64;
          arg1: @int64;
          arg2: ^_SeekOrigin;
       enter (arg1, arg2[])
       exit result
       #);
     EndWrite: proc
       (# 
          arg1: ^_IAsyncResult;
       enter (arg1[])
       #);
     BeginWrite: proc
       (# result: ^_IAsyncResult;
          arg1: [0]@int8u;
          arg2: @int32;
          arg3: @int32;
          arg4: ^_AsyncCallback;
          arg5: ^Object;
       enter (arg1[], arg2, arg3, arg4[], arg5[])
       exit result[]
       #);
     EndRead: proc
       (# result: @int32;
          arg1: ^_IAsyncResult;
       enter (arg1[])
       exit result
       #);
     BeginRead: proc
       (# result: ^_IAsyncResult;
          arg1: [0]@int8u;
          arg2: @int32;
          arg3: @int32;
          arg4: ^_AsyncCallback;
          arg5: ^Object;
       enter (arg1[], arg2, arg3, arg4[], arg5[])
