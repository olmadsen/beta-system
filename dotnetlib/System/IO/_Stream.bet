ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/dotnetlib/System/MarshalByRefObject';
--LIB: attributes--

(* Dotnet Stream class declaration.
 * This wrapper is needed to prevent circular fragment INCLUDE.
 * See Stream.bet for members.
 *)
_Stream: MarshalByRefObject
  (#
  do '[mscorlib]System/IO.Stream' -> className;
  INNER;
  #);

