ORIGIN '_FileAccess';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET FileAccess class. *)
FileAccess: _FileAccess
  (#
     (* Enum Literals *)

     Read:
       (# exit 1 #);
     Write:
       (# exit 2 #);
     ReadWrite:
       (# exit 3 #);
  do 'valuetype [mscorlib]System.IO.FileAccess' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asFileAccess:
  (# _F: ^_FileAccess;
     F: ^FileAccess;
  enter _F[]
  do _F[]->F[];
  exit F[]
  #);
