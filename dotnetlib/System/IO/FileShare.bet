ORIGIN '_FileShare';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET FileShare class. *)
FileShare: _FileShare
  (#
     (* Enum Literals *)

     DotnetNone:
       (# exit 0 #);
     Read:
       (# exit 1 #);
     Write:
       (# exit 2 #);
     ReadWrite:
       (# exit 3 #);
     Inheritable:
       (# exit 16 #);
  do 'valuetype [mscorlib]System.IO.FileShare' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asFileShare:
  (# _F: ^_FileShare;
     F: ^FileShare;
  enter _F[]
  do _F[]->F[];
  exit F[]
  #);
