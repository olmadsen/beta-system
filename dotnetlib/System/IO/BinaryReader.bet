ORIGIN '_BinaryReader';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/IO/_Stream';
INCLUDE '~beta/dotnetlib/System/Text/_Encoding';
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/_Decimal';
--LIB: attributes--

(* .NET BinaryReader class. *)
BinaryReader: _BinaryReader
  (#
     (* Public/family CLS compliant constructors *)

     _init_Stream: cons (* constructor *)
       (# result: ^BinaryReader;
          arg1: ^_Stream;
       enter (arg1[])
       exit result[]
       #);
     _init_Stream_Encoding: cons (* constructor *)
       (# result: ^BinaryReader;
          arg1: ^_Stream;
          arg2: ^_Encoding;
       enter (arg1[], arg2[])
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     ReadBytes: proc
       (# result: [0]@int8u;
          arg1: @int32;
       enter (arg1)
       exit result[]
       #);
     Read_ArrayOfInt8u_int32_int32: proc (* Read *)
       (# result: @int32;
          arg1: [0]@int8u;
          arg2: @int32;
          arg3: @int32;
       enter (arg1[], arg2, arg3)
       do 'Read' -> procname;
       exit result
       #);
     ReadChars: proc
       (# result: [0]@char;
          arg1: @int32;
       enter (arg1)
       exit result[]
       #);
     Read_ArrayOfChar_int32_int32: proc (* Read *)
       (# result: @int32;
          arg1: [0]@char;
          arg2: @int32;
          arg3: @int32;
       enter (arg1[], arg2, arg3)
       do 'Read' -> procname;
       exit result
       #);
     ReadString: proc
       (# result: ^_String;
       exit result[]
       #);
     ReadDecimal: proc
       (# result: ^_Decimal;
       exit result[]
       #);
     ReadDouble: proc
       (# result: @real;
       exit result
       #);
     ReadSingle: proc
       (# result: @real32;
       exit result
       #);
     ReadInt64: proc
       (# result: @int64;
       exit result
       #);
     ReadInt32: proc
       (# result: @int32;
       exit result
       #);
     ReadInt16: proc
       (# result: @int16;
       exit result
       #);
     ReadChar: proc
       (# result: @char;
       exit result
       #);
     ReadByte: proc
       (# result: @int8u;
       exit result
       #);
     ReadBoolean: proc
       (# result: @boolean;
       exit result
       #);
     Read: proc (* Read *)
       (# result: @int32;
       do 'Read' -> procname;
       exit result
       #);
     PeekChar: proc
       (# result: @int32;
       exit result
       #);
     Close: proc
       (# 
       #);
     get_BaseStream: proc
       (# result: ^_Stream;
       exit result[]
       #);
  do '[mscorlib]System.IO.BinaryReader' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asBinaryReader:
  (# _B: ^_BinaryReader;
     B: ^BinaryReader;
  enter _B[]
  do _B[]->B[];
  exit B[]
  #);
