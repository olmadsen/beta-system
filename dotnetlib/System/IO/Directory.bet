     Move: static_proc
       (# 
          arg1: ^_String;
          arg2: ^_String;
       enter (arg1[], arg2[])
       #);
     Delete_String: static_proc (* Delete *)
       (# 
          arg1: ^_String;
       enter (arg1[])
