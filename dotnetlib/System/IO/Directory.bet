ORIGIN '_Directory';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/IO/_DirectoryInfo';
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/_DateTime';
--LIB: attributes--

(* .NET Directory class. *)
DotnetDirectory: _Directory
  (#
     (* Public/family CLS compliant methods *)

     GetParent: static_proc
       (# result: ^_DirectoryInfo;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);
     CreateDirectory: static_proc
       (# result: ^_DirectoryInfo;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);
     Exists: static_proc
       (# result: @boolean;
          arg1: ^_String;
       enter (arg1[])
       exit result
       #);
     SetCreationTime: static_proc
       (# 
          arg1: ^_String;
          arg2: ^_DateTime;
       enter (arg1[], arg2[])
       #);
     SetCreationTimeUtc: static_proc
       (# 
          arg1: ^_String;
          arg2: ^_DateTime;
       enter (arg1[], arg2[])
       #);
     GetCreationTime: static_proc
       (# result: ^_DateTime;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);
     GetCreationTimeUtc: static_proc
       (# result: ^_DateTime;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);
     SetLastWriteTime: static_proc
       (# 
