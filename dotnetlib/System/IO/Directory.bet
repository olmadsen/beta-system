  do '[mscorlib]System.IO.Directory' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asDirectory:
  (# _D: ^_Directory;
     D: ^DotnetDirectory;
  enter _D[]
  do _D[]->D[];
  exit D[]
  #);
