       enter (arg1[])
       exit result[]
       #);
     SetLastAccessTime: static_proc
       (# 
          arg1: ^_String;
          arg2: ^_DateTime;
       enter (arg1[], arg2[])
       #);
     SetLastAccessTimeUtc: static_proc
       (# 
          arg1: ^_String;
          arg2: ^_DateTime;
       enter (arg1[], arg2[])
       #);
     GetLastAccessTime: static_proc
       (# result: ^_DateTime;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);
     GetLastAccessTimeUtc: static_proc
       (# result: ^_DateTime;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);
     GetFiles_String: static_proc (* GetFiles *)
       (# result: [0]^_String;
          arg1: ^_String;
       enter (arg1[])
       do 'GetFiles' -> procname;
       exit result[]
       #);
     GetFiles_String_String: static_proc (* GetFiles *)
       (# result: [0]^_String;
          arg1: ^_String;
          arg2: ^_String;
       enter (arg1[], arg2[])
       do 'GetFiles' -> procname;
       exit result[]
       #);
     GetDirectories_String: static_proc (* GetDirectories *)
       (# result: [0]^_String;
          arg1: ^_String;
       enter (arg1[])
       do 'GetDirectories' -> procname;
       exit result[]
       #);
     GetDirectories_String_String: static_proc (* GetDirectories *)
       (# result: [0]^_String;
