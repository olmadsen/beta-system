       enter (arg1[], arg2[])
       #);
     SetLastWriteTimeUtc: static_proc
       (# 
          arg1: ^_String;
          arg2: ^_DateTime;
       enter (arg1[], arg2[])
       #);
     GetLastWriteTime: static_proc
       (# result: ^_DateTime;
