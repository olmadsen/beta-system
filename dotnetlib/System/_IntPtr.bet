ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/dotnetlib/System/ValueType';
--LIB: attributes--

(* Dotnet IntPtr class declaration.
 * This wrapper is needed to prevent circular fragment INCLUDE.
 * See IntPtr.bet for members.
 *)
_IntPtr: ValueType
  (#
  do '@valuetype [mscorlib]System.IntPtr' -> className;
  INNER;
  #);

