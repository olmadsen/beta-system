ORIGIN '_UnhandledExceptionEventArgs';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_EventArgs';
--LIB: attributes--

(* .NET UnhandledExceptionEventArgs class. *)
UnhandledExceptionEventArgs: _UnhandledExceptionEventArgs
  (#
     (* Public/family CLS compliant constructors *)

     _init: cons
       (# result: ^UnhandledExceptionEventArgs;
          arg1: ^Object;
          arg2: @boolean;
       enter (arg1[], arg2)
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     get_ExceptionObject: proc
       (# result: ^Object;
       exit result[]
       #);
     get_IsTerminating: proc
       (# result: @boolean;
       exit result
       #);
  do '[mscorlib]System.UnhandledExceptionEventArgs' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asUnhandledExceptionEventArgs:
  (# _U: ^_UnhandledExceptionEventArgs;
     U: ^UnhandledExceptionEventArgs;
  enter _U[]
  do _U[]->U[];
  exit U[]
  #);
