ORIGIN '_IComparer';
ON 19 (* Disable warnings about runtime qualification tests *);
--LIB: attributes--

(* .NET IComparer class. *)
IComparer: _IComparer
  (#
     (* Public/family CLS compliant methods *)

     Compare: proc
       (# result: @int32;
          arg1: ^Object;
          arg2: ^Object;
       enter (arg1[], arg2[])
       exit result
       #);
  do '[mscorlib]System.Collections.IComparer' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asIComparer:
  (# _I: ^_IComparer;
     I: ^IComparer;
  enter _I[]
  do _I[]->I[];
  exit I[]
  #);
