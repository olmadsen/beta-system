ORIGIN '_Encoder';
ON 19 (* Disable warnings about runtime qualification tests *);
--LIB: attributes--

(* .NET Encoder class. *)
Encoder: _Encoder
  (#
     (* Public/family CLS compliant methods *)

     GetBytes: proc
       (# result: @int32;
          arg1: [0]@char;
          arg2: @int32;
          arg3: @int32;
          arg4: [0]@int8u;
          arg5: @int32;
          arg6: @boolean;
       enter (arg1[], arg2, arg3, arg4[], arg5, arg6)
       exit result
       #);
     GetByteCount: proc
       (# result: @int32;
          arg1: [0]@char;
          arg2: @int32;
          arg3: @int32;
          arg4: @boolean;
       enter (arg1[], arg2, arg3, arg4)
       exit result
       #);
  do '[mscorlib]System.Text.Encoder' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asEncoder:
  (# _E: ^_Encoder;
     E: ^Encoder;
  enter _E[]
  do _E[]->E[];
  exit E[]
  #);
