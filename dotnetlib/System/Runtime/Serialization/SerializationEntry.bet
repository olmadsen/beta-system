ORIGIN '_SerializationEntry';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_ValueType';
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/_Type';
--LIB: attributes--

(* .NET SerializationEntry class. *)
SerializationEntry: _SerializationEntry
  (#
     (* Public/family CLS compliant methods *)

     get_Value: proc
       (# result: ^Object;
       exit result[]
       #);
     get_Name: proc
       (# result: ^_String;
       exit result[]
       #);
     get_ObjectType: proc
       (# result: ^_Type;
       exit result[]
       #);
  do 'valuetype [mscorlib]System.Runtime.Serialization.SerializationEntry' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asSerializationEntry:
  (# _S: ^_SerializationEntry;
     S: ^SerializationEntry;
  enter _S[]
  do _S[]->S[];
  exit S[]
  #);
