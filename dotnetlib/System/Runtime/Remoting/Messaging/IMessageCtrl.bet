ORIGIN '_IMessageCtrl';
ON 19 (* Disable warnings about runtime qualification tests *);
--LIB: attributes--

(* .NET IMessageCtrl class. *)
IMessageCtrl: _IMessageCtrl
  (#
     (* Public/family CLS compliant methods *)

     Cancel: proc
       (# 
          arg1: @int32;
       enter (arg1)
       #);
  do '[mscorlib]System.Runtime.Remoting.Messaging.IMessageCtrl' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asIMessageCtrl:
  (# _I: ^_IMessageCtrl;
     I: ^IMessageCtrl;
  enter _I[]
  do _I[]->I[];
  exit I[]
  #);
