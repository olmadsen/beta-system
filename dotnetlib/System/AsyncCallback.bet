ORIGIN '_AsyncCallback';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_MulticastDelegate';
INCLUDE '~beta/dotnetlib/System/_IntPtr';
INCLUDE '~beta/dotnetlib/System/_IAsyncResult';
--LIB: attributes--

(* .NET AsyncCallback class. *)
AsyncCallback: _AsyncCallback
  (#
     (* Public/family CLS compliant constructors *)

     _init: cons
       (# result: ^AsyncCallback;
          arg1: ^Object;
          arg2: ^_IntPtr;
       enter (arg1[], arg2[])
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     EndInvoke: proc
       (# 
          arg1: ^_IAsyncResult;
       enter (arg1[])
       #);
     BeginInvoke: proc
       (# result: ^_IAsyncResult;
          arg1: ^_IAsyncResult;
          arg2: ^AsyncCallback;
          arg3: ^Object;
       enter (arg1[], arg2[], arg3[])
       exit result[]
       #);
     Invoke: proc
       (# 
          arg1: ^_IAsyncResult;
       enter (arg1[])
       #);
  do '[mscorlib]System.AsyncCallback' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asAsyncCallback:
  (# _A: ^_AsyncCallback;
     A: ^AsyncCallback;
  enter _A[]
  do _A[]->A[];
  exit A[]
  #);
