ORIGIN '_AssemblyLoadEventArgs';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_EventArgs';
INCLUDE '~beta/dotnetlib/System/Reflection/_Assembly';
--LIB: attributes--

(* .NET AssemblyLoadEventArgs class. *)
AssemblyLoadEventArgs: _AssemblyLoadEventArgs
  (#
     (* Public/family CLS compliant constructors *)

     _init: cons
       (# result: ^AssemblyLoadEventArgs;
          arg1: ^_Assembly;
       enter (arg1[])
       exit result[]
       #);

     (* Public/family CLS compliant methods *)

     get_LoadedAssembly: proc
       (# result: ^_Assembly;
       exit result[]
       #);
  do '[mscorlib]System.AssemblyLoadEventArgs' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asAssemblyLoadEventArgs:
  (# _A: ^_AssemblyLoadEventArgs;
     A: ^AssemblyLoadEventArgs;
  enter _A[]
  do _A[]->A[];
  exit A[]
  #);
