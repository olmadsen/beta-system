ORIGIN '_ApartmentState';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_Enum';
--LIB: attributes--

(* .NET ApartmentState class. *)
ApartmentState: _ApartmentState
  (#
     (* Enum Literals *)

     STA:
       (# exit 0 #);
     MTA:
       (# exit 1 #);
     Unknown:
       (# exit 2 #);
  do 'valuetype [mscorlib]System.Threading.ApartmentState' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asApartmentState:
  (# _A: ^_ApartmentState;
     A: ^ApartmentState;
  enter _A[]
  do _A[]->A[];
  exit A[]
  #);
