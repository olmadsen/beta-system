ORIGIN '_ISite';
INCLUDE '~beta/dotnetlib/System/_Void';
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/ComponentModel/_IContainer';
INCLUDE '~beta/dotnetlib/System/ComponentModel/_IComponent';
--LIB: attributes--

(* .NET ISite class. *)
ISite: _ISite
  (#
     (* Public/family CLS compliant methods *)

     set_Name: proc
       (# result: ^_Void;
          arg1: ^_String;
       enter (arg1[])
       exit result[]
       #);
     get_Name: proc
       (# result: ^_String;
       exit result[]
       #);
     get_DesignMode: proc
       (# result: @boolean;
       exit result
       #);
     get_Container: proc
       (# result: ^_IContainer;
       exit result[]
       #);
     get_Component: proc
       (# result: ^_IComponent;
       exit result[]
       #);
  do '[system]System.ComponentModel.ISite' -> className;
  INNER;
  #);

