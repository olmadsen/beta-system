ORIGIN '_ISite';
ON 19 (* Disable warnings about runtime qualification tests *);
INCLUDE '~beta/dotnetlib/System/_String';
INCLUDE '~beta/dotnetlib/System/ComponentModel/_IContainer';
INCLUDE '~beta/dotnetlib/System/ComponentModel/_IComponent';
--LIB: attributes--

(* .NET ISite class. *)
ISite: _ISite
  (#
     (* Public/family CLS compliant methods *)

     set_Name: proc
       (# 
          arg1: ^_String;
       enter (arg1[])
       #);
     get_Name: proc
       (# result: ^_String;
       exit result[]
       #);
     get_DesignMode: proc
       (# result: @boolean;
       exit result
       #);
     get_Container: proc
       (# result: ^_IContainer;
       exit result[]
       #);
     get_Component: proc
       (# result: ^_IComponent;
       exit result[]
       #);
  do '[System]System.ComponentModel.ISite' -> className;
  INNER;
  #);

(* Utility function to ease casting to fully specified class. *)
asISite:
  (# _I: ^_ISite;
     I: ^ISite;
  enter _I[]
  do _I[]->I[];
  exit I[]
  #);
