ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/dotnetlib/System/MarshalByRefObject';
--LIB: attributes--

(* Dotnet Component class declaration.
 * This wrapper is needed to prevent circular fragment INCLUDE.
 * See Component.bet for members.
 *)
_Component: MarshalByRefObject
  (#
  do '[system]System/ComponentModel.Component' -> className;
  INNER;
  #);

