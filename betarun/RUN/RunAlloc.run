DoAlloS: public
  (#
     stack: StackArea = SP;
  do
     { origin to pReg1 }
     stack[4] -> RegCall;
     RegCall -> pReg1;
     { proto to RegCall }
     stack[8] -> RegCall;
     call AlloS;
     { result in RegCall }
     RegCall -> RegEax;
     Return;
  #);
