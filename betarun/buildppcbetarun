
set echo 1

Begin 
	newfolder :macobjects_v: || ;
	newfolder :macobjects_v:C: || ;
	newfolder :macobjects_v:GC: || ;
	newfolder :macobjects_v:NEWRUN: || ;
	newfolder :macobjects_v:P: || ;
End ³ dev:null  || ;


make ppcbetarun_v -d ObjDir=:macobjects_v: -d Defines="-D RTVALHALLA" -f Makefile-ppcmac > ppcbetarun_v.makeout
ppcbetarun_v.makeout


exit 0

Begin 
	newfolder :macobjects_debug: || ;
	newfolder :macobjects_debug:C: || ;
	newfolder :macobjects_debug:GC: || ;
	newfolder :macobjects_debug:NEWRUN: || ;
	newfolder :macobjects_debug:P: || ;
End ³ dev:null || ;

make ppcdebug -d ObjDir=:macobjects_debug: -d Sym="-sym on" -d Defines="-D RTDEBUG" -f Makefile-ppcmac > ppcdebug.makeout
ppcdebug.makeout


Begin 
	newfolder :macobjects: || ;
	newfolder :macobjects:C: || ;
	newfolder :macobjects:GC: || ;
	newfolder :macobjects:NEWRUN: || ;
	newfolder :macobjects:P: || ;
End ³ dev:null  || ;

make ppcbetarun -d ObjDir=:macobjects: -d Defines="" -f Makefile-ppcmac > ppcbetarun.makeout
ppcbetarun.makeout

set echo 0
