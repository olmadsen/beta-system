ORIGIN '../GENERATOR/asmfile'
[[
---- SingleOperatorLib:attributes ----
  putInstr:
  (# instrName: @text
  enter instrName
  do instrName[] -> putText; '.' -> put;
     (if op.size//1 then 'b'->put //2 then 'w'->put //4 then 'l'->put if);
     ascii.ht -> put
  #)
---- SingleOperatorBeforeInner:descriptor ----
  (# do ascii.ht -> put #)
---- SingleOperatorAfterInner:descriptor ----
  (# do newLine #)
----]]
