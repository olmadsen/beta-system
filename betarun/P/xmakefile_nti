ORD_SRC  = domain.c misc.c objectSet.c objectTable.c PException.c PExport.c PImport.c profile.c proto.c PSfile.c PStore.c PStoreServer.c referenceTable.c sequenceTable.c transitObjectTable.c trie.c unswizzle.c specialObjectsTable.c
INC_SRC  = ..\C\macro.h ..\C\beta.h ..\C\define.h ..\C\constant.h ..\C\heap.h ..\C\object.h ..\C\function.h domain.h misc.h objectSet.h objectTable.h PException.h PExport.h PImport.h proto.h PSfile.h PStore.h PStoreServer.h referenceTable.h sequenceTable.h specialObjectsTable.h transitObjectTable.h trie.h unswizzle.h
RST_SRC  = 
ORD_OBJ  = domain.obj misc.obj objectSet.obj objectTable.obj PException.obj PExport.obj PImport.obj profile.obj proto.obj PSfile.obj PStore.obj PStoreServer.obj referenceTable.obj sequenceTable.obj transitObjectTable.obj trie.obj unswizzle.obj specialObjectsTable.obj
OBJFILES = $(ORD_OBJ)
INCLUDES = $(INC_SRC) ..\C\data.h
CC_OPT  = -I..\C $(C_OPT) $(C_FLAGS)
all: $(OBJFILES)

$(ORD_OBJ): $(INCLUDES) $(RST_SRC)
	$(CC) -c $(CC_OPT) $*.c
clean: 
	-rm -f $(OBJFILES) *~



