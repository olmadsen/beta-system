# Unlike the other xmakefiles this one doesn't seem to be generated
# automatically :-(          -EC

ORD_SRC  = cache.c des.c pit.c storageblock.c storagemanager.c domain.c misc.c objectTable.c PException.c PExport.c PImport.c proto.c PSfile.c referenceTable.c sequenceTable.c transitObjectTable.c trie.c unswizzle.c specialObjectsTable.c crossStoreReferences.c

INC_SRC  = ..\C\macro.h ..\C\beta.h ..\C\define.h ..\C\constant.h ..\C\heap.h ..\C\object.h ..\C\function.h error.h storagemanager.h misc.h objectTable.h PException.h PExport.h PImport.h proto.h PSfile.h referenceTable.h sequenceTable.h specialObjectsTable.h transitObjectTable.h trie.h unswizzle.h

RST_SRC  = 

ORD_OBJ  = cache.obj des.obj pit.obj storageblock.obj storagemanager.obj domain.obj misc.obj objectTable.obj PException.obj PExport.obj PImport.obj proto.obj PSfile.obj referenceTable.obj sequenceTable.obj transitObjectTable.obj trie.obj unswizzle.obj specialObjectsTable.obj crossStoreReferences.obj
OBJFILES = $(ORD_OBJ)
INCLUDES = $(INC_SRC) ..\C\data.h
CC_OPT  = -I..\C -I..\P $(C_OPT) $(C_FLAGS)
all: $(OBJFILES)

$(ORD_OBJ): $(INCLUDES) $(RST_SRC)
	$(CC) -c $(CC_OPT) $*.c
clean: 
	-rm -f $(OBJFILES) *~



