ORIGIN '~beta/basiclib/filerep';
--PROGRAM: descriptor--
(# FR, RR: @filerep;
do 4 -> FR.top -> FR.R.new ;
   4369  -> FR.R[1]; (* 0x1111 *)
   8738  -> FR.R[2]; (* 0x2222 *)
   13107 -> FR.R[3]; (* 0x3333 *)
   17476 -> FR.R[4]; (* 0x4444 *)
   'rep' -> FR.save; (* See content of 'rep' with unix command 'od -t x4 rep' *)
   'rep' -> RR.restore;
   (for i:RR.R.range repeat
        RR.R[i] -> putint;
        newline;
   for)
#)
