ORIGIN 'stack'
--- private: descriptor ---
(# A: [100] @Integer;
   Top: @integer
#)

--- PushBody: Descriptor ---
(# 
do private.top+1 -> private.top;
   e -> private.A[private.top]
#)

--- PopBody: descriptor ---
(#
do private.A[private.top] -> e;
   private.top-1 -> private.top
#)

--- isEmptyBody: descriptor ---
(# 
do (0 = private.Top) -> result 
#)

--- newBody: descriptor ---
(#
do 0->private.top
#)

