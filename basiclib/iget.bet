--LIB: attributes--
iget:
  (* Pattern to get a single character from the keyboard immediately
   * without having to type <RETURN> after the character.
   *)
  (# ch: @char;
  <<SLOT KeyBoardIGet: dopart>>
  exit ch
  #);
