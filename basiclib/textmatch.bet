ORIGIN 'betaenv'
---textlib:attributes---
startsWith: booleanValue
  (# S: ^text
  enter S[]
  do matchHead:
       (if S.length <= length then
           (for i: S.length repeat
                (if S.T[i] <> T[i] then
                    leave matchHead
           if)for);
           true -> value
        if)
  #);
endsWith: booleanValue
  (# S: ^text; tailStart: @integer
  enter S[]
  do matchTail:
       (if S.length <= length then
           length - S.length -> tailStart;
           (for i: S.length repeat
                (if S.T[i] <> T[tailStart+i] then
                    leave matchTail
           if)for);
           true -> value
       if)
  #);

