ORIGIN '~beta/basiclib/betaenv';
INCLUDE '~beta/basiclib/private/systemExceptionHandler';

-- program: descriptor --
(# 
   a: @|(# do 1->putint #);
do runtimeExceptionHandler##->InstallSystemExceptionHandler;
   try
   (#
      name:: (# do 'compTerminatedTry'->n[] #);
      handler::
        (# 
        do when (# type:: compTerminatedException
                do 'compTerminatedTry handler...aborting'->putline;
                   abort;
                #);
        #);
   do a; 
      a; (* CompTerminatedErr: Executing terminated component *)  
   #);
#)
