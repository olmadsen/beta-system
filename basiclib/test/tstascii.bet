ORIGIN '../betaenv'
---program:descriptor--
(# emit: 
     (# ch: @ char 
     enter ch
     do (if (ch<= ' ') or (126<=ch) // true then ' '->put else ch-> put if)
     #);
   emitB:
     (# boo: @ boolean
     enter boo
     do (if boo
	 // true then 'T'->put
	 // false then 'F'->put
	 else '?'->put
     if)#);
   sep: (# do '     '->puttext #)
do 'Cha r UpC   LowC  isUp  IsLo  IsLet IsDig '->putline;
   (for ch: 255 repeat 
	ch->emit; sep;
	ch->ascii.UpCase->emit; sep;
	ch->ascii.LowCase->emit; sep;
	ch->ascii.isUpper->emitB; sep;
	ch->ascii.isLower->emitB; sep;
	ch->ascii.isLetter->emitB; sep;
	ch->ascii.isDigit->emitB; sep;
	newline
   for)
#)