origin '../betaenv'

--program: descriptor--
(# t: @text;
do 
   '0 Empty 0 - 17 RenameDecl 16 VariablePatternDecl 15 BindingDecl 14 VirtualDecl 13 RepetitionDecl 12 SimpleDecl 11 PatternDecl'-> t;
   t.reset;
   'length='->puttext; t.lgth -> putint; newline;
   'pos='->puttext; t.pos -> putint; newline;
   loop:
     (if 1//1 then
         (t.getint,' ',t.getatom) -> (putint,put,putline);
         'pos='->puttext; t.pos -> putint; newline;
         (if t.pos<t.length//true then restart loop if);
     if);
#)
