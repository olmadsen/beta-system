ORIGIN '~beta/basiclib/v1.6/directory';
(* Tests repeated scanning of directories.
 * This involves callbacks, which will eat memory when not freed properly
 *)
---program:descriptor---
(# 
   BigOne:
     (# r: [10000]^IntegerObject;
        dir: @Directory;
     do '.'->dir.name;
        dir.scanEntries
        (# do INNER BigOne #);
     #);
do cycle
   (# 
   do &BigOne(# do '*'->put #)
   #);
#)
       
