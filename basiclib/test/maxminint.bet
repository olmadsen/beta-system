ORIGIN '../betaenv'
---program:descriptor--
  (# sum,b,c: @integer
  do
     (for i: 32 repeat
	  (if i//1 then 1->B; -1->C else B*2->B; C*2->C if);
	  sum+B->sum;
	  i-1->putint; ' '->put; sum->putint; 
	  '   '->puttext; B->putint; '   '->puttext; C->putint; newline
     for);
     ' '->put; maxInt->putint; newline;
     minInt->putint; newline;
     minInt->b;
     (if b//minInt then minint+1->putint; '-1'->puttext if); newline
  #)
