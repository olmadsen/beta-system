ORIGIN '../formatio';
INCLUDE '../numberio'

--program:descriptor--
(# 
do 
   '  12345678901234567890'->putline;
   '1:%x\n'->putformat(# 
                      do 0x123->x;
                      #);
   '2:%x\n'->putformat(# 
                      do 0x87654321->x;
                      #);
   '3:%x\n'->putformat(# 
                      do 0x8432AD->x;
                      #);
   '4:%X\n'->putformat(# 
                      do 0x8432AD->uX;
                      #);
   '5:%013X..\n'->putformat(#   (* zero padding *)
                           do 0x8432AD->uX;
                           #);
   '6:%13X\n'->putformat(#    
                        do 0x32AD->uX;
                        #);
   '7:%o\n'->putformat(# 
                      do 0xffffffff->o;
                      #);
   '8:%5o\n'->putformat(# 
                       do 8x1234567->o;
                       #);
   '9:%012o\n'->putformat(# 
                         do 8x1234567->o;
                         #);
   'A:%y\n'->putformat(# 
                      do 2x10101->y;
                      #);
   'a:%10y\n'->putformat(# 
                        do 2x10101->y;
                        #);
   
   'B:%.16r\n'->putformat(# 
                         do 0x80123456->r;
                         #);
   'C:%.16r\n'->putformat(# 
                         do 0x123->r;
                         #);
   
   
   
   
   newline;
#)
