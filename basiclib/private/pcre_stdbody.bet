-- PcreCompile:dopart--
do (@@exp.T[1],
   options,
   @@private.errtext,
   @@private.erroffset,
   0) 
     -> pcre_compile 
     -> private.compiled_regexp;
   
-- PcreLocaleCompile:dopart--
do (@@exp.T[1],
   options,
   @@private.errtext,
   @@private.erroffset) 
     -> locale_pcre_compile 
     -> private.compiled_regexp;
   
-- PcreStudy:dopart--
do (private.compiled_regexp, options, @@private.errtext) 
     -> pcre_study 
     -> private.extra;
   
-- PcreFullInfo:dopart--
do (private.compiled_regexp, private.extra, pcre_INFO_CAPTURECOUNT, @@subpatterns) 
     -> pcre_fullinfo;    
   
-- PcreExec: dopart--
do (private.compiled_regexp, 
   private.extra, 
   @@subject.T[1], 
   subject.lgth,
   position, 
   options, 
   @@private.oVector[1], 
   private.oVector.range) 
     -> pcre_exec 
     -> status;
   
--PcreError: dopart--
do (# error: ^CString;
   do &CString[] -> error[];
      private.errtext -> error;
      (regexp[],error.get) -> compilation_error;
   #);
