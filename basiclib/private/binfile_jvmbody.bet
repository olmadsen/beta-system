ORIGIN 'binfilebody';
INCLUDE '~beta/basiclib/private/file_jvmbody';
INCLUDE '~beta/basiclib/filerep';
-- BinFileputdouble: dopart --
do i -> private.raf.writeDouble;
-- BinFileputlong: dopart --
do i -> private.raf.writeLong;
-- BinFileputshort: dopart --
do i -> private.raf.writeShort;
-- BinFileputbyte: dopart --
do i -> private.raf.writeChar;
-- BinFileGetDouble: dopart --
do private.raf.readDouble -> i;
-- BinFileGetLong: dopart --
do private.raf.readLong -> i;
-- BinFileGetShort: dopart --
do private.raf.readShort -> i;
-- BinFileGetByte: dopart --
do private.raf.readChar -> i;
   
-- BinFilePutCharRep: dopart --
do 'BinFilePutCharRep' -> NYI;
-- BinFileGetCharRep: dopart --
do 'BinFileGetCharRep' -> NYI;
-- BinFilePutShortRep: dopart --
do 'BinFilePutShortRep' -> NYI;
-- BinFileGetShortRep: dopart --
do 'BinFileGetShortRep' -> NYI;
-- BinFilePutLongRep: dopart --
do 'BinFilePutLongRep' -> NYI;
-- BinFileGetLongRep: dopart --
do (# x: @integer;
   do (if num>0 then
          num -> R.new;
          (for i:num repeat
               0 -> x;
               ((private.raf.readByte %band 0xff) %sll 24) %Bor x -> x;
               ((private.raf.readByte %band 0xff) %sll 16) %Bor x -> x;
               ((private.raf.readByte %band 0xff) %sll  8) %Bor x -> x;
               ((private.raf.readByte %band 0xff) %sll  0) %Bor x -> x;
               x->R[i];
          for);
      if)
   #)
   
-- BinFilePutBytes: dopart--
do 'BinFilePutBytes' -> NYI; 
-- BinFileGetBytes: dopart--
do 'BinFileGetBytes' -> NYI;
   
