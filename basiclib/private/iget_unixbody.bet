ORIGIN 'igetbody';
BUILD default '$$/cbreak.o' 
'external/cbreak/my_getch.c' 
'external/cbreak/my_gets.c'
'external/cbreak/mystrings.c' 
'external/cbreak/sysdefs.h' 
'$CC -c -o $$/mygetch.o $1; $CC -c -o $$/mygets.o $2; $CC -c -o $$/mystrings.o $3; ld -r -o $0 $$/mygetch.o $$/mygets.o $$/mystrings.o';

