ORIGIN 'binfile_stdbody';
INCLUDE '~beta/basiclib/private/file_ntbody';
-- BinFileputdouble: dopart --
do (if ((@@i,1,8,private.ntStream) -> fwrite) <> 8 then
       WriteError;
   if)
-- BinFileputlong: dopart --
do (if ((@@i,1,4,private.ntStream) -> fwrite) <> 4 then
       WriteError;
   if)
-- BinFileputshort: dopart --
do (if ((@@i,1,2,private.ntStream) -> fwrite) <> 2 then
       WriteError;
   if)
-- BinFileputbyte: dopart --
do (if ((@@i,1,1,private.ntStream) -> fwrite) <> 1 then
       WriteError;
   if)
-- BinFileGetDouble: dopart --
do (if ((@@i, 1, 8, private.ntStream) -> fRead) <> 8 then
       ReadError;
   if);
-- BinFileGetLong: dopart --
do (if ((@@i, 1, 4, private.ntStream) -> fRead) <> 4 then
       ReadError;
   if);
-- BinFileGetShort: dopart --
do (if ((@@i, 1, 2, private.ntStream) -> fRead) <> 2 then
       ReadError;
   if);
-- BinFileGetByte: dopart --
do (if ((@@i, 1, 1, private.ntStream) -> fRead) <> 1 then
       ReadError;
   if);
   
-- BinFilePutCharRep: dopart --
do (if R.range>0 then
       (if ((@@R[1], 1, num, private.ntStream) -> fwrite) <> num then
           WriteError;
       if)  
   if)
   
-- BinFileGetCharRep: dopart --
do (if num>0 then
       num -> R.new;
       (if ((@@R[1], 1, num, private.ntStream) -> fread) <> num then
           ReadError;
       if)
   if)
   
-- BinFilePutShortRep: dopart --
do (if R.range>0 then
       (if ((@@R[1], 2, num, private.ntStream) -> fwrite) <> num then
           WriteError;
       if)  
   if)
   
-- BinFileGetShortRep: dopart --
do (if num>0 then
       num -> R.new;
       (if ((@@R[1], 2, num, private.ntStream) -> fread) <> num then
           ReadError;
       if)
   if)
   
-- BinFilePutLongRep: dopart --
do (if R.range>0 then
       (if ((@@R[1], 4, num, private.ntStream) -> fwrite) <> num then
           WriteError;
       if)  
   if)
 
   
-- BinFileGetLongRep: dopart --
do (if num>0 then
       num -> R.new;
       (if ((@@R[1], 4, num, private.ntStream) -> fread) <> num then
           ReadError;
       if)
   if)
   
-- BinFilePutBytes: dopart--
do (if ((addr, 1, num, private.ntStream) -> fwrite) <> num then
       WriteError;
   if)   
-- BinFileGetBytes: dopart--
do (if ((addr, 1, num, private.ntStream) -> fread) <> num then
       ReadError;
   if)
   
