ORIGIN '../binfile';

MDBODY ppcmac  'binfile_macbody'
       nti     'binfile_ntibody'
       default 'binfile_unixbody';

--LIB:attributes--

fwrite: External
(# addr, size, n, stream: @integer; (* n is no of elems. to write. *)
   n1: @integer; (* The number of elements successfully written. *)
enter (addr,size,n,stream)
do CallC
exit n1
#);
			   
fread: External
(# addr, size, n, stream: @integer; (* n is no of elems. to read. *)
   n1: @integer; (* The number of elements successfully read. *)
enter (addr,size,n,stream)
do CallC   
exit n1
#);
