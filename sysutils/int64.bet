ORIGIN '~beta/basiclib/betaenv';    
BUILD  ppcmac	':private:$$:cint64.obj' 'private/cint64.c'  'mrc -w 35 -o $0 $1'
       nti      'private/$$/cint64.obj'  'private/cint64.c'  'betacc $0 $1'
       default	'private/$$/cint64.o'    'private/cint64.c'  '\$CC -c -o $0 $1'
---lib:attributes---
(* The int64 library supports arithmetic and relational
 * operators on 64 bit integers using basic pattern int64
 *)

c64: (* Construct an int64 value from 2 int32 values 
      * (x << 32) + y -> z
      *)
  external(# x,y: @int32; z: @int64 enter(x,y) exit z #);

(******************  arithmetic operations *********************)
a64: (* x + y -> z *) external(# x,y,z: @int64 enter(x,y) exit z #);
s64: (* x - y -> z *) external(# x,y,z: @int64 enter(x,y) exit z #);
m64: (* x * y -> z *) external(# x,y,z: @int64 enter(x,y) exit z #);
d64: (* x div y -> z *) external(# x,y,z: @int64 enter(x,y) exit z #);
r64: (* x mod y -> z *) external(# x,y,z: @int64 enter(x,y) exit z #);

(******************* relational operations ****************)
eq64: (* x = y -> z *) 
  external(# x,y: @int64; b: @boolean enter(x,y) exit b #);
ne64: (* x <> y -> z *) 
  external(# x,y: @int64; b: @boolean enter(x,y) exit b #);
lt64: (* x < y -> z *) 
  external(# x,y: @int64; b: @boolean enter(x,y) exit b #);
le64: (* x <= y -> z *) 
  external(# x,y: @int64; b: @boolean enter(x,y) exit b #);
gt64: (* x > y -> z *) 
  external(# x,y: @int64; b: @boolean enter(x,y) exit b #);
ge64: (* x >= y -> z *) 
  external(# x,y: @int64; b: @boolean enter(x,y) exit b #);

(******************* print operations *********************)
putint64: external(# x: @int64 enter x #);
puthexint64: external(# x: @int64 enter x #);
