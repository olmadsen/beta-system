ORIGIN '../RepetitionObject';
-- RepObjcheapPutLongDP:dopart --
do l.%byteSwapLong;
   l -> r[r[2]];
   r[2]+1 -> r[2]

-- RepObjGetLongDP:dopart --
do r[r[2]] -> l;
   l.%byteSwapLong;
   r[2] + 1 -> r[2]

-- RepObjPutLongDP:dopart --
do r[r[2]].%byteSwapLong;
   (if (r[2] + 1 -> r[2]) > r.range //true then r.range -> r.extend if);
   (if r[2] > r[1] + 1 //true then r[2] - 1 -> r[1] if);


