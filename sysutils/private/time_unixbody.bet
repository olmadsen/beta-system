ORIGIN '../time';
MDBODY sun4s 'time_sun4sbody'
       default 'time_bsdbody';
INCLUDE '~beta/unixlib/v1.5/unixinterface';

-- systemTimeDoPart: dopart --
do 0 -> time -> t; INNER;
   
-- cpuTimeDoPart: dopart --
do clock -> t; INNER;

-- formatTimeDoPart: dopart--
do &text[] -> readable[]; 
   @@t -> ctime -> readable;
   (* delete newline *)
   (readable.length,readable.length) -> readable.delete;
   INNER;
