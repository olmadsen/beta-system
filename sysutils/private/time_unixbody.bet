ORIGIN '../time';
LIB_ITEM 'sysutilstime';
INCLUDE '~beta/unixlib/unixinterface';
MDBODY sun4s   'time_sys5body'
       sgi     'time_sys5body'
       x86sol  'time_sys5body'
       default 'time_bsdbody';

-- LIB: attributes --
free: external
  (# adr: @integer
  enter adr
  #);

-- systemTimeDoPart: dopart --
do 0 -> unixtime -> t; INNER;
   
-- cpuTimeDoPart: dopart --
do clock -> t; INNER;

-- formatTimeDoPart: dopart--
do &text[] -> readable[]; 
   @@t -> ctime -> readable;
   (* delete newline *)
   (readable.length,readable.length) -> readable.delete;
   INNER;
