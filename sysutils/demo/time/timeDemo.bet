ORIGIN '~beta/sysutils/v1.6/time';
(* A small demo showing the use of the time library *)
---program: descriptor--
(# t1, t2, m: @integer
do 'CPU time:' -> putline;
   cpuTime->putint; 
   
   '\n\n(Precise time, milli):' -> putline;
   preciseTime->(t1,m); systemTime->t2;
   '('->put; t1->putint; ','->put; m->putint; ') '->putline; 
   'System time:' -> putline;
   t2->putint; 
   
   '\n\nCPU time:' -> putline;
   cpuTime->putint; 
   
   '\n\n(Precise time, milli):' -> putline;
   preciseTime->(t1,m); systemTime->t2;
   '('->put; t1->putint; ','->put; m->putint; ') '->putline; 
   'System time:' -> putline;
   t2->putint; 
   
   '\n\nCPU time:' -> putline;
   cpuTime->putint; newline;
#)

