origin '~beta/basiclib/betaenv';
INCLUDE '../miniCompiler/generator'
---lib:attributes---
content:
 (#
    getx:
      (# inx,jnx: @integer; L1,L2: @text;
         L: [2] ^text
      enter (inx,jnx)
      do 'Lisa:' -> L1;
         inx -> L1.putint; ':' -> L1.put; jnx -> L1.putint;
         'Larsen:'->L2;
         inx -> L2.putint; ':' -> L2.put; jnx -> L2.putint;
         L1[] -> L[1][];
         L2[] -> L[2][];
         (if true
           // (inx = 2) and (jnx = 1) then 
             1 -> L.extend;
             'Junior' -> L[3][];
          // (inx = 3) and (jnx = 2) then 
             1 -> L.extend;
             'Senior' -> L[3][];
         if)
      exit L
      #);
   get:<
     (# inx,jnx: @integer; L: [0] ^text;
        add:
          (# T: ^text
          enter T[]
          do 1 -> L.extend;
             T[] -> L[L.range][]
          #)
     enter(inx,jnx)
     do inner
     exit L
    #);

 #);




          
