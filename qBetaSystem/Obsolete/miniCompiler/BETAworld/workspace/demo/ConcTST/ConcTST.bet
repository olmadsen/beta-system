ConcTST: @
  (# S: @
       (# fox: (# ch: @char enter ch do ch -> put #);
       do (for i: 100 repeat
               '0' + (i mod 10) -> fox;
               (if i = 33 then
                   disable;
                   'Entered critical region > '.print
               if);
               (if i = 50 then
                   ' < Leaving critical region'.print; newline;
                   enable
               if)
          for)
       #);
     R: @
       (# foo: 
            (# V: @integer 
            enter V 
            do 'a' + ((V - 1) mod 26) -> put;
               (if V < 4 then V + 1 -> foo if)
            #);
          
       do (for i: 100 repeat 
               i -> foo
          for)
       #);
     
     ch: @char
       
  do 
     (for i: 100 repeat
          'A' + ((i - 1) mod 26) -> put; 
          newline;
          130 -> S.attach;
          newline;
          100 -> R.attach;
          newline;
     for)
  #)
