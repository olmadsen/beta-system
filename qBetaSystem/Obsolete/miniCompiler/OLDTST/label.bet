label: @
  (# integer:  (# % simple, 32 % #);
     put: (# % prim % #);
     object:
       (# % simple %
          suspend: (# % prim % #);
       #);     
     a: @integer
  do 97 -> a;
     a -> put;
     L:
       (if a = 100 then
           leave L
        else
           a + 1 -> a;
           a -> put;
           restart L
       if);
     L: (for i: 10 repeat 
             a + 1 -> a -> put;
             (if a = 5 then leave L if)
        for);
     
  #)
     
