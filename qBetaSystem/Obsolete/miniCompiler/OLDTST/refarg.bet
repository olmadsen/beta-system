refArg: @
  (# integer:  (# % simple, 32 % #);
     put: (# % prim % #);
     object:
       (# % simple %
          suspend: (# % prim % #);
       #);     
     
     pap: (# go: (# do ch -> put; ch + 2 -> ch; 'b' -> put; #);
             gogo: (# do ch -> put; 'd' -> put #);
          #);
     flikker:
       (# s,r: ^pap
       enter(s[],r[])          
       do s.go;
          r.gogo
       #);
     ch: @integer;
     q,w: ^pap
  do 'a' -> ch;
     &pap[] -> q[];
     &pap[] -> w[];
     (q[],w[]) -> flikker
  #)

     
     
