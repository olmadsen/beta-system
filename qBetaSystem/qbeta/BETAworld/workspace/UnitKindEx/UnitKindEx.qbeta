UnitKindEx: obj
   Dimension:
      Unit: Value
         %unit "U",1      
         m: var Float
   Length: Dimension
      Meter: Unit
         %unit "m", 1
   Area: obj Dimension
      SquareMeter: Unit
         %unit "m2", "m*m"
         ...
   Width: obj Length
         ...
   WaveLength: obj Length
         ...
   l1: var Width.Meter
   l2: var Width.Meter
   l3: var WaveLength.Meter
   A: var Area.SquareMeter
   l1 := l2
   l1 := 3 m
   l2 := l3
   A := l1 * l3
   
   