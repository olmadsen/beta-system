miniq: obj
   for(for:  var integer):to(to: var integer):repeat{repeat:< Object}:
      %globals inSub 
      %public      
      inx: var integer
      %private
      doIt: 
         loop: obj
            if (inx <= to) :then 
               repeat
               inx := inx + 1
               restart(loop)
      inx := for
      outint(for)
      put(10)
      outint(to)
      put(10)
      doIt
      inner(for)
   for (1) :to (10) :repeat
         put('a' + inx - 1)
   put(10)