ORIGIN '~beta/basiclib/betaenv';
---program:descriptor---
(#
   top:
     (#
     do 'a'->put;
        inner top;
        'd'->put
     #);
   Second: Top
     (#
     do 'b' -> put;
        (if true then
            'c'->put;
            leave second
        if);
        'x'->put;
     #);
   i: @integer 
do Second;
   LLL: cycle
     (# n: @integer
     do (if (n + 1 -> n) > 3 then
            leave LLL
         else
            'd' + n -> put
        if)
     #);
   XXX: Top
     (#
     do 'h' -> put;
        (if true then
            'i'->put;
            leave XXX
        if);
        'y'->put
     #);
    
   LccLab: 
     (# 
     do 'y' -> put;
        (if (i + 1 -> i) > 2 then 
            '!'-> put; 
            leave LccLab; 
            '+'->put
         else
            'y' + i -> put;
            restart LccLab
        if);
        '?'->put;
     #);
#)
