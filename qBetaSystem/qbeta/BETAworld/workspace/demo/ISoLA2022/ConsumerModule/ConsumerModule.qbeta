ConsumerModule: obj
   %requires BufferModule
   %visible BufferModule
   %public
   Consumer: MonitorProcess
      in buf: ref Buffer
      V: var integer
      console.display
         "Consumer:start\n".print
      for (1) :to (10):repeat
          V := buf.gett
          console.display
             "Consumer: ".print	  
             putint(V)
             newline	     
      --console.display
      --    "Consumer-end\n".print
   "ConsumerModule:\n".print
