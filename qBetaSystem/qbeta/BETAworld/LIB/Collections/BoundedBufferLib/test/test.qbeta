test: obj
      Record:
         %globals
         in V: var integer
         print:
            ("record: " + I2S(V) + "\n").print	 
      BB: obj BoundedBuffer(#record,10)
      for (1):to(15):repeat
         "inx: ".print
         putint(inx)	 
         " first: ".print
         putint(BB.first)
         " last: ".print
         putint(BB.last)
         " size: ".print
         putint(BB.size)
         newline      
         if (BB.full.not):then
             BB.insert(Record(inx))
         :else
            "Buffer is full: ".print
            "first: ".print
            putint(BB.first)
            " last: ".print
            putint(BB.last)
            newline
            R: ref record
            R := BB.next
            R.print
      for(1):to(5):repeat
          R: ref record
          R := BB.next
          R.print
