RadioNuclide: obj Dimension
   %requires Time, ElectricFrequency
   %dimension "R", "T^-1"
   BaseUnit:: Becquerel
   Becquerel: Unit
      %unit "Bq", "s^-1"
      print::
         "Becquerel".print
   test:
      t: var Time.Second
      X: var Becquerel
      X2: var Becquerel
      Y: var ElectricFrequency.Hertz
      t := 12 s
      X := 1.0 / t
      X.print
      newline
      Y := 1.0 / t
      Y.print
      X := Y
      X := X2
   test      
