ORIGIN '~beta/guienv/guienvsystemenv';
---lib:attributes---
nameOf: external
  (# obj: @integer; T: [1]@char
  enter obj
  do callC
  exit T
  #);
descNoOf: external
  (# obj,descNo: @integer
  enter obj
  do callC
  exit descNo
  #);
myCorigin: external
  (# obj,org: @integer
  enter obj
  do callC
  exit org
  #);
getIsObj: external
  (# obj: @integer; isObj: @boolean
  enter obj
  do callC
  exit isObj
  #);
getV: external
  (# obj,inx,V: @integer
  enter(obj,inx)
  do callC
  exit V
  #);
getR: external
  (# obj,inx,R: @integer
  enter(obj,inx)
  do callC
  exit R
  #);
cMyLscTop: external
  (# obj,top: @integer
  enter obj
  do callC
  exit top
  #);
topOfLsc: external
  (# obj,inx,top: @integer;
  enter(obj,inx)
  do callC
  exit top
  #);
cSaveReturn: external
  (# obj,descNo,lsc: @integer
  enter(obj,descNo,lsc)
  do callC
  #);
cRestoreReturn: external
  (# obj,V: @integer
  enter obj
  do callC
  exit V
  #);
