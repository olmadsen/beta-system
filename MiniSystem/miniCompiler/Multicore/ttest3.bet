origin '~beta/basiclib/systemenv';
---program:descriptor---
(# F: @integer;
   fac: 
     (# V: @integer
     enter V
     do (if V > 1 then
            (V - 1 -> fac) * V -> V
        if)
     exit V
     #);
do 'Hello'->putline;
   (for i: 50000 repeat 7 -> fac -> F for);
   F -> putint; ' ' -> put;
   'Bye\n' ->puttext;  
#)

