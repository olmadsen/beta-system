range: @
  (# integer:  (# % simple, 32 % #);
     put: (# % prim % #);
     object:
       (# % simple %
          suspend: (# % prim % #);
       #);
     repetition:
       (# % simple %
          index: (# % simple % #);
          range: (# % simple % #)
       #);
     
     T: [6] @ integer;
     
  do
     (for i: 6 repeat 'a' + i - 1 -> T[i] for); 
     (for i: 6 repeat T[i] -> put for);
     
     (for i: T.range repeat 'a' + i - 1 -> T[i] for);
     (for i: T.range repeat T[i] -> put for);
  #)
