forTst: @
  (# integer:  (# % simple, 32 % #);
     put: (# % prim % #);
     object:
       (# % simple %
          suspend: (# % prim % #);
       #);     
     
     q,w: @integer
  do 96 -> q;
     (for i: 10 repeat
          q + i -> put;
          0 -> w;
          (for j: 3 repeat w+j -> w for)
     for);
     107 -> put;
     (if w = 6 then 108 -> put else 66 -> put if);
  #)
