               (if theDesc[] = none then 'desc:still:none' -> LG.Tn if)
            #);
          enterType:: 
            (# NA: ^denotation 
            do (if theDesc[] = none then
                   ('AttributeDenotation:not:checked:',doPT) -> LG.TTn;
                   check
