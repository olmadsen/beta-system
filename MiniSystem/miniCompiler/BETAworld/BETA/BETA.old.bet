BETA: @
  (# integer: (#  #);
     char: (#  #);
     put: (#  #);
     object: (# suspend: (#  #) #);
     repetition: 
       (# index: (#  #);
          range: (#  #) 
       #);
     putInt: 
       (# V: @ integer;
          ch: @ char;
          R:[12] @ char;
          top: @ integer
       enter V
       do (if V < 0 then
              '-' -> put; - V -> V
          if);
          (if V = 0 then
              1 -> top; 0 -> R[top]
           else 
              L:
            (if V > 0 then
                top + 1 -> top;
                V mod 10 -> R[top];
                V div 10 -> V;
                restart L
            if)
          if);
          (for i: top repeat R[top - i + 1] + '0' -> put for)
       #)
  #)
