guitest: @
  (# with gui;
     W: @ Window
       (# F: @Frame 
            (# 
               G: @ Frame;
               open:: 
                 (#
                 do F[] -> G.open;
                    G.setBackground;
                    (90,240) -> G.setPosition;
                 #);
            #);
          open::
            (#
            do this(Window)[] ->  F.open
            #);
       #);
     run: 
       (#
       do 'olsen' -> W.open;
       #)
