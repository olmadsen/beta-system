guitest: @
  (# with gui;
     W: @ Window
       (# F: @Frame 
            (# 
               G: @ Frame
                 (# mouseClicked::
                      (# T: ^text
                      do 'G:Frame:mouseClicked:'-> T[]; T.print                         
                      #)
                 #);
               L: @Label;
               TF: @TextField;
               B: @Button;
               open:: 
                 (# arg: ^text
                 do F[] -> G.open;
                    G.setBackground;
                    (40,40) -> G.setPosition;
                    (F[],'Hello') -> L.open;
                    F[] -> TF.open;
                    (80,30) -> TF.setSize;
                    'Burger' -> TF.setText;
                    (F[],'Push') -> B.open;
                 #);
               mouseClicked::
                 (# T: ^text;
                 do 'F:Frame:mouseClicked:'-> T[]; T.print;
                    TF.getText -> T[];
                    T.print; 10->put
                 #)               
            #);
          open::
            (#
            do this(Window)[] ->  F.open;
               (300,400) -> F.setSize;
            #);
       #);
     run: 
       (# T: ^Component
       do (T[] (*none*),'olsen') -> W.open;
       #);
     T: ^text
  do run;
  #)
