coComp: @
  (# co1: @
       (# R: @
            (# fool:(# do 'c' -> put; co1.suspend #);
            #);
          co1A:
            (# do 'b' -> put;  
               co1.suspend; 
               R.fool
            #);
       do 'a' -> put;
          co1.suspend;
          co1A;
          'd' -> put
       #);
     co2: @
       (# do '*' -> put;
          co2.suspend;
          '+' -> put;
          co2.suspend;
          '?' -> put
       #);
     T: ^text 
  do co1;
     co2; 
     co2; 
     co1; 
     co1;
     co1;
     0 -> putint; ' ' -> put;
     1 -> putint; ' '-> put;
     12 -> putint; ' '-> put;
     98 -> putint; ' '-> put;
     124 -> putint; ' ' -> put;
     -12 -> putint; ' ' -> put;
     255 -> putint; ' ' -> put;
     -255 -> putint; ' ' -> put;
     1234 -> putint; ' ' -> put;
     -9876 -> putint; ' ' -> put;
     ' hello' -> T; T.print
  #)
