rep: @
  (# R: [10] @integer;
     testGlobalRep:
       (#
          S: [3] @integer;
          globalRep:
            (#
            do (for k: 3 repeat 107 + k -> S[k] for); 
               (for k: 3 repeat S[k] -> put for); (* 'lmn' *)
            #);
       do globalRep
       #)
     forTst2: @
       (# q,w: @integer
       do 'n' -> q;
          (for i: 10 repeat (* 'o-x' *)
               q + i -> put;
               0 -> w;
               (for j: 3 repeat w+j -> w for)
          for);
          'y' -> put;
          (if w = 6 then 'z' -> put else 66 -> put if);
       #);
     
  do 97 -> R[3];
     R[3] -> put; (* 'a' *)
     (for i: 10 repeat 97 + i -> R[i] for); 
     (for i: 10 repeat R[i] -> put for); (* 'b-k' *)
     testGlobalRep; (* 'lmn' *) 
     forTst2;
     'z' -> last
  #);
