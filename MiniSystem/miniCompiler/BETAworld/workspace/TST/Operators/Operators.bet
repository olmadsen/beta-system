Operators: @
  (# rRelational:
       (# rFoo: (# #);
          r1,r2,r3: ^rFoo
       do &rFoo[] -> r1[] -> r2[];
          &rFoo[] -> r3[];
          
          (if r1[] = r2[] then 'a' -> put else '?' -> put if);
          (if r1[] <> r2[] then '?' -> put else 'b' -> put if);
          (if r1[] = r3[] then '?' -> put else 'c' -> put if);
          (if r1[] <> r3[] then 'd' -> put else '?' -> put if);
       #)
  do rRelational;
     'd' -> last
  #)
