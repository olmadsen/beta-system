VIRT: @
  (# foo:
       (# fisk:< (# do ch -> put; ch + 1 -> ch; inner; ch -> put #);
       #);
     bar: foo
       (# fisk::< (# do ch -> put; ch+1 -> ch; inner; ch -> put; ch + 1 -> ch #);
       #);
     snorf: bar
       (# fisk:: (# do ch -> put; ch + 1 -> ch #)
       #);
     R1: @foo;
     R2: @bar;
     R3: ^snorf;
     ch: @integer
  do 97 -> ch;
     R1.fisk;
     ch + 1 -> ch;
     R2.fisk;
     ch + 1 -> ch;
     &snorf[] -> R3[];
     R3.fisk;
     'k' -> last
  #);
