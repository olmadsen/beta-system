VIRT: @
  (# foo:
       (# fisk:< (# do ch -> put; ch + 1 -> ch;  inner; ch -> put #);
       #);
     bar: foo
       (# fisk::< (# do ch -> put; ch+1 -> ch; inner; ch -> put; ch + 1 -> ch #);
       #);
     snorf: bar
       (# fisk:: (# do ch -> put; ch + 1 -> ch #)
       #);
     ex6: 
       (# ex6_foo: 
            (# fool_6:< 
                 (# 
                 do 'm' -> put; inner; 'o' -> put
                 #)
            do fool_6
            #);
          ex6_fisk: ex6_foo
            (#  fool_6:: (#do 'n' -> put #) 
            #)
       do 'l'->put; ex6_fisk
       #)

     R1: @foo;
     R2: @bar;
     R3: ^snorf;
     ch: @integer
