     (*  /VIRT (5756,5780) *);
     (*  /basic (5787,5812) *);
     (*  /rep (5819,5842) *)(*  /rep (5253,0) *) 
  do 'A' -> mark;
     basic;
     'B' -> mark;
     Operators;
     'C' -> mark;
     super;
     'D' -> mark;
     rep;
     'E' -> mark;
     label;
     'F' -> mark;
     iff;
     'G' -> mark;
     virt;
     'H' -> mark;
