TST: @
  (# nl: (#  exit 10 #);
     chk: 
       (# a,b,c: @ integer
       enter (a,b,c)
       do (if a = b then
              c -> put
           else
              63 -> put
          if)
       #);
     last: 
       (# ch: @ integer
       enter ch
       do (if ('a' <= ch) and (ch <= 'z') then
              (for i: 'z' - ch repeat ch + i -> put for)
          if);
          newline;
       #);
     mark: 
       (# ch: @ integer
       enter ch
       do ch -> put; ':' -> put;
          (for i: ch - 'A' repeat ' ' -> put for)
       #); 
     (*  /Block (5565,5590) *);
     (*  /IFF (5597,5620) *);
     (*  /Label (5627,5652) *);
     (*  /SLMR (5659,5683) *);
     (*  /Struc (5690,5715) *);
