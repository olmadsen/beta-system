Block: @
  (# Thea:
       (# hej: @ (# do 'a' -> put #);
       do inner
       #);
     Robin: Thea
       (# du: @ (# do 'b' -> put #)
       do inner; 
       #);
     Linnea: Robin
       (# gamle: @ (# do ch -> put #)
       do hej; du; gamle
       #);
     ch: @integer;
     snaps:
       (# ch: @integer;
          roed: @ (# do ch -> put; ch + 1 -> ch #)
       do 'd'-> ch; inner; ch -> put
       #);
     
     vin:
       (# vand: snaps 
            (#  chainti: @ (# do ch -> put; ch + 1 -> ch #);
            do ch -> put; ch +1 -> ch; roed; chainti;
            #);
          
       do vand
       #)
  do 'c' -> ch;
     Linnea;
     vin;
     'g' -> last
  #);
