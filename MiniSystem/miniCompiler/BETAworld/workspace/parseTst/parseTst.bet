parseTst: @
  (# btap: @ (# x: @integer #);
     a,b,c,margin: @integer;
     
     R: @
       (# foo:
            (# S: ^Fisk;
               Fisk: (# do inner #);
            do &Fisk[] -> S[]
            exit this(foo)[]
            #)
       #)
  do 90 -> b; 7 -> c;
     b + c -> a;
     -b -> a;
     (-b+c) -> a;
     -(-b+c) -> a;
     (+b+c) -> a;
     a -> put;
     (for 3 repeat 
          a + 1 -> a -> put
     for)
     (for inx : 3 repeat
          a + inx -> put
     for);
     (for p : btap.x repeat for);
     (for margin+b repeat for);
     
     (R.foo).Fisk(# do 'Q'->put  #);
     &(R.foo).Fisk(# do 'Q'->put  #);
     
  #)

  
