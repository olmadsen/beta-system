origin '~beta/basiclib/betaenv';
INCLUDE '~beta/basiclib/file';
---program:descriptor---
(# F: @file;
   i: @integer;
   ch: @char
do 2 -> arguments -> F.name;
   F.openRead;
   L: (if ch <> 255 then
          i + 1 -> i;
          F.get -> ch;
          i -> putint; ':' -> put; ch -> putint; 
          ' "' -> puttext; ch -> put; '"' -> put; newline;
          restart L
      if)
#)
          
   
