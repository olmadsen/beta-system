     a: @integer;
     S: ^fisk;
     x: @integer;
     fisk: 
       (# go: 
            (# [kind[method]]
            do 'Hello: '.print
            #);
          show: proc
            (# t: @text;
            do 'How are you?\n'.print;
               inner;
            #);
          dance: show
            (# s: @text do 'I am fine, thanks!' -> t.puttext; t.print; #)
       #);
     
  do 
     10 -> a; 
