ListMerger: @
  (# List: 
       (# init: (#  do 'Z' -> put #);
          node: 
            (# elm: ^text;
               next: ^Node;
               display: 
                 (# 
                 do elm.print;
                    ' ' -> put;
                    (if next[] <> none then
                        next.display
                    if)
                 #)
            #);
          add: 
            (# S: ^text; N: ^Node
            enter S[]
            do 'a' -> put;
               &Node[] -> N[];
               'x' -> put;
               S[] -> N.elm[];
               'b' -> put;
               (if first[] = none[] then
                   'c' -> put;
                   N[] -> first[];
                   first[] -> last[]
                else
                   'd' -> put;
                   N[] -> last.next[];
                   N[] -> last[]
               if)
            #);
          display: 
            (# 
            do (if first[] <> none[] then
                   first.display; 10 -> put
               if)
            #);
          traverse: (#  do  #);
          first,last: ^Node
       #);
     init: 
       (# 
       do 'V' -> put;
          &List[] -> L1[];
          'W' -> put;
          L1.init;
          'E' -> put;
          &List[] -> L2[];
          L2.init;
          'Q' -> put;
          'Lars' -> L1.add;
          'Dave' -> L1.add;
          'Scarlett' -> L1.add;
          'Bak' -> L2.add;
          'Ungar' -> L2.add;
          'Johanson' -> L2.add
       #);
     L1,L2: ^List;
     none: ^object
  do 'Here is ListMerger'.print;
     init;
     L1.display;
     L2.display
  #)