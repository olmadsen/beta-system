SimpleAccounts: @
  (# Account: 
       (# deposit: 
            (# amount,bal: @ integer
            enter amount
            do balance + amount -> balance;
               balance -> bal
            exit bal
            #);
          withdraw: 
            (# amount,bal: @ integer
            enter amount
            do (if (balance - amount) >= 0 then
                   balance - amount -> balance
               if);
               balance -> bal
            exit bal
            #);
          balance: @ integer;
       #);
     SavingsAccount: Account
       (#
       #);
     CheckAccount: Account
       (#
       #);     
  #)
