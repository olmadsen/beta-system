Stream:
  (# putx:<(# ch: @ char enter ch do inner #);
     putText:< (# T: ^text enter T[] do inner #);
     putint: 
       (# V: @ integer;
          ch: @ char;
          top: @ integer;
          R:[12] @ char;
       enter V
       do (if V < 0 then
              '-' -> put; - V -> V
          if);
          (if V = 0 then
              1 -> top; 0 -> R[top]
           else 
              L:
            (if V > 0 then
                top + 1 -> top;
                V mod 10 -> R[top];
                V div 10 -> V;
                restart L
            if)
          if);
          (for i: top repeat R[top - i + 1] + '0' -> putx for)
       #);
     newline: (# do 10 -> putx #);
     print:<(#  do inner #);
  #)
