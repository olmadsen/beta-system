            do &descs.OD[descNo].template[] -> obj[];
               descs.OD[descNo].objSize  -> obj.fields.new;
               descNo -> obj.myDescInx;
               obj[] -> descs.ObjPool;
               isObj -> obj.isObj;
               descs.OD[descNo].OSDvisibility -> obj.OSDvisibility; 
