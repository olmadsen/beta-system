                   (3,(3 -> Z.get) + off) -> VP.Put;
                   (4,descNo) -> VP.put;
               else
                   (1,thisStack.rPop->X[]) -> VP.rput;
                   (2,thisStack.rPop->Y[]) -> VP.rput;
                   (3, off) -> VP.Put;
                   (4,descNo) -> VP.put;
               if);
               VP[] -> thisStack.rPush;
               (if withDumps then
                   ('ValueProxy',true,out[]) -> VP.dump;
                   (if X[] <> none then
                       ('ValueOrigin',true,out[]) -> X.dump;
