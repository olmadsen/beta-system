               (descNo,false,0,0) -> allocValueProxyTemplate -> VP[];

               (if originIsValueObj = 1 then
                   'originIsValueObj:'->out.puttext;
                   1 -> thisObj.rget -> Z[]; (* assuming orginOff = 1 *)
                   ('thisObj.origin',true,out[]) -> Z.dump;
                   thisStack.rPop->X[]; (* probaly not ok *)
                   thisStack.rPop->Y[]; (* ignored *) 
