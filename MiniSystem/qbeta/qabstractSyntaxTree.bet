     xPattern: Decl
       (# label::< (# do 'pattern' -> lab[]; inner #); 
          PT::< 
            (# 
            do (pos,break) -> sig.PT; ':' -> lx.put;
               inner;
               ' ' -> lx.put;
               (pos,break) -> OD.PT
            #);
          length:: (# do sig.length + 3 + OD.length -> value #);
          ptnKind: @integer;
          isBasicPtn: @boolean;
          setUpOrigin::
            (#
            do origin[] -> sig.setUpOrigin;
               sig[] -> OD.sig[];
               origin[] -> OD.setupOrigin;
            #);
          isPatternDecl:: TrueValue;
          check::< 
            (# setImplSuper:< (# do inner #)
            do (*'\n>>Pattern: ' -> puttext; sig.id[] -> putline; *)
               setImplSuper;
               (* Must check sig before OD;
                * Items in OD may refer Decl's in sig
                * Allocation of storage for sig must be in OD.IS
                * Scope for Decl's in sig must be basicScope
                *)
               (none,OD.IS[],OD.IS[],true) -> sig.check; 
               (none,basicScope[],basicScope[],true) -> OD.check;
               inner;
               true -> isChecked
            #);
          alloc::< (# do IT[] -> OD.alloc; inner #);
          desc::<
            (# elm:: items
            do (if OD.IS.isEmpty then
                   (* V: < A, V::< B, or V:: C *)
                   OD.super.desc -> D[]
                else
                   OD.IS[] -> D[]
               if);
               inner
            #);
          isBasic:: (# do OD.IS.isBasic -> value #);
          primNo:: 
            (#
            do (if OD.IS.props[] <> none then 
                   OD.IS.props.basicProp.primNo -> value 
               if)
            #);
          isCore:: (#do OD.IS.isCore -> value #);
          isRecursive: @boolean;
          getQual::
            (# 
            do (if false and not isChecked then 
                   '\n>>> Pattern: ' -> puttext; sig.doPT -> puttext;
                   ' is not checked'->putline;
               if);
               (if isRecursive then
                   OD[] -> qual[];
                   false -> isRecursive;
                   leave getQual
               if);
               inner;
               (if traceCheckQual then
                   'Pattern getQual: "' -> putline; sig.doPT -> puttext; 
                   '"' ->put; newline;
               if);
               true -> isRecursive;
               sig.getQual -> qual[];
               false -> isRecursive;
               (if qual[] = none then
                   OD[] -> qual[];
               if);
               (if traceCheckQual then
                   'Pattern: "\n' -> puttext; 
                   (if qual[] <> none then 
                       qual.doPT -> puttext
                    else 'none'->puttext
                   if);
                   '"' -> put; newline
               if)
            #);
          entity:: (#do OD.entity -> E[] #);
          skind:: (# do ': ' -> S[] #);
          getEntries:: (# do OD.getEntries -> (names[],entries[]) #);
          objDescEQ:: (# do DN -> OD.objDescEQ -> value #);
          isChecked: @boolean;
          OD: ^ObjectGenerator
       enter (sig[]->append,OD[]->append)
       #);
