                    (*
                    args.scanSons
                    (#
                    do (pos,break) -> current.PT;
                       (if not isLast then ' ' -> lx.put if);
                    #)*)
                 #);
               length:: 
                 (# do scanSons(# do current.length + value -> value #)#);
            #);
