                          ','-> lx.put;
                          ' '-> lx.put
                      if) 
                   #)
               if)
            #);
          length:: 
            (# 
            do 4 -> value;
               scanSons(# do current.length + value -> value #)
            #);
          Label:: (# do 'Value' -> lab[] #);
          PP:: (# do Label -> lx.append #)
       #);
