                    if)if)
               #);

               (1,T.length - 1) -> T.sub -> sig.id[];
               T[] -> (sig.args.son[] -> N[]).T[]; (* why? *)
               (if false then
                   'setNewId: ' -> puttext; T[] -> puttext; 
                   ' sig: ' -> puttext; sig.id[] -> putline;
               if)
            #);
          isPattern:: TrueObject;
