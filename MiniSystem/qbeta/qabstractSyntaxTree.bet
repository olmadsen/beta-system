          store:< 
            (# gdb:< debug(#do 'store:' -> TT; inner #);
               noSwap: @boolean;  enter noSwap do inner #);
          popReturn:< (# do inner #);
          (*sort:: (# do theObjectGenerator -> S[] #)*)
       do inner
       #);
     Scope: Node
       (# display:<
            (# org: ^Scope
            do '>>>Scope:' -> puttext; inner;
