                        *)
                       '!!!! FIX:Invocation:desc:ATd is none: ' -> puttext; 
                       primNo -> putint; newline;
                       doPT -> putline; '---'->putline;
                       doPP -> putline; '---'->putline;
                       father.father.doPT -> putline;
                   if)
