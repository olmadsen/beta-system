                     do i + 1 -> i;
                        (if i > top then
                            (* '\n *** Too many actual arguments' -> putline;
                             E.dopt -> putline; sig.doPT -> putline;*)
                            leave L
                        if);
                        HandleVirtual:
                          (if currentArg.isVirtual then
                              (if false and 
                                  ('ObjectCall' -> (Ex.father.father.label).equalNCS) then
                                  '**** FormalArg: ' -> puttext; 
                                  currentArg.doPT -> putline;
                                  '**   ActualArg: ' -> puttext; 
