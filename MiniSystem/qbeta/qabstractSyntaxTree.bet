          setNewId::
            (# T,first: ^text; N: ^name
            do sig.id.copy -> T[]; ':' -> T.put; 
               (2,T.length) -> T.sub -> first[];
               (*'setNewId:A: ' -> puttext; T[] -> puttext; newline;*)
               OG.IS.scanSons
               (# D: ^Decl; inx: @integer; arg: ^text
               do L:
                    (if current.isDecl then
                        (if (current[] -> D[]).isInArg then
                            D.sig.id.copy -> arg[]; 
