               comment[] = none -> emitBR;
               none -> comment[];
               
               (if not IS.isEmpty then
                   IS.scanSons
                   (#
                   do 
                      (if true 
                       // current.isNewProp then
                          (if false and emitBR then
                              ' true:  '->lx.append 
                          if);
                          (pos,emitBR) -> current.HTML;
                          true -> emitBR;
                       // current.isDecl then
                          (if accL
                           // acc_public 
                           // acc_domain 
