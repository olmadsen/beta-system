                        // no = 1 then (* first keyword *)
                           current[] -> N[];
                           (if (1->N.T.inxGet) = '%' then
                               (*N.T[] -> puttext; ' ' -> put;*)
                               ('@',1) -> N.T.inxPut;
                               (*N.T[] -> puttext; ',' -> put;*)
