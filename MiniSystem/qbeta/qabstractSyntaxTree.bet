            enter(NA[],isFirst)
            do (*'Search: ' -> puttext; CS[] -> puttext;
                ' in: ' -> puttext; doPP -> putline;*)
               (if (1 -> NA.T.inxGet) = '#' then 
                   (2,NA.T.length) -> NA.T.sub -> CS[]
                else
                   NA.T[] -> CS[];
               if);
               inner;
            exit(ATd[],on,pn)
            #); 
