       do inner
       #);
     Signature:< Scope
       (* In xbeta, Signature have subpatterns UnarySignature, etc *)
       (# PT:: 
            (# inDecl: @boolean
            enter inDecl
            do (if inDecl then
                   (if type[] <> none then
                       type.id[] -> lx.puttext; 
                       (if type## = Binary## then ' '-> lx.put if)
                    else
                       ' type is none ' -> lx.puttext
                   if)
                else
                   (if type[] <> none then
                       (pos,break,inDecl) -> type.PT;
                       (if val[] <> none then (pos,break) -> val.PT if)
                    else
                       scanSons
                       (#do (pos,break) -> current.PT; 
                          (if not isLast then ' ' -> lx.put if)
                       #);
                          (*(if val[] <> none then val.PT if)*)
                   if)
               if)
            #);
          length:: 
            (# 
            do (if type[] <> none then
                   type.length -> value;
                   (if args[] <> none then value + args.length -> value if);
                   (if val[] <> none then val.length +value -> value if);
                else
                   scanSons(#do current.length + value -> value #)
               if)
            #);
          Label:: (# do 'Signature' -> lab[] #);
          PP::(# do Label -> lx.append #);
          display:: (# do 'Signature:'->putline #);
          type: ^SigType;
          SigType: Node 
            (# id: ^text;
               PT::< (# inDecl: @boolean enter inDecl do inner #);
            enter id[] 
            do id[] -> name -> append 
            #);
          Unary:< SigType
            (# Label:: (# do 'UnaryPtn' -> lab[] #);
               PT:: (# do id[] -> lx.puttext #);
            #);
          Binary:< SigType
            (# label :: (# do 'Binary' -> lab[] #);
               PT:: (# do id[] -> lx.puttext;
                       ' ' -> lx.put; (pos,break,inDecl) -> args.PT #);
            #);
          Function:< SigType
            (# label:: (# do 'Function' -> lab[] #);
               PT:: (# do id[] -> lx.puttext;
                       (pos,break,inDecl) -> args.PT #)
            #);
          KeyWord:< SigType
            (# label :: (# do 'KeyWord' -> lab[] #);
               PT:: (#do (pos,break,inDecl) -> args.PT #);
            #);
          getReturnOG:
            (# DI: ^DataItem
            do getReturnDecl -> DI[]
            exit DI.OG[]
            #);
          getReturnDecl:
            (# ptn: ^Pattern; DI: ^DataItem
            do L:
                 (if val[] <> none then
                     '\n!!!!! getReturnDecl:should not come here!!!' -> putline;
                     father.doPT -> putline;
                     val.scanSons(# do current[] -> DI[] #)
                  else
                     father[] -> ptn[];
                     ptn.OG.IS.scanAllItemDecls
                     (#
                     do (if currentDcl.isOutArg then
                            currentDcl[] -> DI[];
                            leave L
                     if)#);
                     (if (ptn.OG.superPtn -> ptn[]) <> none then 
                         ptn.sig.getReturnDecl -> DI[]                         
                 if)if)
            exit DI[]
            #);          
          args: ^Arguments;
          val: ^Value;
          id: ^text; (* should be a NAme *)
          isKeyWord: @boolean;
          init:
            (# 
            enter(id[],args[],isKeyWord)
            do (if not isKeyWord then
                   id[] -> Name -> Append
                else 
                   true -> args.isKeyword;
                   (*this(Signature)[] -> args.father[]*)
               if);
               (if args[] <> none then args[] -> append if);
            #);
          setId:
            (# 
            do ''-> id[];
               args.scanSons
               (# B: @boolean; L: ^Lexem
               do (if not B then 
                      current[] -> L[];
                      L.T.copy -> id.append;
                  if);
                  not B -> B
               #);
            #);
          hasVirtualArgs: BooleanValue
            (#
            do (* 'Signature:hasVirtualArgs: '->puttext; doPT -> putline;*)
               (if args[] <> none then
                   args.hasVirtualArgs -> value
            if)#);
          scanArgs:
            (# currentArg: ^Decl
            do (if args[] <> none then
                   args.scanArgs(# do current[] -> currentArg[]; inner scanArgs #)
               if)
            #);
          scanArgsReverse:
            (# currentPar: ^Decl; first: @boolean
            do (if args[] <> none then 
                   args.scanArgsReverse
                   (#do current[] -> currentPar[]; inner scanArgsReverse #)
               if)
            #);
          getQual:<
            (# qual: ^objectGenerator
            do inner
            exit qual[]
            #);         
          theObjectGenerator:: (# ptn: ^Pattern do (father[]->ptn[]).OG[] -> OGx[] #);
       enter(init) 
          (* OBS! (init[with only enter id[]],isKeyWord) does not work 
           * Perhaps compiler error!?
           *)
       #);     
     Sig_Generator:
       (# id: ^Text; args: ^Arguments; isKeyword: @boolean; sig: ^Signature
       enter(id[],args[],isKeyWord)
       do (id[],args[],isKeyWord) -> Signature -> sig[];
          inner;
       exit sig[]
       #);
     UnarySigGen: Sig_Generator(# do id[] -> sig.Unary -> sig.type[] #);
     FunctionSigGen: Sig_Generator(# do id[] -> sig.Function -> sig.type[] #);
     KeyWordSigGen: Sig_Generator(# do id[] -> sig.KeyWord -> sig.type[] #);
     BinarySigGen: Sig_Generator(# do id[] -> sig.Binary -> sig.type[] #);     
     Arguments:< Node
       (# elm:< Item;
          PT:: 
            (# inDecl: @boolean
            enter inDecl
            do (if true
                // isBinary then 
                   (if inDecl then
                       scanSons
                       (#
                       do 'in ' -> lx.puttext;
                          (pos,break) -> current.PT;
                          (pos,true) -> mkBreak
                       #)
                    else
                       scanSons(# do (pos,break) -> current.PT #);
                   if)
                // isKeyWord then 
                   (if not isNewKeyWord then
                       scanSons
                       (# 
                       do (pos,break) -> current.PT; 
                          (if not isLast then ' ' -> lx.put if) 
                       #);
                   if)
                // isFunction then 
                   (if inDecl then
                       scanSons
                       (# 
                       do 'in ' -> lx.puttext; 
                          (* if current.isDataItem then current is not marked as
                           * inArg since it is in the arg list
                           * Only dataitems in maipart are marked as inArg
                           *)
                          (pos,break) -> current.PT; 
                          (pos,true) -> mkBreak
                       #);
                    else
                       '(' -> lx.put;
                       scanSons
                       (# 
                       do (pos,break) -> current.PT; (if not isLast then ',' -> lx.put if) 
                       #);
                       ')' -> lx.put
                   if)
                else
                   (* unary *) 
                   scanSons(# do (pos,break) -> current.PT #);
               if)
            #);
          length:: (# do scanSons(# do current.length + 1 -> value #)#);
          Label:: (# do 'Arguments' -> lab[] #);
          PP:: (# do Label -> lx.append #);
          isBinary,isKeyWord,isFunction: @boolean;
          isArguments:: TrueValue;
          isNewKeyWord: @boolean; (* as in @if: ... with only >> args are used*)
          scanArgs:
            (# current: ^Elm; isLast: @boolean
            do (if isKeyWord then
                   (*'arguments:length: ' -> puttext; 
                    * noOfSons -> putint; newline;*)
                   scanSons
                   (# isArg: @boolean
                   do 
                      (if isArg then
                          current[] -> this(scanArgs).current[]; 
                          isLast -> this(scanArgs).isLast;
                          inner scanArgs
                       (*else
                          'isNotArg:' -> puttext; 
                          current.dopt -> putline*)
                      if);
                      not isArg -> isArg
                   #)
                else
                   scanSons
                   (# do current[] -> this(scanArgs).current[]; 
                      isLast -> this(scanArgs).isLast;
                      inner scanArgs #)
               if)
            #);
          scanArgsReverse:
            (# current: ^Decl; first: @boolean
            do (if isKeyword then 
                   scanReverse
                   (# isPar: @boolean
                   do not isPar -> isPar;
                      (if isPar then
                          current[] -> this (scanArgsReverse).current[];
                          inner scanArgsReverse;
                      if);
                   #)
                else
                   true -> first;
                   scanReverse
                   (# 
                   do (*'Arg: ' -> puttext; 
                       * current.doPT -> puttext; ' ' -> put;
                       *)
                      current[] -> this (scanArgsReverse).current[];
                      inner scanArgsReverse;
                      false -> first
                   #);
                   (*newline*)
               if)
            #);
          hasVirtualArgs: BooleanValue
            (#
            do (*'Arguments:hasVirtualArgs:' -> puttext; dopt -> putline;*)
               L:
                 scanArgs
                 (# D: ^Decl 
                 do current[] -> D[];
                    (*'HasVirtualArgs: ' -> puttext; D.doPT -> putline;*)
                    (if D.isVirtual then
                        true -> value;
                        leave L
                    if)
                 #)
            #)
       enter isKeyword
       #);
     Value:< Node
       (* A list of Decl's *)
       (# PT::
            (#
            do (if son[] <> none then 
                   (if true then
                       'out ' -> lx.append
                    else
                       ' -> ' -> lx.append;
                   if);
                   scanSons
                   (# 
                   do (pos,break) -> current.PT; 
