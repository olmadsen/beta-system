           *)
       #);     
     Sig_Generator:
       (# id: ^Text; names: ^NameList; args: ^Arguments; 
          isKeyword: @boolean; sig: ^Signature
       enter(id[],names[],args[],isKeyWord)
       do (id[],names[],args[],isKeyWord) -> Signature -> sig[];
          inner;
          (if false then
              '\n**** Sig_generator:id: '->puttext; id[] ->putline;
              '**   names:' -> puttext; names.dopt -> putline;
              '**   gen:args: '->puttext; args.dopt -> putline;
              '**   isKW: ' -> puttext; isKeyword -> putboolean; newline;
              '**   sig: ' -> puttext;
              sig.scanSons
              (#do current.label -> puttext; ' ' ->put;
                 current.dopt -> putline #)
