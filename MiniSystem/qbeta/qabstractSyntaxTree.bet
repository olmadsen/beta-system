                      (if not isLast then ',' -> lx.put
               if)#)if);
               ')' -> lx.put 
            #);
          bracketedListLength:: (# do noOfSons -> value #);
          length:: (# do 2 + IV.length -> value #);
          copyAST::
            (#
            do IV.copyAST -> BracketedExp -> nAst[]
            #);
          IV: ^Invocation; 
       enter IV[] -> append
