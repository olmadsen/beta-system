               if)
            #);
          superPtn:
            (# D: ^Decl; P: ^Pattern
            do (if ((super.ATd[]-> D[]) <> none) and D.isPatternDecl then
                   D[] -> P[]
               if)
            exit P[]
            #);
          inSuper: BooleanValue
            (# lQ: ^ObjectGenerator
            enter lQ[]
            do ('inSuper:lQ',lQ[]) -> display;
               ('rQ',this(ObjectSpecification)[]) -> display;
               L:
                 (if lQ[] = this(ObjectSpecification)[] then
                     true -> value 
                  else
