                            (if (inx + 1 -> inx) = 1 then
                                (*'arg 1: "' -> puttext; arg[] -> puttext;
                                 '"  ' -> puttext; first[] -> putline;
                                 *)
                                ':' -> T.put;
                                (if arg[] -> first.equalNCS then
                                    leave L
                            if)if);

                            arg[] -> T.append; 
