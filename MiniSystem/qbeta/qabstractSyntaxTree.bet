     Pattern:< Decl
       (# label::< (# do 'pattern' -> lab[]; inner #); 
          PT::< 
            (# 
            do (pos,break) -> sig.PT; ':' -> lx.put;
               inner;
               ' ' -> lx.put;
               (pos,break) -> OD.PT
            #);
          length:: (# do sig.length + 3 + OD.length -> value #);
          ptnKind: @integer;
          isBasicPtn: @boolean;

          isBasic:: (# do OD.IS.isBasic -> value #);
          primNo:: 
            (#
            do (if OD.IS.props[] <> none then 
                   OD.IS.props.basicProp.primNo -> value 
