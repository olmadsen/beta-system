            #);
          lastHasActualArgs: BooleanValue
            (# lastE: ^Exp                                (* length is for PT!*)
            do ((last -> lastE[]).args[] <> none) and (lastE.args.length > 0) 
                 -> value
            #); 
          bracketedListLength:: 
            (* if BE, assume exactly one element *)
            (# do (last).bracketedListLength -> value #);
       #);
