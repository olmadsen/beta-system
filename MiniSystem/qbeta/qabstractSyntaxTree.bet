            (# Label:: (# do 'ObjectInvocation_KeyWord' -> lab[] #);
               PP:: (# do Label -> lx.append #);
               PT:: 
                 (# 
                 do (* first element in sons is receiver - may be empty *)
                    args.scanSons
                    (# no: @integer; even: @boolean; n: ^Name
