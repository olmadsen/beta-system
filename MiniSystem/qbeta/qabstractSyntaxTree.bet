          mkOG:
            (* used with new syntax for fat comma: foo{...}
             * where foo{...} may be a subpattern or an invocation.
             * The latter requires that foo has an in parameter
             * If foo{...} appears in a pattern we assume that foo is
             * a super pattern and {...} is a mainpart
             * This may just be a preliminary solution
             *)
            (# E: ^Exp; id,rB: ^Name; IS: ^Items; OGa: ^ObjectGenerator; 
               IV: ^Invocation
            do last -> E[];
               (if E.args.noOfSons <> 4 then
                   '\n!!!! Invocation:mkOG: must have 4 sons!\n' -> putline;
                   leave mkOG
               if);
               E.args.scanSons
               (# i: @integer; OGisMet: @boolean
               do (if i + 1 -> i
                   // 1 then current[] -> id[]
                   // 2 then 
                   else
                      (if current## = ObjectGenerator## then
                          (if OGisMet then
                              '**** mkOG: OG met twice in arguments:'->putline;
                              E.dopt -> putline
                          if);
                          (current[] -> OGa[]).IS[] -> IS[]
                      if);
                      true -> OGisMet;
                      (*
                       // 3 then (current[] -> OGa[]).IS[] -> IS[]
                       // 4 then current[] -> rB[]
                       *)
                  if)
               #);
               Invocation -> IV[];
               scanSons
               (#
               do (if isLast then
                      (if true then
                          (id.T[], Arguments) -> UnaryGen -> IV.append
                       else
                          ((1,id.length - 1) -> id.T.sub, Arguments) 
                            -> UnaryGen -> IV.append
                      if)
                   else
                      current[] -> IV.append
                  if)
               #)
            exit(IV[],IS[])               
            #);
          desc:
            (# D: ^Items
            do (if false then
                   '**** Invocation:desc:\n'->puttext; dopt -> puthead
               if);
               (if (ATd[] = none) and ((last).ATd[] = none)  then 
                   (last).ATd[] = none ->putboolean;
                   (if toFix then
                       (* May happen in circular definitions like
