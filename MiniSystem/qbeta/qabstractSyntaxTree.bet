                    // D.isPatternDecl then
                       D[] -> P[];
                       P.OG.isValueObj -> value
                    // D.isDataitem then
                       D[] -> DI[];
                       DI.isValue and not DI.isPrimitive -> value
                       (* do we mean isPrimitive or isBasicPattern? *)
