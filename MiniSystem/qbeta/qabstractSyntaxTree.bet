     Arguments:< Node
       (# elm:< Item;
          PT:: 
            (# inDecl: @boolean
            enter inDecl
            do (if false then
                   '**** arguments:PT: ' -> puttext; 
                   'isBinary: ' -> puttext; isBinary -> putboolean;
                   ' isKeyWord: ' -> puttext; isKeyword -> putboolean;
                   ' isFunction: ' -> puttext; isKeyword -> putboolean;
                   ' ' -> put; newline;

                   scanSons
                   (# N: ^Name
                   do current.label -> putline;
                      (if current## <= lexem## then
                          current[] -> N[];
                          N.T[]->putline
                      if);
                   #); '---'->putline;
               if);
               (if isReturn then ' -> ' -> lx.append if);
               (if true
                // isBinary then
                   (if inDecl then
                       scanSons
                       (#
                       do (*'in ' -> lx.puttext;*) ' ' -> lx.put;
                          (pos,break) -> current.PT;
                          (pos,true) -> mkBreak
                       #)
                    else
                       scanSons(# do (pos,break) -> current.PT #);
                   if)
                // isKeyWord then
                   scanSons
                   (# doBreak,blankAfter: @boolean;
                      skipBrace:
                        (# go: @Boolean; N: ^Name;
                        do true -> go;
                           (if current.isNameLiteral then
                               current[] -> N[];
                               (if true
                                // '(' -> N.T.equal then
                                   (*' ' -> lx.put*)
                                // ')' -> N.T.equal then
                                   (*true -> blankAfter*)
                                // '{' -> N.T.equal then
                                   false -> go
                                // '}' -> N.T.equal then
                                   (pos,true) -> mkBreak0;
                                   false -> go
                                // (1->N.T.inxGet) = '@' then
                                   true -> blankAfter
                           if)if);
                           (if go then inner if)
                        #);
                      ix: @integer
                   do (* if (E) :then { S1 } :else { S2} *)
                      (if not doBreak then
                          pos > lineWidth -> doBreak
                      if);
                      (pos,doBreak) -> mkBreak0;
                      (if false and nameListInDcl then
                          (if (ix + 1 -> ix) > 1 then
                              skipBrace(#do (pos,true) -> current.PT #);
                          if)
                       else
                          skipBrace(#do (pos,true) -> current.PT #);
                      if);
                      (if blankAfter then 
                          ' ' -> lx.put; false -> blankAfter 
                   if) #)
                // isFunction then
                   (if inDecl then
                       scanSons
                       (# 
                       do (*'in ' -> lx.puttext;*) ' ' -> lx.put; 
                          (* if current.isDataItem then current is not marked
                           * as inArg since it is in the arg list
                           * Only dataitems in maipart are marked as inArg
                           *)
                          (pos,break) -> current.PT; 
                          (pos,true) -> mkBreak
                       #);
                    else
                       '(' -> lx.put;
                       scanSons
                       (# 
                       do (pos,break) -> current.PT; 
                          (if not isLast then ',' -> lx.put if) 
                       #);
                       ')' -> lx.put
                   if)
                else
                   (* unary *) 
                   scanSons(# do (pos,break) -> current.PT #);
               if);
               (*newline*)
            #);
          length:: (# do scanSons(# do current.length + 1 -> value #)#);
          HTML::
            (#
            do (if true
                // isBinary then
                // isKeyWord then
                // isFunction then
                   '(' -> lx.put;
                   scanSons
                   (# 
                   do (pos,false) -> current.HTML;
                      (if not isLast then ',' -> lx.put if) 
                   #);
                   ')' -> lx.put
                else
                   (* unary *)
               if)
            #);
          Label:: (# do 'Arguments' -> lab[] #);
          PP:: (# do Label -> lx.append #);
          isBinary,isKeyWord,isFunction,inBraces,isReturn: @boolean;
          (* inBraces: experiment with new fat comma syntax: 
           *   if(be)then{S1}else{S2}
           * {S1} and {S2} parse to Argumens with inBraces = true
           *)
          isArguments:: TrueValue;
          isNewKeyWord: @boolean; (*as in @if: ... with only >>args are used*)
          copyAST::<
            (# nArgs: ^Arguments
            do (*'arguments:'->puttext; dopt -> putline;*)
               Arguments -> nArgs[];
               scanSons(#do current.copyAST -> nArgs.append #);
               isFunction -> nArgs.isFunction;
               isKeyWord -> nArgs.isKeyWord;
               isNewKeyWord -> nArgs.isNewKeyWord;
               isBinary -> nArgs.isBinary;
               nArgs[] -> nAST[];
            #);
          scanArgs:
            (# current: ^Elm; isLast: @boolean; i: @integer
            do (if isKeyWord then
                   (if false then
                       '**** arguments:'->puttext; dopt -> puttext;
                       ' noOfArgs: ' -> puttext; 
                       noOfSons -> putint; newline;
                   if);
                   scanSons
                   (# isArg: @boolean
                   do (if false then current.dopt -> puttext; ',' -> put; 
                          CURRENT.Label -> puttext; ';'->put;
                      if);
                      (* With new fatcomma we have arguments like
                       * if  (  OG1 ) :x ( OG2 ) ...
                       * we can thus not just alternate as with ild KW syntax
                       *)
                      (if current## <= ObjectSpecification## (*isArg*) then
                          current[] -> this(scanArgs).current[]; 
                          isLast -> this(scanArgs).isLast;
                          inner scanArgs
                       (*else
                          'isNotArg:' -> puttext; 
                          current.dopt -> putline*)
                      if);
                      (if (i + 1 -> i) > 1 then not isArg -> isArg if)
                   #); (*newline*)
                else
                   scanSons
                   (# 
                   do current[] -> this(scanArgs).current[]; 
                      isLast -> this(scanArgs).isLast;
                      inner scanArgs
                   #)
               if);
            #);
          hasVirtualArgs: BooleanValue
            (#
            do L:
                 scanArgs
                 (# D: ^Decl 
                 do current[] -> D[];
                    (if nameListInDcl then D.asDecl -> D[] if);
                    (if D.isVirtual then
                        true -> value;
                        leave L
                    if)
                 #);
            #)
       enter isKeyword
       #);
     DataItem:< SuperDataItem
