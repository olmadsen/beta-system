               (if (origin[] -> org[]) <> none then org.display if)
            #);
          localSearch:<
            (# CS: ^ Text; trySig: @boolean; ATd: ^Decl; pn: @integer
            enter(CS[],trySig)
            do(* 'localSearch: ' -> puttext; CS[] -> puttext;
