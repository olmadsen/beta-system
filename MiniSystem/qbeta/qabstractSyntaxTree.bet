                 (#
                 do (if true
                     // '^' -> ptnID.T.equal then '^' -> op[]
                     // '*' -> ptnID.T.equal then '*' -> op[]
                     // '/' -> ptnID.T.equal then '/' -> op[]
                     // '-' -> ptnID.T.equal then '-' -> op[]
