            #)
       do inner 
       #);
     Properties:< Node
       (* Properties = [Lexem[Properties],Lexem[Properties],...]
        * where Lexem[Properties] may be juts Lexem, ie no arguments
        *)
       (# Label:: (# do 'Properties' -> lab[] #);
          PP:: (# do Label -> lx.append #);
          PT::
            (# l: @integer; PR: ^Property
            do (if (length -> l) > 0 then
                   (if false and (l > 25) then
                       '*** Properties: [' -> puttext; 
                       scansons
                       (#
                       do current[] -> PR[]; PR.T[] -> puttext;
                          (if not isLast then ',' -> put if)#);
                       ']' -> put;
                       ' width: '->puttext; 
                       lineWidth -> putint;
                       ' length: ' -> puttext; l -> putint;
                       ' break: ' -> puttext; break -> putBoolean; newline;
                   if);
                   '['->lx.put;
                   scanSons
                   (#
                   do (pos,break) -> current.PT; 
                      (if not isLast then ','->lx.put if) 
                   #);
                   ']'->lx.put;
                   (pos,break) -> mkBreak
               if)
            #);
          length:: 
            (# 
            do scanSons
               (#
               do current.length + value -> value;
                  (if not isLast then value + 1 -> value if) (* ',' *)
               #);
               (if value > 0 then value + 2 -> value if) (* [...] *)
            #);
          scanProps:
            (# currentProp: ^Property
            do scanSons
               (#
               do current[] -> currentProp[];
                  inner scanProps
               #)
            #);
          handleProps: booleanValue
            (#
            do scanProps
               (#
               do L:
                    (if true
                     // 'basic' -> currentProp.T.equalNCS then
                        currentProp[] -> basicProp[];
                        basicProp.getPrimNo -> basicProp.primNo;
                        true -> value;
                        leave L
                     // 'external' -> currentProp.T.equalNCS then
                        currentProp[] -> basicProp[];
                        basicProp.getPrimNo -> basicProp.externalNo;
                        false -> value; (* OBS - clean-up*)
                     // 'resume' -> currentProp.T.equalNCS then
                        currentProp[] -> basicProp[];
                        resume_Prim -> basicProp.primNo;
                        (if resumeDecl <> none then
                            'Resume pattern: ' -> puttext; 
                            resume_prim -> putint; ' ' -> put;
                            doPT -> putline;
                         else
                            'resumeDecl none' -> putline
                        if);
                     // 'core' -> currentProp.T.equalNCS then
                        currentProp[] -> basicProp[];
                        basicProp.getPrimNo -> basicProp.coreNo;
                        false -> value; (* OBS - clean-up*)
                        leave L 
                     // 'globals' -> currentProp.T.equalNCS then
                        currentProp.arg[] -> globals[]
                        
                     // 'arguments' -> currentProp.T.equalNCS 
                     // 'interface' -> currentProp.T.equalNCS 
                     // 'signature' -> currentProp.T.equalNCS 
                     // 'kind' -> currentProp.T.equalNCS then
                     // 'abstract' -> currentProp.T.equalNCS 
                     // 'immutable' -> currentProp.T.equalNCS then
                        
                     // 'OSDvisibility' -> currentProp.T.equalNCS then
                        (if traceProps then
                            '\n*** OSDvisibility: ' -> puttext;
                        if);
                        (if currentProp.arg[] <> none then
                            currentProp.arg.scanSons
                            (# prop: ^Property
                            do current[] -> prop[];
                               (if true
                                // ('Disguised' -> prop.T.equalNCS) then
                                   OSDvisibility_Disguised -> OSDvisibility
                                // ('SkipInternal' -> prop.T.equalNCS) then
                                   OSDvisibility_SkipInternal -> OSDvisibility
                                // ('Show' -> prop.T.equalNCS) then
                                   OSDvisibility_open -> OSDvisibility
                                else
                                   '\n**** Unkown value for OSDvisibility: ' 
                                     -> puttext;
                                   prop.T[] -> putline
                               if);
                               (if traceProps then
                                   prop.T[] -> putline;
                                   dopt -> putline;
                               if)
                        #)if)
                     else
                        '\n**** Unknown property: ' -> puttext;
                        currentProp.T[] -> putline
                    if)
               #)
            #);         
          basicProp: ^Property;
          globals: ^Properties;
          inSub: @boolean;
          OSDvisibility: @integer
       do
       #);
     Property:< Lexem
       (# Label:: (# do 'Property:' -> lab[]; T[] -> lab.append #);
          PP:: (# do label -> lx.append #);
          PT:: 
            (# 
            do T[] -> lx.puttext;
               (if arg.length > 0 then (pos,break) -> arg.PT if) 
            #);
          length:: 
            (# 
            do (if T[] <> none then
                   T.length -> value
               if); 
               (if arg[] <> none then arg.length + value -> value if) 
            #);
          (*P: ^Lexem;*)
          arg: ^Properties;
          
          getPrimNo:
            (# V: ^Const; res: @integer
            do L:
                 (if arg[] <> none then
                     arg.ScanSons
                     (# prp: ^Property
                     do current[] -> prp[];
                        prp.T[] ->  Const -> V[];                   
                        V.T.setPos; V.T.getInt -> res;
                        leave L
                     #)
                 if)
            exit res
            #);
          primNo,coreNo,externalNo: @integer; (* clean-up here *)
       #);
     Include:< Item
       (# PT:: 
            (# 
            do '@include ' -> lx.append;
