                        else
                           (*(if isAdr then loadAdr if);*)
                           cd.rswap;
                           (nameOfE_OG,off) -> cd.rstoreg
                   if)if)if)
            #);
          toString::
            (# 
            do ':Adr:'-> T.append; 
               'off:'-> T.puttext; off -> T.putint;
