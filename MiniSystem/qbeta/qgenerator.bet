     FunctionalPattern::
       (# gen:: (# do (sig.id[],sig[],OD[],OD.super[]) -> objTmpStack.add #);
          pushThis:: (# do cd.pushThis #);
          loadArgs::
            (# args: [6]^ObjectGenerator; top: @integer;
            do E.args.scanArgs
               (#
               do (if (top+1 -> top) > args.range then 
                      args.range -> args.extend 
                  if);
                  current[] -> args[top][];
               #);
               sig.args.scanArgs
               (# D: ^Decl; i: @integer; OG: ^ObjectGenerator; name: ^text;
               do i + 1 -> i; 
                  current[] -> D[]; 
                  (if D.isVirtual then
                      D.sig.id.copy -> name[];
                      '#SB#' -> name.append;
                      (args[i].desc).descno -> name.putint;
                      (name[],none,args[i][],args[i].super[]) 
                        -> ObjTmpStack.add;
                      E.father.father[] -> OG[]; (* need OG.descNo for VDT *)
                      (D.off,(args[i].desc).descno,(OG.desc).descNo) 
                        -> cd.vdtTableAt
                   else
                      (true,superAdj,0) -> args[i].gen
                  if);
               #);               
            #);          
          load:: 
            (# 
            do ('FunctionalPattern:load: ',sig.doPT) -> cd.com2;
               (if primNo = indexed_prim then
                   '\n***FP:invoke indexed'-> putline
               if);
               (sig.id[],OD.IS.descno,staticOff,false) -> cd.invoke;
               (if not useReturnValue then
                   sig.popReturn
               if);               
            #);
       #);
     BasicFunctionalPattern::
       (# gen:: 
            (# 
            do (if primNo 
                // indexed_prim // string_prim then
                   (sig.id[],sig[],OD[],OD.super[]) -> objTmpStack.add 
                // resume_prim then
                   ('Resume','D') -> cd.call;
               if)
            #);
          pushThis:: (# do (if primNo = indexed_prim then cd.pushThis if) #);
          loadArgs::
            (#
            do E.args.scanArgs
               (#
               do (true,superAdj,0) -> current.gen 
               #)
            #);
          load:: 
            (# 
            do (if OD.primNo
                // integer_prim then
                // put_prim then 
                   ('put',OD.primNo) -> cd.Callprim
                // attach_prim then
                   ('attach',OD.primNo) -> cd.callPrim;
                   cd.rPop;
                // cmpAndSwap_prim then
                   (# I: ^Invocation
                   do E.father -> I[];
                      L:
                        I.scanSons
                        (# R: ^Exp
                        do current[] -> R[];
                           cd.swap;
                           ('cmpAndSwap',OD.primNo ) -> cd.callPrim;
                           leave L
                        #)
                   #)
                // fork_prim then
                   ('fork',OD.primNo ) -> cd.callPrim
                // sleep_prim then
                   ('sleep',OD.primNo ) -> cd.callPrim;
                // indexed_prim then
                   (*100 -> cd.pushConst; -- range - pushed as argument *)
                   0 -> cd.pushConst;   (* isRindexed *)
                   OD.IS.rsize -> cd.pushConst; (* dinx *)
                   ('StaticRep',OD.IS.descNo,false (*OD.isRindexed*),true) 
                     -> cd.allocIndexed;
                   cd.pushThis;
                   ('indexed',staticOff) -> cd.rstoreg
               if)
            #);
       #);
     KeyWordPattern::
       (# gen:: (# do (sig.id[],sig[],OD[],OD.super[]) -> objTmpStack.add #);
          pushThis:: (# do cd.pushThis #);
          loadArgs::
            (# args: [6]^ObjectGenerator; top: @integer;
            do ('KWP:loadArgs:',doPT) -> cd.com2;
               E.args.scanArgs
               (#
               do (if (top+1 -> top) > args.range then 
                      args.range -> args.extend 
                  if);
                  current[] -> args[top][];
                  ('KWP:Arg:',current.doPT) -> cd.com2;
               #);
               sig.args.scanArgs
               (# D: ^Decl; i: @integer; OG: ^ObjectGenerator; name: ^text;
               do i + 1 -> i; 
                  current[] -> D[]; 
                  (if D.isVirtual then
                      D.sig.id.copy -> name[];
                      '#SB#' -> name.append;
                      (args[i].desc).descno -> name.putint;
                      (name[],none,args[i][],args[i].super[]) 
                        -> ObjTmpStack.add;
                      E.father.father[] -> OG[]; (* need OG.descNo for VDT *)
                      (D.off,(args[i].desc).descno,(OG.desc).descNo) 
                        -> cd.vdtTableAt
                   else
                      (true,superAdj,0) -> args[i].gen
                  if);
               #);               
            #);
          load::
            (# args: [6]^ObjectGenerator; top: @integer;
               hasVirtualArgs: @boolean
            do (* 'KWP:load: ' -> puttext; sig.dopt -> putline;*)
               E.args.scanArgs
               (# 
               do (*'Actual arg: ' -> puttext; current.doP -> putline;*)
                  (if (top+1 -> top) > args.range then 
                      args.range -> args.extend 
                  if);
                  current[] -> args[top][]
               #);
               sig.args.scanArgs
               (# D: ^Decl; i: @integer
               do i + 1 -> i;
                  current[] -> D[]; (*D.doP -> putline;*)
                  (if D.isVirtual then
                      true -> hasVirtualArgs
                   else
                      args[i].gen
                  if);
               #);
               (if hasVirtualArgs then 
                   '\n>>> Has Virtual arguments'->putline;
                   (* father.father[] -> OG[];
                    ('KeyWord',none,OG[],OG.super[]) -> ObjTmpStack.add*)
                else
                   (sig.id[],OD.IS.descno,staticOff,false) -> cd.invoke;
                   (if not useReturnValue then
                       sig.popReturn
                   if)
               if)               
            #)
       #);
     BasicKeyWordPattern::
       (# pushThis::
            (#
            do ('BasicKeyWordPattern:pushThis',doPT) -> cd.com2;
               (if OD.primNo
                // indexedGet_prim (* not tested*)
                // indexedGet_prim (* ditto *)
                // stringGet_prim then
                   cd.pushThis
               if)                   
            #);
          load::
            (#  exitLab: @labelHandler.label
            do (if OD.primNo
                // ifthen_prim then 'BKP:'->cd.comment; doPT -> cd.comment;
                   exitLab.new;                   
                   E.args.scanSons
                   (# N: @integer
                   do N + 1 -> N;
                      (if N
                       // 1 // 3 then
                       // 2 then 
                          (true,0,staticOff) -> current.gen;
                          exitlab -> cd.jmpFalse;
                       // 4 then
                          current.gen;
                          exitLab -> cd.defLab;
                       else
                          'load:%if%then: too many arguments' -> putline;
                          E.doPt -> putline
                      if)
                   #);   
                // this_prim then
                   (* Must check that argument is the name of an enclosing 
                    * object or the name of the superpattern of an enclosing
                    * object 
                    *)
                   E.args.scanArgs
                   (# A: ^ObjectGenerator; org: ^Items
                   do current[] -> A[]; 
                      cd.pushThis;
                      A.super.on - 1 -> (E.origin[]->org[]).goOrigin; 
                   #);
                // inner_prim then
                   E.args.scanArgs
                   (# A: ^ObjectGenerator; org: ^Items
                   do current[] -> A[]; 
                      cd.pushThis;
                      A.super.on - 1 -> (E.origin[]->org[]).goOrigin -> org[]; 
                      org.innerInx -> cd.innerP; 
                      cd.rpopThisObj
                   #)
                // indexedPut_prim then
                   (* scan args and push index and element *)
                   E.args.scanSons(#do (true,0,staticOff) -> current.gen #);
                   '%put%at!' -> cd.comment;
                   ('inx',1) -> cd.xstoreg
                // indexedGet_prim // stringGet_prim then
                    E.args.scanSons(#do (true,0,staticOff) -> current.gen #);
                   '%get' -> cd.comment;
                   ('inx',1) -> cd.xpushg
               if);
            #);
          loadArgs:: 
            (# 
            do 'BasicKW:loadArgs'->cd.comment;
               E.args.scanSons(# do  (true,superAdj,0) -> current.gen #)
            #)
       #);            
     Invocation::
       (* E1.E2.E3
        *    E1.pushThis
        *    E1.on -> gorigin
        *    E1.pushOff
        *    E2.pushOff
        *    E3.execute
        * R.foo
        * R.foo{...}
        * X:= y + 12
        * R ++ E  -- problem E1 = R ++ E , i.e. first = true 
        * perhaps same problem for keyWord
        * We must reconsider parsing/AST and/or gen below
        *)
       (# gen:: 
            (# rec,E: ^Exp; T: ^Text
            do cd.bcPos -> beginBCP;
               (if false then
               L:
                 scanSons 
                 (#
                 do current[] -> E[];
                    'Invocation:gen:pushThis' -> cd.comment;
                    E.pushThis;
                    (* cd.pushThis does not work, 
                     * E may be a constant or basic pattern,
                     * where pushThis shall not be generated
                     *)
                    leave L
                 #);
               if);
               ('Invocation:gen:A',doPT) -> cd.com2;
               (useReturnValue,staticOff,superAdj) -> loadOrigin -> (rec[],E[]);
               
               'Invocation:gen:B:' -> T[]; superAdj -> T.putint; ':' -> T.put;
               (T[],doPT) -> cd.com2;
               (rec[],useReturnValue,staticOff,superAdj) -> E.execute;
               cd.bcPos -> endBCP
            #);
          loadOrigin:
            (# usereturnValue: @boolean;
               staticOff,superAdj: @integer;
               rec,E,nE: ^Exp;
            enter(useReturnValue,staticOff,superAdj)
            do
               scanSons
               (# Ex: ^Exp
               do current[] -> E[]; 
                  (*current.doP -> cd.comment;*)
                  (if isFirst then 
                      E.pushThis;
                      superAdj -> E.loadOrigin 
                  if); 
                  (if not isLast then
                      E[] -> rec[];
                      current.doP -> cd.comment;
                      (if not (next -> nE[]).isAssign then
                          ('next not assign:',(next).doP) -> cd.com2;
                          (next).doP -> cd.comment;
                          E.doPT -> cd.comment;
                          next -> Ex[];
                          Ex.doPT -> cd.comment;
                          (rec[],useReturnValue,staticOff,superAdj,Ex[]) -> E.pushOff (* Often calls execute *)
                      if);                      
                  if);
               #)
            exit(rec[],E[])
            #);
       #);
     adjustOrigin:
       (# originOff: @integer
       enter originOff
       do 'AdjustOrigin' -> cd.comment;
          (if originOff > 0 then
              ('origin',originOff) -> cd.rpushg
          if)
       #);
     ObjectInvocation::
       (* May be 
        *  A simple variable X declared as X:@integer; 
        *  An object R declared as R: @Foo
        *  A pattern Foo declared as Foo: Bar{ ... }
        *  ATd = operator.Decl
        *)
       (# pushThis:: (# do cd.bcPos -> beginBCP; ATd.pushThis #);; 
          loadOrigin::
            (# org: ^Items
            do ('ObjectInvocation:loadOrigin:A',doPT) -> cd.com2;
               (if ATD.primNo
                // 0 
                // indexed_prim 
                // string_prim
                // stringGet_prim // indexedGet_prim then 
                   superAdj -> AdjustOrigin;
                   on -> (origin[]->org[]).goOrigin; 
                // this_prim then
                   (* AD hoc and clumsy !*)
                   ('ObjectInvocation:loadOrigin:this:',doPT) -> cd.com2;
                   (this(Exp)[],true,0) -> ATd.load
                // thisCore_prim then 
                   ('thisCore',thisCore_prim) -> cd.callPrim
               if)
            #);
          pushOff:: 
            (# 
            do (rec[],useReturnValue,staticOff,superAdj,next[]) -> ATd.pushOff 
            #);
          loadArgs:: 
            (# 
            do (if (*(true and not isAssign) or*) (type ## = KeyWord##) then
                   ('OI:loadArgs:',doPT) -> cd.com2;
                   (this(Exp)[],superAdj) -> ATd.loadArgs
                else
                   args.scanSons(# do (true,superAdj,0) -> current.gen #) 
               if)
            #);
          execute:: 
            (# doBreak:
                 (# OD: ^Items; E : ^ObjectGenerator; on: @integer;
                    primNo: @integer
                 enter primNo
                 do args.scanSons
                    (# isArg: @boolean
                    do (* current.doP -> puttext;  ' ' -> put; *)
                       (if isArg then
                           current[] -> E[];
                           E.super.on -> on;
                           (*  on -> putint; ' ' -> put *)
                       if);
                       noT isarg -> isarg
                    #);
                    (* ' on:' -> puttext; on -> putint; newline;*)
                    origin[] -> OD[];
                    (for i: ON - 1 repeat 
                         (if OD.basicNo = ifthen_prim then OD.origin[] -> OD[] if);
                         OD.origin[] -> OD[] 
                    for);
                    (if OD.basicNo = ifthen_prim then OD.origin[] -> OD[] if);
                    (*OD.doP -> putline;
                     ' descNo:' -> puttext; OD.descNo -> putint;*)
                    (if primNo
                     // restart_prim then 
                        (if on = 1 then
                            OD.restartLab -> cd.jmp
                         else
                            (on - 1, OD.restartLab) -> cd.break
                        if)
                     // leave_prim then 
                        (if on = 1 then
                            OD.leaveLab -> cd.jmp
                         else
                            (on - 1, OD.leaveLab) -> cd.break
                        if)
                    if)
                 #);
               T: @text
            do (if isAssign then
                  superAdj -> loadArgs;
                   rec.ATd.store;
                else 
                   (if ATd.primNo
                    // restart_Prim then
                       restart_Prim -> doBreak
                    // leave_Prim then 
                       leave_Prim -> doBreak
                    // this_prim then
                       ('ObjectInvocation:execute',doPT) -> cd.com2;
                    // asString_prim then
                       ('asString_prim:',doPT) -> cd.com2;
                       ('asString',ATd.primNo) -> cd.callPrim
                    // thisCore_prim then
                       (* Currently we dont come here with thisCore.foo
                        * Perhaps with myCore := thisCore?
                        *)
                       ('ObjectInvocation:execute:thisCore:',doPT) -> cd.com2;
                    else 
                       ('OI:execute:', dopt) -> cd.com2;
                       (if (rec[] <> none) 
                           and (rec.ATD## <= DataItem##)
                           and (rec.ATd.getQual).isSimpleValue 
                           and (ATd.primno <> cmpAndSwap_prim) then 
                           rec.ATd.primNo -> T.putint;
                           ('Binary:load rec:primNo:',T[]) ->cd.com2;
                           (rec.ATd.doPT,(rec.ATd.getQual).doPT) -> cd.com2;
                           (this(Exp)[],useReturnValue,staticOff) -> rec.ATd.load
                       if);
                       'OI:exe:before:loadArgs'->cd.comment;
                       (if (*true or*) (type## <> Keyword##) then 
                           superAdj -> loadArgs
                       if);
                       'OI:exe:after:loadArgs'->cd.comment;
                       (this(Exp)[],useReturnValue,staticOff) -> ATd.load;
                       'OI:exe:after:load'->cd.comment;
               if)if);
               cd.bcPos -> endBCP;
            #);
       #);       
     BracketedExp::
       (* a := b + (c * d)
        *     execute c * d
        * (foo).x
        *    loadOrigin foo
        * (foo(e1,e2)).x
        *    execute foo(e1,e29?
        * (%this P).suspend
        *     loadOrigin?
        *)
       (# gen:: 
            (# 
            do ('BE:gen:',doPT) -> cd.com2;
               (*(useReturnValue,superAdj,staticOff) -> IV.gen *)
            #);
          loadOrigin:: 
            (# 
            do ('BE:loadOrigin:',doPT) -> cd.com2;
               (* OBS! useReturnValue, staticOff must be handled!
                *)
               (false,0,superAdj)  -> IV.loadOrigin 
            #);
          pushThis:: 
            (# 
            do ('BE:pushThis',doPT) -> cd.com2; (* IV.pushThis* ) 
            #);
          loadArgs:: 
            (# 
            do ('BE:loadArgs!',doPT) -> cd.com2; (* superAdj -> IV.loadArgs *) 
            #);
          execute:: 
            (#
            do ('BE:execute',doPT) -> cd.com2;
               (*(rec[],useReturnValue,staticOff,superAdj) -> IV.execute *)
               (useReturnValue,0,staticOff) -> IV.gen
            #);
       #);
     Const::
       (# gen:: (# do execute #);
          pushOff:: (# do execute #);
          execute:: (# do T.setpos; T.getInt -> cd.pushConst #);
       #);
     CharObj::
       (# gen:: (* Never called? *) (# do execute #);
          pushOff:: (# do execute #);
          execute:: 
            (# ch: @char
            do (if T.length = 1 then
                   1 -> T.inxget -> cd.pushConst 
                else
                   (if  2 -> T.inxget -> ch
                    // 'n' then 10 -> cd.pushConst
                    else
                       'CharObj:Special symbol: ' -> puttext; ch -> putint;
                       ':' -> put; '"' -> put; ch -> put; '"' -> put;
                       newline;
                       ch -> cd.pushConst
                   if)
               if)
            #);
