ORIGIN '~beta/basiclib/betaenv'
---lib:attributes---
chdir: external
  (# dir: [0] @char; res: @integer
  enter dir
  do callC
  exit res
  #);
system: external
  (# cmd: [0] @char; res: @integer
  enter cmd
  do callC
  exit res
  #);
--- program:descriptor ---
(# WS: ^text; T: ^text;
   exe:
     (# cmd: ^text; T: ^text
     enter cmd[]
     do cmd[] -> (WS.copy).append -> T[]; T -> chdir;
        T[] -> putline;
        'pwd'->system;
        'qcompiler ' -> T[];
        cmd[] -> T.append;
        T -> system;
        '..' -> chdir;
     #);   
do 
   'c:/beta/r5.5/MiniSystem/qbeta/BETAworld/workspace/' -> WS[];
   'Testing:\n\t ' -> puttext;
   WS[] -> putline;
   WS -> chdir;
   'ls' -> system; 
   'TST' -> (WS.copy).append -> T[]; T -> chdir;
   'qcompiler TST' -> system;
   'demo/' -> WS.append;
   WS -> chdir;
   'Bank' -> exe;
   'ComplexEx' -> exe;
   'BinTree' -> exe;
   'CoArg' -> exe;
   'ContainerEx' -> exe;
   'Static' -> exe;
#)

   
