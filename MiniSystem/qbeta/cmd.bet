ORIGIN '~beta/basiclib/betaenv'
---lib:attributes---
chdir: external
  (# dir: [0] @char; res: @integer
  enter dir
  do callC
  exit res
  #);
system: external
  (# cmd: [0] @char; res: @integer
  enter cmd
  do callC
  exit res
  #);
_popen: external
  (# cmd,mode: [0]@char; res: @integer
  enter(cmd,mode)
  do callC
  exit res
  #);
fgetc: external
  (# F: @integer; ch: @char
  enter F
  do callC
  exit ch
  #);
--- program:descriptor ---
(# WS: ^text;
   exe:
     (# cmd,subPath,T: ^text; usingC: @boolean
     enter(cmd[],subPath[], usingC)
     do subPath[] -> (WS.copy).append -> T[]; 
        cmd[] -> (T.copy).append -> T[]; 
        T -> chdir;
        'qcompiler ' -> T[];
        (if usingC then ' -c ' -> T.append if);
        cmd[] -> T.append;
        T -> system;
        WS -> chdir;
     #); 
   T: [10000] @char; S: @text;
   FFF: @integer
do 
   'c:/beta/r5.5/MiniSystem/qbeta/BETAworld/workspace/' -> WS[];
   WS -> chdir;
   (if false then
       ('ls','r') -> _popen -> FFF;
       (for i: 200 repeat
            FFF -> fgetc -> put;
   for)if);
   ('TST','',false)-> exe;
   ('Bank','demo/',false) -> exe;
   ('ComplexEx','demo/',false) -> exe;
   ('BinTree','demo/',false) -> exe;
   ('CoArg','demo/',false) -> exe;
   ('ContainerEx','demo/',false) -> exe;
   ('Static','demo/',false) -> exe;
   ('ArrayExperiment','demo/ArrayDemos/',false) -> exe;
   ('EMC2arrayEx','demo/ArrayDemos/',false) -> exe;
   ('SimpleThreadEx','demo/',true) -> exe;
   ('ForkJoinEx','demo/',true) -> exe;
   ('MonitorDemo','demo/',true) -> exe;

#)

   
