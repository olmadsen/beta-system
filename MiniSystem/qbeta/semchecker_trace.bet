---sem_ptn_argAtInx:descriptor---
(#
do (if traceGetVqual or traceUnit or trx then 
       '**   argAtInx: ' -> puttext; inx -> putint;
       ' in:' -> puttext; dopt->puthead;
       '**   theOI: ' -> puttext; theOI.label -> puttext;
       ' ' -> put; theOI.father.label -> puttext; ' ' -> put;
       theOI.dopt -> puthead;
       theOI.father.father.label -> putline;
       theOI.father.father.dopt -> puthead;
       '**   fArg: ' -> puttext; fArgs[inx].dopt -> puthead;
       '**   aArg: ' -> puttext; current.dopt -> puthead;
       '**   label: ' -> puttext; current.label -> puttext; ' ' -> put;

       fArgs[inx].sig.dopt -> putline;
       '**   OG:' -> puttext; OG.dopt -> puthead;
       (# ATdx: ^Decl; pn: @integer
       do fArgs[inx].sig.dopt -> OG.IS.localSearch -> (ATdx[],pn);
          (if ATdx[] <> none then
              '**   pn=' -> puttext; pn -> putint; newline
       if)#);
       '**   encOG:' -> puttext; encOG.doPT -> puthead;
       (if (theOI.savedOGS[]->OGS[]) <> none then
           (if traceGetVqual or traceUnit then
               '**   savedOGS:'->puttext;
               (if theOI.savedOI[] <> none then 
                   theOI.savedOI.dopt -> puthead
                else
                   'noOI'->putline
               if);
               theOI.savedOGS.print
           if);
        else
           '**   savedOGS is none: '->puttext;
           theOI.dopt -> puthead;
   if)if)
#)
