ORIGIN '~beta/basiclib/betaenv';
INCLUDE '../compiler_IF/compiler_IF';
INCLUDE 'qCommonLexer'
---lib:attributes---
CommonAST: Tree
  (# Node: TopNode
       (# vNode::< Node;    
          isObjDesc:< booleanValue;

          getODorigin:: 
            (# ODx: ^Node;
            do origin[] -> ODx[];
               L:
                 (if not ODx.isObjDesc then 
                     ODx.getODorigin -> ODx[];
                     restart L
                 if);
               ODx[] -> OD[]
            #); 
       do inner
       #);
     CommonObjectDesc: Node
       (#
       #);
     LexerType:< CommonLexer;
     lex: ^LexerType
  do inner
  #)
