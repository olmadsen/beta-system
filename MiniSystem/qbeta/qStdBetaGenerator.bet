ORIGIN '~beta/basiclib/betaenv';
INCLUDE 'qStdBetaAllocator';
INCLUDE '../VM/objectcode'
---LIB:attributes---
qStdBetaGenerator: qStdBetaAllocator
  (# StdBetaModule::<
       (# gen:: 
            (# 
            do 'StdBetaModule:gen:'-> dumpT; moduleName.doPT->dumpTN;

            #)
       #);   
     AttSlot::<
       (# gen::
            (#
            do scanBindings(#do current.gen #)
            #);
       #);
     ObjDescSlot::<
       (# gen::
            (#
            do scanBindings(#do current.gen #)
            #);
       #);
     ObjectDesc::
       (# gen::
            (#
            do  'ObjectDesc:gen:'-> dumpT; 
               (if NS[] <> none then NS.doPT->dumpTN; else newline if)
       #)#);
     cd:  ^ObjectCode;
  enter cd[]
  do '**** generator ****'->dumptn;
     topModule.gen
  #);

