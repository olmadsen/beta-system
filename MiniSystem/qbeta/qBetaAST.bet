     (*lexemSy:: (# do N.lexemSy -> L[] #);*)
     isDataItem:< booleanValue(# do inner #);
     isPtn:< booleanValue(# do inner #);
     N: ^BetaLexem;
     isSimple,isForInx: @boolean;
     forNo: @integer;
     off: @integer
  enter N[] -> append
  #);
