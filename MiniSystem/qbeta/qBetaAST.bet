       (#
       do (pos,break) -> lE.PT; ' ' -> lx.put;
          op -> lex.symbols.asText -> lx.append;
          ' ' -> lx.put; (pos,break) -> rE.PT ;
       #);
     lE,rE: ^Expr; op: @integer
  enter(lE[]->append,op,rE[]->append) 
  #);
