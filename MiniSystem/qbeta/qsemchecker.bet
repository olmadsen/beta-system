            #);
          pathToDcl::
            (#
            do (if trace_PathToDcl then
                   '*** Module:pathToDcl:' -> puttext;
                   sig.doPT -> putline;
               if);
               (* we do come here, but what to do? *)
               (OG[],this(decl)[],none,0,0,P[]) -> OGpath -> P[]
