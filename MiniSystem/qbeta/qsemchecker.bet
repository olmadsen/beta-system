               if)
            #);      
          getActualReceiver::
            (#
            do (if (ATd.asDecl).primNo = this_prim then
                   '**** OI:getACtualReceiver:'->dumpT; dopt->putheadN;
                   args.scanSons
                   (# i: @integer; OG: ^ObjectCall
                   do (if (i + 1 -> i) = 2 then
                          (*'** primArg:'->puttext; current.doPT -> putline;*)
                          ((current[] -> OG[]).super).last -> rec[]
                      if)
                   #)
               if)
            #);
          getDecl::
