                        '**   PathToDclX:B:\n'->puttext; P.print;
                        (if trx then
                            '**   PathToDclX:kuk:'->puttext; E.on -> putint;
                            newline;
                            E.dopt -> putline;
                            E.Atd.dopt -> putline;
                            '**   PathToDclX:additional path:'->putline;
                            P.print;
                        if);
                        S[] -> P.getSeqAndQual -> (S[],qual[]);
                        (# Q: ^ObjectGenerator
                        do S[] -> P.getB -> Q[];
                           (if false and trx then
                               '****  PathToDclX:next:has:OUT:'->putline;
                               Q.dopt -> putline;
                               (if (Q.getReturnDecl) <> none then
