                           if)if)
                        #);                        
                        (if false and trx then
                            '**** final OGseq:n='->puttext; n -> putint;
                            newline;
                            S.print
                        if);
                    if);
                    (if n = i then 
                        true -> P.isNest;
                        '****  PathToDclX:FINAL:'->putline;
