                  UT[] -> mkBasevector  -> BV[];
                  (if false then
                      '**   parsed: ' -> puttext; true -> UT.print -> putline;
                      BV.print
                  if)
             for)
          #);
       #)
  do '**** SEMCHECK ****' -> putline;
     decodeUnits;
     (*main.defUnit;*)
     main (*rootModule*).semCheck;

     (if semanticErrors then
         '\n***** Semantic errors: compilation stops' -> putline
      else
         (if printHTMLfile then
             rootModule.emitHTML;
         if);
         inner;
         (if showUnits then rootmodule.printUnits if);
     if)
  #)
