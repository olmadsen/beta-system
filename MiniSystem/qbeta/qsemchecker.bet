                   ATd.origin.father[] -> OGsequence -> OGSx[];
                   OGS[] -> OGSx.pred[];
                   true -> OGSx.isNest;
                   OGSx[] -> OGS[];
                   (for pn repeat OGS.PRE -> OGS[] for);
               if)
            exit(encOG[],OGS[])
            #);
