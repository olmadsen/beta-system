                            '**** P.dcl:'->putline; P.dcl.dopt -> putline;
                            '**** P.vDcl: ' -> putline; P.vDcl.dopt -> putline;
                            '**** in PathToDclX'->putline; S.print;
                        if);
                        (if false then
                        (# Q: ^ObjectGenerator
                        do S[] -> P.getB -> Q[];
                           (if false and trx then
                               '****  PathToDclX:next:has:out:'->putline;
                               Q.dopt -> putline;
