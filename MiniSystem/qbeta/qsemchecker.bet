          getV_Qual::
            (* The primary entry to computing virtual bindings as part
             * of a qualification
             * Called from checker::ObjectInvocation:check
             * enter arg: encOG = enclosing ObjectInvocation
             * Cases:   R.foo where R: ref V and V:< T
             *                or R: ref W and W: { ... out R: ref V ... } 
             *          Q(e).bar where Q: { ... out R: ref V ...}
             *)
            (# Px,Py: ^OGpath; DI: ^DataItem; MI: ^ModuleItem; ptn: ^Pattern;
               INV: ^Invocation; rN: @integer; usePDX: @boolean;
               trace: (#exit false #);
            do(if (trx) or traceGetV_qual then
                   '**** ObjectInvocation:getV_qual: ' -> puttext;
                   dopt -> putline;
                   '**   ATd: ' -> puttext; ATd.sig.doPT -> putline;
                   (if trx then
                       '**   EncOG: ' -> putline;
                       (if encOG[] <> none then encOG.doPT -> puthead if)
                   if)
               if);
               (if (ATd.getV_qual -> qual[]) = none then
                   (* qual[] <> none: no virtuals involved and qua is correct
                    * qual[] = none: a virtual is involed: compute most
                    * specific bind as shown below
                    *)
                   L:
                   (if true and ('Invocation' -> (father.label).equal) then
                       (father[] -> INV[]).scanSons
                       (# 
                       do rN + 1 -> rN; 
                          (if current[] = this(ObjectInvocation)[] then
                              (if trx then
                                  'Bingo! i='->puttext; rN -> putint; 
                                  ' ' -> puttext; dopt -> putline
                              if);
                              (if (rN > 1) then
                                  true -> usePDX;
                                  leave L;
                              if);
                   if)#)if);
                   (if useNewVclass and usePDx and (rN > 1) then
                       (if true->trx then
                           '**** usePDX: ' -> putline;
                           INV.dopt -> putline;
                           dopt -> putline;
                       if);
                       (none,rN) -> INV.pathToDclX -> qual[];
                    else                       
                       (encOG[],ATd[],this(ObjectInvocation)[],on,pn,none) 
                         -> OGpath 
                         -> ATd.pathToDcl 
                         -> Px[];
                       (if trx or traceGetV_qual then
                           '*** ObjectInvocation: found path:'->putline;
                           Px.print;
                       if);
                       Px.findQual -> qual[];
                   if)
               if);
               (if trx and traceGetV_qual then
                   '*** ObjectInvocation:QUAL:OF: "'->puttext;
                   dopt -> puttext; '" IS: '->putline; 
                   (if qual[] <> none then
                       qual.doPT -> putline; 
                    else
                       'none'->putline
               if)if);
            #);;
