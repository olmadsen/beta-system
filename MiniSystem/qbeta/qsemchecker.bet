                        -> current.semCheck 
                        -> srcQual[];
                      (*current.getQual -> srcQual[];*)
                      (if false and ((srcQual.getUnitPropShort -> U[]) <> none) then
                          '**** srcQual: ' -> putline; srcQual.dopt -> putline;
                          '**   unit: ' -> puttext; U[] -> putline;
                          current.doPP -> putline;
                          floatDecl.OG[] -> srcQual[]
                      if);
                      receiver.getQual -> dstQual[];
                      (if trx  then
