ORIGIN '~beta/basiclib/betaenv';
INCLUDE 'qStdBetaChecker';
---LIB:attributes---
qStdBetaAllocator: qStdBetaChecker
  (#
     StdBetaModule::<
       (# alloc:: 
            (# 
            do 'StdBetaModule:alloc:'-> dumpT; moduleName.doPT->dumpTN;
            #)
       #);
     BetaFragments::<
       (# alloc:: 
            (# 
            do 'BetaFragments:alloc:'-> dumpT;
            #)
       #);
     ObjectDesc::<
       (# alloc:: 
            (# 
            do 'ObjectDesc:alloc:'-> dumpTN;
            #)
       #);
     BetaDecl::<
       (# alloc:: 
            (# 
            do 'BetaDecl:alloc:'-> dumpT; dopt -> dumpTN;
            #);
       #);
     StaticItem::<
       (#  alloc:: 
            (# 
            do 'StaticItem:alloc:'-> dumpTN;
            #);
          isValItem::
            (#
            do 1 -> size
            #); 
       #);
     DynamicItem::<
       (#  alloc:: 
            (# 
            do 'DynamicItem:alloc:'-> dumpTN;
            #);
          isValItem::
            (#
            do 'DynamicItem:isValItem:'->dumpt; 1 -> size
            #)
       #);
     BetaNameDecl::<
       (# alloc:: 
            (# S: @integer
            do 'BetaNameDecl:alloc:'-> dumpT; dopt -> dumpt;
               (if (theDcl.IT.isValItem -> S) > 0 then
                   S -> putint;
                else '???? '->dumpt;
               if);
               newline
            #);
       #);
     
  do '\n**** Alloc ****'->putline;
     topModule.alloc;
  #);
          
