Functional: =
--   %include LIB.Containers.List

   IntToIntFunction(i: ?integer) -> r: ?integer:
      %inner
      
   Fib: IntToIntFunction
      %if ((i=0) ||(i=1)) %then% r:= i %else%
         r:= Fib(i-1)+Fib(i-2)
	 
	    
   Square: IntToIntFunction
      r:= i*i
      

--   MapList(f:<IntToIntFunction, l:?List) -> r: ?List:
     
   "Fib(7)=".print
   putint(Fib(7))
   newline

   "Fib(2*2)=".print
   putint(Fib(Square(2)))
   newline



