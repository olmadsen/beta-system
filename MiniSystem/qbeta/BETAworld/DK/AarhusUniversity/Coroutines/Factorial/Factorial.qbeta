Factorial: obj      
   PlainFactorial:  obj
        V: var integer
        N: var integer
        V := 1	
        cycle
           PlainFactorial.suspend
           N := N + 1
           V := V * N
   RecursiveFactorial: obj
      V: var integer
      N: var integer
      next:
         RecursiveFactorial.suspend
         N := N + 1
         V := V * N
         next
      V := 1
      next
   "PlainFactorial    : ".print
   for (1) :to (5) :repeat
      PlainFactorial.call
      putint(PlainFactorial.V)
      put(',')
   newline
   "RecursiveFactorial: ".print   
   for (1) :to (5) :repeat
       RecursiveFactorial.call
       putint(RecursiveFactorial.V)
       put(',')