SimpleEx: obj
   Fisk:
        ch: var char
   TFP:
      in ch: var char
      in R: ref Fisk
      X: var integer
      TF.suspend;
      X := 97
      put(X)
      put(ch)
      TF.suspend
      put(ch)
      X := X + 1
      put(',')
      put(X);
      TF.suspend
   TF: obj TFP('Q',Fisk) 
   TF('q',Fisk)
   put('#')
   TF.resume('w',Fisk)
   TF.resume('e',Fisk)
   TF.resume('r',Fisk)
   newline
