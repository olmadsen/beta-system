expAst: obj
   Top:
      print:<
         inner(print)
      isEmpty:<
         out B: var Boolean
         inner(isEmpty)	 
      inner(Top)
      eval:<
          out V: var integer
          inner(eval)
   Node(label: var char,left: ref top,right: ref Top): Top
      print::
         newline
         for (1) :to (ind):repeat
             put(' ')
         ind := ind + 1	     
         put('(')
         put(label)
         left.print
         put(',')
         right.print
         put(')')
         ind := ind - 1
   expNode: Node
      eval::
         V := left.eval + right.eval
   termNode: Node
      eval::
         V := left.eval * right.eval
   numberNode: Node
      eval::
         if (right.isEmpty) :then
             V := left.eval
         :else
             V := (left.eval * 10) + right.eval
   digitNode: Node
      eval::
         V := label - 48
   empty: top
      isEmpty::
         B := true         
      eval::
         "-:".print
         V := 0      
   ind: var integer      