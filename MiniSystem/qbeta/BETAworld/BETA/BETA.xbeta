      %globals inSub --,OSDvisibility[disguised]]
      
      --Declaration of if then else control pattern
      
      in if: var Boolean
      in then:< Object
      in else:< Object
      if_L: obj 
         @ifX if :thenX -- problem when @if if :then ... !!!
            then
            @leave if_L
         else
   cycle: 
      %globals inSub
