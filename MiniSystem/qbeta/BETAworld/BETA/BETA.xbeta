   @for: 
      %globals inSub 
      in for: var integer
      in to: var integer
      in do:< Object
      %public      
      inx: var integer
      %private
      doIt: 
         loop: obj 
            @if inx <= to :then 
               do
               inx := inx + 1
               @restart loop
      inx := for
      doIt
      @inner for
   switch:
      in V: var integer
      %public
      case: 
         in x: ref Indexed(1,#Integer)
         @for 1 :to x.length :do	 
            @if (V = x.@get inx) :then 
               @inner case
               @leave switch
      @inner switch
   OLDswitch: 
      in V: var integer
      %public
