   putFloatX:
      %globals
      -- A poor mans pattern for printing a float - just for testing
      in V: var Float
      N: var integer
      X: var float
      Y: var Float
      afterDot:
         lz: var integer
         rest: var float
         Irest: var integer
         ch: var char
         Y := N
         rest:= V - Y
         @for 1 :to 4 :do
            rest := rest * 10.0
            Irest := rest
            @if Irest = 0 :then
                lz := lz + 1
         @for 1 :to lz :do put('0')
         S: ref LIB.String.String
         ix: var integer
         S := LIB.String.I2s(Irest)
         elimTrailingBlanks:
            @for 1 :to S.length :do
               ch := S.@get (S.length - inx + 1)
               @if (ch > '0') || (inx = S.length)  :then
                   ix := S.length - inx + 1
                   @leave elimTrailingBlanks
         elimTrailingBlanks
         --putint(Irest)
         --put(':')
         @for 1 :to ix :do
              put(S.@get inx)  
      N := V
      putint(N)
      put('.')
      afterDot      
