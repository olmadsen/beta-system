Volume: obj Dimension
   %dimension "V"
   PrimaryUnit:: CubicMeter
   CubicMeter: Unit
      %unit "m3", "m*m*m"
      thisUnit:: CubicMeter
      toPrimary:: 
         base.magnitude := magnitude
      fromPrimary:: 
         magnitude := base.magnitude
      print:: 
         "cubicMeter".print