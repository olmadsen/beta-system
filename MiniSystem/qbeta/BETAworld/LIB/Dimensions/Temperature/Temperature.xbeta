Temperature: obj Dimension
   Celsius: Unit
      thisUnit:: Celsius
      toBase:: 
         base := magnitude
      fromBase:: 
         magnitude := base
      print:: 
         "Celsius".print
   Fahrenheit: Unit
      thisUnit:: Fahrenheit
      toBase:: 
         base := magnitude * 18 + 32
      fromBase:: 
         base := magnitude * (9 // 5) + 32
      print:: 
         "Fahrenheit".print