Time: obj Dimension -- 5: 
   %Public
   Second: Unit
      %unit "s"
      %public
      thisUnit:: Second
      print::
         "Second".print      
   Minutes: Unit
      %unit "min", 60
      %Public
      thisUnit:: Minutes
      toBase:: 
         base := magnitude / 60
      fromBase:: 
         magnitude := base * 60
      print::
         "Minutes".print
   F2M: 
      in V: var float
      out R: var Minutes
      R.magnitude := V