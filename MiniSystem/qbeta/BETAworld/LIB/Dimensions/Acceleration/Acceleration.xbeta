Acceleration: obj Dimension
     MperMin2: Unit
        thisUnit:: MperMin2
        print::
           "MperMin2".print
     div:
        in L: var Length.Meter
        in T: var Time.Minutes
        out R: var MperMin2
        R.magnitude := L.magnitude / T.magnitude
     FasMperM2:
        in V: var integer
        out R: var MperMin2
        R.magnitude := V        
     shortName::
        T := "A"
