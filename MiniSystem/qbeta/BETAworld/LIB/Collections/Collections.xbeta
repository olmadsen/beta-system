Collections: obj 
   Collection:
      %id "Collection"
      in elm:< Object
      scan:<
         current: ref elm
         inner(scan)
      insert:<
        in e: ref elm
        inner(insert)
      has:<
         in E: ref elm
         out B: var boolean
         inner(has)
      remove:<
         in E: ref elm
         inner(remove)
      inner(Collection)
