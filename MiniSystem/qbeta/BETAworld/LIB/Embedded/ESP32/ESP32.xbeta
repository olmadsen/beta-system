ESP32: obj 
   pinMode: 
      [external[1]]
      in pn: var integer
      in mode: var Boolean
   digitalWrite:  -- external function
      [external[2]]
      in pn: var integer
      in state: var Boolean
   delay: 
      in t: var integer
      t := t - 1
      %if 0 < t 
      %then% 
         %restart delay