Units: =
   Watt(W: ?integer):
      + V: ?integer -> R: ?integer:
        R := W + V
      := X: ?Watt:
         W := X.W
   Volt(V: ?integer):
      * X: ?Ampere -> W: ?Object:
        put('E')
        W := Watt(0)
        W := V * X.A
        put('R')
   Ampere(A: ?Integer):
      d: ?integer
   
   W: =Watt(100)
   V: =Volt(220)
   A: =Ampere(10)
   put('Q')
   W := V * A
   put('W')
   putint(W.W)
   newline
