Complex: =
   Complex(re: ?integer, im: ?integer): 
      --[immutable]
      + C: ?Complex -> res: ?Complex:
         res := Complex(re + C.re,im + C.im)
      - C: ?Complex -> res:  ?Complex:
         res := Complex(re + C.re,im + C.im)
      display:
         put('C')
         putint(re)
         put(',')
         putint(im)
         put(10)



