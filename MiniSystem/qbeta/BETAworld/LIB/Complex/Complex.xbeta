Complex: obj
   %domain_boundary
   %Public
   Complex: 
      --[immutable]
      in re: var integer
      in im: var integer
      %Public
      + : 
         in C: ref Complex
         out res: ref Complex
         res := Complex(re + C.re,im + C.im)
      - : 
         in C: ref Complex
         out res: ref Complex
         res := Complex(re + C.re,im + C.im)
      display: 
         put('C')
         putint(re)
         put(',')
         putint(im)
         put(10)