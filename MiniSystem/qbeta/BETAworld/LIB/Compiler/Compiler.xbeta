Compiler: obj 
   dummy: var integer
   sourceF: ref FileSys.File
   sourceFN: ref String.String
   "New qBeta compiler\n".print
   sourceFN := "test.x"
   sourceF := FileSys.open(SourceFN)
   lexer.init(sourceFN,sourceF)
   read: 
      lexer.next
      putint(lexer.token)
      put(' ')
      lexer.tokenStr.print
      newline
      @if lexer.token <> lexer.symbols.eos :then 
         @restart read
   read