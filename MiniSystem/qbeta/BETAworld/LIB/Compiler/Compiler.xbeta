Compiler: obj 
   %include String
   dummy: var integer
   sourceF: ref FileSys.File
   sourceFN: ref String
   "New qBeta compiler\n".print
   sourceFN := "test.x"
   sourceF := FileSys.open(SourceFN)
   lexer.init(sourceFN,sourceF)
   read: 
      lexer.next
      "Got: {I2S(lexer.token)} {lexer.tokenStr}\n".print
      @if lexer.token <> lexer.symbols.eos :then 
         @restart read
   read
   