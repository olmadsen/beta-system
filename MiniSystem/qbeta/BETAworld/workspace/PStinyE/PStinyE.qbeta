PStinyE: obj
   %visible Stringlib
   doScan:
      @ix:< var integer
      from:< var integer
      to:< var integer
      doIt:< object
      ix := from
      L: do
         if (ix < to) :then
             doIt
             ix := ix + 1
             restart (L)
   doScan(myIx)
      from:: 3
      to:: 10
      doIt::
         putint(ix)
         put(' ')
         putint(myIx)
         put(' ')