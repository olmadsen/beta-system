UnitKindEx: obj
   Dimension:
      Unit: Value
         %unit "U",1      
         m: var Float
   Length: Dimension
      Meter:< Unit
         %unit "m", 1
   Width: obj Length
      Meter::<
         %id "Width.Meter"      
         ...
   WaveLength: obj Length
      Meter::<
         %id "WaveLength.Meter"            
         ...
   l1: var Width.Meter
   l2: var WaveLength.Meter
   l1 := l2
   l1 := 3 m
   