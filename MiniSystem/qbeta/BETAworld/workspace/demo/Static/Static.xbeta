Static: =
    x: ?integer
    y: ?integer
    z: ?integer
    add: =
       add.suspend
       "Add: ".print
       z := x + y
       %restart add
    mult: =
       mult.suspend
       "Mult: ".print
       z := x * y
       %restart mult
    x:= 10
    y:= 20
    add
    putint(z)
