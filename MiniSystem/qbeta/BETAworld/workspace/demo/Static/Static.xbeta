Static: obj 
   x: var integer
   y: var integer
   z: var integer
   add: obj 
      add.suspend
      "Add: ".print
      z := x + y
      %restart add
   mult: obj 
      mult.suspend
      "Mult: ".print
      z := x * y
      %restart mult
   x := 10
   y := 20
   add.resume
   putint(z)
   put(10)
   mult.resume
   putint(z)
   put(10)
   add.resume
   putint(z)
   put(10)