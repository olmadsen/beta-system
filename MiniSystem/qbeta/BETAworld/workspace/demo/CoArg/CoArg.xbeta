CoArg: obj 
   %include LIB.String
   RockMe: 
      in S: ref String
      doCall: 
         in T: ref String
         out V: var integer
         S := S + T
         (@this RockMe).call
         V := S.length
      loop: obj 
         S.print
         (@this RockMe).suspend
         @restart loop
   N: var integer
   F: obj RockMe("Jimmy ")
   newline
   N := F.doCall("Hendrix ")
   putint(N)
   newline
   N := F.doCall("plays ")
   putint(N)
   newline
   N := F.doCall("the guitar ")
   putint(N)
   newline