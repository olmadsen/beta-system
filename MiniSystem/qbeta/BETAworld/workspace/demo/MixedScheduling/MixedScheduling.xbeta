MixedScheduling: =  LIB.BasicSystem.BasicSystem 
   P1: = BasicProcess("P1")
       cycle
          console.display
              "P1\n".print
   P2 : = BasicProcess("P2")
       cycle
           console.display
              "P2\n".print
   F: = BasicProcess("F")
        cycle
          "F\n".print;
          F.suspend
   cycle
      P1.attach(10)
      P2.attach(10)
      F.resume
      keyboard.resume
