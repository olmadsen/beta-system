             "ColorPoint := ".print
             newline	     
             x:= p.y
             y:= p.x
	     
       print:
          "ColorPoint: ".print
          putint(x)
          putint(y)
          putint(c)    
          newline            
              
       "ColorPoint made: ".print
       putint(c)    
       newline        	      

