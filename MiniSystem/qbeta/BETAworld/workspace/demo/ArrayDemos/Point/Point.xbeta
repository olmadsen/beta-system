Point: =

    Point(x:?integer, y:?integer):
       := p:? Point:<
             "Point := ".print
             newline	     
             x:= p.y
             y:= p.x	     
        	  
       "Point made: ".print
       putint(x)
       putint(y)
       newline
       %inner Point

    ColorPoint(c:?integer): Point
