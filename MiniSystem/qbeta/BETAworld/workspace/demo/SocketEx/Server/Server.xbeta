Server: =
   S: = LIB.Socket.Socket  -- Ref is none without LIB - adr(Socket) is wrong
   S2: ?Lib.socket.Socket
   T: ?String.string
   "Server\n".print
   S.init(3000)
   S.bind
   S.listen
   S2:= S.accept
   "Try to receive\n".print
   T := S2.receive
   "Server:received: ".print
   T.print
   newline

 
