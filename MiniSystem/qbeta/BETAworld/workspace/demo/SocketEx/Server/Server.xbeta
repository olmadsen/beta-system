Server: obj 
   thisServer: obj LIB.Socket.Socket -- Ref=none without LIB - adr(Socket) 
   aClient: ref Lib.Socket.Socket
   "Server:\n".print
   thisServer.init(3000)
   thisServer.bind
   thisServer.listen
   aClient := thisServer.accept
   ("Server:received: " + aClient.receive + "\n").print
   aClient.send("Got your message:-)")
   "Type a char to terminate server: ".print
   put(_iget)
   newline