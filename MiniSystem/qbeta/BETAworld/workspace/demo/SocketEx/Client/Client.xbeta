Client: =
   thisClient: = LIB.Socket.Socket  -- Ref=none without LIB - adr(Socket)
   S: ? String.String
   "Client:\n".print
   thisClient.init(3000)
   thisClient.connect("localhost")
   thisClient.send("Hello world from client")
   S := thisClient.receive
   "Client received: ".print
   S.print
   thisClient.close;

