ConsumerModule: obj
   %requires BufferModule
   %include BufferModule
   %public
   Consumer: Process
      in buf: ref Buffer
      V: var integer
      console.display
         "Consumer:start\n".print
      @for 1 :to 10
      :do
          V := buf.gett
          console.display
             "Consumer: ".print	  
             putint(V)
             newline	     
      --console.display
      --    "Consumer-end\n".print
   "ConsumerModule:\n".print
