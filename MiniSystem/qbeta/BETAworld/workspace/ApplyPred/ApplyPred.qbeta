   Registration:
      in owner: ref Person
      in v: ref Vehicle
      
   Mary: obj Person("Mary")
   Jens: obj Person("Jens")
   MVR: obj Set
      xlm:: Registration	 

   MVR.e := Registration(Mary,Vehicle)
   MVR.applyPred
      pred::
         --"predBinding:".print      
         Byyy := XXX.owner == Mary
