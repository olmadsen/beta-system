NoneEx: =
   record:
      a: ?integer
      foo(x:?integer,y:?integer):
         a := x + y
      bar(q:?integer,w:?integer) -> v: ?integer:
         v := q + w + a
   R: = record
   S: ? Record
   V: ? integer
   R.a := 1
   V := R.a
   R.foo(2,3)
   V := R.bar(5,6)
   putint(V)
   newline
   S := record
   S.a := 7
   V := S.a
   S.foo(12,13)
   V := S.bar(15,16)
   putint(V)
   newline

   
