tinyC: obj
   %visible Dimensions
   %requires Dimensions, Dimensions.Length, Dimensions.Area
   m1: var Length.Meter
   m2: var Length.Meter
   c1: var Length.Centimeter
   c2: var Length.Centimeter
   a: var Area.SquareMeter
   ascm: var Area.SquareCentimeter
   m1 := 10 "m"
   m2 := 20 "m"
   c1 := 10 "cm"
   c2 := 20 "cm"
   a := m1 * m2
   a.print
   newline
   a := c1 * c2
   a.print
   newline
   ascm := c1 * c2
   ascm.print
   newline
