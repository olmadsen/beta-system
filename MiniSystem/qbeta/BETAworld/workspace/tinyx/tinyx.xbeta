tinyx: obj
   %include Dimensions
   %requires Length, Area, Velocity, Density, Force, Mass, Dimensions
   v: var Velocity.MeterPerSecond
   T: var Force.Newton
   kkk: var Mass.KiloGram
   p: var Density.kgPerCubicMeter
   S: var Area.SquareMeter
   c: var length.centimeter
   m: var Length.Meter
   S := 4.6 "m2"
   p := 1.2 "kg*m^-3"
   T := 560000.0 "N"
   v := sqrt(T / (p * S)) 
   v.print
   c := 12"cm"
   m := c
   m.print
   