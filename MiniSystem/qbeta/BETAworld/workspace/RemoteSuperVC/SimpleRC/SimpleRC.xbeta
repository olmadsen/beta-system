SimpleRC: obj
   doIT:
      R: obj
         P:
            %id "P"      
            V:< {foo: {"foo".print}}
            @inner P
      D3: R.P{ %id "D3"; S: ref V; S:= V; S.foo; @inner D3 }
      D2: D3
         %id "D2"
         V:: {%id "D2::V"; bar: {"bar".print}}
         @inner D2      
      D1: D2 { %ID "D1"; S.bar;}
      D1
