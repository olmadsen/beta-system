StringEx: obj 
   %include MetaSystem,String
   SN: ref ObjSnap
   X: var integer
   T: ref String
   T2: ref string
   ch: var char
   emitString: 
      "The value of X is {I2S(X)}!\n".print
      "\n---\n".print
      "The value of X + 2 is {I2S(X + 2)} and X * X is {I2S(X * X)}!!\n".print
   X := 17
   T := I2S(X)
   newline
   emitString
   ch := '$'
   "Here is a {C(ch)} char\n".print
   