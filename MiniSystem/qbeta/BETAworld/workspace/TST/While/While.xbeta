While: @
  do:
    a: @integer
    b: @integer
    BooleanValue -> V: @Boolean:
       V:= 0
       inner
    doWhile:
       cond:< BooleanValue
       doPart:< Object
       %if cond %then
           doPart
           %restart doWhile
    put(97)
    a := 90
    b := 8
    doWhile
       cond::
          V := b < 15
       doPart::
          put(a + b)
          b := b + 1
