Sig: @
  do:
   max(ax: @integer, bx: @integer):
      ax := ax + bx
      put(ax)
      inner
   S: @ max(90,7)
      bx:=ax + 1
      put(bx)
   %put q: @integer %at inx: @integer:
       a: @integer
       a := q + inx
       put(a)
       inner
       put(a + 1)
   R: @ (%put 90 %at 9)
      put(a + 1)
      a := q + inx + 2
      put(a)
