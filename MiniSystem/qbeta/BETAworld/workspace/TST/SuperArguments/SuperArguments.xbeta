      Bar1(a,1) -- d
      Bar2(a,3)   -- ef
   Arg2: 
      y: ?integer
      Foo(a: ?integer,b:?integer):
         put(a + b)
         inner
         put(b)
      Bar(x: ?integer, y: ?integer): Foo(x,y)
         put(a + y + 1)
         b := x + y + 2
      Bar(96,7)  -- ghi
