         inner
         put(a)
      Foo2: Foo1
         put(a)
         a := a + 1 
      Bar1(x: ?integer, y: ?integer):
         put(x + y)
         inner
      Bar2(q: ?integer, w: ?integer): Bar1(q,w - 1) 
         put(q + w)
      a := 97
