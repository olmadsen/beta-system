testQ: obj
   doit:
      %include Containers
      Container:
         in elm:< Object
         scan:<
            current: ref elm
            @inner scan
      SetXX: obj
        Set: Container        
           insert: 
              in e: ref elm        
              @if head = none :then
                    head:= link(e,none)
              :else
                   L: ref link
                   L := link(e,head)
                   head := L        
           scan::
              nxt: ref link
              Loop:
                   @if nxt <> none :then
                      current:= nxt.e;
                      @inner scan
                      nxt:= nxt.next
                      @restart Loop
                   :else
                     nxt:= nxt
              nxt := head
              loop
           link:
              in e: ref elm
              in next: ref link
           head: ref link
         
      Person:
         in name: ref String.String   
      Qualification:
         in Q: var integer
         print:
            "Qualification:".print
            putint(Q)
      Qualifications: SetXX.Set(#Qualification)
         print:
            scan
               Q: ref Qualification
               Q := current    
               Q.print   
      Expert:
         in name: ref String.String
         in quali: ref Qualifications
         
      QS: obj Qualifications

      S: obj SetXX.Set(#Person){}

      S.insert(Person("jkl"))
      S.scan
         current.name.print
