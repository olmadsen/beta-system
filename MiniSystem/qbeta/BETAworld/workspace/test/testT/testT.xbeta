testT: obj
   doit:
      --%include Containers
      Qualification:
         in Q: var integer
         print: {"Qualification:".print; putint(Q)}
      Qualifications: Containers.Set.Set(#Qualification)
         print: {scan{current.print}}      
      Expert:
         in name: ref String.String   
         in quali: ref Qualifications
         print:
            "Expert:".print
      Experts: Containers.Set.Set(#Expert)
         print: {scan{current.print}}
      
      Alarm:
         in descr: ref String.String
         in reqQuali: ref Qualification
      Alarms: Containers.Set.Set(#Alarm)

      Period:
         in start: var integer
         in end: var integer
 
      Schedule: Containers.MapLib.Map(#Period,#Experts)
         xxx: var integer -- problem if IS isEmpty withj findbingh

      theSchedule: obj Schedule
         B: var Boolean
         theSchedule.forAll
            B := current.elm.isEmpty