tinyB: obj
   %visible LIB.Dimensions
   B: var boolean
   X: var integer
   B := true
   X := -97
   if (X = - 97) :then
      put(-X)
   :else
      put ('?')
   X := -X
   put(X + 1)
   put(+X +2)
   if (B) :then
      put('T')
   B := ! B
   if (!B) :then
      put('F')
   
   
   
   
   
      


   
  

