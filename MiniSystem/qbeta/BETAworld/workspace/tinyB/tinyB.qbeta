tinyB: obj
   %visible Dimensions
   %requires Dimensions.Area,Dimensions.Length
   mapi(V: var integer) -> res: var integer:
      res := V + V
   mapf(V: var float) -> res: var float:
      res := V + V
   L1: var Length.meter
   L2: var Length.Meter

   L2 := 8 m
   L1 := mapi(L2)
   L1.print
   newline
   L2 := 6 m
   L1 := mapf(L2)
   L1.print
   newline

   
      


   
  

