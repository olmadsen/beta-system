tinyJ: obj
   %requires MetaSystem
   %include MetaSystem
   Point: Value
      in x: var integer
      in y: var integer
   X: obj Indexed(5,Integer)
   Y: Obj Indexed(5,Point(0,0))
   for(1):to(5):repeat 
      X.put(inx*inx):at[inx]
   for (1):to(5):repeat
      P: var Point(0,0)
      P := Point(inx+1,inx+2)
      Y.put(P):at[inx]
   SN: ref ObjSnap
   SN := ObjSnap(X)
   newline
   SN.print   
   SN := ObjSnap(Y) 
   SN.print
      


   
   