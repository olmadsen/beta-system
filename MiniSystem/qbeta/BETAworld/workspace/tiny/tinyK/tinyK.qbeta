tinyK: obj
   Point: value
      in x: var float
      in y: var Float
      z: var float
      translate:
         in dx: var float
         in dy: var float
         x := x + dx
         y := y + dy
      print:
         putFloat(x)
         put(',')
         putFloat(y)
         put(',')
         putFloat(z)
         newline
      x := x + 1.1
      z := x + y
   p1: var Point(1.0,1.1)
   p1.print
   p1.translate(2,4)
   p1.print

