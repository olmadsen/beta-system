origin '~beta/basiclib/betaenv'
---program:descriptor---
(# hello:
     (# 
     do 'Q' -> put;
        ch -> put;
        inner hello
     #);
   nest:
     (#
        you: hello
          (#
          do ch1 -> put
          #);
        foo: 
          (# world:< hello;
             fisk:<
               (#
               do 'T' -> put;
                  world
               #);
          #);
        bar: foo
          (# world::< you
          #);
        R: ^Foo;
        ch: @char;
        ch1: @char
     do
        &bar[] -> R[];
        'W' -> ch;
        'E' -> ch1;
        R.fisk
     #);
   ch: @char;
   ch1: @char   
do
   nest
#)
