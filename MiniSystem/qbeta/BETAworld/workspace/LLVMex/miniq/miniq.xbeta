miniq: obj
   @for: 
      %globals inSub 
      in for: var integer
      in to: var integer
      in do:< Object
      %public      
      inx: var integer
      %private
      doIt: 
         loop: obj
            @if inx <= to :then 
               do
               inx := inx + 1
               @restart loop
      inx := for
      outint(for)
      put(10)
      outint(to)
      put(10)
      doIt
      inner(for)
   @for 1 :to 10 :do
         put('a' + inx - 1)
   put(10)