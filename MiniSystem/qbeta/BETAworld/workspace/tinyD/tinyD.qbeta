tinyD: obj
   Record:
      in id: var integer
      print:
         put(id)
   Point: Value
      in x: var integer
      in y: var integer
      print:
         put(x)
         put(y)	 
   A: var integer      -- 2
   R1: Ref Record      -- 3 
   R2: ref Record      -- 4
   P1: var Point(1,2)  -- 5
   P2: var Point(3,4)  -- 7 
   A := 97
   put(A)
   R1 := Record(98)
   R1.print
   R2 := R1
   R2.print
   P1 := Point(99,100)
   P1.print
   P2 := P1
   P2.print
   
   

