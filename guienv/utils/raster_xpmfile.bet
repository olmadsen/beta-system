ORIGIN '../raster';
BODY 'private/X11/xpmfile_unixbody';

--bitmapLib: attributes--
    
readFromXPMfile:
  (* Reads the contents of the XPMfile into THIS(bitmap) *)
  (# filename: ^text;
     failure:<
       (# do inner #);
  enter filename[]
  do <<SLOT GUIENVbitmapReadFromXPMfile: descriptor>>;
  #);
