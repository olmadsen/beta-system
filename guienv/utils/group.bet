ORIGIN '../guienv';
BODY 'private/groupbody';

-- windowLib: attributes --

group: canvas
  (# <<SLOT groupLib: attributes>>;
     label:
       (# theLabel: ^text;
       enter (# enter theLabel[] <<SLOT groupSetLabel: doPart>> #)
       exit (#  <<SLOT groupGetLabel: doPart>> exit theLabel[] #)
       #);
     open::<
       (# create::< (# <<SLOT groupCreate: doPart>> #)
       <<SLOT groupOpen: doPart>>
       #);
     eventHandler::<
       (#
          onRefresh::<
            (# <<SLOT groupOnRefresh: doPart>> #);
       #);
     private: @<<SLOT groupPrivate: descriptor>>;
  #);
