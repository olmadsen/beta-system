ORIGIN '../graphics';
BODY 'private/graphicsaddsbody';
INCLUDE 'rasteradds';

-- graphicsLib: attributes --
clipRectangle:
  (# r: @rectangle;
  enter r
  <<SLOT graphicsSetClipRectangle: doPart>>
  #);
isInClip: booleanValue
  (# r: @rectangle;
  enter r
  <<SLOT graphicsIsInClip: doPart>>
  #);
drawIconResource:
  (# r: ^iconResource;
     h,v: @integer;
  enter (r[],h,v)
  <<SLOT graphicsDrawIcon: doPart>>
  #);

