ORIGIN '../guienv';
BODY 'private/movabletextfieldbody';


-- windowItemLib: attributes --

movableTextField:
  (# <<SLOT movableTextFieldLib: attributes>>;
     init:<
       (# <<SLOT movableTextFieldInit: doPart>> #);
     activate:
       (# str: ^Text;
          style: ^textStyle;
          p: @point;
       enter (str[], style[], p)
       <<SLOT movableTextFieldActivate: doPart>> 
       #);
     deactivate:
       (# <<SLOT movableTextFieldDeactivate: doPart>> #);
     verify:<
       (# okToDeactivate: @boolean;
       do true -> okToDeactivate;
          inner;
       exit okToDeactivate
       #);
     theText:
       (# str: ^text;
       <<SLOT MovableTextFieldTheText: doPart>>
       exit str[]
       #);
     private: @<<SLOT movableTextFieldPrivate: descriptor>>;
  #);
