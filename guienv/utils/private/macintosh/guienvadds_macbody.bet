-- windowItemPreferredSize: doPart --
do (# style: ^textStyle;
		theStaticText: ^staticText;
	do (if true 
			//this(windowItem)## <= staticText## then
				this(windowItem)[] -> theStaticText[];
				theStaticText.style -> style[];
				theStaticText.label -> style.widthOftext -> preferredWidth;
				preferredWidth + 1 -> preferredWidth;
				style.lineHeight -> preferredHeight;
			//this(windowItem)## <= optionButton## then
				(200, 30) -> (preferredWidth, preferredHeight);
		if);
	#)

