ORIGIN '../iconnamebody';

-- iconButtonEnterIconName: doPart --
do l:
     (# pm: ^pixmap;
        fileName: ^text;
     do &pixmap[] -> pm[];
        (if value[] <> none then
            fileName[] -> pm.read
            (# 
               error::
                 (# 
                 do leave l;
                 #);
            #);
            pm[] -> icon;
        if);
     #);
   
