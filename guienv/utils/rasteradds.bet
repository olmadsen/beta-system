ORIGIN '../guienv';
BODY 'private/rasteraddsbody';
(* INCLUDE '../raster'; *)

-- guienvlib: attributes --
iconResource: pixmap
  (# <<SLOT pixmapLib: attributes>>;
     loadById:
       (# id: @integer;
       enter id
       do <<SLOT iconLoadById: descriptor>>
       #);
  #);

bitmapResource: pixmap
  (# <<SLOT pixmapLib2: attributes>>;
     loadById:
       (# id: @integer;
          success: @boolean;
       enter id
       <<SLOT bitmapResLoadById: doPart>>
       exit success
       #);
     loadByName:
       (# name: ^text;
          success: @boolean;
       enter name[]
       <<SLOT bitmapResLoadByName: doPart>>
       exit success
       #);
  #);

