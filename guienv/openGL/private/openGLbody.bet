ORIGIN '../openGL';
MDBODY nti 'winnt/openGL_ntibody'
       ppcmac 'macintosh/openGL_macbody'
       default 'X11/openGL_X11body';
BUILD nti '$$/betaopengl.obj' 'external/betaopengl.c' 'betacc $0 $1'
      sgi '$$/betaopengl.o' 'external/betaopengl.c' '$CC -32 -D$$ -c -o $0 $1'
      sun4s '$$/betaopengl.o' 'external/betaopengl.c' '$CC -c -D$$ -o $0 $1'; 
