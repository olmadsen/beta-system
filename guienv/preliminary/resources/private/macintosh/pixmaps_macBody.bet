ORIGIN '../pixmapsBody';
INCLUDE '../../../../private/macintosh/pixmapbody';
INCLUDE '~beta/maclib/v3.1/icons';

-- loadIconResourceImpl: doPart --
do (# iconSuiteHandle: @longInt;
		error: @integer;
	do 'loading icon resource, id = ' -> putText;
		ID -> putInt;
		newLine;
		(iconSuiteHandle[], ID, kSelectorAllAvailableData)
			-> GetIconSuite
			-> error;
		(if error = 0 then
			(if iconSuiteHandle <> NIL then
				iconSuiteHandle -> THIS(pixmap).iconSuite;
			else
				'iconSuiteHandle is nil' -> putLine;
			if);
		else
			'error = ' -> putText;
			error -> putInt;
			newLine;
		if);
   #);
   
