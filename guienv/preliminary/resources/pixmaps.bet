ORIGIN '~beta/guienv/guienv';
BODY 'private/pixmapsBody';

-- pixmapLib: attributes --


loadIconResource:
  (# name: ^text;     
     id: @integer;
     error:< exception
       (# what: ^text;
       enter what[]
       do what[] -> msg.putText;
          INNER;
       #);
  enter (name[],id)
  <<SLOT loadIconResourceImpl: doPart>>  
  #);

loadBitmapResource:
  (# name: ^text;     
     id: @integer;
     error:< exception
       (# what: ^text;
       enter what[]
       do what[] -> msg.putText;
          INNER;
       #);
  enter (name[],id)
  <<SLOT loadBitmapResourceImpl: doPart>>     
  #);
