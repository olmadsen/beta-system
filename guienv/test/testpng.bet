
ORIGIN '~beta/guienv/guienv';
INCLUDE '~beta/guienv/graphics';






-- program: descriptor --
guienv
(#
 
   image: @pixmap;
   
   w: @window
     (# c: @canvas
          (# eventHandler::
               (# 
                  onRefresh::
                    (#
                    do graphics
                       (#
                       do (image[], (0, 0), (0, 0),  image.width, image.height) -> drawRaster;
                       #);
                    #);
                  
               #);
             open::
               (#
               do (image.width, image.height) -> size;
               #);
          #);
        open::
          (#
          do (image.width, image.height) -> size;
             c.open;
          #);
     #);
   name: ^text;
do 2 -> arguments -> name[];
   name[] -> image.read;
   w.open;
#)
