ORIGIN 'guienv_macprivate';
BODY 'applegrayscalebody';
INCLUDE '../../graphics';

-- windowItemLib: attributes --



appleGrayscale: graphics
	(# <<SLOT applGrayscaleLib: attributes>>;
		drawBevelMediumOff:
			(# left, top, right, bottom: @integer;
			enter (left, top, right, bottom)
			<<SLOT appleGrayscaleDrawBevelMediumOff: doPart>>
			#);
		drawBevelMediumOn:
			(# left, top, right, bottom: @integer;
			enter (left, top, right, bottom)
			<<SLOT appleGrayscaleDrawBevelMediumOn: doPart>>
			#);
		drawBevelMediumOnDisabled:
			(# left, top, right, bottom: @integer;
			enter (left, top, right, bottom)
			<<SLOT appleGrayscaleDrawBevelMediumOnDisabled: doPart>>
			#);
		drawBevelMediumOffDisabled:
			(# left, top, right, bottom: @integer;
			enter (left, top, right, bottom)
			<<SLOT appleGrayscaleDrawBevelMediumOffDisabled: doPart>>
			#);
	do INNER;
	#);