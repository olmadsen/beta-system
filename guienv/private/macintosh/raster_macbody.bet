-- GUIENVbitmapReadFromX11file: descriptor --
(# width,height: @integer;
	address,data: @integer;
	t: @text;
	error: @integer;
do (filename,@@address,@@data) -> CreatePixmapFromPictureFile;
	(if address<>0 then
		address -> rasterAddress;
		data -> rasterData;
		address -> BitmapGetWidth -> width;
		address -> BitmapGetHeight -> height;
		width -> rasterWidth;
		height -> rasterHeight;
		lock;
		(address, data) -> CreatePixPatFromPixMap -> rasterPrivatePart.pixPatAddress;
		unLock;
		true -> isPixPat;
	if);
#)

