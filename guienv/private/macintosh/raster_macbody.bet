-- GUIENVpixmapReadFromX11file: descriptor --
(# width,height: @integer;
	address,data: @integer;
	t: @text;
do (X11fileName,@@address,@@data) -> CreatePixmapFromPictureFile;
	(if address<>0 then
		address -> rasterAddress;
		data -> rasterData;
		address -> PixMapGetWidth -> width;
		address -> PixMapGetHeight -> height;
		width -> rasterWidth;
		height -> rasterHeight;
	if);
#)

