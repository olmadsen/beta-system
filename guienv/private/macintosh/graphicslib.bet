ORIGIN '../../graphics';
INCLUDE 'guienv_macprivate';
-- graphicsLib: attributes --
clipChildren:
	(# doClipChildren: @boolean;	
		visibleRgn: @integer;
	enter	doClipChildren
	do (if doClipChildren then
			this(window).private.windowPointer + 28 -> TOS'%adrGetLong' 
				-> visibleRgn -> getVisibleRgn;
		else
			this(windowItem).private.clip -> SetClip;
		if);
	#);