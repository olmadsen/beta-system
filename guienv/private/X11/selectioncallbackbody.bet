ORIGIN 'selectioncallback';

INCLUDE 'guienvattributes';
INCLUDE 'guienv_unixprivate';
INCLUDE 'callbacks';

[[
-- GUIENVloseSelection: descriptor --
(#  
   loseProcessor: callbackProcessor
     (# 
     do &caller[] -> cb[];
     #);
   caller: motifCallbackSelector
     (#
     do callback;
     #);
do 
   (loseProcessor##, XmNlosePrimaryCallback ) -> addCallbackProcessor;
#)

-- GUIENVgainSelection: descriptor --
(#  
   gainProcessor: callbackProcessor
     (# 
     do &caller[] -> cb[];
     #);
   caller: motifCallbackSelector
     (#
     do callback;
     #);
do 
   (gainProcessor##, XmNgainPrimaryCallback ) -> addCallbackProcessor;
#)
--]]
