ORIGIN '../../guienv';
BODY 'widgetbody'
[[
-- Include '~beta/Xt/v1.8/motif/callbackstruct'
-- Include 'motifstuff'
-- interfaceObjectLib: attributes --

widgetID:
  (# ID: @integer;
  enter (# enter ID <<SLOT GUIENVinterfaceObjectSetWidgetID: doPart>> #)
  exit (# <<SLOT GUIENVinterfaceObjectGetWidgetID: doPart>> exit ID #)
  #);
create:
  (# name: ^text;
     father: @integer;
     class: @integer;
  enter (class,father,name[])
  do <<SLOT GUIENVwidgetCreate: descriptor>>;
  #);
createManaged: 
  (# name: ^text;
     father: @integer;
     class: @integer;
  enter (class,father,name[])
  do <<SLOT GUIENVwidgetCreateManaged: descriptor>>;
  #);
createArgs:
  (# name: ^text;
     father: @integer;
     class: @integer;
     args: ^argList;
     numArgs: @integer;
  enter (class,father,name[],args[],numArgs)
  do <<SLOT GUIENVwidgetCreateArgs: descriptor>>;
  #);
createManagedArgs:
  (# name: ^text;
     father: @integer;
     class: @integer;
     args: ^argList;
     numArgs: @integer;
  enter (class,father,name[],args[],numArgs)
  do <<SLOT GUIENVwidgetCreateManagedArgs: descriptor>>;
  #);

windowID: integerValue
  (#
  do WIDGETID -> XtWindowOfObject -> value;
  #);
displayID: integerValue
  (# 
  do WIDGETID -> XtDisplayOfObject -> value;
  #);
screenID: integerValue
  (# 
  do WIDGETID -> XtScreenOfObject -> value;
  #);

realize:
  (# 
  do WIDGETID -> XtRealizeWidget;
  #);
unRealize:
  (# 
  do WIDGETID -> XtUnRealizeWidget;
  #);
map:
  (# 
  do WIDGETID -> XtMapWidget;
  #);
unMap:
  (# 
  do WIDGETID -> XtUnmapWidget;
  #);
manage:
  (# 
  do WIDGETID -> XtManageChild;
  #);
unManage:
  (# 
  do WIDGETID -> XtUnmanageChild;
  #);

setIntegerResource:
  (# name: @integer;
     value: @integer;
  enter (name,value)
  do <<SLOT GUIENVwidgetSetIntegerResource: descriptor>>;
  #);
getIntegerResource:
  (# name: @integer;
     value: @integer;
  enter name
  do <<SLOT GUIENVwidgetGetIntegerResource: descriptor>>;
  exit value
  #);
getShortResource:
  (# name: @integer;
     value: @shortint;
  enter name
  do <<SLOT GUIENVwidgetGetShortResource: descriptor>>;
  exit value
  #);
getCharResource:
  (# name: @integer;
     value: @char;
  enter name
  do <<SLOT GUIENVwidgetGetCharResource: descriptor>>;
  exit value
  #);
getBooleanResource:
  (# name: @integer;
     value: @boolean;
  enter name
  do <<SLOT GUIENVwidgetGetBooleanResource: descriptor>>;
  exit value
  #);
setStringResource:
  (# name: @integer;
     value: ^text;
  enter (name,value[])
  do <<SLOT GUIENVwidgetSetStringResource: descriptor>>;
  #);
getStringResource:
  (# name: @integer;
     value: ^text;
  enter name
  do <<SLOT GUIENVwidgetGetStringResource: descriptor>>;
  exit value[]
  #);
setMotifStringResource:
  (# name: @integer;
     value: ^text;
  enter (name,value[])
  do <<SLOT GUIENVwidgetSetMotifStringResource: descriptor>>
  #);
getMotifStringResource:
  (# name: @integer;
     value: ^text;
  enter name
  do <<SLOT GUIENVwidgetGetMotifStringResource: descriptor>>;
  exit value[]
  #);
getPointerResource:
  (# name: @integer;
     value: @integer;
  enter name
  do <<SLOT GUIENVwidgetGetPointerResource: descriptor>>;
  exit value
  #);

--]]


