ORIGIN '../../guienv';
BODY 'guienvattributesbody'
[[
-- include 'widget'
-- GUIENVLib: attributes --

idleProcedure:
  (# add:
       (# 
       do <<SLOT GUIENVidleProcedureAdd: descriptor>>;
       #);
     delete:
       (# 
       do <<SLOT GUIENVidleProcedureDelete: descriptor>>;
       #);
     private: @<<SLOT idleProcedurePrivate: descriptor>>;
     done: @boolean;
  do inner;
  exit done
  #);


mouseInfo:
  (# global,local: @point;
     button: @integer;
     shiftKey,
     controlKey,
     altKey,
     metaKey,
     capsLock: @boolean;
  #);

whatButton:
  (# mask: @integer;
     button: @integer;
  enter mask
  do <<SLOT GUIENVwhatButton: descriptor>>;
  exit button
  #);

display: integerValue
  (# 
  do <<SLOT GUIENVgetDisplay: descriptor>>;
  #);

blackPixelValue: integerObject
  (# 
  do <<SLOT GUIENVblackPixelValue: descriptor>>
  #);
whitePixelValue: integerObject
  (# 
  do <<SLOT GUIENVwhitePixelValue: descriptor>>
  #);
-- mouseLib: attributes --


getMouseInfo:
  (# windowID: @integer;
     info: ^mouseInfo;
  enter windowID
  do <<SLOT GUIENVmouseGetMouseInfo: descriptor>>;
  exit info[]  
  #);

-- interfaceObjectLib: attributes --

assertOpen:
  (# location:< (# t: ^text do inner exit t[] #);
  do <<SLOT GUIENVinterfaceObjectAssertOpen: descriptor>>;
  #);
createSimple:
  (# father: @integer;
     class: @integer;
  enter (class,father)
  do <<SLOT GUIENVinterfaceObjectCreateSimple: descriptor>>;
  #);

-- windowitemLib: attributes --
hitMethod: booleanValue
  (# p: @point;
  enter p
  do inner;
  #);
hit: booleanValue
  (# p: @point;
  enter p
  do <<SLOT GUIENVwindowitemHit: descriptor>>;
  #);
setHit:
  (# hit: ##hitMethod;
  enter hit##
  do <<SLOT GUIENVwindowitemSetHit: descriptor>>;
  #);

-- textStyleLib: attributes --
fontID:
  (# value: @integer;
  do <<SLOT GUIENVtextStyleFontID: descriptor>>;
  exit value
  #);
fontStruct:
  (# value: @integer;
  do <<SLOT GUIENVtextStyleFontStruct: descriptor>>;
  exit value
  #);
setFontStruct:
  (# value: @integer;
  enter value
  do <<SLOT GUIENVtextStyleSetFontStruct: descriptor>>;
  #);
fontInfo:
  (# id: @integer;
     ascent: @integer;
     descent: @integer;
     leading: @integer;
  #);
getFontInfo:
  (# info: ^fontInfo;
  do <<SLOT GUIENVtextStyleGetFontInfo: descriptor>>;
  exit info[]
  #);
allocate:
  (# 
  do <<SLOT GUIENVtextStyleAllocate: descriptor>>;
  #);
dispose:
  (# 
  do <<SLOT GUIENVtextStyleDispose: descriptor>>;
  #);
createFontList:
  (# fontList: @motifFontList;
  do <<SLOT GUIENVTextStyleCreateFontList: descriptor>>;
  exit fontList
  #);
initWithFontList:
  (# fontList: @motifFontList;
  enter fontList
  do <<SLOT GUIENVTextStyleinitWithFontList: descriptor>>;
  #);
--]]
