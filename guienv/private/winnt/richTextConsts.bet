-- lib: attributes --
memset: external
  (# dest,c,count: @integer;
     result: @integer;
  enter (dest,c,count)
  do callC;
  exit result
  #);

_tcscpy: external
  (# str1,str2: @integer;
     res: @integer;
  enter (str1,str2)
  do 'strcpy' -> callC;
  exit res
  #);

CommDlgExtendedError: external
  (# errorCode: @integer;
  do callStd;
  exit errorCode
  #);

sizeof_TCHAR: external
  (# size: @integer;
  do callC;
  exit size
  #);

calFormat: external
  (# hf: @integer;
     format: @integer;
  enter hf
  do callC
  exit format
  #);

