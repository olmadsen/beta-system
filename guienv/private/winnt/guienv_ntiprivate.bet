(# children: @list
     (# element::< windowItem;
        remove:
          (# e: ^element;
          enter e[]
          do e[] -> at -> delete;
          #);
        findHitObject:
          (# pt: @point;
             e: ^element;
          enter pt
          do l: scanReverse
               (# where::<
                    (#
                    do (if current.enabledFlag then
                           pt -> current.hit -> value;
                        else
                           false -> value;
                       if);
                    #);
               do current[] -> e[];
                  leave l;
               #);
          exit e[]
          #);
     #);
   selection: @list
     (# element::< windowitem;
     #);
#)

