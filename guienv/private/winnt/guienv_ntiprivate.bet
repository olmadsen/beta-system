     remove:
       (# ev: ##theEventHandler.event;
       enter ev##
       do l: scan
            (# 
            do (if (current.type## = ev##) then
                   current[] -> delete;
                   leave l;
               if);
            #);
       #);
     has: booleanValue
       (# ev: ##theEventHandler.event;
       enter ev##
       do false -> value;
          l: scan
            (# 
            do (if (ev## <= current.type##) then
                   true -> value;
                   leave l;
               if);
            #);
       #);
  #);

