[[
-- textfieldLib: attributes --

loseSelection:
  (# callback: ^object;
  enter callback[]
  do <<SLOT GUIENVloseSelection: descriptor>>
  #);
gainSelection:
  (# callback: ^object;
  enter callback[]
  do <<SLOT GUIENVgainSelection: descriptor>>
  #);
--]]
