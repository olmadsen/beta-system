ORIGIN '~beta/guienv/v1.6/guienv';
-- program: descriptor --
GUIenv
	(# menubarType::
			(# fileMenu: @menu
					(# quitItem: @menuItem
							(# eventHandler::
									(# onSelect::
											(#
											do	Quit;
											#);
									#);
								open::
									(#
									do 'Quit' -> name;
										'Q' -> key;
									#);
							#);
						open::
							(#
							do 'File' -> name;
								quitItem.open;
								quitItem[] -> append;
							#);
					#);
				open::
					(#
					do fileMenu.open;
						fileMenu[] -> append;
					#);
			#);
		basicWindow: @window
			(# open::
					(#
					do  (300, 40) -> minsize;	  (* The minsize, is the minimum size
						  									* the user is allowed to give
															* the content area of the window.
															*)
						 (300, 10000) -> maxsize; (* The maxsize, is the minimum size
						  									* the user is allowed to give
															* the content area of the window.
															*)							
															
						 (300, 200) -> size;		  (* 
															* In this case, width = maxwidth = minwidth, which means
															* that the user will only be able to chang the height
															* of the window.
														   *)
						 						 
						'Window with Constrained Size' -> title;
						(10, 40) -> position;
					#);
				close::
					(#
					do Quit;  (* Quit the application when this window closes *)
					#);
			#);
	do basicWindow.open;
	#)
  