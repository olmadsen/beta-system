ORIGIN '~beta/guienv/guienv';
INCLUDE '~beta/guienv/controls';
-- program: descriptor --
GUIenv
(# mainWindow: @window
     (# resizeable:: (# do false -> value #);
        anIconButton: @iconButton
          (# open::
               (# aPixmap: @pixmap;
               do 'arrow' -> aPixmap.read;
                  aPixmap[] -> icon;
                  (13, 13) -> position;
                  (100, 100) -> size;
                  false -> showLabel;
               #);
             eventHandler::
               (#
                  onMouseUp::
                    (#
                    do 'bing!' -> putline;
                    #);
               #);
          #);
        open::
          (# width, height: @integer;
          do 
             '' -> title;
             anIconButton.open;
             anIconButton.size -> (width, height);
             (width+ 26, height + 26) -> size;
          #);
        close::
          (#
          do Terminate;
          #);
     #);		
do mainWindow.open;
#)
