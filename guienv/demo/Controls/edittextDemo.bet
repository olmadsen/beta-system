ORIGIN '~beta/guienv/guienv';
INCLUDE '~beta/guienv/controls';
-- program: Descriptor --
GUIenv
  (#
     mainWindow: @window
       (#
          resizeable::  (#  do false->value #);
          anEditText: @editText
            (#
               open:: 
                 (# aTextStyle: ^textStyle; 
                 do
                    &textStyle[]->aTextStyle[];
                    'Times'->aTextStyle.name;
                    12->aTextStyle.size;
                    aTextStyle[]->style;
                    'http://www.altavista.com'->contents;
                    (13,13)->position;
                    (200,26)->size;
                    
                 #);
               
            #);
          aPushbutton: @pushbutton
            (#
               eventHandler:: 
                 (# onMouseUp::  (#  do anEditText.contents->putline;  #)
                 #);
               open:: 
                 (# 
                 do 'putline'->label; (52,26)->size; (226,13)->position; 
                 #)
            #);
          open:: 
            (# width,height: @integer; 
            do
               'Simple EditText Example'->title;
               anEditText.open;
               aPushbutton.open;
               (300,60)->size;
               anEditText[]->target;
               (* Activate the edittext *)
               
            #);
          close::  (#  do terminate #);
          
       #);
     
  do mainWindow.open; 
  #)  

