ORIGIN '~beta/guienv/guienv';
INCLUDE '~beta/guienv/controls'
        '~beta/guienv/utils/tooltip';
-- WindowLib: Attributes --
toolTipCanvas: canvas
  (#
     open::
       (# 
       do
          father.size->size;
          thePushbutton.open;
          'this is a Tooltip'->thePushbutton.makeToolTip;
          
       #);
     thePushbutton: @pushbutton
       (#
          makeToolTip:
            (# theTip: ^Text; 
            enter theTip[]
            do theTip[]->THIS(pushButton).addToolTip
            #);
          open::< 
            (# 
            do
               'do not push this button'->label;
               (120,40)->size;
               (70,65)->position;
               INNER
            #);
          eventHandler::<  (# onMouseUp::<  (#  #) #)
       #)
  #)  

