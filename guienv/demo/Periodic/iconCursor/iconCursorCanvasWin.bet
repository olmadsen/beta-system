ORIGIN '~beta/guienv/guienv';
INCLUDE '~beta/guienv/utils/guienvadds'
        '~beta/guienv/controls';
-- WindowLib: Attributes --
iconCursorCanvas: canvas
  (#
     uglyIconButton: @pushbutton
       (#
          open::< 
            (# 
            do
               'Set ugly icon'->label;
               (130,30)->size;
               (175,50)->position;
               INNER
            #);
          eventHandler::< 
            (#
               onMouseUp::< 
                 (# 
                 do
                    'images\\ugly.ico'->loadWindowIcon;
                    INNER
                 #)               
            #)
       #);
     mjolnerIconButton: @pushbutton
       (#
          open::< 
            (# 
            do
               'Set Mjolner icon'->label;
               (130,30)->size;
               (25,50)->position;
               INNER
            #);
          eventHandler::< 
            (#
               onMouseUp::< 
                 (# 
                 do
                    'images\\beta.ico'->loadWindowIcon;
                    INNER
                 #)               
            #)
       #);
     arrowCursorButton: @pushbutton
       (#
          open::< 
            (# 
            do
               'Set arrow cursor'->label;
               (130,30)->size;
               (175,180)->position;
               INNER
            #);
          eventHandler::< 
            (#
               onMouseUp::< 
                 (#  do 'images\\arrow.cur'->loadMouseCursor; INNER #)                         
            #)
       #);
     crossCursorButton: @pushbutton
       (#
          open::< 
            (# 
            do
               'Set crosshair cursor'->label;
               (130,30)->size;
               (25,180)->position;
               INNER
            #);
          eventHandler::< 
            (#
               onMouseUp::< 
                 (#  do 'images\\cross.cur'->loadMouseCursor; INNER #)               
            #)          
       #);
     cursorLabel: @statictext
       (#
          open::< 
            (# 
            do
               'Cursors:'->label;
               (60,16)->size;
               (25,150)->position;
               INNER
            #)
       #);
     iconsLabel: @statictext
       (#
          open::< 
            (# 
            do
               'Icons:'->label;
               (60,16)->size;
               (25,20)->position;
               INNER
            #)
       #);
     open::< 
       (# x,y: @integer
       do
          father.size->(x,y);
          (x-10,y-10)->size;
          uglyIconButton.open;
          mjolnerIconButton.open;
          arrowCursorButton.open;
          crossCursorButton.open;
          cursorLabel.open;
          iconsLabel.open;
          INNER
       #)
  #)

