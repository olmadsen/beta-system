SQL_NULL_HENV: (#  exit 0 #);
SQL_NULL_HDBC: (#  exit 0 #);
SQL_NULL_HSTMT: (#  exit 0 #);
SQL_NULL_HDESC: (#  exit 0 #);
SQL_NULL_HANDLE: (#  exit 0 #);
