 *********************)
(* #define ODBCVER 0x0300 *)
ODBCVER: (#  exit 16x0300 #);
(* #define SQL_NULL_DATA             (-1) 
 * #define SQL_DATA_AT_EXEC          (-2) 
 *)
SQL_NULL_DATA: (#  exit - 1 #);
SQL_DATA_AT_EXEC: (#  exit - 2 #);
(*
 * /* return values from functions */
 * #define SQL_SUCCESS                0
 * #define SQL_SUCCESS_WITH_INFO      1
 * #if (ODBCVER >= 0x0300)
 * #define SQL_NO_DATA              100
 * #endif
 * #define SQL_ERROR                 (-1)
 * #define SQL_INVALID_HANDLE        (-2)
 * 
 * #define SQL_STILL_EXECUTING        2
 * #define SQL_NEED_DATA             99
 *)
SQL_SUCCESS: (#  exit 0 #);
SQL_SUCCESS_WITH_INFO: (#  exit 1 #);
SQL_NO_DATA: (#  exit 100 #);
SQL_ERROR: (#  exit - 1 #);
SQL_INVALID_HANDLE: (#  exit - 2 #);
SQL_STILL_EXECUTING: (#  exit 2 #);
SQL_NEED_DATA: (#  exit 99 #);
(*
 */* test for SQL_SUCCESS or SQL_SUCCESS_WITH_INFO */
 *#define SQL_SUCCEEDED(rc) (((rc)&(~1))==0)
 *)
(*
 /* flags for null-terminated string */
 * #define SQL_NTS                   (-3)
 * #define SQL_NTSL                  (-3L)
 *)
SQL_NTS: (#  exit - 3 #);
SQL_NTSL: (#  exit - 3 #);
(*
 * /* maximum message length */
 * #define SQL_MAX_MESSAGE_LENGTH   512
 *)
SQL_MAX_MESSAGE_LENGTH: (#  exit 512 #);
(*
 * /* handle type identifiers */
 * #if (ODBCVER >= 0x0300)
 * #define SQL_HANDLE_ENV             1
 * #define SQL_HANDLE_DBC             2
 * #define SQL_HANDLE_STMT            3
 * #define SQL_HANDLE_DESC            4
 * #endif *)
SQL_HANDLE_ENV: (#  exit 1 #);
SQL_HANDLE_DBC: (#  exit 2 #);
SQL_HANDLE_STMT: (#  exit 3 #);
SQL_DEFAULT: (#  exit 99 #);

(* #define SQL_FETCH_NEXT      1
 * #define SQL_FETCH_FIRST     2
 * #define SQL_FETCH_LAST      3
 * #define SQL_FETCH_PRIOR     4
 * #define SQL_FETCH_ABSOLUTE  5
 * #define SQL_FETCH_RELATIVE  6
 *)
SQL_FETCH_NEXT:(# exit 1 #);
SQL_FETCH_FIRST:(# exit 2 #);
SQL_FETCH_LAST:(# exit 3 #);
SQL_FETCH_PRIOR:(# exit 4 #);
SQL_FETCH_ABSOLUTE:(# exit 5 #);
SQL_FETCH_RELATIVE:(# exit 6 #);

(* null handles returned by SQLAllocHandle() */
 * #define SQL_NULL_HENV       0
 * #define SQL_NULL_HDBC       0
 * #define SQL_NULL_HSTMT      0
 * #if (ODBCVER >= 0x0300)
 * #define SQL_NULL_HDESC      0
 * #endif
