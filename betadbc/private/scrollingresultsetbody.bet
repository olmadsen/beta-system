ORIGIN '../scrollingresultset';
INCLUDE 'betadbcbody';
-- scrollingResultSetFetchAbsolute: DoPart --
do
     (# rc: @shortint; pcrow: @integer
     do
        (THIS(resultSet).private.hstmt,SQL_FETCH_ABSOLUTE,index,@@ pcrow,NULL)
          ->SQLExtendedFetch->rc;
        (if rc
         // SQL_SUCCESS then
            THIS(resultSet).private.setData->result[]
         // SQL_NO_DATA then
            EOT->result[]
         // SQL_SUCCESS_WITH_INFO then
            (SQL_HANDLE_STMT,THIS(resultSet).private.hstmt)
              ->resultSetNotification
         else
            (SQL_HANDLE_STMT,THIS(resultSet).private.hstmt)->resultSetException
        if);
        INNER fetchAbsolute
     #)  

