ORIGIN '/users/kursus/dprog2/BetaDBC/betadbc';
INCLUDE '~beta/basiclib/formatio'
        '../tables';
-- program: Descriptor --
(# sqlCon: @Connection
do
   (2->arguments,3->arguments,4->arguments)->sqlCon.open;
   sqlCon.scanTables
     (# j: @integer
     do
        j+1->j;
        'Table %i\n'->putFormat (#  do j->i #);
        ' CatalogName: %s\n SchemaName: %s\n TableName: %s\n TableType: %s\n'
          ->putFormat
            (# 
            do
               current.catalogName[]->s;
               current.schemaName[]->s;
               current.tableName[]->s;
               current.tableType[]->s
            #)
     #)
#)  

