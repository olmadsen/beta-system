ORIGIN '~beta/basiclib/v1.6/betaenv';
INCLUDE '../private/sql'
        '~beta/basiclib/v1.6/formatio';
-- program: Descriptor --
(# henv,hdbc,hstmt: @integer; con,stmt: ^text; rc: @shortint
do
   (@@ henv)->SQLAllocEnv->rc;
   'rc = %i\n'->putFormat (#  do rc->i #);
   (SQL_HANDLE_DBC,henv,@@ hdbc)->SQLAllocHandle->rc;
   'rc = %i\n'->putFormat (#  do rc->i #)
#)  

