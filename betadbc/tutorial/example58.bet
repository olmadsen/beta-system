ORIGIN '../betadbc';
INCLUDE '~beta/basiclib/formatio';
-- program: Descriptor --
(#
   sqlCon: @connection;
   stmtTxt: ^text;
   stmt: @sqlCon.directSqlStatement;
   rs: ^sqlCon.resultSet
do
   (2->arguments,3->arguments,4->arguments)->sqlCon.open;
   'SELECT title from Movie WHERE title LIKE ''%%''''s%%'''->stmt;
   stmt.execute->rs[];
   'title'->putline;
   newline;
   (stmt.execute).scan
     (#  do current.s->putline #)
#)  

