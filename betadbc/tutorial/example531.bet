ORIGIN '../betadbc';
-- program: Descriptor --
(#
   sqlCon: @connection;
   stmt: @sqlCon.directSqlStatement;
   rs: ^sqlCon.resultSet;
   stmtTxt: ^text
do
   (2->arguments,3->arguments,4->arguments)->sqlCon.open;
   'UPDATE MovieExec SET name = ''Pres. '' + name\n'->stmtTxt[];
   'WHERE certNo IN (SELECT presCNo FROM Studio)'->stmtTxt.append->stmt
     ->putline;
   stmt.execute->rs[];
   'You have updated '->puttext;
   rs.rowCount->putint;
   ' row(s).'->putline;
   stmt.close;
   sqlCon.close
#)  

