ORIGIN '../betadbc';
-- program: Descriptor --
(#
   sqlCon: @connection;
   stmt: @sqlCon.directSqlStatement;
   rs: ^sqlCon.resultSet
do
   (2->arguments,3->arguments,4->arguments)->sqlCon.open;
   'DELETE FROM MovieExec WHERE netWorth < 10000000'->stmt.open;
   stmt->putline;
   stmt.execute->rs[];
   'You have deleted '->puttext;
   rs.tupleCount->putint;
   ' row(s).'->putline;
   stmt.close;
   sqlCon.close
#)  

