ORIGIN '../betadbc';
-- program: Descriptor --
(#
   sqlCon: @connection;
   stmt: @sqlCon.directSqlStatement;
   rs: ^sqlCon.resultSet;
   stmtTxt: ^text
do
   (2->arguments,3->arguments,4->arguments)->sqlCon.open;
   'INSERT INTO Studio(name) SELECT DISTINCT studioName FROM Movie\n'
     ->stmtTxt[];
   'WHERE studioName NOT IN (SELECT name FROM Studio)'->stmtTxt.append
     ->stmt.open;
   stmt->putline;
   stmt.execute->rs[];
   'You have inserted '->puttext;
   rs.tupleCount->putint;
   ' row(s).'->putline;
   stmt.close;
   sqlCon.close
#)  

