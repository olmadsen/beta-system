ORIGIN '../betadbc';
-- program: Descriptor --
(# sqlCon: @connection; stmt: @sqlCon.directSqlStatement; stmtTxt: ^text
do
   (2->arguments,3->arguments,4->arguments)->sqlCon.open;
   'SELECT DISTINCT title FROM Movie AS Old WHERE year < ANY \n'->stmtTxt[];
   '(SELECT year FROM Movie where title = Old.title)'->stmtTxt.append
     ->stmt.open;
   stmt->putline;
   newline;
   'name'->putline;
   newline;
   (stmt.execute).scan
     (#  do current.s->putline #)
#)  

