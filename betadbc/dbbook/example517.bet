ORIGIN '../betadbc';
-- program: Descriptor --
(# sqlCon: @connection; stmt: @sqlCon.directSqlStatement; stmtTxt: ^text
do
   (2->arguments,3->arguments,4->arguments)->sqlCon.open;
   'SELECT name FROM MovieExec WHERE certNo = \n'->stmtTxt[];
   '(SELECT producerCNo FROM Movie WHERE title = ''Star Wars'')'->stmtTxt.append
     ->stmt->putline;
   newline;
   'name'->putline;
   newline;
   (stmt.execute).scan
     (#  do current.s->putline #)
#)  

