ORIGIN '../betadbc';
-- program: Descriptor --
(#
   sqlCon: @connection;
   stmtTxt: ^text;
   stmt: @sqlCon.directSqlStatement;
   rs: ^sqlCon.resultSet
do
   (2->arguments,3->arguments,4->arguments)->sqlCon.open;
   'SELECT title from Movie WHERE title LIKE ''Star ____'''->stmt;
   stmt.execute->rs[];
   'title'->putline;
   newline;
   (stmt.execute).scan
     (#  do current.s->putline #)
#)  

