ORIGIN '~beta/basiclib/betaenv';
INCLUDE '../../private/patternparser'
        '../betaunit/textbetaunit';
-- program: Descriptor --
(#
   testPatternParser: testSuite
     (#
        t: ^text;
        p: @PatternParser;
        startTestCase::  (#  do &text[]->t[] #);
        start::  (#  do  #);
        end::  (#  do  #);
        testSimpleNamedResultSetPattern: testCase
          (#
             res: ^P.ResultSetPattern;
             res2: ^p.NamedResultSetPattern;
             aNamedSpecifier: ^p.NamedSpecifier
          do
             'def1%c d2:varName2'->t[];
             t.reset;
             t[]->p.parseResultSetPatternString->res[];
             (if (res## = p.NamedResultSetPattern##)->assert then
                 res[]->res2[];
                 (2 = res2.NamedSpecifiers.size)->assert;
                 T1:
                   (# thePlaceHolder: ^p.placeholder
                   do
                      (res2.namedSpecifiers.head).elm[]->aNamedSpecifier[];
                      ('def1'->aNamedSpecifier.theNameDecl.value.equalNCS)
                        ->assert;
                      (if
                      (p.PlaceHolder## = aNamedSpecifier.theUnnamedSpecifier##)
                        ->assert then
                          aNamedSpecifier.theUnnamedSpecifier[]
                            ->thePlaceholder[];
                          ('c' = thePlaceholder.mark)->assert
                      if)
                   #);
                 T2:
                   (# theVariable: ^p.Variable
                   do
                      (res2.namedSpecifiers.last).elm[]->aNamedSpecifier[];
                      ('d2'->aNamedSpecifier.theNameDecl.value.equalNCS)
                        ->assert;
                      (if
                      (p.Variable## = aNamedSpecifier.theUnnamedSpecifier##)
                        ->assert then
                          aNamedSpecifier.theUnnamedSpecifier[]->theVariable[];
                          ('varName2'->theVariable.value.equalNCS)->assert
                      if)
                   #);
                 T3:
                   (# s: ^text
                   do
                      res[]->p.unparseNode->t[];
                      t.copy->s[];
                      s.reset;
                      s[]->p.parseResultSetPatternString->res[];
                      res[]->p.unparseNode->s[];
                      (s[],t[])->assertTextEqual
                   #)
             if)
          #);
        testSimpleUnnamedResultSetPattern: testCase
          (#
             res: ^p.ResultSetPattern;
             res2: ^p.UnnamedResultSetPattern;
             anunNamedSpecifier: ^p.UnnamedSpecifier
          do
             ' %c   :varName2 '->t[];
             t.reset;
             t[]->p.parseResultSetPatternString->res[];
             (if (res## = p.UnnamedResultSetPattern##)->assert then
                 res[]->res2[];
                 (2,res2.UnnamedSpecifiers.size)->assertIntegerEqual;
                 T1:
                   (# thePlaceHolder: ^p.placeholder
                   do
                      (res2.unnamedSpecifiers.head).elm[]->anunNamedSpecifier[];
                      (if (p.PlaceHolder## = anUnnamedSpecifier##)->assert then
                          anUnnamedSpecifier[]->thePlaceholder[];
                          ('c' = thePlaceholder.mark)->assert
                      if)
                   #);
                 T2:
                   (# theVariable: ^p.Variable
                   do
                      (res2.unnamedSpecifiers.last).elm[]->anunNamedSpecifier[];
                      (if (p.Variable## = anUnnamedSpecifier##)->assert then
                          anUnnamedSpecifier[]->theVariable[];
                          ('varName2'->theVariable.value.equalNCS)->assert
                      if)
                   #);
                 T3:
                   (# s: ^text
                   do
                      res[]->p.unparseNode->t[];
                      t.copy->s[];
                      s.reset;
                      s[]->p.parseResultSetPatternString->res[];
                      res[]->p.unparseNode->s[];
                      (s[],t[])
                        ->assertTextEqual (#  do 'parseunparse'->msg #)
                   #)
             if)
          #);
        testStatementPattern: testCase
          (#
             start::  (#  do putline; false->p.theTracer #);
             end::  (#  do false->p.theTracer #);
             res: ^p.StatementPattern;
             res2: ^p.StatementSpecifier;
             anunNamedSpecifier: ^p.UnnamedSpecifier;
             t2: ^text;
             i: @integer
          do
             'xyz def\\%t \\\\ \\:foo \\var1:var v_ar2%f :var3%t %s%ty \\\\er\\r'
               ->t[];
             t.reset;
             t[]->p.parseStatementPatternString->res[];
             T1:
               (# 
               do
                  (22,res.StatementStringOrSpecifiers.size)
                    ->assertIntegerEqual (#  do 'size'->msg #)
               #);
             T2:
               (# 
               do
                  0->i;
                  res.StatementStringOrSpecifiers.scan
                    (# 
                    do
                       i+1->i;
                       (if i
                        // 1 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '1 current## = p.StatementString##'->msg
                               #)
                        // 2 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '2 current## = p.StatementString##'->msg
                               #)
                        // 3 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '3 current## = p.StatementSpecifier##'->msg
                               #)
                        // 4 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '4 current## = p.StatementSpecifier##'->msg
                               #)
                        // 5 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '5 current## = p.StatementSpecifier##'->msg
                               #)
                        // 6 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '6 current## = p.StatementString##'->msg
                               #)
                        // 7 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '7 current## = p.StatementString##'->msg
                               #)
                        // 8 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '8 current## = p.StatementString##'->msg
                               #)
                        // 9 then
                           current## = p.StatementSpecifier##
                             ->assert
                               (# 
                               do '9 current## = p.StatementSpecifier##'->msg
                               #)
                        // 10 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '10 current## = p.StatementString##'->msg
                               #)
                        // 11 then
                           current## = p.StatementSpecifier##
                             ->assert
                               (# 
                               do '11 current## = p.StatementSpecifier##'->msg
                               #)
                        // 12 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '12 current## = p.StatementString##'->msg
                               #)
                        // 13 then
                           current## = p.StatementSpecifier##
                             ->assert
                               (# 
                               do '13 current## = p.StatementSpecifier##'->msg
                               #)
                        // 14 then
                           current## = p.StatementSpecifier##
                             ->assert
                               (# 
                               do '14 current## = p.StatementSpecifier##'->msg
                               #)
                        // 15 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '15 current## = p.StatementString##'->msg
                               #)
                        // 16 then
                           current## = p.StatementSpecifier##
                             ->assert
                               (# 
                               do '16 current## = p.StatementSpecifier##'->msg
                               #)
                        // 17 then
                           current## = p.StatementSpecifier##
                             ->assert
                               (# 
                               do '17 current## = p.StatementSpecifier##'->msg
                               #)
                        // 18 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '18 current## = p.StatementString##'->msg
                               #)
                        // 19 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '19 current## = p.StatementString##'->msg
                               #)
                        // 20 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '20 current## = p.StatementString##'->msg
                               #)
                        // 21 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '21 current## = p.StatementString##'->msg
                               #)
                        // 22 then
                           current## = p.StatementString##
                             ->assert
                               (# 
                               do '18 current## = p.StatementString##'->msg
                               #)
                       if)
                    #)
               #);
             T3:
               (# 
               do
                  (res[]->p.unparsenode,
                   'xyz def%t \\ :foo var1:var v_ar2%f :var3%t %s%ty \\err')
                    ->assertTextEqual
               #)
          #)
     do
        &testSimpleNamedResultSetPattern[]->addTest;
        &testSimpleUnnamedResultSetPattern[]->addTest;
        &testStatementPattern[]->addTest
     #)
do &writingTestResult[]->testPatternParser
#)  

