ORIGIN 'betaunit';
-- lib: Attributes --
writingTestResult: testResult
  (#
     s:<
       (# theStream: ^stream
       do screen[]->theStream[]; INNER
       exit theStream[]
       #);
     addFailure::< 
       (# 
       do
          failure[]->getPatternName->(s).puttext;
          ' failed'->(s).putline;
          msg[]->(s).putline;
          INNER
       #);
     addSuccess::<  (#  do '.'->(s).puttext; INNER #)
  #)  

