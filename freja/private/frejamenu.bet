     (# 
     do
        currentPage->thePage[];
        (if (count[],list[])->DSStrGetCurGroup then
            true->isGroup;
            (count,list)->GetIntList->group;
            (for i: group.range repeat
              group[i]->theObjectList.find->theObject[];
              (if (theObject## <= thePage.UserDataNode##) or
              (theObject## <= thePage.DeletableConnector##) then
                  'Copying groups including parts of diagrams is not allowed'
                    ->alertUser;
                  leave thisOp
              if);
              
            for)
        if);
        (if isGroup then
            false->thePage.private.doOADPaste;
            menuID.EditMenu.copy->DSUIMenuExecute;
            FrejaEditMenu.iPaste.enable
         else
