ORIGIN '~beta/maclib/mactypes';
INCLUDE 'files';
INCLUDE 'processes';


BUILD
macosx '$$/signaturetoapp_ext.o' 'external/signaturetoapp_ext.c' '$CC -D$$ -c -o $0 $1'
ppcmac ':$$/signaturetoapp_ext.obj' ':external/signaturetoapp_ext.c' 'mrc -shared_lib_export on -align mac68k -o $0 $1';


BUILD 
macosx '$$/sendgeturl.o' 'external/sendgeturl.c' '$CC -D$$ -c -o $0 $1'
ppcmac ':$$/sendgeturl.obj' ':external/sendgeturl.c' 'mrc -shared_lib_export on -o $0 $1';

-- lib: attributes --


Sig2App_FindProcess: (# exit 0 #);
Sig2App_FindApplication: (# exit 1 #);
Sig2App_LaunchApplication: (# exit 2 #);


launchContinue: (# exit  0x4000 #);

ttxtSignature: (# exit 0x74747874 #); (* 'ttxt' *)
netscapeSignature: (# exit 0x4D4F5353 #); (* 'MOSS' *)
explorerSignature: (# exit 0x4D534945 #); (* 'MSIE' *)



SignatureToApp1: external
	(# sig, document, psn, fileSpec, launched, mode, launchControlFlags: @integer;
		err: @shortint;
	enter (sig, document, psn, fileSpec, launched, mode, launchControlFlags)
	do 'SignatureToApp' -> callC;
	exit err
	#);

		
SignatureToApp: external
	(# sig: @Integer;
		document: ^FSSpec;
		psn: ^ProcessSerialNumber;
		fileSpec: ^FSSpec;
		launched: ^LongInt;
		mode: @integer;
		launchControlFlags: @integer;
		err: @shortInt;
	enter (sig, document[], psn[], fileSpec[], launched[], mode, launchControlFlags)
	exit err
	#);

SendGetURL: external
	(# url: @integer;
	   url_length: @integer;
		psn: ^ProcessSerialNumber;
		err: @ShortInt;
	enter (url, url_length, psn[])
	exit err
	#);
	
