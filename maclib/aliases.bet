ORIGIN 'mactypes';

-- lib: attributes --

NewAliasMinimalFromFullPath: external
	(#	fullPathLength: @shortInt;
		fullPath: @integer;
		zoneName: [1] @char;
		serverName: [1] @char;
		alias: ^longInt;
		error: @shortInt;
	enter (fullPathLength, fullPath, zoneName, serverName, alias[])
	do '{$7009,$A823}' -> pascalTrap;
	exit error
	#);

