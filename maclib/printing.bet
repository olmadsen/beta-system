PrOpenPage: External
  (# pPrPort: @Integer; pPageFrame: ^MacRect; 
  enter (pPrPort,pPageFrame[])
  do CallPascal; 
  #);
PrClosePage: External (# pPrPort: @Integer;  enter pPrPort do CallPascal;  #);
  

