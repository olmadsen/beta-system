  (# hPrint: @Integer; Confirmed: @Boolean;  enter hPrint do CallPascal;  exit Confirmed #);
PrOpenDoc: External
  (# hPrint: @Integer; pPrPort: @Integer; pIOBuf: @Integer; TPPrPort: @Integer; 
  enter (hPrint,pPrPort,pIOBuf)
  do CallPascal; 
  exit TPPrPort
  #);
