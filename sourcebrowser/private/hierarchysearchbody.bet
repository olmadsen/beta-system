ORIGIN 'hierarchy';
  
--HIRfindFGAndSelect:dopart--
do
   items.scan
   (# pe:^propertyelement;
      hf:^hirfolder;
   do (if current##<=propertyElement## then 
          current[]->pe[];
          (if fgname[]->pe.location.equal then
              true->found;
              (*current.select*)
              current[]->grp[]
          if)
       else
          (if current##<=hirfolder## then
              current[]->hf[];
              fgname[]->hf.findFgandSelect->(found,grp[])
          if)
      if)
   #)
