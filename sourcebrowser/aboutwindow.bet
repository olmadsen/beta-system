ORIGIN '~beta/guienv/guienv';
INCLUDE '~beta/guienv/controls'
        '~beta/guienv/graphics';
-- guienvLib: Attributes --
aboutwindow: window
  (#
     pm: @pixmap;
     open::< 
       (# 
       do
          (360,215)->size;
          'mjolner'->pm.read;
          statictext1.open;
          ok.open;
          statictext2.open;
          statictext3.open;
          INNER ;
          
       #);
     statictext1: @statictext
       (#
          open::< 
            (# 
            do
               'Mjolner Version 5.0'->label;
               (135,30)->size;
               (180,25)->position;
               INNER
            #)
       #);
     OK: @pushbutton
       (#
          open::<  (#  do (60,20)->size; (255,175)->position; INNER #);
          eventhandler:: 
            (# onMouseUp::  (#  do THIS(window).close #);  #);
          
       #);
     eventhandler:: 
       (#
          onRefresh:: 
            (# 
            do
               (contents).graphics
                 (# 
                 do (pm[],(0,0),(20,20),pm.width,pm.height)->drawraster
                 #)
            #);
          
       #);
     statictext2: @statictext
       (#
          open::< 
            (# 
            do
               'Mjolner Informatics'->label;
               (140,25)->size;
               (180,70)->position;
               INNER
            #)
       #);
     statictext3: @statictext
       (#
          open::< 
            (# 
            do
               '8200 Aarhus N'->label;
               (120,20)->size;
               (180,100)->position;
               INNER
            #)
       #)
  #)  

