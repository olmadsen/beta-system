ORIGIN '~beta/guienv/v1.4/guienv';
INCLUDE '../private/depGraph'

--- program: descriptor ---
guienv
(# AST: @astInterface;
     fg: ^ast.fragmentGroup;
     fgWindow: @graphBrowser
     (# fgChanged:: (# do location[]->putline #);
        eventhandler::
          (# onAboutToClose:: (# do true->okToClose; terminate #) #)
     #);
   filename: ^text;
   
  do ('',true)->ast.init;
   
     (if noOfArguments > 1 then
	 2 -> arguments -> filename[]
      else 'depGraphTest'->filename[]
     if);
   
     fgWindow.open;
   
     (filename[],ast[])->fgWindow.display;
          
  #)
